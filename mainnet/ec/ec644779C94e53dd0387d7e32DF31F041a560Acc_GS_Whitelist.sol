pragma solidity ^0.8.10;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
//import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./AddressToUintMap.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";

contract GS_Whitelist is Ownable {
    using EnumerableMap for EnumerableMap.AddressToUintMap;
    using EnumerableSet for EnumerableSet.AddressSet;

    uint public WL_nodeLimit = 5;
    uint public WL_nodePrice = 250 * 10 ** 6;
    bool WL_limited = true;
    bool WL_open = false;

    address payable WL_fundsWallet = payable(0x72927a3C16A636f5dFB7bED20d27F4C01d531E36);

    EnumerableMap.AddressToUintMap private WL_nodes;
    EnumerableSet.AddressSet private WL_access;

    IERC20 token;

    event NodesPurchased(address indexed purchaser, uint indexed amountPurchased);

    modifier onlyWhitelist(address wallet) {
        require(WL_open, 'Whitelist not yet open');
        require(WL_limited == false || WL_access.contains(wallet), 'Wallet is not whitelisted');
        _;
    }

    modifier onlyGuard(address requester) {
        require(requester == owner(), 'NO ACCESS');
        _;
    }

    constructor(address _token) Ownable() {
        token = IERC20(_token);
    }

    function addToWhitelistAccess(address addressToAdd) onlyOwner external {
        require(!WL_access.contains(addressToAdd), 'Already on the access list');
        WL_access.add(addressToAdd);
    }

    function addManyToWhitelistAccess(address[] memory addressesToAdd) onlyOwner external {
        for (uint i = 0; i < addressesToAdd.length; i++) {
            if (!WL_access.contains(addressesToAdd[i])) {
                WL_access.add(addressesToAdd[i]);
            }
        }
    }

    function getWhitelistAddresses() external view onlyOwner returns (bytes32[] memory) {
        bytes32[] memory addressList = WL_access._inner._values;
        return addressList;
    }

    //    Remove a wallet from accessing the whitelist
    function removeFromWhitelistAccess(address addressToRemove) onlyOwner external {
        require(WL_access.contains(addressToRemove), 'Not on the list');
        WL_access.remove(addressToRemove);
    }

    function buyWhitelistNode(uint buyAmount) onlyWhitelist(msg.sender) external {
        require(buyAmount > 0 && buyAmount <= 5, 'Invalid buy amount');
        require(_withinNodeLimit(msg.sender, buyAmount), 'You are going over the node limit, leave some for others');
        uint totalPrice = WL_nodePrice * buyAmount;

        require(token.transferFrom(msg.sender, WL_fundsWallet, totalPrice), 'Transfer failed');

        uint currentAmount = 0;
        if (WL_nodes.contains(msg.sender)) {
            currentAmount = WL_nodes.get(msg.sender);
        }

        WL_nodes.set(msg.sender, currentAmount + buyAmount);

        emit NodesPurchased(msg.sender, buyAmount);
    }

    function _withinNodeLimit(address wallet, uint nodesRequested) private view returns (bool) {
        return _getNodesForAddress(wallet) + nodesRequested <= 5;
    }

    function extractNodeOwners() onlyGuard(msg.sender) internal view returns (EnumerableMap.AddressToUintMap storage) {
        return WL_nodes;
    }

    function getNodeOwnerWallets() onlyOwner external view returns (address[] memory) {
        uint256 nodeOwnersSize = uint256(WL_nodes.length());
        address[] memory nodeOwnerWallets = new address[](nodeOwnersSize);

        for (uint256 i = 0; i < nodeOwnersSize; i++) {
            (address wallet, uint256 _nodeCount) = WL_nodes.at(i);
            nodeOwnerWallets[i] = wallet;
        }
        return nodeOwnerWallets;
    }

    function getNodesForWallet(address wallet) onlyOwner external view returns (uint) {
        require(wallet != address(0));
        require(WL_nodes.contains(wallet), 'Wallet does not exists');
        return WL_nodes.get(wallet);
    }

    function setWLLimited(bool limited) onlyOwner external returns (bool) {
        WL_limited = limited;
        return WL_limited;
    }

    function setWLOpen(bool _open) onlyOwner external returns (bool) {
        WL_open = _open;
        return WL_open;
    }

    function updateToken(address _token) onlyOwner external {
        token = IERC20(_token);
    }

    function getNodeCount() external view returns (uint) {
        return _getNodesForAddress(msg.sender);
    }

    function updateWallet(address _WL_fundsWallet) external onlyOwner {
        WL_fundsWallet = payable(_WL_fundsWallet);
    }

    function _getNodesForAddress(address wallet) internal view returns (uint) {
        if (!WL_nodes.contains(wallet)) {
            return 0;
        } else {
            return WL_nodes.get(wallet);
        }
    }

}

