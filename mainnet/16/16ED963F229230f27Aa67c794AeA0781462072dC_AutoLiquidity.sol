pragma solidity ^0.8.9;





contract AutoLiquidity is Ownable {
    address public tokenAddress;

    address public wNativeAddress;

    address public usdAddress;

    address public routerAddress;

    uint256 slippageFactor = 950; // 5% default slippage tolerance

    address[] public usdToWnativePath;

    constructor(
        address _tokenAddress,
        address _wNativeAddress,
        address _usdAddress,
        address _router
    ) {
        tokenAddress = _tokenAddress;
        wNativeAddress = _wNativeAddress;
        usdAddress = _usdAddress;
        routerAddress = _router;
        usdToWnativePath = [address(usdAddress), address(wNativeAddress)];
        _allowance();
    }

    /// External functions ///
    receive() external payable {}

    function withdrawProjectLiquidity() external onlyOwner {
        IERC20(usdAddress).transfer(msg.sender, IERC20(usdAddress).balanceOf(address(this)));
        IERC20(tokenAddress).transfer(msg.sender, IERC20(tokenAddress).balanceOf(address(this)));
    }

    function withdrawETHLiquidity() external onlyOwner {
        uint256 etherBalance = address(this).balance;
        payable(msg.sender).transfer(etherBalance);
    }

    function withdrawAllLiquidity() external onlyOwner {
        uint256 etherBalance = address(this).balance;
        payable(msg.sender).transfer(etherBalance);
        IERC20(usdAddress).transfer(msg.sender, IERC20(usdAddress).balanceOf(address(this)));
        IERC20(tokenAddress).transfer(msg.sender, IERC20(tokenAddress).balanceOf(address(this)));
    }

    function execute() external onlyOwner {
        uint256 usdcBalanceBefore = IERC20(usdAddress).balanceOf(address(this));

        _safeSwapWnative(
            usdcBalanceBefore / 2,
            usdToWnativePath,
            address(this)
        );

        uint256 usdBalanceAfter = IERC20(usdAddress).balanceOf(address(this));
        uint256 wNativeBalance = address(this).balance;
        uint256 tokenBalance = IERC20(tokenAddress).balanceOf(address(this));
        uint256 halfTokenAmount = tokenBalance / 2;

        if (usdBalanceAfter > 0 && wNativeBalance > 0 && tokenBalance > 0) {
            // add the liquidity token-wNative
            IJoeRouter(routerAddress).addLiquidityAVAX{value: wNativeBalance}(
                address(tokenAddress),
                halfTokenAmount,
                0,
                0,
                msg.sender,
                block.timestamp
            );

            // add the liquidity token-usdc
            IJoeRouter(routerAddress).addLiquidity(
                address(tokenAddress),
                address(usdAddress),
                halfTokenAmount,
                usdBalanceAfter,
                0,
                0,
                msg.sender,
                block.timestamp
            );
        }
    }

    /// Internal functions ///
    function _safeSwapWnative (
        uint256 amountIn,
        address[] memory path,
        address to
    ) internal {
        uint256[] memory amounts = IJoeRouter(routerAddress).getAmountsOut(amountIn, path);
        uint256 amountOut = amounts[amounts.length - 1 ];

        IJoeRouter(routerAddress).swapExactTokensForAVAX(
            amountIn,
            (amountOut * slippageFactor  / 1000),
            path,
            to,
            block.timestamp
        );
    }

    function _allowance() internal {
        IERC20(usdAddress).approve(address(routerAddress), type(uint256).max);
        IERC20(tokenAddress).approve(address(routerAddress), type(uint256).max);
    }
}