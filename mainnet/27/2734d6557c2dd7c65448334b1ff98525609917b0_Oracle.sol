contract ICO is Operator {
  ERC20 token;
  ERC20 usdc;

  // 0.1 per USDC, or 1 per 10 USDC
  uint256 public RATE = 100000;

  /// @notice A modifier that throws if the ICO is not active.
  /// @dev Operator can end ICO by withdrawing tokens.
  modifier icoactive {
    require(token.balanceOf(address(this)) > 0, "bro party's over");
    _;
  }

  event TokenBought(address sender, uint256 amount, uint256 remaining);

  constructor(
    address _tokenAddr,
    address _usdcAddr,
    uint256 _rate
  ) {
    token = ERC20(_tokenAddr);
    usdc = ERC20(_usdcAddr);
    RATE = _rate;
  }

  /// @notice Withdraw the ICO's native balance to an address.
  function withdrawNative(address account, uint256 amount) public onlyOperator {
    require(address(this).balance > 0, "I'm broke m8");
    // send money
    payable(account).transfer(amount);
  }

  /// @notice Withdraw the ICO's tokens to an address.
  function withdrawTokens(address account, uint256 amount) public onlyOperator {
    uint256 bal = token.balanceOf(address(this));

    require(bal >= amount, "lol im broke");

    // send money
    token.transfer(account, amount);
  }

  /// @notice Buy some tokens.
  function buy(uint256 amount) external payable icoactive {
    require(usdc.allowance(msg.sender, address(this)) >= amount, "Not enough allowance");
    require(usdc.balanceOf(msg.sender) >= amount, "Not enough balance");

    _buy(amount, msg.sender);
  }

  /// @dev An internal function with buy logic.
  /// @param value The amount of ETH being used.
  /// @param sender The address who will get the tokens.
  function _buy(uint256 value, address sender) internal {
    uint256 _amount = _getTokenAmount(value);
    uint256 bal = token.balanceOf(address(this));

    emit TokenBought(sender, _amount, bal - _amount);

    usdc.transferFrom(sender, address(this), value);
    token.transfer(sender, _amount);
  }

