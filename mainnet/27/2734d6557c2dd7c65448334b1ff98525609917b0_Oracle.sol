  /// @param _weiAmount The amount of Ether (in wei) being spent
  /// @return uint256 The amount of tokens to give
  function _getTokenAmount(uint256 _weiAmount) internal view returns (uint256) {
    return (_weiAmount / RATE) * 10e18;
  }
}

