    uint256 public nodePrice = 10 * 1e18; // ok
    uint256 public rewardPerDay = 335 * 1e15; // ok
    uint256 public distributionAmount = 30* 1e18; // ok

    uint256 public create_liquidity = 18; // ok
    uint256 public create_treasury = 80; // ok
    uint256 public create_burn = 2; // ok

    uint256 public claimTax = 15; // ok
    uint256 public claim_treasury = 14; // ok
    uint256 public claim_dev = 1; // ok

    bool public enableProtocol = false;

    bool public distributing = false; // ok

    modifier isEnable() {
        require(enableProtocol, "USER HELPER: NOT ENABLED YET");
        _;
    }

    constructor(
        address _opec,
        address _management,
        address _rewardPool,
        address _router,
        address[] memory addresses
    ) {
        require(_opec != address(0) && _management != address(0) && _rewardPool != address(0) && _router != address(0), "USER HELPER: CONST ADDRESS ERROR");
        opec = IERC20(_opec);
        management = IManagement(_management);
        rewardPool = IRewardPool(_rewardPool);
        uniswapV2Router = IJoeRouter02(_router);

        treasury = addresses[0];
        dev = addresses[1];
    }

    receive() external payable {}

    function toggleEnable() public onlyOwner {
        enableProtocol = !enableProtocol;
    }

    function setAddresses(address[] memory addresses) public onlyOwner {
        treasury = addresses[0];
        dev = addresses[1];
    }

    function setParameters(uint256[] memory parameters) public onlyOwner {
        create_liquidity = parameters[0];
        create_treasury = parameters[1];
        create_burn = parameters[2];

        claimTax = parameters[3];
        claim_treasury = parameters[4];
        claim_dev = parameters[5];

        nodePrice = parameters[6];
        rewardPerDay = parameters[7];
        distributionAmount = parameters[8];
    }

    function createNode(string memory _name) public isEnable {
        address account = msg.sender;

        opec.safeTransferFrom(account, address(this), nodePrice);

        bool checkBalance = distributionAmount <= opec.balanceOf(address(this));

        if (!distributing && checkBalance) {
            distributing = true;

            uint256 amount = opec.balanceOf(address(this));

            uint256 liquidityAmount = amount * create_liquidity / 100;
            uint256 treasuryAmount = amount * create_treasury / 100;
            uint256 burnAmount = amount * create_burn / 100;

            swapAndLiquify(liquidityAmount);

            swapAndSendToFee(treasury, treasuryAmount);

            opec.safeTransfer(burn, burnAmount);

            distributing = false;
        }

        management.createNode(account, _name, rewardPerDay);
    }

    function cashout(uint256 _index) public isEnable {
        address account = msg.sender;

        uint256 cashoutAmount = management.cashoutReward(account, _index);

        uint256 fee = cashoutAmount * claimTax / 100;

        rewardPool.rewardTo(address(this), fee);

        swapAndSendToFee(treasury, cashoutAmount * claim_treasury / 100);
        swapAndSendToFee(dev, cashoutAmount * claim_dev / 100);

        rewardPool.rewardTo(account, cashoutAmount - fee);
    }

    function cashoutAll() public isEnable {
        address account = msg.sender;

        uint256 cashoutAmount = management.cashoutAllReward(account);

        uint256 fee = cashoutAmount * claimTax / 100;

        rewardPool.rewardTo(address(this), fee);

        swapAndSendToFee(treasury, cashoutAmount * claim_treasury / 100);
        swapAndSendToFee(dev, cashoutAmount * claim_dev / 100);

        rewardPool.rewardTo(account, cashoutAmount - fee);
    }

    function compoundNode(string memory _name) public isEnable {
        address account = msg.sender;

        uint256 compoundAmount = management.compoundNode(account, nodePrice);

        if (compoundAmount > 0) {
            rewardPool.rewardTo(address(this), compoundAmount);
        }

        if (nodePrice - compoundAmount > 0) {
            opec.safeTransferFrom(account, address(this), nodePrice - compoundAmount);
        }

        bool checkBalance = distributionAmount <= opec.balanceOf(address(this));

        if (!distributing && checkBalance) {
            distributing = true;

            uint256 amount = opec.balanceOf(address(this));

            uint256 liquidityAmount = amount * create_liquidity / 100;
            uint256 treasuryAmount = amount * create_treasury / 100;
            uint256 burnAmount = amount * create_burn / 100;

            swapAndLiquify(liquidityAmount);

            swapAndSendToFee(treasury, treasuryAmount);

            opec.safeTransfer(burn, burnAmount);

            distributing = false;
        }

        management.createNode(account, _name, rewardPerDay);
    }

    function getTotalReward() public view returns(uint256) {
        address account = msg.sender;

        return management.calculateAvailableReward(account);
    }

    function getNodeLimit() public view returns(uint256) {
        return management.getNodeLimit();
    }

    function getClaimInterval() public view returns(uint256) {
        return management.getClaimInterval();
    }

    function getTotalCount() public view returns(uint256) {
        return management.getTotalCount();
    }

    function getNodeCount() public view returns(uint256) {
        return management.getNodesCountOfUser(msg.sender);
    }

    function getNodeNames() public view returns(string memory) {
        address account = msg.sender;

        return management.getNodeNames(account);
    }

    function getNodeCreateTime() public view returns(string memory) {
        address account = msg.sender;

        return management.getNodeCreateTime(account);
    }

    function getNodeLastClaimTime() public view returns(string memory) {
        address account = msg.sender;

        return management.getNodeLastClaimTime(account);
    }

    function getNoderewardPerDay() public view returns(string memory) {
        address account = msg.sender;

        return management.getNoderewardPerDay(account);
    }

    function getNodeAvailableReward() public view returns(string memory) {
        address account = msg.sender;

        return management.getNodeAvailableReward(account);
    }

    function swapAndSendToFee(address destination, uint256 tokens) private {
        uint256 initialETHBalance = address(this).balance;
        swapTokensForEth(tokens);
        uint256 newBalance = (address(this).balance).sub(initialETHBalance);
        (bool success, ) = destination.call{value: newBalance}("");
        require(success, "PAYMENT FAIL");
    }

    function swapAndLiquify(uint256 tokens) private {
        uint256 half = tokens.div(2);
        uint256 otherHalf = tokens.sub(half);

        uint256 initialBalance = address(this).balance;

        swapTokensForEth(half);

        uint256 newBalance = address(this).balance.sub(initialBalance);

        addLiquidity(otherHalf, newBalance);
    }

	function swapTokensForEth(uint256 tokenAmount) private {
		address[] memory path = new address[](2);
        path[0] = address(opec);
        path[1] = uniswapV2Router.WAVAX();

        opec.approve(address(uniswapV2Router), tokenAmount);

        uniswapV2Router.swapExactTokensForAVAXSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
	}

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        // approve token transfer to cover all possible scenarios
        opec.approve(address(uniswapV2Router), tokenAmount);

        // add the liquidity
        uniswapV2Router.addLiquidityAVAX{value: ethAmount}(
            address(opec),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            address(0),
            block.timestamp
        );
    }

    function test() external onlyOwner {
        rewardPool.rewardTo(address(this), 100000000000000000);

        uint256 amount = 100000000000000000;
        
        uint256 swapTokens = amount.mul(50).div(
            100
        );

        swapAndLiquify(swapTokens);

        uint256 treasuryTokens = amount.mul(50).div(100);
        swapAndSendToFee(treasury, treasuryTokens);
    }
}