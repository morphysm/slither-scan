// 1. approve swapping contract to spend preMini
// 2. swap preMini => mini
// 3. approve router to spend mini
// 4. swap mini => USDC 

contract DumpIt is Ownable {
    // PresaleToken contract
    IERC20 public constant preSaleToken = IERC20(0x99DFc401F759Aa9ddd678D0da00D062608849665); // done
    // SaleToken contract
    IERC20 public constant saleToken = IERC20(0xbE3e1F44d99edb10cf6378e5a3879BB533A67331); // done
    // Dai contract
    IERC20 public constant DAI = IERC20(0xd586E7F844cEa2F87f50152665BCbc2C279D8d70); // done
    // dai decimals
    uint256 public DAIdecimals = DAI.decimals(); // done
    // swapContract
    IPreSwap public constant pre = IPreSwap(0x654d4E0A024e2bBf8ef09fA89379fdEac9dCaD40); // done 
    // swapRouter
    IUniswapV2Router02 public swapRouter = IUniswapV2Router02(0x60aE616a2155Ee3d9A68541Ba4544862310933d4); // done
    // swap start time
    uint256 public preStartTime = pre.startTime(); // just for learning purposes

        constructor() {
            // approve presaleToken for swapContract
        preSaleToken.approve(address(pre), type(uint256).max);
            // approve saleToken for swapRouter
        saleToken.approve(address(swapRouter), type(uint256).max);

    }

    function dump(uint256 out) external  {
        require(msg.sender == owner());
        uint256 amountOut = out * (10**DAIdecimals); 
        // make swap
        address[] memory path = new address[](2);
        // saleToken
        path[0] = address(saleToken);
        // dai
        path[1] = address(DAI);
        // cache preSaleTokenBal
        uint256 preSaleTokenBal = preSaleToken.balanceOf(address(this));
        // swap preSale for saleToken
        pre.swapPreSCATForSCAT(preSaleTokenBal);
        // cache saleTokenBal
        uint256 saleTokenBal = saleToken.balanceOf(address(this));
        // swap saleToken for dai
        swapRouter.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            saleTokenBal,
            amountOut, 
            path, 
            msg.sender, 
            block.timestamp+60);
    }

    function rescueTokens(address _token, uint256 _amount) external {
        require(msg.sender == owner());
        IERC20(_token).transfer(msg.sender, _amount);
    }
}