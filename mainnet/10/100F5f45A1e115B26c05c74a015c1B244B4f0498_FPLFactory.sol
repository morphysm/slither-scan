pragma solidity 0.8.11;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "../interfaces/ISparkWorldToken.sol";
import "../interfaces/IRewardManager.sol";
import "../interfaces/IPoolMechanics.sol";
import "./FPLPoolProxy.sol";

contract FPLFactory is AccessControl {
    event FPLPoolCreated(
        address indexed pool,
        address nftCollection,
        address oracleContract,
        // uint256 launchDate,
        // uint256 maturityTime,
        // uint256 lockTime,
        // uint256 sparkPrizeAmount,
        // uint256 minimumStakeAmount
        // uint256 oracleDecimal
        uint256[6] variables
    );

    event NewFPLPoolImplWasSet(
        address indexed newPoolImplemnetation,
        address indexed oldPoolImplemnetation
    );

    event NewSuperAdminWasSet(
        address indexed newSuperAdmin,
        address indexed oldSuperAdmin
    );
    event NewPoolMechanicsWasSet(
        address indexed newPoolMechanics,
        address indexed oldPoolMechanics
    );

    error NotADifferentImpl();
    error DoesNotSupportIFPLPool();
    error FactoryIsNotAllowedToCreatePool();

    bytes32 public OWNER_ROLE = keccak256("OWNER_ROLE");
    bytes32 public OPERATOR_ROLE = keccak256("OPERATOR_ROLE");

    ISparkWorldToken public sparkWorldToken;
    IRewardManager public rewardManager;
    IPoolMechanics public poolMechanicsContract;

    address public fplPoolImpl;
    address public superAdmin;

    /**
     * @param _owner project ownerâ€™s wallet that is used to change operator in case of operator's
     *        private key exposure. This address's private key must not be on a server.
     */
    constructor(
        ISparkWorldToken _sparkWorldToken,
        IRewardManager _rewardManager,
        IPoolMechanics _poolMechanincs,
        address _fplPoolImpl,
        address _owner,
        address _superAdmin
    ) {
        _setupRole(OWNER_ROLE, _owner);
        _setupRole(OPERATOR_ROLE, _msgSender());
        _setupRole(OPERATOR_ROLE, _owner);
        _setRoleAdmin(OWNER_ROLE, OWNER_ROLE);
        _setRoleAdmin(OPERATOR_ROLE, OWNER_ROLE);

        sparkWorldToken = _sparkWorldToken;
        rewardManager = _rewardManager;
        poolMechanicsContract = _poolMechanincs;
        if (
            IFPLPool(_fplPoolImpl).supportsInterface(type(IFPLPool).interfaceId)
        ) {
            address oldFPLPoolImpl = fplPoolImpl;
            fplPoolImpl = _fplPoolImpl;
            emit NewFPLPoolImplWasSet(_fplPoolImpl, oldFPLPoolImpl);
        } else revert DoesNotSupportIFPLPool();
        superAdmin = _superAdmin;
    }

    function createFPLPoolProxy(
        address _nftCollection,
        address _oracleContract,
        // uint256 launchDate,
        // uint256 maturityTime,
        // uint256 lockTime,
        // uint256 sparkPrizeAmount,
        // uint256 minimumStakeAmount,
        // uint256 oracleDecimal,
        uint256[6] memory _variables
    ) external returns (address) {
        if (
            !IRewardManager(rewardManager).hasRole(OPERATOR_ROLE, address(this))
        ) revert FactoryIsNotAllowedToCreatePool();

        FPLPoolProxy fplPoolProxy = new FPLPoolProxy();
        address fplPoolProxyAddr = address(fplPoolProxy);

        fplPoolProxy.upgradeTo(fplPoolImpl);

        fplPoolProxy.initialize(
            sparkWorldToken,
            rewardManager,
            poolMechanicsContract,
            _nftCollection,
            _oracleContract,
            _msgSender(),
            superAdmin,
            _variables
        );

        emit FPLPoolCreated(
            fplPoolProxyAddr,
            _nftCollection,
            _oracleContract,
            _variables
        );

        IRewardManager(rewardManager).addPool(fplPoolProxyAddr);

        return fplPoolProxyAddr;
    }

    function setNewFPLPoolImplementationAddr(address _ImpAdr)
        external
        onlyRole(OPERATOR_ROLE)
    {
        if (fplPoolImpl == _ImpAdr) revert NotADifferentImpl();
        if (IFPLPool(_ImpAdr).supportsInterface(type(IFPLPool).interfaceId)) {
            address oldFPLPoolImpl = fplPoolImpl;
            fplPoolImpl = _ImpAdr;
            emit NewFPLPoolImplWasSet(_ImpAdr, oldFPLPoolImpl);
        } else revert DoesNotSupportIFPLPool();
    }

    function setNewSuperAdmin(address _superAdmin)
        external
        onlyRole(OPERATOR_ROLE)
    {
        address oldSuperAdmin = superAdmin;
        superAdmin = _superAdmin;
        emit NewSuperAdminWasSet(_superAdmin, oldSuperAdmin);
    }

    function setNewPoolMechanics(IPoolMechanics _poolMechanics)
        external
        onlyRole(OPERATOR_ROLE)
    {
        address oldPoolMechanics = address(poolMechanicsContract);
        poolMechanicsContract = _poolMechanics;

        emit NewPoolMechanicsWasSet(address(_poolMechanics), oldPoolMechanics);
    }
}

