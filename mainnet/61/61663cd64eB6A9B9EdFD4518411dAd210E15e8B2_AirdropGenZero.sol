pragma solidity ^0.8.4;



contract Crop is ERC20("Crop", "CROP"), Ownable {
    uint256 public constant ONE_CROP = 1e18;

    address public barnAddress;
    address public farmAddress;
    address public locustGodAddress;
    address public farmersAddress;
    address public upgradeAddress;

    bool public promotionalCropMinted = false;
    bool public avaxLPCropMinted = false;
    bool public milkLPCropMinted = false;

    // ADMIN

    /**
     * farm yields crop
     */
    function setFarmAddress(address _farmAddress) external onlyOwner {
        farmAddress = _farmAddress;
    }

    function setBarnAddress(address _barnAddress) external onlyOwner {
        barnAddress = _barnAddress;
    }

    function setUpgradeAddress(address _upgradeAddress) external onlyOwner {
        upgradeAddress = _upgradeAddress;
    }

    function setLocustGodAddress(address _locustGodAddress) external onlyOwner {
        locustGodAddress = _locustGodAddress;
    }

    function __mint(address _to, uint256 _amount) external onlyOwner {
        _mint(_to, _amount);
    }


    /**
     * farmers consumes crop
     * farmers address can only be set once
     */
    function setFarmerAddress(address _farmersAddress) external onlyOwner {
        require(address(farmersAddress) == address(0), "farmers address already set");
        farmersAddress = _farmersAddress;
    }

    // external

    function mint(address _to, uint256 _amount) external {
        require(farmAddress != address(0) && farmersAddress != address(0) && barnAddress != address(0) && upgradeAddress != address(0) && locustGodAddress != address(0), "missing initial requirements");
        require(_msgSender() == farmAddress,"msgsender does not have permission");
        _mint(_to, _amount);
    }

    function burn(address _from, uint256 _amount) external {
        require(farmersAddress != address(0) && barnAddress != address(0) && upgradeAddress != address(0) && locustGodAddress != address(0), "missing initial requirements");
        require(
            _msgSender() == farmersAddress 
            || _msgSender() == barnAddress 
            || _msgSender() == upgradeAddress
            || _msgSender() == locustGodAddress,
            "msgsender does not have permission"
        );
        _burn(_from, _amount);
    }

    function transferToBarn(address _from, uint256 _amount) external {
        require(barnAddress != address(0), "missing initial requirements");
        require(_msgSender() == barnAddress, "only the barn contract can call transferToBarn");
        _transfer(_from, barnAddress, _amount);
    }

    function transferForUpgradesFees(address _from, uint256 _amount) external {
        require(upgradeAddress != address(0), "missing initial requirements");
        require(_msgSender() == upgradeAddress, "only the upgrade contract can call transferForUpgradesFees");
        _transfer(_from, upgradeAddress, _amount);
    }
}
