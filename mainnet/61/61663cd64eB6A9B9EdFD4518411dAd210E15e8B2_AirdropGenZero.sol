pragma solidity ^0.8.4;
//SPDX-License-Identifier: MIT


contract AirdropGenZero is Ownable {

    Farmer public farmer;

    constructor(Farmer _farm) {
        farmer = _farm;
    }

    function airdropToGenZero(uint256 startIndex, uint256 endIndex, uint256 userValue) payable external {
        require (endIndex >= startIndex, "invalid given index");
        while (startIndex <= endIndex)  {
            address userAddress = farmer.ownerOf(startIndex);
            if (farmer.tokenGen(startIndex) == 0) { // GEN0 
                // if it is gen0, pay the user otherwise skip and return balance to sender.
                payable(userAddress).transfer(userValue);
            }
            startIndex = startIndex + 1;
        }
        // Transfer back any remaining balance.
        payable(msg.sender).transfer(address(this).balance);
    }
}