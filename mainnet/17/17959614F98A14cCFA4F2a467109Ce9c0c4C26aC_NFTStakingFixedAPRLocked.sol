pragma solidity 0.8.14;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./ERC721Min.sol";
import "hardhat/console.sol";

contract NFTStakingFixedAPRLocked is Ownable {
    struct UserInfo {
        uint256 tokenAmount; // How many Staking Tokens the user has provided.
        uint256 nftAmount; // Number of NFTs staked
        uint256 rewardDebt; // Reward debt.
        uint256 stakeTimestamp; // Last stake time. Resets locking period for user
        uint256 lastRewardBlock;
    }

    address private constant DEAD_ADDRESS =
        0x000000000000000000000000000000000000dEaD;

    bool public stakingEnabled;
    address public immutable rewardToken; // Reward ERC-20 Token.
    address public immutable stakingNFT; // Staking NFT
    address public immutable stakingToken; // Staking ERC20 token
    address public treasuryAddress;
    address public stabilatorAddress; // address to send Stabilator unstake fees
    uint256 public stakingTokenPerNFT; // Number of staking tokens required per NFT staked
    uint256 public blocksPerDay = 28700;
    uint256 public stakeLockPeriod; // Period to lock stake; 0 for no lock; used to calc tokens per minute for fixed apr
    uint256 public maxNFTStake; // Max total amount to stake
    uint256 public maxNFTStakePerUser = 20; //basically, unlimited
    uint256 public totalReward; // Total pool rewards
    uint256 public rewardPerBlockPerNFT; // Tokens distributed per block * 1e12
    uint256 public totalNFTStaked;
    uint256 public totalTokensStaked;
    uint256 public unstakeFeeStabilator; // 500 = 5%
    uint256 public unstakeFeeRewards; // 500 = 5%
    uint256 public unstakeFeeBurn; // 500 = 5%
    uint256 public endBlock;

    mapping(address => UserInfo) public userInfos; // User stake
    mapping(address => uint256[]) public stakedNFTsByUser;

    event Claim(address indexed user, uint256 indexed pending);
    event Deposit(
        address indexed user,
        uint256 indexed numNFTs,
        uint256 indexed tokenAmount
    );
    event EmergencyWithdraw(
        address indexed user,
        uint256 indexed nftAmount,
        uint256 indexed tokenAmount
    );
    event Withdraw(
        address indexed user,
        uint256 indexed nftAmount,
        uint256 indexed tokenAmount
    );

    modifier stakingNotStarted() {
        require(totalNFTStaked == 0, "STAKING_STARTED");
        _;
    }

    // _stakeLockPeriod is days
    constructor(
        address _rewardToken,
        address _stakingToken,
        address _treasuryAddress,
        uint256 _stakeLockPeriod,
        uint256 _rewardPerDayPerNFT,
        uint256 _maxNFTStake,
        uint256 _totalReward,
        address _stakingNFT,
        uint256 _stakingTokenPerNFT
    ) {
        require(address(_rewardToken) != address(0), "REWARD_TOKEN_ADDRESS_0");
        rewardToken = _rewardToken;
        stakingToken = _stakingToken;
        treasuryAddress = _treasuryAddress;
        stakeLockPeriod = _stakeLockPeriod * 1 days;
        maxNFTStake = _maxNFTStake;
        totalReward = _totalReward;
        rewardPerBlockPerNFT = (_rewardPerDayPerNFT * 1e12) / blocksPerDay;
        stakingNFT = _stakingNFT;
        stakingTokenPerNFT = _stakingTokenPerNFT;
        endBlock = block.number + blocksPerDay * 730;
    }

    function deposit(uint256[] calldata nftList) external {
        require(stakingEnabled, "STAKING_DISABLED");
        require(block.number < endBlock, "AFTER_ENDBLOCK");
        require(totalNFTStaked + nftList.length <= maxNFTStake, "MAX_STAKED");
        require(
            nftList.length + stakedNFTsByUser[_msgSender()].length <=
                maxNFTStakePerUser,
            "MAX_USER_NFT_STAKED"
        );
        UserInfo storage user = userInfos[_msgSender()];
        if (user.nftAmount > 0) claim();
        if (nftList.length == 0) return;

        // confirm sender owns NFTs
        require(
            ERC721Min(stakingNFT).isOwnerOf(_msgSender(), nftList),
            "NOT_NFT_OWNER"
        );

        ERC721Min(stakingNFT).batchTransferFrom(
            _msgSender(),
            address(this),
            nftList
        );

        for (uint256 x; x < nftList.length; x++) {
            stakedNFTsByUser[_msgSender()].push(nftList[x]);
        }

        uint256 tokenAmt = nftList.length * stakingTokenPerNFT;
        IERC20(stakingToken).transferFrom(
            address(_msgSender()),
            address(this),
            tokenAmt
        );
        user.nftAmount += nftList.length;
        user.tokenAmount += tokenAmt;
        user.stakeTimestamp = block.timestamp;
        user.lastRewardBlock = block.number;
        totalTokensStaked += tokenAmt;
        totalNFTStaked += nftList.length;
        emit Deposit(_msgSender(), nftList.length, tokenAmt);
    }

    // Withdraw without caring about rewards. EMERGENCY ONLY.
    function emergencyWithdraw() external {
        UserInfo storage user = userInfos[_msgSender()];
        require(user.nftAmount > 0, "NO_STAKE");
        uint256 tokenAmount = user.tokenAmount;
        uint256 nftAmount = user.nftAmount;
        user.nftAmount = 0;
        user.tokenAmount = 0;
        user.stakeTimestamp = 0;
        user.rewardDebt = 0;
        totalTokensStaked -= tokenAmount;
        totalNFTStaked -= nftAmount;
        delete stakedNFTsByUser[_msgSender()];
        ERC721Min(stakingNFT).batchTransferFrom(
            address(this),
            _msgSender(),
            stakedNFTsByUser[_msgSender()]
        );
        IERC20(stakingToken).transfer(address(_msgSender()), tokenAmount);
        emit EmergencyWithdraw(_msgSender(), nftAmount, tokenAmount);
    }

    function emergencyWithdrawByAdmin(address _user) external onlyOwner {
        UserInfo storage user = userInfos[_user];
        require(user.nftAmount > 0, "NO_STAKE");
        uint256 tokenAmount = user.tokenAmount;
        uint256 nftAmount = user.nftAmount;
        user.nftAmount = 0;
        user.tokenAmount = 0;
        user.stakeTimestamp = 0;
        totalTokensStaked -= tokenAmount;
        totalNFTStaked -= nftAmount;
        delete stakedNFTsByUser[_user];
        ERC721Min(stakingNFT).batchTransferFrom(
            address(this),
            _user,
            stakedNFTsByUser[_msgSender()]
        );
        IERC20(stakingToken).transfer(address(_user), tokenAmount);
        emit EmergencyWithdraw(_user, nftAmount, tokenAmount);
    }

    function setBlocksPerDay(uint256 _blocksPerDay) external onlyOwner stakingNotStarted {
        require(
            _blocksPerDay >= 1000 && _blocksPerDay <= 40000,
            "Out of range 1000-40000"
        );
        blocksPerDay = _blocksPerDay;
    }

    function setEndBlock(uint value) external onlyOwner stakingNotStarted {
        endBlock = value;
    }

    function setMaxNFTStake(uint256 amount) external onlyOwner {
        maxNFTStake = amount;
    }

    function setMaxNFTStakePerUser(uint256 amount) external onlyOwner {
        require(amount < 21, "AMOUNT>20");
        maxNFTStakePerUser = amount;
    }

    function setRewardPerDayPerNFT(uint256 value) external onlyOwner stakingNotStarted {
        rewardPerBlockPerNFT = (value * 1e12) / blocksPerDay;
    }

    function setStabilatorAddress(address stabilator) external onlyOwner {
        stabilatorAddress = stabilator;
    }

    function setStakingEnabled(bool enabled) external onlyOwner {
        stakingEnabled = enabled;
    }

    function setTotalReward(uint value) external onlyOwner {
        totalReward = value;
    }

    function setTreasuryAddress(address addr) external onlyOwner {
        treasuryAddress = addr;
    }

    function setUnstakeFees (
        uint256 stabilator,
        uint256 rewards,
        uint256 burn
    ) external onlyOwner stakingNotStarted {
        require(stabilator + rewards + burn < 1001, "FEES>10%");
        unstakeFeeStabilator = stabilator;
        unstakeFeeRewards = rewards;
        unstakeFeeBurn = burn;
    }

    function transferTokens(address _tokenAddr) external onlyOwner {
        uint256 amount = IERC20(_tokenAddr).balanceOf(address(this));
        if (_tokenAddr == stakingToken) {
            amount -= amount >= totalTokensStaked ? totalTokensStaked : amount;
        }
        if (amount == 0) return;
        IERC20(_tokenAddr).transfer(treasuryAddress, amount);
    }

    function withdraw() external {
        UserInfo storage user = userInfos[_msgSender()];
        require(user.nftAmount > 0, "NO_STAKE");
        require(
            user.stakeTimestamp + stakeLockPeriod <= block.timestamp,
            "STAKE_LOCKED"
        );
        uint256 fee;
        uint256 tokenAmt = user.tokenAmount;
        if (stabilatorAddress != address(0)) {
            fee = (tokenAmt * unstakeFeeStabilator) / 1e4;
            if (fee > 0) {
                IERC20(stakingToken).transfer(stabilatorAddress, fee);
                user.tokenAmount -= fee;
            }
        }
        fee = (tokenAmt * unstakeFeeRewards) / 1e4;
        if (fee > 0) {
            //no transfer required
            user.tokenAmount -= fee;
        }
        fee = (tokenAmt * unstakeFeeBurn) / 1e4;
        if (fee > 0) {
            IERC20(stakingToken).transfer(DEAD_ADDRESS, fee);
            user.tokenAmount -= fee;
        }

        // send pending rewards
        uint256 pending = getPending(_msgSender());
        if (rewardToken == stakingToken) {
            require(
                IERC20(rewardToken).balanceOf(address(this)) - pending >=
                    totalTokensStaked,
                "INSUFFICIENT_REWARDS"
            );
        }
        uint256 nftAmount = user.nftAmount;
        totalTokensStaked -= tokenAmt;
        tokenAmt = user.tokenAmount;
        user.nftAmount = 0;
        user.tokenAmount = 0;
        user.rewardDebt = 0;
        user.stakeTimestamp = 0;
        if (pending > 0) {
            _tokenTransfer(_msgSender(), pending);
        }
        // send pending stake
        uint256[] memory tokenIds = stakedNFTsByUser[_msgSender()];
        delete stakedNFTsByUser[_msgSender()];
        totalNFTStaked -= tokenIds.length;
        ERC721Min(stakingNFT).batchTransferFrom(
            address(this),
            _msgSender(),
            tokenIds
        );
        IERC20(stakingToken).transfer(address(_msgSender()), tokenAmt);

        emit Withdraw(_msgSender(), nftAmount, tokenAmt);
    }

    function getRewardPerDayPerNFT() external view returns (uint) {
        return (rewardPerBlockPerNFT * blocksPerDay) / 1e12;
    }

    function getStakeLockTime(address _user) external view returns (uint256) {
        if (userInfos[_user].stakeTimestamp == 0) return 0;
        uint256 lockedUntil = userInfos[_user].stakeTimestamp + stakeLockPeriod;
        return
            block.timestamp > lockedUntil ? 0 : lockedUntil - block.timestamp;
    }

    function nftCountByUser(address user) external view returns (uint256) {
        return stakedNFTsByUser[user].length;
    }

    function claim() public {
        UserInfo storage user = userInfos[_msgSender()];
        require(user.nftAmount > 0, "NO_STAKE");
        uint256 pending = getPending(_msgSender());
        if (pending > 0) {
            if (rewardToken == stakingToken) {
                require(
                    IERC20(rewardToken).balanceOf(address(this)) - pending >=
                        totalTokensStaked,
                    "INSUFFICIENT_REWARDS"
                );
            }
            user.rewardDebt += pending;
            user.lastRewardBlock = block.number;
            _tokenTransfer(_msgSender(), pending);
        }
        emit Claim(_msgSender(), pending);
    }

    function getPending(address _user) public view returns (uint256) {
        UserInfo memory user = userInfos[_user];
        if (user.nftAmount > 0 && block.number > user.lastRewardBlock) {
            uint multiplier = _getMultiplier(
                user.lastRewardBlock,
                block.number
            );
            if (multiplier == 0) return 0;
            return (user.nftAmount * rewardPerBlockPerNFT * multiplier) / 1e12;
        }
        return 0;
    }

    function _getMultiplier(uint from, uint to) private view returns (uint) {
        if (from < endBlock && to < endBlock) {
            return to - from;
        } else if (from < endBlock && to > endBlock) {
            return endBlock - from;
        }
        return 0;
    }

    // Safe token transfer function, just in case if
    // rounding error causes pool to not have enough tokens
    function _tokenTransfer(address _to, uint256 _amount) private {
        uint256 amount = _amount > totalReward ? totalReward : _amount;
        totalReward -= amount;
        IERC20(rewardToken).transfer(_to, amount);
    }
}

