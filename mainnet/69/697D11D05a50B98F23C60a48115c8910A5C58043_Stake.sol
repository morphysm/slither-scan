// File contracts/NFDNAStaking/Stake.sol

pragma solidity ^0.8.0;

contract Stake is Pausable, Ownable, IERC721Receiver {
    uint256 public constant initialLockedDuration = 1 days;
    uint256 public constant rewardPerSecs = 1125257 * 1e11 * 1 wei;

    IERC721 private immutable stakable;
    StakeWallet private immutable wallet;

    struct Stake {
        uint256 tokenId;
        uint256 stakeDate;
        uint256 latestClaimDate;
    }

    struct StakeIndex {
        uint256 index;
        bool active;
    }

    struct StakeData {
        uint256 tokenId;
        uint256 rarity;
    }

    mapping(address => Stake[]) private stakes;
    mapping(address => uint256) public remainingRewards;
    mapping(address => uint256) public stakeCount;
    mapping(uint256 => StakeIndex) public tokenIdStakeIndexMapping;
    mapping(uint256 => uint256) public rarityMapping;
    mapping(uint256 => address) public tokenIdStakerMapping;

    uint256 public totalRarity;
    uint256 public rewardMultiplier = 1;

    event Staked(
        address indexed staker,
        uint256 indexed tokenId,
        uint256 indexed rarity
    );
    event BatchStaked(address indexed staker, uint256 indexed count);
    event Withdrawn(address indexed staker, uint256 indexed tokenId);
    event BatchWithdrawn(address indexed staker);
    event ClaimedReward(address indexed staker, uint256 amount);

    constructor(address stakableAddress, address walletAddress) {
        stakable = IERC721(stakableAddress);
        wallet = StakeWallet(walletAddress);
    }

    function stakesOf(address account) external view returns (Stake[] memory) {
        return stakes[account];
    }

    function stake(StakeData[] calldata stakeDatas) external whenNotPaused {
        uint256 count = stakeDatas.length;
        uint256 stakedCount = 0;

        for (uint256 i = 0; i < count; i++) {
            StakeData calldata data = stakeDatas[i];

            if (_stakeExists(msg.sender, data.tokenId)) continue;

            _stake(data);
            stakedCount++;
            if (rarityMapping[data.tokenId] == 0) {
                rarityMapping[data.tokenId] = data.rarity;
            }
        }

        emit BatchStaked(msg.sender, stakedCount);
    }

    function _stake(StakeData calldata stakeData) internal {
        stakable.safeTransferFrom(msg.sender, address(this), stakeData.tokenId);

        stakes[msg.sender].push(
            Stake({
                tokenId: stakeData.tokenId,
                stakeDate: block.timestamp,
                latestClaimDate: block.timestamp
            })
        );
        tokenIdStakerMapping[stakeData.tokenId] = msg.sender;
        tokenIdStakeIndexMapping[stakeData.tokenId] = StakeIndex({
            index: stakes[msg.sender].length - 1,
            active: true
        });

        stakeCount[msg.sender]++;

        emit Staked(msg.sender, stakeData.tokenId, stakeData.rarity);
    }

    function claimReward(uint256[] calldata tokenIds) external {
        uint256 reward = _rewardOf(msg.sender, tokenIds, false) +
            remainingRewards[msg.sender];

        delete remainingRewards[msg.sender];

        uint256 count = tokenIds.length;
        for (uint256 i = 0; i < count; i++) {
            uint256 tokenId = tokenIds[i];

            if (!_stakeExists(msg.sender, tokenId)) continue;

            if (!_vestingComplete(msg.sender, tokenId)) continue;

            stakes[msg.sender][tokenIdStakeIndexMapping[tokenId].index]
                .latestClaimDate = block.timestamp;
        }

        wallet.transferTo(msg.sender, reward);

        emit ClaimedReward(msg.sender, reward);
    }

    function withdraw(uint256[] calldata tokenIds) external {
        uint256 count = tokenIds.length;
        uint256 remainingReward = _rewardOf(msg.sender, tokenIds, false);

        for (uint256 iteration = 0; iteration < count; iteration++) {
            uint256 tokenId = tokenIds[iteration];

            if (_stakeExists(msg.sender, tokenId)) {
                _withdraw(msg.sender, tokenId);
            }
        }

        remainingRewards[msg.sender] += remainingReward;

        emit BatchWithdrawn(msg.sender);
    }

    function _withdraw(address account, uint256 tokenId) internal {
        uint256 index = tokenIdStakeIndexMapping[tokenId].index;

        Stake storage stake = stakes[account][index];
        stakable.safeTransferFrom(address(this), account, stake.tokenId);

        delete stakes[account][index];
        delete tokenIdStakeIndexMapping[tokenId];
        delete tokenIdStakerMapping[tokenId];
        stakeCount[account]--;

        if (stakeCount[account] == 0) delete stakes[account];

        emit Withdrawn(account, tokenId);
    }

    function rewardOf(uint256[] calldata tokenIds)
        external
        view
        returns (uint256)
    {
        return
            _rewardOf(msg.sender, tokenIds, false) +
            remainingRewards[msg.sender];
    }

    function rewardOfVested(uint256[] calldata tokenIds)
        external
        view
        returns (uint256)
    {
        return _rewardOf(msg.sender, tokenIds, true);
    }

    function _rewardOf(
        address account,
        uint256[] calldata tokenIds,
        bool includeVested
    ) internal view returns (uint256) {
        uint256 reward = 0;
        uint256 count = tokenIds.length;
        for (uint256 i = 0; i < count; i++) {
            uint256 tokenId = tokenIds[i];

            if (!_stakeExists(account, tokenId)) continue;

            if (!includeVested && !_vestingComplete(account, tokenId)) continue;
            if (includeVested && _vestingComplete(account, tokenId)) continue;

            Stake storage stake = stakes[account][
                tokenIdStakeIndexMapping[tokenId].index
            ];

            uint256 stakeDuration = block.timestamp - stake.latestClaimDate;

            reward += _calculateReward(rarityMapping[tokenId], stakeDuration);
        }

        return reward;
    }

    function _calculateReward(uint256 tokenRarity, uint256 stakeDuration)
        internal
        view
        returns (uint256)
    {
        uint256 reward = (rewardPerSecs * stakeDuration * tokenRarity) /
            totalRarity;

        return reward * rewardMultiplier;
    }

    function _stakeExists(address account, uint256 tokenId)
        internal
        view
        returns (bool)
    {
        return
            stakes[account].length > tokenIdStakeIndexMapping[tokenId].index &&
            tokenIdStakeIndexMapping[tokenId].active;
    }

    function _vestingComplete(address account, uint256 tokenId)
        internal
        view
        returns (bool)
    {
        Stake storage stake = stakes[account][
            tokenIdStakeIndexMapping[tokenId].index
        ];

        return block.timestamp - stake.stakeDate > initialLockedDuration;
    }

    function onERC721Received(
        address operator,
        address from,
        uint256 tokenId,
        bytes calldata data
    ) external override returns (bytes4) {
        return IERC721Receiver.onERC721Received.selector;
    }

    function pause() external onlyOwner {
        _pause();
    }

    function unpause() external onlyOwner {
        _unpause();
    }

    function setTotalRarity(uint256 newTotalRarity) external onlyOwner {
        totalRarity = newTotalRarity;
    }

    function setRewardMultiplier(uint256 newMultiplier) external onlyOwner {
        rewardMultiplier = newMultiplier;
    }

    function setRarity(uint256 tokenId, uint256 rarity) external onlyOwner {
        rarityMapping[tokenId] = rarity;
    }

    function emergencyWithdraw(address account, uint256[] calldata tokenIds)
        external
        onlyOwner
    {
        uint256 count = tokenIds.length;
        uint256 remainingReward = _rewardOf(account, tokenIds, true);

        for (uint256 iteration = 0; iteration < count; iteration++) {
            uint256 tokenId = tokenIds[iteration];

            if (_stakeExists(account, tokenId)) {
                _withdraw(account, tokenId);
            }
        }

        remainingRewards[account] += remainingReward;
    }
}