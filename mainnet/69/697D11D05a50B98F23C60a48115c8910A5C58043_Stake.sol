contract StakeWallet is AccessControl {
    bytes32 public constant STAKE_ROLE = keccak256("STAKE_ROLE");

    IERC20 public immutable _cmtd;

    constructor(address cmtd) {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);

