pragma solidity ^0.8.0;

import "./SafeMathUint.sol";
import "./SafeMathInt.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "hardhat/console.sol";

interface IRAT is IERC20 {
	function reinvestDividends(address account, uint256 usdAmount, uint256 minTokens) external;
}

contract RatifyDividendToken is ERC20, Ownable {
	using SafeMath for uint256;
	using SafeMathUint for uint256;
	using SafeMathInt for int256;

	uint256 internal constant magnitude = 2**128;
	uint256 internal magnifiedDividendPerShare;
	mapping(address => int256) internal magnifiedDividendCorrections;
	mapping(address => uint256) internal withdrawnDividends;
	uint256 public totalDividendsDistributed;
	mapping(address => bool) public excludedFromDividends;

	IRAT public irat;
	IERC20 public usdc;
	address developmentAddress = 0x74175b8dC64c897fE42b170DB980eCdc4d01f278;

	struct Stake {
		address staker;
		uint256 tokensStaked;
		uint256 dividendsStaked;
		uint256 releasable;
		bool released;
		uint256 nonce;
		uint256 pool;
		uint256 bonus;
	}

	struct Pool {
		uint256 minLockup;
		uint256 bonusMultiplier;
		bool active;
	}

	uint256 stakingNonce;
	uint256 poolNonce;
	mapping(uint256 => Stake) stakes;
	mapping(uint256 => Pool) pools;
	mapping(address => uint256[]) userStakes;

	uint256 public totalStaked;
	mapping(address => uint256) stakedAmounts;
	mapping(address => uint256) lockedDividends;

	uint256 burnRateOnUnstake = 90;

	event ExcludeFromDividends(address indexed account);
	event DividendsDistributed(address indexed from, uint256 weiAmount);
	event DividendWithdrawn(address indexed to, uint256 weiAmount, address received);

	event StakeAdded(address indexed staker, uint256 nonce, uint256 tokensStaked, uint256 bonus, uint256 releasable);
	event StakeReleased(address indexed staker, uint256 nonce, uint256 tokensStaked, uint256 bonus, uint256 released);

	constructor(
		string memory _name,
		string memory _symbol,
		address _rat,
		address _usdc
	) ERC20(_name, _symbol) {
		irat = IRAT(_rat);
		usdc = IERC20(_usdc);
	}

	function decimals() public view virtual override returns (uint8) {
		return 6;
	}

	receive() external payable {}

	function setDevelopmentAddress(address _developmentAddress) external onlyOwner {
		developmentAddress = _developmentAddress;
	}

	// allow rat token to notify of dividends deposited
	function notifyDividendDeposit(uint256 tokenAmount) external {
		require(msg.sender == address(irat), "Only callable by token");
		distributeDividends(tokenAmount);
	}

	function depositDividends(uint256 tokenAmount) public {
		usdc.transferFrom(msg.sender, address(this), tokenAmount);
		distributeDividends(tokenAmount);
	}

	function distributeDividends(uint256 tokenAmount) internal {
		require(totalSupply() > 0);

		if (tokenAmount > 0) {
			uint256 devAllocation = tokenAmount.div(2);
			uint256 divAllocation = tokenAmount.sub(devAllocation);
			usdc.transfer(developmentAddress, devAllocation);

			magnifiedDividendPerShare = magnifiedDividendPerShare.add((divAllocation).mul(magnitude) / totalSupply());
			emit DividendsDistributed(msg.sender, divAllocation);
			totalDividendsDistributed = totalDividendsDistributed.add(divAllocation);
		}
	}

	function withdrawDividend() public virtual {
		_withdrawDividendOfUser(msg.sender);
	}

	function reinvestDividend(uint256 minTokens) public {
		_reinvestDividend(msg.sender, minTokens);
	}

	function _withdrawDividendOfUser(address user) internal {
		uint256 _withdrawableDividend = withdrawableDividendOf(user);
		if (_withdrawableDividend > 0) {
			withdrawnDividends[user] = withdrawnDividends[user].add(_withdrawableDividend);
			usdc.transfer(user, _withdrawableDividend);
		}
	}

	function _reinvestDividend(address user, uint256 minTokens) internal {
		uint256 _withdrawableDividend = withdrawableDividendOf(user);
		if (_withdrawableDividend > 0) {
			withdrawnDividends[user] = withdrawnDividends[user].add(_withdrawableDividend);
			usdc.approve(address(irat), _withdrawableDividend);
			irat.reinvestDividends(user, _withdrawableDividend, minTokens);
		}
	}

	function dividendOf(address _owner) public view returns (uint256) {
		return withdrawableDividendOf(_owner);
	}

	function withdrawableDividendOf(address _owner) public view returns (uint256) {
		return accumulativeDividendOf(_owner).sub(withdrawnDividends[_owner]);
	}

	function withdrawnDividendOf(address _owner) public view returns (uint256) {
		return withdrawnDividends[_owner];
	}

	function accumulativeDividendOf(address _owner) public view returns (uint256) {
		return magnifiedDividendPerShare.mul(balanceOf(_owner)).toInt256Safe().add(magnifiedDividendCorrections[_owner]).toUint256Safe() / magnitude;
	}

	function _transfer(
		address from,
		address to,
		uint256 value
	) internal virtual override {
		require(false);
	}

	function _mint(address account, uint256 value) internal override {
		super._mint(account, value);

		magnifiedDividendCorrections[account] = magnifiedDividendCorrections[account].sub((magnifiedDividendPerShare.mul(value)).toInt256Safe());
	}

	function _burn(address account, uint256 value) internal override {
		super._burn(account, value);

		magnifiedDividendCorrections[account] = magnifiedDividendCorrections[account].add((magnifiedDividendPerShare.mul(value)).toInt256Safe());
	}

	function setBalance(address account, uint256 newBalance) external {
		require(msg.sender == address(irat), "Only callable by token");
		if (excludedFromDividends[account]) {
			return;
		}
		_setBalance(account, newBalance);
	}

	function _setBalance(address account, uint256 newBalance) internal {
		uint256 currentBalance = balanceOf(account);

		if (newBalance > currentBalance) {
			uint256 mintAmount = newBalance.sub(currentBalance);
			_mint(account, mintAmount);
		} else if (newBalance < currentBalance) {
			uint256 burnAmount = currentBalance.sub(newBalance);
			_burn(account, burnAmount);
		}
	}

	function excludeFromDividends(address account) external onlyOwner {
		require(!excludedFromDividends[account]);
		excludedFromDividends[account] = true;
		_setBalance(account, 0);
		emit ExcludeFromDividends(account);
	}

	function includeInDividends(address account) external onlyOwner {
		require(excludedFromDividends[account]);
		excludedFromDividends[account] = false;
	}

	function setToken(address _irat) external onlyOwner {
		irat = IRAT(_irat);
	}

	function accountInfo(address account)
		external
		view
		returns (
			uint256 withdrawable,
			uint256 withdrawn,
			uint256 balance,
			uint256 stakedTokens,
			uint256 dividendsLocked,
			Stake[] memory _stakes
		)
	{
		withdrawable = withdrawableDividendOf(account);
		withdrawn = withdrawnDividendOf(account);
		balance = balanceOf(account);
		stakedTokens = stakedAmounts[account];
		dividendsLocked = lockedDividends[account];
		_stakes = getUserStakes(account);
	}

	//STAKING

	function stakeOf(address account) external view returns(uint256) {
		return stakedAmounts[account];
	}

	function getStake(uint256 nonce) external view returns(Stake memory){
		return stakes[nonce];
	}

	function getUserStakes(address account) public view returns(Stake[] memory){
        uint256[] storage userStakeNonces = userStakes[account];
        Stake[] memory _stakes = new Stake[](userStakeNonces.length);
        for (uint i = 0; i < userStakeNonces.length; i ++) {
            uint256 _stakeNonce = userStakeNonces[i];
            Stake storage stake = stakes[_stakeNonce];
            _stakes[i] = stake;
        }
        return _stakes;
	}

	function stakeTokens(uint256 tokenAmount, uint256 _pool) external {
		require(irat.balanceOf(msg.sender).sub(tokenAmount) >= 1 ether, "Insufficient Balance");

		Pool storage pool = pools[_pool];
		require(pool.active, "Pool is not active");

		(uint256 bonus, uint256 stakedDividends) = calculateBonus(msg.sender, tokenAmount, pool.bonusMultiplier);
		irat.transferFrom(msg.sender, address(this), tokenAmount);

		Stake storage newStake = stakes[stakingNonce];
		newStake.releasable = block.timestamp.add(pool.minLockup);
		newStake.tokensStaked = tokenAmount;
		newStake.staker = msg.sender;
		newStake.nonce = stakingNonce;
		newStake.pool = _pool;
		newStake.bonus = bonus;
		newStake.dividendsStaked = stakedDividends;

		_setBalance(msg.sender, balanceOf(msg.sender).add(bonus));
		lockedDividends[msg.sender] = lockedDividends[msg.sender].add(stakedDividends.add(bonus));
		stakedAmounts[msg.sender] = stakedAmounts[msg.sender].add(tokenAmount);

		userStakes[msg.sender].push(stakingNonce);
		totalStaked = totalStaked.add(tokenAmount);
		stakingNonce ++;
		emit StakeAdded(msg.sender, stakingNonce, tokenAmount, _pool, newStake.releasable);
	}

	function removeStake(uint256 nonce) external {
		Stake storage stake = stakes[nonce];
		require(stake.staker == msg.sender, "Sender does not own stake");
		require(block.timestamp > stake.releasable, "Stake is not releasable");
		require(!stake.released, "Stake is already released");

		stake.released = true;
		irat.transfer(msg.sender, stake.tokensStaked);
		totalStaked = totalStaked.sub(stake.tokensStaked);
		lockedDividends[msg.sender] = lockedDividends[msg.sender].sub(stake.dividendsStaked);
		stakedAmounts[msg.sender] = stakedAmounts[msg.sender].sub(stake.tokensStaked);

		uint256 burnAmount = stake.bonus.mul(burnRateOnUnstake).div(100);
		uint256 newBalance = balanceOf(msg.sender).sub(burnAmount);
		_setBalance(msg.sender, newBalance);

		emit StakeReleased(msg.sender, nonce, stake.tokensStaked, stake.bonus, block.timestamp);
	}

	function calculateBonus(address staker, uint256 tokenAmount, uint256 rate) public view returns(uint256 bonus, uint256 stakedDividends) {
		uint256 holderBalance = irat.balanceOf(staker);//.add(stakedAmounts[staker]);
		// find the percentage of the holders balance that they are staking
		uint256 perc = tokenAmount.mul(magnitude).div(holderBalance);
		// calculate the amount of div tokens the reward will be applied to
		uint256 unlockedDividends = balanceOf(msg.sender).sub(lockedDividends[msg.sender]);
		stakedDividends = unlockedDividends.mul(perc).div(magnitude);
		bonus = stakedDividends.mul(rate).div(100);
	}

	function addPool(uint256 minLockup, uint256 multiplier) external onlyOwner {
		Pool storage newPool = pools[poolNonce];
		newPool.minLockup = minLockup;
		newPool.bonusMultiplier = multiplier;
		newPool.active = true;
		poolNonce ++;
	}

	function updatePool(uint256 _poolNonce, uint256 minLockup, uint256 multiplier) external onlyOwner {
		Pool storage pool = pools[_poolNonce];
		pool.minLockup = minLockup;
		pool.bonusMultiplier = multiplier;
	}

	function disablePool(uint256 _poolNonce) external onlyOwner {
		Pool storage pool = pools[_poolNonce];
		pool.active = false;
	}

	function getPools() external view returns(Pool[] memory){
        Pool[] memory _pools = new Pool[](poolNonce);
        for (uint i = 0; i < poolNonce; i ++) {
            Pool storage pool = pools[i];
            _pools[i] = pool;
        }
        return _pools;
	}

	function setBurnRateOnUnstake(uint256 _burnRateOnUnstake) external onlyOwner {
		burnRateOnUnstake = _burnRateOnUnstake;
	}
}

