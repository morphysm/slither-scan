pragma solidity ^0.8.0;

import "./RatifyDividendToken.sol";
import 'hardhat/console.sol';
import '@traderjoe-xyz/core/contracts/traderjoe/interfaces/IJoeRouter02.sol';
import '@traderjoe-xyz/core/contracts/traderjoe/interfaces/IJoePair.sol';
import '@traderjoe-xyz/core/contracts/traderjoe/interfaces/IJoeFactory.sol';

contract RAT is ERC20, Ownable {
	using SafeMath for uint256;
	uint256 internal constant magnitude = 2**128;
    IERC20 USDC;

	IJoeRouter02 public joeRouter;
    IJoePair public joePair;

	bool private removeFee;
	bool private tradingEnabled;

	RatifyDividendToken public dividendTracker;
	address private airDropper = 0x632e1207a652D73C3149fF2229518878AE5e461A;
	uint256 public rewardFee = 6;
	uint256 public developmentFee = 6;
	uint256 public totalFee = 12;

	uint256 public discountedFee = 10; // discounted when bought through this contract

	// exlcude from fees and max transaction amount
	mapping(address => bool) private _isExcludedFromFees;

	// store addresses that a automatic market maker pairs. Any transfer *to* these addresses
	// could be subject to a maximum transfer amount
	mapping(address => bool) public automatedMarketMakerPairs;

	event UpdateDividendTracker(address indexed newAddress, address indexed oldAddress);
	event UpdateJoeRouter(address indexed newAddress, address indexed oldAddress);
	event ExcludeFromFees(address indexed account, bool isExcluded);
	event SetAutomatedMarketMakerPair(address indexed pair, bool indexed value);
	event SendDividends(uint256 amount);

	constructor(address usdc) ERC20("Ratify", "RTFY") {
		USDC = IERC20(usdc);
		_mint(owner(), 1_000_000_000 * (10**18));
	}

	receive() external payable {}

	function deployTracker() external onlyOwner {
		IJoeRouter02 _joeRouter = IJoeRouter02(0x60aE616a2155Ee3d9A68541Ba4544862310933d4);
		address _joePair = IJoeFactory(_joeRouter.factory()).createPair(address(this), address(USDC));

		joeRouter = _joeRouter;
		joePair = IJoePair(_joePair);

		_setAutomatedMarketMakerPair(_joePair, true);

		require(address(dividendTracker) == address(0), "Already deployed");
		dividendTracker = new RatifyDividendToken("Ratify Dividend", "dRTFY", address(this), address(USDC));
		dividendTracker.excludeFromDividends(address(dividendTracker));
		dividendTracker.excludeFromDividends(address(this));
		dividendTracker.excludeFromDividends(owner());
		dividendTracker.excludeFromDividends(address(joeRouter));
		dividendTracker.excludeFromDividends(address(joePair));
		dividendTracker.excludeFromDividends(0x000000000000000000000000000000000000dEaD);
		dividendTracker.transferOwnership(msg.sender);

		// exclude from paying fees or having max transaction amount
		excludeFromFees(address(this), true);
		excludeFromFees(owner(), true);
		excludeFromFees(address(dividendTracker), true);
		excludeFromFees(airDropper, true);
	}

	function setDevelopmentFee(uint256 _developmentFee) external onlyOwner {
		developmentFee = _developmentFee;
		setTotalFee();
	}

	function setRewardFee(uint256 _rewardFee) external onlyOwner {
		rewardFee = _rewardFee;
		setTotalFee();
	}

	function setTotalFee() internal {
		totalFee = rewardFee + developmentFee;
	}

	function enableTrading() external onlyOwner {
		tradingEnabled = true;
	}

	function updateDividendTracker(address newAddress) public onlyOwner {
		require(newAddress != address(dividendTracker), "RTFY: The dividend tracker already has that address");

		RatifyDividendToken newDividendTracker = RatifyDividendToken(payable(newAddress));
		emit UpdateDividendTracker(newAddress, address(newDividendTracker));
		dividendTracker = newDividendTracker;
	}

	function updateJoeRouter(address newAddress) public onlyOwner {
		require(newAddress != address(joeRouter), "RTFY: The router already has that address");
		emit UpdateJoeRouter(newAddress, address(joeRouter));
		joeRouter = IJoeRouter02(newAddress);
	}

	function excludeFromFees(address account, bool excluded) public onlyOwner {
		require(_isExcludedFromFees[account] != excluded, "RTFY: Account is already the value of 'excluded'");
		_isExcludedFromFees[account] = excluded;

		emit ExcludeFromFees(account, excluded);
	}

	function setAutomatedMarketMakerPair(address pair, bool value) public onlyOwner {
		require(pair != address(joePair), "RTFY: The Joe pair cannot be removed from automatedMarketMakerPairs");

		_setAutomatedMarketMakerPair(pair, value);
	}

	function _setAutomatedMarketMakerPair(address pair, bool value) private {
		require(automatedMarketMakerPairs[pair] != value, "RTFY: Automated market maker pair is already set to that value");
		automatedMarketMakerPairs[pair] = value;
		emit SetAutomatedMarketMakerPair(pair, value);
	}

	function isExcludedFromFees(address account) public view returns (bool) {
		return _isExcludedFromFees[account];
	}

	function reinvestDividends(address account, uint256 usdAmount, uint256 minTokens) external {
		require(msg.sender == address(dividendTracker), "RTFY: Only callable by dividend tracker");
		USDC.transferFrom(address(dividendTracker), address(this), usdAmount);
		removeFee = true;
		_swapUSDCforTokens(usdAmount, minTokens, account);
		removeFee = false;
	}

	function _transfer(
		address from,
		address to,
		uint256 amount
	) internal override {
		require(from != address(0), "ERC20: transfer from the zero address");
		require(to != address(0), "ERC20: transfer to the zero address");
		if (!tradingEnabled) {
			require(from == owner() || from == airDropper, "RTFY: Cannot transfer before trading enabled");
		}

		if (amount == 0) {
			super._transfer(from, to, 0);
			return;
		}

		if (!removeFee && from != airDropper) {allocateDividends(from, to, amount);}
		if (from == airDropper) {allocateAirdropDividends(to, amount);}

		bool isExcluded = _isExcludedFromFees[from] || _isExcludedFromFees[to];
		if (!isExcluded) {		
			if (!removeFee && automatedMarketMakerPairs[to]) {
				removeFee = true;
				swapAndDistribute();
				removeFee = false;
			}
			if (!removeFee) {
				uint256 fees = amount.mul(totalFee).div(100);
				amount = amount.sub(fees);
				super._transfer(from, address(this), fees);
			}
		}
		super._transfer(from, to, amount);
	}

	function allocateDividends(address from, address to, uint256 amount) internal {
		if (to != address(dividendTracker) && from != address(dividendTracker)) {
			// Allow dividend tracker contract to update balances for staking
			
			if (automatedMarketMakerPairs[from]) {
				// mint dividends on a buy
				(uint256 reserve0, uint256 reserve1,) = joePair.getReserves();
				(uint256 rtfyBalance, uint256 usdcBalance) = address(this) < address(USDC) ? (reserve0, reserve1) : (reserve1, reserve0);

				uint256 amountIn = joeRouter.getAmountIn(amount, usdcBalance, rtfyBalance);
				try dividendTracker.setBalance(payable(to), dividendTracker.balanceOf(to).add(amountIn)) {} catch {}
			} else {
				// subtract dividends relative to the amount transferred
				uint256 combinedBalance = balanceOf(from).add(dividendTracker.stakeOf(from));
				uint256 perc = amount.mul(magnitude).div(combinedBalance);
				uint256 sub = dividendTracker.balanceOf(from).mul(perc).div(magnitude);
				try dividendTracker.setBalance(payable(from), dividendTracker.balanceOf(from).sub(sub)) {} catch {}
			}
		}
	}

	function allocateAirdropDividends(address to, uint256 amount) internal {
		(uint256 reserve0, uint256 reserve1,) = joePair.getReserves();
		(uint256 rtfyBalance, uint256 usdcBalance) = address(this) < address(USDC) ? (reserve0, reserve1) : (reserve1, reserve0);

		uint256 tokenValue = joeRouter.getAmountOut(amount, rtfyBalance, usdcBalance);
		uint256 bonus = tokenValue.mul(60).div(100);
		try dividendTracker.setBalance(payable(to), dividendTracker.balanceOf(to).add(tokenValue.add(bonus))) {} catch {}
	}

	function swapTokensForUSDC(uint256 tokenAmount) public {
		address[] memory path = new address[](2);
		path[0] = address(this);
		path[1] = address(USDC);

		_approve(address(this), address(joeRouter), tokenAmount);
		uint256 usdcBalanceBefore = USDC.balanceOf(address(dividendTracker));

		joeRouter.swapExactTokensForTokensSupportingFeeOnTransferTokens(
			tokenAmount,
			0,
			path,
			address(dividendTracker),
			block.timestamp
		);
		uint256 amountDividends = USDC.balanceOf(address(dividendTracker)).sub(usdcBalanceBefore);
		dividendTracker.notifyDividendDeposit(amountDividends);
		emit SendDividends(amountDividends);
	}

	function _swapUSDCforTokens(
		uint256 usdAmount,
		uint256 minTokens,
		address account
	) internal returns (uint256) {
		address[] memory path = new address[](2);
		path[0] = address(USDC);
		path[1] = address(this);

		USDC.approve(address(joeRouter), usdAmount);
		uint256 balanceBefore = balanceOf(account);
		joeRouter.swapExactTokensForTokensSupportingFeeOnTransferTokens(usdAmount, minTokens, path, account, block.timestamp);
		uint256 tokenAmount = balanceOf(account).sub(balanceBefore);
		return tokenAmount;
	}

	function swapUSDCforTokens(uint256 usdAmount, uint256 minTokens) external {
		USDC.transferFrom(msg.sender, address(this), usdAmount);
		uint256 dividendAmount = usdAmount.mul(discountedFee).div(100);
		USDC.transfer(address(dividendTracker), dividendAmount);
		dividendTracker.notifyDividendDeposit(dividendAmount);
		removeFee = true;
		_swapUSDCforTokens(usdAmount.sub(dividendAmount), minTokens, msg.sender);
		removeFee = false;
		try dividendTracker.setBalance(payable(msg.sender), dividendTracker.balanceOf(msg.sender).add(usdAmount)) {} catch {}
	}

	function swapAndDistribute() private {
		uint256 tokenBalance = balanceOf(address(this));
		swapTokensForUSDC(tokenBalance);
	}
}

