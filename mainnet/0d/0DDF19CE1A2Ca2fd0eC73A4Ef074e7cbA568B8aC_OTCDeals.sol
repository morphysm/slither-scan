pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Pair.sol";
import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";

contract ChainPriceFetcher is Ownable {
    address public chainFactory;
    address public chainNativeCurrency;
    address public chainStableCoinToken;

    constructor(uint32 _chainId) {
        // ---------- Testnets ---------

        // Avalanche Fuji Testnet
        if (_chainId == 43113) {
            chainFactory = 0xDd0e1107d71f38d9201481FB8FD29D480bB2A2D0;          // Huricane test
            chainNativeCurrency = 0xd00ae08403B9bbb9124bB305C09058E32C39A48c;   // WAVAX
            chainStableCoinToken = 0x08a978a0399465621e667C49CD54CC874DC064Eb;  // aUSDT (not actually stable coin)
        }

        // ---------- Mainnets ---------

        // Avalanche Mainnet
        else if (_chainId == 43114) {
            chainFactory = 0x9Ad6C38BE94206cA50bb0d90783181662f0Cfa10;          // JoeFactory
            chainNativeCurrency = 0xB31f66AA3C1e785363F0875A1B74E27b85FD66c7;   // WAVAX
            chainStableCoinToken = 0xA7D7079b0FEaD91F3e65f86E8915Cb59c1a4C664;  // USDC.e
        }

        // Fantom
        else if  (_chainId == 250) {
            chainFactory = 0x152eE697f2E276fA89E96742e9bB9aB1F2E61bE3;          // SpookySwapFactory
            chainNativeCurrency = 0x21be370D5312f44cB42ce377BC9b8a0cEF1A4C83;   // Wrapped FTM
            chainStableCoinToken = 0x04068DA6C83AFCFA0e13ba15A6696662335D5B75;  // USDC
        }

        // Polygon
        else if  (_chainId == 137) {
            // For some reason, stablecoins like USDC or USDT don't work for price calculation
            // Maybe because of the pair they are based on (USDC/USDT) instead of (USDC/WMATIC)
            chainFactory = 0xc35DADB65012eC5796536bD9864eD8773aBc74C4;          // SushiSwap: V2 Factory
            chainNativeCurrency = 0x0d500B1d8E8eF31E21C99d1Db9A6444d3ADf1270;   // Matic Token
            chainStableCoinToken = 0x8f3Cf7ad23Cd3CaDbD9735AFf958023239c6A063;  // DAI
        }

        else {
            revert("Chain not supported");
        }
    }

    function updateChainFactory(address _newFactory) external onlyOwner {
        chainFactory = _newFactory;
    }

    function updateChainNativeCurrency(address _newNativeCurrency) external onlyOwner {
        chainNativeCurrency = _newNativeCurrency;
    }

    function updateChainStableCoinToken(address _newStableCoinToken) external onlyOwner {
        chainStableCoinToken = _newStableCoinToken;
    }

    function getCurrentPriceActionOfToken(address _token, bool _reverse) public view returns (uint) {
        // We take the JoeFactory, but actually, getPair is present on every Uniswap fork factory, so we just have to adjust the address depending on current context
        IUniswapV2Factory factory = IUniswapV2Factory(chainFactory); // TraderJoe Factory address
        address pairAddress = factory.getPair(_token, chainNativeCurrency);

        // Same as factory, every Uniswap fork has a getReserve method
        IUniswapV2Pair pair = IUniswapV2Pair(pairAddress);
        uint tokenPriceInNativeCurrency;
        uint NativeCurrencyPriceInToken;

        (uint112 reserve0, uint112 reserve1,) = pair.getReserves();

        ERC20 token0 = ERC20(pair.token0());
        ERC20 token1 = ERC20(pair.token1());

        if (pair.token0() == chainNativeCurrency) {
            tokenPriceInNativeCurrency = (reserve0 * (10 ** token1.decimals())) / reserve1;
            NativeCurrencyPriceInToken = (reserve1 * (10 ** token0.decimals())) / reserve0;
        }
        else {
            tokenPriceInNativeCurrency = (reserve1 * (10 ** token0.decimals())) / reserve0;
            NativeCurrencyPriceInToken = (reserve0 * (10 ** token1.decimals())) / reserve1;
        }

        return _reverse ? NativeCurrencyPriceInToken : tokenPriceInNativeCurrency;
    }

    function getTokenPriceInStableCoin(address _token) public view returns (uint) {
        uint priceNativeCurrencyinStableCoin = getCurrentPriceActionOfToken(chainStableCoinToken, true);
        uint priceTokenInNativeCurrency = getCurrentPriceActionOfToken(_token, false);

        ERC20 stableCoin = ERC20(chainStableCoinToken);
        uint8 stableCoinDecimals = stableCoin.decimals();

        return (priceNativeCurrencyinStableCoin * priceTokenInNativeCurrency) / (10 ** stableCoinDecimals);
    }
}

