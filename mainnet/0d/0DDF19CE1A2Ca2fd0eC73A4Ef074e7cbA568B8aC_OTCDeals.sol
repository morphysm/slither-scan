pragma solidity ^0.8.0;

import "hardhat/console.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";

contract TokenTransporter {
    function _transferERC20ToContract(address _owner, address _token, uint _amount) internal {
        uint balanceBefore = ERC20(_token).balanceOf(address(this));
        ERC20(_token).transferFrom(_owner, address(this), _amount);
        uint balanceAfter = ERC20(_token).balanceOf(address(this));

        require(balanceAfter == balanceBefore + _amount, 'Token has transfer taxes which is not supported');
    }

    function _transferERC20FromContract(address _recipient, address _token, uint _amount) internal {
        ERC20(_token).transfer(_recipient, _amount);
    }

    function _transferERC721ToContract(address _owner, address _token, uint _tokenID) internal {
        ERC721(_token).safeTransferFrom(_owner, address(this), _tokenID);
    }

    function _transferERC721FromContract(address _recipient, address _token, uint _tokenID) internal {
        ERC721(_token).safeTransferFrom(address(this), _recipient, _tokenID);
    }
}

