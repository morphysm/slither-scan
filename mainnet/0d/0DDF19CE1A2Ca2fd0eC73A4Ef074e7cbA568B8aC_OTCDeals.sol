pragma solidity ^0.8.0;

//import "hardhat/console.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./ChainPriceFetcher.sol";
import "./TokenTransporter.sol";


// MultiChain inherit from Ownable so we don't need to add it here
contract OTCDeals is Ownable, TokenTransporter, ReentrancyGuard {

    struct OTCRequest {
        uint32 id; // 0 to 4 294 967 295
        address from;
        address tokenToSend;
        uint tokenToSendAmount;
        uint amountFilled;
        uint createdAtBlock;
        uint createdAt;
    }

    struct Partner {
        uint index;
        address token;
        address manager;
        uint reserve;
    }

    bool public maintenanceMode;
    uint public flatFees;
    uint public dynamicFees;
    uint8 public minimumAmountInUSD;
    uint32 public requestId; // incremental identifier for every request

    address private treasuryContractAddress;
    address public chainPriceFetcherAddress;

    mapping(uint32 => uint) public OTCRequestFinder;                // find OTCRequest by id

    address[] public listTokenPartners;
    mapping(address => Partner) private partners;                   // mapping Partner object from token address
    mapping(address => address) private managerTokenPartnerMapping; // mapping token partner addresses from manager address
    mapping(address => uint32[]) private userOpenedRequestsIds;

    OTCRequest[] private openedRequests;

    event OTCRequestCreated (
        uint32 indexed id,
        address indexed from,
        address tokenToSend,
        uint tokenToSendAmount,
        uint createdAt
    );

    event OTCRequestPartiallyFilled (
        uint32 indexed id,
        address indexed filledBy,
        uint amountFilled,
        uint filledAt
    );

    modifier ifNotInMaintenanceMode() {
        if (msg.sender != owner()) {
            require(!maintenanceMode, "503"); // 503: Service Unavailable => maintenance mode is active
        }

        _;
    }

    constructor(address _treasuryContractAddress, address _chainPriceFetcherAddress) {
        treasuryContractAddress = _treasuryContractAddress;
        chainPriceFetcherAddress = _chainPriceFetcherAddress;
        maintenanceMode = false;
        flatFees = 0.05 ether; // 0.05 ETH
        dynamicFees = 50; // will be calculated as .5% in the calculateFees function
        minimumAmountInUSD = 100;

        // We push an empty OTC Request at first position so we could exclude the index 0 returned by OTCRequestFinder
        // (OTCRequestFinder is a mapping so a non existing id will return 0 and we want this to be considered as an error
        // instead of returning the first real OTC Request)
        openedRequests.push(OTCRequest({id: 0, from: address(0), tokenToSend: address(0), tokenToSendAmount: 0, amountFilled: 0, createdAtBlock: 0, createdAt: 0}));
    }

    // ----- Contract configuration functions
    function setMaintenanceMode(bool _maintenanceMode) external onlyOwner {
        maintenanceMode = _maintenanceMode;
    }

    function setTreasuryContractAddress(address _treasuryContractAddress) external onlyOwner {
        treasuryContractAddress = _treasuryContractAddress;
    }

    function setChainPriceFetcherAddress(address _chainPriceFetcherAddress) external onlyOwner {
        chainPriceFetcherAddress = _chainPriceFetcherAddress;
    }

    function updateFlatFees(uint _flatFees) external onlyOwner {
        flatFees = _flatFees;
    }

    function updateDynamicFees(uint _dynamicFees) external onlyOwner {
        dynamicFees = _dynamicFees;
    }

    function updateMinimumAmountInUSD(uint8 _amount) external onlyOwner {
        minimumAmountInUSD = _amount;
    }
    // -----

    function createOTCRequest(address _tokenToSend, uint _tokenToSendAmount) public ifNotInMaintenanceMode {
        uint32 newId = ++requestId;

        ERC20 token = ERC20(_tokenToSend);
        uint8 tokenDecimals = token.decimals();

        ChainPriceFetcher chainPriceFetcher = ChainPriceFetcher(chainPriceFetcherAddress);
        uint tokenPriceInStableCoin = chainPriceFetcher.getTokenPriceInStableCoin(_tokenToSend);
        uint amountToSendUSDValue = (tokenPriceInStableCoin * _tokenToSendAmount)  / 10 ** tokenDecimals;

        require(amountToSendUSDValue > minimumAmountInUSD*10**tokenDecimals, "low_usd_value");

        OTCRequest memory newOTCRequest = OTCRequest({
            id: newId,
            from: msg.sender,
            tokenToSend: _tokenToSend,
            tokenToSendAmount: _tokenToSendAmount,
            amountFilled: 0,
            createdAtBlock: block.number,
            createdAt: block.timestamp
        });

        _transferERC20ToContract(newOTCRequest.from, newOTCRequest.tokenToSend, newOTCRequest.tokenToSendAmount);

        OTCRequestFinder[newId] = openedRequests.length;
        openedRequests.push(newOTCRequest);

        userOpenedRequestsIds[newOTCRequest.from].push(newId);

        emit OTCRequestCreated(
            newId,
            newOTCRequest.from,
            newOTCRequest.tokenToSend,
            newOTCRequest.tokenToSendAmount,
            block.number
        );
    }

    function getNbOpenedRequests() public view returns (uint) {
        return openedRequests.length;
    }

    function getNbOpenedRequestsForUser(address _user) external view returns (uint) {
        return userOpenedRequestsIds[_user].length;
    }

    function getOpenedRequestForUser(address _user, uint _index) external view returns (OTCRequest memory) {
        uint32 id = userOpenedRequestsIds[_user][_index];
        return _getOpenedRequestById(id);
    }

    function getOpenedRequestByIndex(uint _index) external view returns (OTCRequest memory) {
        OTCRequest storage openedRequest = openedRequests[_index];
        require(openedRequest.from != address(0), '404');

        return openedRequests[_index];
    }

    function _getOpenedRequestById(uint32 _id) private view returns (OTCRequest storage) {
        uint index = OTCRequestFinder[_id];

        require(index > 0, "404");
        return openedRequests[index];
    }

    function fillOTCRequest(uint32 _id, uint _toFillAmount, uint16 _slippage) public payable ifNotInMaintenanceMode nonReentrant {
        require(msg.value > 0.1 ether, 'Minimum tx amount is 0.1');
        OTCRequest storage openedRequest = _getOpenedRequestById(_id);
        ERC20 token = ERC20(openedRequest.tokenToSend);
        uint8 tokenDecimals = token.decimals();

        require(_toFillAmount <= openedRequest.tokenToSendAmount - openedRequest.amountFilled, 'Cannot buy more than available tokens');

        ChainPriceFetcher chainPriceFetcher = ChainPriceFetcher(chainPriceFetcherAddress);
        uint tokenCurrentPA = chainPriceFetcher.getCurrentPriceActionOfToken(openedRequest.tokenToSend, false);
        uint toPay = (tokenCurrentPA * _toFillAmount) / 10 ** tokenDecimals;

        uint16 evol = 0;

        // * 100 000 gives us a 3 decimals precision which is needed to handle small slippage percentage
        if (msg.value > toPay) {
            evol = uint16(((msg.value*100000 - toPay*100000) / toPay*100000) / 100000);
        }
        else if (msg.value < toPay) {
            evol = uint16(((toPay*100000 - msg.value*100000) / msg.value*100000) / 100000);
        }

        // We may have this error if:
        //      - Slippage is too low and current token PA has moved fast
        //      - User attempted to buy at a lower/higher price than the current token value
        require(evol < _slippage, 'invalid_price');

        uint tokensToDeliver = _toFillAmount;

        // Price gone down, user has paid too much for willing amount, excedent will be refunded
        if (msg.value > toPay) {
            //console.log('Price gone down, user has paid too much for willing amount, excedent will be refunded');
            uint toRefund = msg.value - toPay;

            (bool refund, ) = payable(msg.sender).call{value: toRefund}("");
            require(refund, "Failed to refund money");
        }
        // Price is same or higher than expected, we re calculate the max amount to deliver and perform the transfer
        else {
            //console.log('Price is same or higher than expected, we re calculate the max amount to deliver and perform the transfer');
            // By multiply every amount by 10 000, we got a 4 decimal precision
            tokensToDeliver = ((msg.value*10000 / tokenCurrentPA*10000) * 10 ** tokenDecimals) / 100000000;

            toPay = msg.value;
        }

        openedRequest.amountFilled += tokensToDeliver;
        _transferERC20FromContract(msg.sender, openedRequest.tokenToSend, tokensToDeliver);

        Partner storage partner = partners[openedRequest.tokenToSend];

        uint fees = calculateFees(toPay);

        if (partner.reserve > fees) {
            partner.reserve -= fees;

            (bool paid, ) = payable(openedRequest.from).call{value: toPay}("");
            require(paid, "Failed to send money");
        }
        else {
            (bool paid, ) = payable(openedRequest.from).call{value: toPay - fees}("");
            require(paid, "Failed to send money minus fee");
        }

        (bool paidFees, ) = payable(treasuryContractAddress).call{value: fees}("");
        require(paidFees, "Failed to send fees");

        // If the OTC is completely filled, we remove it from the opened list
        if (openedRequest.amountFilled == openedRequest.tokenToSendAmount) {
            _deleteTradeRequestFromStorage(openedRequest);
        }
        // If the OTC is almost entirely filled, we close it and reimburse the owner because nobody will buy these small amounts
        else if ((openedRequest.tokenToSendAmount - openedRequest.amountFilled) < ((10 ** tokenDecimals) / 100)) {
            _transferERC20FromContract(openedRequest.from, openedRequest.tokenToSend, openedRequest.tokenToSendAmount - openedRequest.amountFilled);
            _deleteTradeRequestFromStorage(openedRequest);
        }

        emit OTCRequestPartiallyFilled(openedRequest.id, msg.sender, tokensToDeliver, block.timestamp);
    }

    function cancelOTCRequest(uint32 _id) public {
        OTCRequest storage openedRequest = _getOpenedRequestById(_id);
        require(openedRequest.from == msg.sender || msg.sender == owner(), '403');

        _transferERC20FromContract(openedRequest.from, openedRequest.tokenToSend, openedRequest.tokenToSendAmount - openedRequest.amountFilled);
        _deleteTradeRequestFromStorage(openedRequest);
    }

    function emergencyCancelOpenedRequests(uint8 _limit) external onlyOwner {
        uint8 i = 0;

        while (openedRequests.length > 1 && i < _limit) {
            OTCRequest storage openedRequest = openedRequests[1]; // 1 because the first element is an empty OTCDeal
            cancelOTCRequest(openedRequest.id);

            i++;
        }

        i = 0;

        while (listTokenPartners.length > 0) {
            removePartner(listTokenPartners[0]);
        }
    }

    function createPartner(address _token, address _manager) external onlyOwner {
        require(partners[_token].manager == address(0), '409'); // 409 = conflict => partner already exists

        Partner memory newPartner = Partner({
            index: listTokenPartners.length,
            token: _token,
            manager: _manager,
            reserve: 0
        });

        partners[_token] = newPartner;
        managerTokenPartnerMapping[_manager] = _token;
        listTokenPartners.push(_token);
    }

    function removePartner(address _token) public onlyOwner {
        Partner storage partner = partners[_token];
        require(partner.manager != address(0), '404'); // partner does not exist
        partnerRemoveReserve(_token);
        uint index = partner.index;
        address lastPartnerAddress = listTokenPartners[listTokenPartners.length - 1];
        Partner storage lastPartner = partners[lastPartnerAddress];
        lastPartner.index = index;

        listTokenPartners[index] = lastPartnerAddress;
        listTokenPartners.pop();

        delete managerTokenPartnerMapping[partner.manager];
        delete partners[_token];
    }

    function partnerAddReserve(address _token) external payable {
        Partner storage partner = partners[_token];
        require(msg.sender == partner.manager, '403');

        partner.reserve += msg.value;
    }

    function partnerRemoveReserve(address _token) public nonReentrant {
        Partner storage partner = partners[_token];
        require(msg.sender == partner.manager || msg.sender == owner(), '403');

        (bool reserveSentBack, ) = payable(partner.manager).call{value: partner.reserve}("");
        require(reserveSentBack, "Failed to withdraw the reserve");

        partner.reserve = 0;
    }

    function getPartner(address _token) external view returns (Partner memory) {
        Partner storage partner = partners[_token];
        require(partner.manager != address(0), '404');
        require(partner.manager == msg.sender || msg.sender == owner(), '403');

        return partner;
    }

    function nbTokenPartners() external view returns (uint) {
        return listTokenPartners.length;
    }

    // Get list of tokens managed by the caller
    function getManagedTokenPartner(address _manager) external view returns (Partner memory) {
        address tokenPartnerAddress = managerTokenPartnerMapping[_manager];
        Partner storage partner = partners[tokenPartnerAddress];

        require(partner.manager == _manager, '404'); // manager doesn't manage any token partner
        require(partner.manager == _manager || msg.sender == owner(), '403');

        return partner;
    }

    function isPartnerManager() external view returns (bool) {
        return managerTokenPartnerMapping[msg.sender] != address(0);
    }

    function calculateFees(uint _amount) public view returns (uint) {
        uint calculatedDynamicFees = (_amount * dynamicFees) / 10000; // .5% by default
        return calculatedDynamicFees + flatFees;
    }

    function _deleteTradeRequestFromStorage(OTCRequest storage _openedRequestToRemove) private {
        // First we remove the reference to OTCDeals from the creator user mapping
        uint32[] storage listUserOpenedRequestsIds = userOpenedRequestsIds[_openedRequestToRemove.from];

        // May be gas consuming, but we don't expect a user to have a lot of OTC Deals simultaneously opened
        for (uint i = 0; i < userOpenedRequestsIds[_openedRequestToRemove.from].length; i++) {
            if (listUserOpenedRequestsIds[i] == _openedRequestToRemove.id) {
                listUserOpenedRequestsIds[i] = listUserOpenedRequestsIds[listUserOpenedRequestsIds.length - 1];
                listUserOpenedRequestsIds.pop();
            }
        }

        // Then we replace element to delete by the last of the list so we don't have gap in our array
        OTCRequest storage lastOpenedRequest = openedRequests[openedRequests.length - 1];
        OTCRequestFinder[lastOpenedRequest.id] = OTCRequestFinder[_openedRequestToRemove.id];
        delete OTCRequestFinder[_openedRequestToRemove.id];

        openedRequests[OTCRequestFinder[lastOpenedRequest.id]] = lastOpenedRequest;
        openedRequests.pop();
    }
}

