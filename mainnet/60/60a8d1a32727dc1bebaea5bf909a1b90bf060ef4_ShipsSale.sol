// File contracts/ShipsSale.sol


pragma solidity =0.8.7;








contract ShipsSale is Ownable, ReentrancyGuard {
    using ECDSA for bytes32;
    using SafeCast for uint256;
    using SafeERC20 for IERC20;

    event ShipChanged(address newShip);
    event TokenPriceChanged(uint256 newTokenPrice);
    event TreasuryChanged(address newTreasury);
    event TokenSold(address buyer, uint256 tokenId);
    event MarkTokenSold(address buyer, uint256 tokenId);
    event MaxBuyCountPerWalletChanged(uint256 newMaxBuyCountPerWallet);
    event PresaleConfigChanged(address whitelistSigner, uint32 startTime, uint32 endTime);

    struct PresaleConfig {
        address whitelistSigner;
        uint32 startTime;
        uint32 endTime;
    }

    PresaleConfig public presaleConfig;
    uint256 public maxBuyCountPerWallet;
    uint256 public tokenPrice;
    uint256 public tokenSoldCount;
    uint256 public tokenToSell;
    uint256 public nftTotalSupply;
    mapping(address => uint256) public boughtCounts;

    // For drawing without replacement
    mapping(uint256 => uint256) movedTokens;

    IERC721 public ship;
    IERC20 public paymentToken;
    address public treasury;

    bytes32 public DOMAIN_SEPARATOR;
    bytes32 public constant PRESALE_TYPEHASH = keccak256("Presale(address buyer)");

    constructor(address _shipAddress, address _paymentTokenAddress, uint256 _tokenToSell, uint256 _nftTotalSupply){
        require(_shipAddress != address(0), "ShipsSale: _shipAddress not set");
        require(_paymentTokenAddress != address(0), "ShipsSale: _paymentTokenAddress not set");
        require(_tokenToSell > 0, "ShipsSale: _tokenToSell needs to be > 0");
        require(_nftTotalSupply > 0, "ShipsSale: _nftTotalSupply needs to be > 0");
        require(_tokenToSell < _nftTotalSupply, "ShipsSale: tokenToSell need to be smaller to total supply of nft");

        ship = IERC721(_shipAddress);
        paymentToken = IERC20(_paymentTokenAddress);
        tokenToSell = _tokenToSell;

        // Assume ERC721 may not be Enumerable so passing total supply ourself
        nftTotalSupply = _nftTotalSupply;

        uint256 chainId;
        assembly {
            chainId := chainid()
        }
        DOMAIN_SEPARATOR = keccak256(
            abi.encode(
                keccak256("EIP712Domain(string name,string version,uint256 chainId,address verifyingContract)"),
                keccak256(bytes("IME Ships Sale")),
                keccak256(bytes("1")),
                chainId,
                address(this)
            )
        );

    }

    function buyTokens(uint256 count) external {
        _buyTokens(count, "");
    }

    function buyTokensWithSignature(uint256 count, bytes calldata signature) external {
        _buyTokens(count, signature);
    }

    // a function to a number of NFTs
    // if user are willing to participate in presale, they need to call this function with the EIP-712 approval signature that we signed
    // if user just want to participate in public sale, can pass anything to signature parameter
    // if user has quota to buy less than count that user pass in, he will end up buying the quota he left to buy
    function _buyTokens(uint256 count, bytes memory signature) private nonReentrant {
        // Make sure presale has been set up
        PresaleConfig memory _presaleConfig = presaleConfig;
        require(_presaleConfig.startTime > 0, "ShipsSale: presale not configured");

        require(treasury != address(0), "ShipsSale: treasury not set");
        require(tokenPrice > 0, "ShipsSale: token price not set");
        require(count > 0, "ShipsSale: invalid count");
        require(maxBuyCountPerWallet > 0, "ShipsSale: maxBuyCountPerWallet not set");

        require(block.timestamp >= _presaleConfig.startTime, "ShipsSale: pre sale not started");
        if (block.timestamp < _presaleConfig.endTime) {
            // presale period
            // Verify EIP-712 signature
            bytes32 digest = keccak256(
                abi.encodePacked("\x19\x01", DOMAIN_SEPARATOR, keccak256(abi.encode(PRESALE_TYPEHASH, msg.sender)))
            );
            (address recoveredAddress, ECDSA.RecoverError error) = digest.tryRecover(signature);
            require(
                error == ECDSA.RecoverError.NoError && recoveredAddress != address(0) && recoveredAddress == _presaleConfig.whitelistSigner,
                "ShipSales: invalid approval for presale"
            );
        }

        uint256 saleTokenLeft = tokenToSell - tokenSoldCount;
        require(saleTokenLeft > 0, "ShipsSale: All token sold");

        uint256 walletQuotaLeft = maxBuyCountPerWallet - boughtCounts[msg.sender];
        require(walletQuotaLeft > 0, "ShipsSale: max count of this wallet exceeded");

        uint256 eligibleToBuy = Math.min(count, Math.min(walletQuotaLeft, saleTokenLeft));

        boughtCounts[msg.sender] += eligibleToBuy;
        // This contract do not hold tokens
        paymentToken.safeTransferFrom(msg.sender, treasury, tokenPrice * eligibleToBuy);

        for (uint256 ind = 0; ind < eligibleToBuy; ind++) {
            uint256 tokenIdDrawn = _draw(ind);
            ship.transferFrom(treasury, msg.sender, tokenIdDrawn);
            emit TokenSold(msg.sender, tokenIdDrawn);
        }
    }

    // get a random number within [0, max)
    // @param salt is the salt to avoid duplication for the same tx
    // @param max exclusive
    function getRandom(uint256 salt, uint256 max) private view returns (uint256) {
        uint256 random = uint256(keccak256(abi.encodePacked(block.difficulty, block.timestamp, salt)));
        uint256 index = random % (max);
        return index;
    }

    function tokenAt(uint256 i) private view returns (uint256) {
        if (movedTokens[i] != 0) {
            return movedTokens[i];
        } else {
            return i;
        }
    }
    
    //This is to mark a token being sold before the sale starts, have to be done before any token being sold.
    function markSoldBeforeSale(uint256 tokenId) external onlyOwner {
        uint256 remainingTokenIdsLengthToDraw = nftTotalSupply - tokenSoldCount;
        uint256 lastTokenId = remainingTokenIdsLengthToDraw - 1;
        movedTokens[tokenId] = tokenAt(lastTokenId);
        movedTokens[lastTokenId] = 0;
        tokenSoldCount += 1;
        emit MarkTokenSold(msg.sender, tokenId);
    }

    // Draw another token without replacement
    // This function does not validate whether there are tokens to sell, assuming validation done before calling
    function _draw(uint256 round) private returns (uint256) {
        uint256 remainingTokenIdsLengthToDraw = nftTotalSupply - tokenSoldCount;

        uint256 i = getRandom(round, remainingTokenIdsLengthToDraw);
        uint256 outToken = tokenAt(i);

        uint256 lastTokenId = remainingTokenIdsLengthToDraw - 1;
        movedTokens[i] = tokenAt(lastTokenId);
        movedTokens[lastTokenId] = 0;
        tokenSoldCount += 1;
        return outToken;
    }

    function setShip(address newShip) external onlyOwner {
        require(tokenSoldCount == 0, "ShipsSale: ship cannot be set after first one being sold");
        ship = IERC721(newShip);
        emit ShipChanged(newShip);
    }

    function setMaxBuyCountPerWallet(uint256 _maxBuyCountPerWallet) external onlyOwner {
        maxBuyCountPerWallet = _maxBuyCountPerWallet;
        emit MaxBuyCountPerWalletChanged(_maxBuyCountPerWallet);
    }

    function setTokenPrice(uint256 _tokenPrice) external onlyOwner {
        tokenPrice = _tokenPrice;
        emit TokenPriceChanged(_tokenPrice);
    }

    function setTreasury(address payable _treasury) external onlyOwner {
        treasury = _treasury;
        emit TreasuryChanged(_treasury);
    }

    function setUpPresale(
        address whitelistSigner,
        uint256 startTime,
        uint256 endTime
    ) external onlyOwner {
        uint32 _startTime = startTime.toUint32();
        uint32 _endTime = endTime.toUint32();

        // Check params
        require(whitelistSigner != address(0), "ShipsSale: zero address");
        require(_startTime > 0 && _endTime > _startTime, "ShipsSale: invalid time range");

        presaleConfig = PresaleConfig({whitelistSigner : whitelistSigner, startTime : _startTime, endTime : _endTime});

        emit PresaleConfigChanged(whitelistSigner, _startTime, _endTime);
    }


}