pragma solidity ^0.8.12;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";

import "./ITokenSale.sol";
import "../token/IMultinodeToken.sol";
import "../acl/IACL.sol";
import "../acl/ACLControlled.sol";

contract TokenSale is ITokenSale, ACLControlled {
    event TokensSold();

    using SafeERC20 for IERC20;
    using SafeERC20 for IERC20;

    IERC20 public tokenToSell;
    IERC20 public paymentToken;
    IACL public acl;
    uint256 public price;
    uint256 public maxAllocation;
    uint256 public totalPurchased;
    IMultinodeToken mnode;
    address public override tokenVault;
    SaleState public saleState;
    bool public whitelistMode;
    bytes32 public merkleRoot;

    mapping(address => uint256) public currentPurchases;
    mapping(address => bool) public overrideWhitelisted;

    modifier whenSaleStateIs(SaleState expectedSaleState) {
        require(expectedSaleState == saleState, "sale has not started yet");
        _;
    }

    constructor(
        IMultinodeToken mnode_,
        IERC20 paymentToken_,
        IACL acl_,
        uint256 price_,
        address tokenVault_
    ) {
        tokenToSell = IERC20(address(mnode_));
        paymentToken = IERC20(address(paymentToken_));
        tokenVault = tokenVault_;
        price = price_;
        acl = acl_;
        saleState = SaleState.NOT_STARTED;
        whitelistMode = true;
        maxAllocation = 2000 ether;

        mnode = mnode_;
    }

    function manualWhitelistAdd(address accountToAdd) external override onlyAdmin {
        overrideWhitelisted[accountToAdd] = true;
    }

    function manualWhitelistRemove(address accountToAdd) external override onlyAdmin {
        overrideWhitelisted[accountToAdd] = false;
    }

    function isWhitelisted(bytes32[] memory proof, address account) public view override returns (bool) {
        return overrideWhitelisted[account] || MerkleProof.verify(proof, merkleRoot, _calculateHash(account));
    }

    function buy(uint256 amountOfTokensToBuy, bytes32[] calldata proof)
        external
        override
        whenSaleStateIs(SaleState.SELLING)
    {
        if (whitelistMode) {
            require(isWhitelisted(proof, msg.sender), "Invalid merkle proof");
        }

        _receiveAndBuy(amountOfTokensToBuy);
    }

    function getLeftToBuy() external view override returns (uint256) {
        return tokenToSell.allowance(tokenVault, address(this));
    }

    function disableWhitelistSale() external override onlyAdmin {
        whitelistMode = false;
    }

    function startSale() external override onlyAdmin {
        require(merkleRoot != bytes32(0), "Merkle root not yet set");
        whitelistMode = true;
        saleState = SaleState.SELLING;
    }

    function stopSale() external override onlyAdmin {
        saleState = SaleState.FINISHED;

        mnode.disableSalesMode();
    }

    function setWhitelistMerkleRoot(bytes32 whitelistMerkleRoot_) external override onlyAdmin {
        merkleRoot = whitelistMerkleRoot_;
    }

    function setTokenVault(address tokenVault_) external override onlyAdmin {
        tokenVault = tokenVault_;
    }

    /// @dev price is set in units. 2.5 USDC -> 2500000
    function setPrice(uint256 price_) external override onlyAdmin {
        price = price_;
    }

    function maxAllocationPerUser(uint256 maxAllocation_) external override onlyAdmin {
        maxAllocation = maxAllocation_;
    }

    function withdrawFunds() external override onlyAdmin {
        paymentToken.transfer(msg.sender, paymentToken.balanceOf(address(this)));
    }

    /// @dev  amountOfTokensToBuy is set in tokens. 1 MNODE -> 1
    function _receiveAndBuy(uint256 amountOfTokensToBuy) public {
        uint256 amountOfPaymentTokens = amountOfTokensToBuy * price;
        uint256 adjustedTokenToSell = amountOfTokensToBuy * (10**IERC20Metadata(address(tokenToSell)).decimals());
        require(
            currentPurchases[msg.sender] + adjustedTokenToSell <= maxAllocation,
            "User cannot buy any more tokens!"
        );

        currentPurchases[msg.sender] += adjustedTokenToSell;
        totalPurchased += adjustedTokenToSell;

        paymentToken.safeTransferFrom(msg.sender, address(this), amountOfPaymentTokens);
        tokenToSell.safeTransferFrom(tokenVault, msg.sender, adjustedTokenToSell);

        emit TokensSold();
    }

    function _calculateHash(address account) internal view virtual returns (bytes32) {
        return keccak256(abi.encodePacked(address(this), account, block.chainid));
    }

    function _getACl() internal virtual override returns (IACL) {
        return acl;
    }
}

