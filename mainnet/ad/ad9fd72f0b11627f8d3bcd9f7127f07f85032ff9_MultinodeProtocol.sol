pragma solidity ^0.8.12;

import "@openzeppelin/contracts-upgradeable/token/ERC20/ERC20Upgradeable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/introspection/ERC165Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";

import "../protocol/IMultinodeProtocol.sol";
import "../sale/TokenSale.sol";
import "../acl/ACLControlled.sol";
import "../acl/IACL.sol";
import "./IMultinodeToken.sol";

contract MultinodeToken is IMultinodeToken, ERC20Upgradeable, ERC165Upgradeable, UUPSUpgradeable, ACLControlled {
    using EnumerableSet for EnumerableSet.AddressSet;
    using EnumerableSet for EnumerableSet.UintSet;
    using SafeERC20Upgradeable for IERC20;
    using Address for address;

    uint256 internal constant PROPORTION_BASE = 1000000000;
    uint256 public constant TAX_AGGREGATE = 24 hours;
    uint256 public constant TAX_BASE = 100;

    address public protocol;
    TokenSale public tokenSale;
    IACL public acl;
    bool public tokenSaleEnabled;
    bool private _salesTaxEnabled;
    EnumerableSet.AddressSet internal _taxTaggers;
    EnumerableSet.AddressSet internal _taxExempt;

    TaxBracket public MAX_TAX;
    TaxBracket public MIN_TAX;

    Timestamp public TAX_TIME_DURATION;

    mapping(address => uint256) internal _taxSafeIndex;
    mapping(address => TaxSafe[]) internal _taxSafe;

    modifier onlyProtocol() {
        require(msg.sender == protocol, "Only the protocol can execute this method!");
        _;
    }

    // Empty constructor for the proxy
    constructor() initializer {}

    function __initialize(
        address mintTo,
        uint256 initMint,
        address acl_
    ) external initializer {
        // init
        __UUPSUpgradeable_init();
        __ERC20_init("MultiNode", "MNode");

        _mint(mintTo, initMint);
        acl = IACL(acl_);

        MAX_TAX = TaxBracket.wrap(50);
        MIN_TAX = TaxBracket.wrap(10);
        TAX_TIME_DURATION = Timestamp.wrap(15 days);

        _salesTaxEnabled = false;
        tokenSaleEnabled = true;
    }

    function setTokenSale(address tokenSale_) external onlyAdmin {
        tokenSale = TokenSale(tokenSale_);
    }

    function setProtocolContract(address protocol_) external override onlyAdmin {
        _taxExempt.add(protocol_);
        protocol = protocol_;
    }

    function addTaxExemptAddress(address account) external override onlyAdmin {
        _taxExempt.add(account);
    }

    function removeTaxExemptAddress(address account) external override onlyAdmin {
        _taxExempt.remove(account);
    }

    function addTaxTaggingAddress(address account) external override onlyAdmin {
        _taxTaggers.add(account);
    }

    function removeTaxTaggingAddress(address account) external override onlyAdmin {
        _taxTaggers.remove(account);
    }

    function mint(address to, uint256 amount) external override onlyProtocol {
        _mint(to, amount);
    }

    function burn(address from, uint256 amount) external override onlyProtocol {
        _burn(from, amount);
    }

    function disableSalesMode() external override {
        require(msg.sender == address(tokenSale), "Only the token sales contract can disable this");
        require(protocol != address(0), "Can only be enabled after protocol contract has been set");

        tokenSaleEnabled = false;
    }

    function disableGlobalTax() external override onlyAdmin {
        _salesTaxEnabled = false;
    }

    function enableGlobalTax() external override onlyAdmin {
        require(protocol != address(0), "The protocol address must be set!");
        _salesTaxEnabled = true;
    }

    function setMaxTaxBracket(TaxBracket maxTax) external override onlyAdmin {
        require(
            TaxBracket.unwrap(maxTax) >= TaxBracket.unwrap(MIN_TAX),
            "Max tax bracket must be larger or equal than min tax"
        );
        MAX_TAX = maxTax;
    }

    function setMinTaxBracket(TaxBracket minTax) external override onlyAdmin {
        require(
            TaxBracket.unwrap(MAX_TAX) >= TaxBracket.unwrap(minTax),
            "Min tax bracket must be lesser or equal than max tax"
        );
        MIN_TAX = minTax;
    }

    function setTaxCountdownDuration(Timestamp duration) external override onlyAdmin {
        TAX_TIME_DURATION = duration;
    }

    function getTaxInfo(address taxed) external view override returns (TaxSafe[] memory) {
        TaxSafe[] storage taxInfo = _taxSafe[taxed];
        uint256 taxIndexStart = _taxSafeIndex[taxed];
        uint256 activeTaxEntries = taxInfo.length - taxIndexStart;

        TaxSafe[] memory returnableTaxInfo = new TaxSafe[](activeTaxEntries);
        for (uint256 i = 0; i < activeTaxEntries; i++) {
            returnableTaxInfo[i] = taxInfo[taxIndexStart + i];
        }

        return returnableTaxInfo;
    }

    function getTaxExemptAddresses() external view override returns (address[] memory) {
        return _taxExempt.values();
    }

    function getTaxTaggingAddresses() external view override returns (address[] memory) {
        return _taxTaggers.values();
    }

    function isTaxEnabled() public view override returns (bool) {
        return _salesTaxEnabled && protocol != address(0);
    }

    function supportsInterface(bytes4 interfaceId) public pure override returns (bool) {
        return interfaceId == type(IMultinodeToken).interfaceId || interfaceId == type(IERC20).interfaceId;
    }

    function calculateAmountToBurn(
        uint256 amountToSpend,
        uint256 timestamp,
        TaxSafe[] memory taxData,
        TaxBracket minTax,
        TaxBracket maxTax,
        Timestamp taxTimeDuration
    ) public pure returns (uint256) {
        uint256 iteratedAmount = 0;
        uint256 taxAppliedAmount = 0;

        for (uint256 newSafeIndex = 0; newSafeIndex < taxData.length; newSafeIndex++) {
            uint128 iterTaxed = TokenCount.unwrap(taxData[newSafeIndex].amount);
            uint128 taxedAt = Timestamp.unwrap(taxData[newSafeIndex].received);
            iteratedAmount += iterTaxed;

            if (iteratedAmount > amountToSpend) {
                uint128 taxLeftover = uint128(iteratedAmount - amountToSpend);
                uint128 amountToTax = iterTaxed - taxLeftover;

                taxAppliedAmount += _calculateProportionalTax(
                    taxedAt,
                    amountToTax,
                    timestamp,
                    maxTax,
                    minTax,
                    taxTimeDuration
                );

                break;
            }
            taxAppliedAmount += _calculateProportionalTax(
                taxedAt,
                iterTaxed,
                timestamp,
                maxTax,
                minTax,
                taxTimeDuration
            );
        }
        return taxAppliedAmount;
    }

    /// @dev special handling for token tax:
    ///       -[from || to == protocol] => don't apply tax
    ///       -([from == user && to == user || exchange)] => transfer to protocol
    ///       -[from == exchange] => apply transfer tax counter
    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual override {
        if (isTaxEnabled()) {
            if (!_taxExempt.contains(from) && !_taxTaggers.contains(from)) {
                bool fromIsUser = !from.isContract();
                if (fromIsUser) {
                    require(
                        IMultinodeProtocol(protocol).getUserNodeCount(from) > 0,
                        "Can only transfer tokens if you own some nodes (and you're not a contract)!"
                    );
                }
                uint256 toBurn = _spendTaxedTokens(from, amount);
                if (!_taxExempt.contains(to)) {
                    amount = amount - toBurn;
                    _burn(from, toBurn);
                }
            }
        }

        super._transfer(from, to, amount);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal virtual override {
        if (tokenSaleEnabled) {
            require(from == tokenSale.tokenVault(), "Currently only the sales contracts vault can send tokens!");
        }

        if (isTaxEnabled() && _taxTaggers.contains(from)) {
            _appendTaxedData(to, uint128(amount));
        }

        super._beforeTokenTransfer(from, to, amount);
    }

    function _spendTaxedTokens(address account, uint256 amount) internal returns (uint256 toTax) {
        // Read common storage
        TaxBracket maxTax = MAX_TAX;
        TaxBracket minTax = MIN_TAX;
        Timestamp taxTimeDuration = TAX_TIME_DURATION;

        // Prepare for iterating
        TaxSafe[] storage taxData = _taxSafe[account];
        uint256 iteratedAmount = 0;
        uint256 taxAppliedAmount = 0;

        // Oldest entries are at the beginning
        uint256 newSafeIndex = _taxSafeIndex[account];
        for (newSafeIndex; newSafeIndex < taxData.length; newSafeIndex++) {
            uint128 iterTaxed = TokenCount.unwrap(taxData[newSafeIndex].amount);
            uint128 taxedAt = Timestamp.unwrap(taxData[newSafeIndex].received);
            iteratedAmount += iterTaxed;

            if (iteratedAmount > amount) {
                uint128 taxLeftover = uint128(iteratedAmount - amount);
                uint128 amountToTax = iterTaxed - taxLeftover;

                taxAppliedAmount += _calculateProportionalTax(
                    taxedAt,
                    amountToTax,
                    block.timestamp,
                    maxTax,
                    minTax,
                    taxTimeDuration
                );
                taxData[newSafeIndex].amount = TokenCount.wrap(taxLeftover);

                break;
            }
            taxAppliedAmount += _calculateProportionalTax(
                taxedAt,
                iterTaxed,
                block.timestamp,
                maxTax,
                minTax,
                taxTimeDuration
            );
            taxData[newSafeIndex].amount = TokenCount.wrap(0);
            taxData[newSafeIndex].received = Timestamp.wrap(0);
        }
        _taxSafeIndex[account] = newSafeIndex;

        return taxAppliedAmount;
    }

    function _appendTaxedData(address to, uint128 amount) internal {
        _appendTaxedData(to, amount, Timestamp.wrap(uint128(block.timestamp)));
    }

    function _hasTransferrableTax(address account) internal view returns (bool) {
        return _taxSafeIndex[account] < _taxSafe[account].length;
    }

    function _appendTaxedData(
        address to,
        uint128 amount,
        Timestamp taxedAt
    ) internal {
        // If receiving tokens from an exchange, append the taxed data
        // Either append at the latest entry or create a new one
        if (_hasTransferrableTax(to)) {
            TaxSafe[] storage taxData = _taxSafe[to];
            TaxSafe storage lastEntry = taxData[taxData.length - 1];

            // if the current latest `tax` entry will expire later than the incoming tax data, then
            // we just append the incoming to that one.
            // else we create a new entry
            if ((Timestamp.unwrap(lastEntry.received) + TAX_AGGREGATE) > Timestamp.unwrap(taxedAt)) {
                lastEntry.amount = TokenCount.wrap(TokenCount.unwrap(lastEntry.amount) + amount);
                return;
            }
        }
        _taxSafe[to].push(TaxSafe({received: taxedAt, amount: TokenCount.wrap(amount)}));
    }

    function _calculateProportionalTax(
        uint256 taxedAt,
        uint256 amount,
        uint256 currentTime,
        TaxBracket maxTax,
        TaxBracket minTax,
        Timestamp taxTimeDuration
    ) internal pure returns (uint256 amountToSpendOnTax) {
        uint256 timePassedDelta = currentTime - taxedAt;
        uint256 proportionallyPassedTime = (timePassedDelta * PROPORTION_BASE) / Timestamp.unwrap(taxTimeDuration);

        uint256 taxPercentage = (TaxBracket.unwrap(maxTax) * PROPORTION_BASE) -
            (proportionallyPassedTime * (TaxBracket.unwrap(maxTax) - TaxBracket.unwrap(minTax)));

        uint256 tokensToTax = (amount * taxPercentage) / (PROPORTION_BASE * TAX_BASE);
        return tokensToTax;
    }

    function _getACl() internal virtual override returns (IACL) {
        return acl;
    }

    function _authorizeUpgrade(address newImplementation) internal virtual override onlyAdmin {}
}

