    function burn(address from, uint256 amount) external;

    function disableGlobalTax() external;

    function enableGlobalTax() external;

    function disableSalesMode() external;

    function setMaxTaxBracket(TaxBracket maxTax) external;

    function setMinTaxBracket(TaxBracket minTax) external;

    function calculateAmountToBurn(
        uint256 amountToSpend,
        uint256 timestamp,
        TaxSafe[] memory taxData,
        TaxBracket minTax,
        TaxBracket maxTax,
        Timestamp taxTimeDuration
    ) external pure returns (uint256);

    function setTaxCountdownDuration(Timestamp duration) external;

    function getTaxExemptAddresses() external view returns (address[] memory);

    function getTaxTaggingAddresses() external view returns (address[] memory);

    function isTaxEnabled() external view returns (bool);

    function getTaxInfo(address taxed) external returns (TaxSafe[] memory);
}

