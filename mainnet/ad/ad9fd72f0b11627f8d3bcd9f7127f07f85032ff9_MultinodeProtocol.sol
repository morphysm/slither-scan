// SPDX-License-Identifier: MIT
pragma solidity ^0.8.12;

import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";

import "./IACL.sol";

contract ACL is IACL, AccessControlEnumerable {
    constructor() AccessControlEnumerable() {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
    }

    function getAdminRole() external pure returns (bytes32) {
        return DEFAULT_ADMIN_ROLE;
    }

    function checkRole(address account, bytes32 requiredRole) external view {
        _checkRole(requiredRole, account);
    }
}

