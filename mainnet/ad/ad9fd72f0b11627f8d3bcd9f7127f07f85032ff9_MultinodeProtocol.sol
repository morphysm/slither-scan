// SPDX-License-Identifier: MIT
pragma solidity ^0.8.12;

import "@openzeppelin/contracts/utils/Multicall.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/utils/introspection/ERC165.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlEnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";

import "./IMultinodeProtocol.sol";
import "./MultinodeProtocolStorage.sol";
import "../token/IMultinodeToken.sol";
import "../acl/ACLControlled.sol";

contract MultinodeProtocol is
    IMultinodeProtocol,
    MultinodeProtocolStorage,
    ACLControlled,
    UUPSUpgradeable,
    ERC165,
    Multicall
{
    event NodeMinted(address indexed to, NodeType indexed nodeType);
    event InterestWithdrawn(address indexed by, uint256 amount);
    event StablecoinsWithdrawn(address indexed to, uint256 amount);
    event AwardNodeFunctionDisabled();
    event CannotWithdrawYet();
    event NodeAdded(NodeType indexed nodeType, NodePrice indexed nodePrice, ReturnRate indexed returnRate);
    event NodeRemoved(NodeType indexed nodeType);
    event NodeReturnRate(NodeType indexed nodeType, ReturnRate indexed returnRate);
    event NodePriceSet(NodeType indexed nodeType, NodePrice indexed nodePrice);
    event WithdrawalPeriodSet(uint256 withdrawalPeriod);

    using EnumerableSet for EnumerableSet.Bytes32Set;

    modifier nodeTypeMustExist(NodeType nodeType) {
        require(_nodeTypeSet.contains(NodeType.unwrap(nodeType)), "Node type does not exist!");

        _;
    }

    modifier nodeTypeMustNotExist(NodeType nodeType) {
        require(!_nodeTypeSet.contains(NodeType.unwrap(nodeType)), "Node type already exists!");

        _;
    }

    // Empty constructor for the proxy
    constructor() initializer {}

    function __initialize(
        IMultinodeToken multinodeToken_,
        IERC20Upgradeable stableCoin_,
        IACL acl_
    ) external override initializer {
        // init
        __UUPSUpgradeable_init();

        // Set state
        multinodeToken = multinodeToken_;
        stableCoin = stableCoin_;

        deploymentBlock = uint64(block.number);
        _nodeAwardingEnabled = true;
        _setWithdrawalPeriod(24 hours);

        bool mnodeSupported = ERC165(address(multinodeToken_)).supportsInterface(type(IMultinodeToken).interfaceId);
        require(mnodeSupported, "The provided mnode token is not supported!");

        acl = acl_;
    }

    function mintNode(
        NodeType nodeType,
        uint256 mnodePrice,
        uint256 stablecoinPrice
    ) external override nodeTypeMustExist(nodeType) {
        NodePrice memory prices = _nodePricing[nodeType];
        require(stablecoinPrice == prices.stableCoinPrice, "stablecoin price has changed");
        require(mnodePrice == prices.multinodePrice, "mnode price has changed");

        multinodeToken.burn(msg.sender, prices.multinodePrice);
        stableCoin.transferFrom(msg.sender, address(this), prices.stableCoinPrice);

        _awardNode(msg.sender, nodeType);
    }

    function withdrawInterest() public override {
        _withdrawInterest(msg.sender);
    }

    function withdrawalTimePassed(address account) public view override returns (bool, uint256) {
        uint256 deltaSinceLastClaim = block.timestamp - Timestamp.unwrap(_lastWithdrawal[account]);
        uint256 claimCount = deltaSinceLastClaim / _withdrawalPeriod;
        uint256 leftTilNextPayout = _withdrawalPeriod - (deltaSinceLastClaim % _withdrawalPeriod);
        return (
            claimCount > 0 && _ownedNodeCount[account] > 0,
            _ownedNodeCount[account] > 0 ? leftTilNextPayout : _withdrawalPeriod
        );
    }

    function withdrawTokens(address account, uint256 amount) external override onlyAdmin {
        require(account != address(0), "account is address 0");
        stableCoin.transfer(account, amount);

        emit StablecoinsWithdrawn(account, amount);
    }

    function disableAwardNodeFunction() external onlyAdmin {
        _nodeAwardingEnabled = false;

        emit AwardNodeFunctionDisabled();
    }

    function awardNode(address account, NodeType nodeType) external override onlyAdmin nodeTypeMustExist(nodeType) {
        require(_nodeAwardingEnabled, "node awarding has been disabled");

        _awardNode(account, nodeType);
    }

    function registerNewNode(
        NodeType nodeType,
        NodePrice memory nodePrice,
        ReturnRate returnRate
    ) external override onlyAdmin nodeTypeMustNotExist(nodeType) {
        _nodeTypeSet.add(NodeType.unwrap(nodeType));

        _setNodePrice(nodeType, nodePrice);
        _setNodeReturnRate(nodeType, returnRate);

        emit NodeAdded(nodeType, nodePrice, returnRate);
    }

    function removeNode(NodeType nodeType) external override onlyAdmin nodeTypeMustExist(nodeType) {
        _nodeTypeSet.remove(NodeType.unwrap(nodeType));

        delete _nodePricing[nodeType];
        _nodeReturnRates[nodeType] = ReturnRate.wrap(0);

        emit NodeRemoved(nodeType);
    }

    function setNodeReturnRate(NodeType nodeType, ReturnRate returnRate)
        external
        override
        onlyAdmin
        nodeTypeMustExist(nodeType)
    {
        _setNodeReturnRate(nodeType, returnRate);
    }

    function setNodePrice(NodeType nodeType, NodePrice memory nodePrice)
        external
        override
        onlyAdmin
        nodeTypeMustExist(nodeType)
    {
        _setNodePrice(nodeType, nodePrice);
    }

    function supportsInterface(bytes4 interfaceId) public pure override(ERC165) returns (bool) {
        return interfaceId == type(IMultinodeProtocol).interfaceId;
    }

    function setWithdrawalPeriod(uint256 withdrawalPeriod) external onlyAdmin {
        _setWithdrawalPeriod(withdrawalPeriod);
    }

    function getTotalClaimedInterest(address account) external view override returns (uint256 totalClaimedInterest) {
        return _totalClaimed[account];
    }

    function getCurrentInterest(address account) external view override returns (uint256 accumulatedInterest) {
        (accumulatedInterest, ) = _calculateInterest(account);
    }

    function getUserNodes(address account) external view override returns (OwnedNodeMetadata[] memory) {
        OwnedNodeMetadata[] memory ownedNodes = new OwnedNodeMetadata[](_nodeTypeSet.length());
        for (uint256 i = 0; i < _nodeTypeSet.length(); i++) {
            NodeType nodeType = NodeType.wrap(_nodeTypeSet.at(i));
            OwnedNode memory ownedNode = _nodeStates[account][nodeType];
            ownedNodes[i] = OwnedNodeMetadata({ownedNode: ownedNode, nodeType: nodeType});
        }
        return ownedNodes;
    }

    function getUserNodeCount(address account) external view override returns (uint256) {
        return _ownedNodeCount[account];
    }

    function getMultinodeToken() external view override returns (address) {
        return address(multinodeToken);
    }

    function getStableCoin() external view override returns (address) {
        return address(stableCoin);
    }

    function getNodePrice(NodeType nodeType) external view override returns (NodePrice memory) {
        return _nodePricing[nodeType];
    }

    function getNodeReturnRate(NodeType nodeType) external view override returns (ReturnRate) {
        return _nodeReturnRates[nodeType];
    }

    function getWithdrawalPeriod() external view override returns (uint256) {
        return _withdrawalPeriod;
    }

    function getTotalNodes() external view override returns (uint256) {
        return _totalNodes;
    }

    function isNodeAwardingEnabled() external view override returns (bool) {
        return _nodeAwardingEnabled;
    }

    function getNodeMetaData() external view override returns (NodeMetadata[] memory) {
        uint256 amountOfNodes = _nodeTypeSet.length();
        NodeMetadata[] memory metadata = new NodeMetadata[](amountOfNodes);

        for (uint128 i = 0; i < amountOfNodes; i++) {
            NodeType nodeType = NodeType.wrap(_nodeTypeSet.at(i));
            ReturnRate returnRate = _nodeReturnRates[nodeType];
            NodePrice memory nodePrice = _nodePricing[nodeType];

            metadata[i] = NodeMetadata({nodeType: nodeType, price: nodePrice, returnRate: returnRate});
        }

        return metadata;
    }

    function _awardNode(address account, NodeType nodeType) internal {
        if (Timestamp.unwrap(_lastWithdrawal[account]) == 0) {
            _lastWithdrawal[account] = Timestamp.wrap(block.timestamp);
        }

        // Force withdraw all accumulated interest
        _withdrawInterest(account);

        _ownedNodeCount[account] += 1;
        _nodeStates[account][nodeType].count += 1;
        _totalNodes += 1;

        emit NodeMinted(account, nodeType);
    }

    function _withdrawInterest(address account) internal {
        (uint256 accumulatedInterest, uint256 rolloverSecs) = _calculateInterest(account);

        // Set the last withdrawal timestamp to the current ts
        if (accumulatedInterest > 0) {
            _lastWithdrawal[account] = Timestamp.wrap(block.timestamp - rolloverSecs);
        }
        _totalClaimed[account] += accumulatedInterest;

        // Mint tokens
        multinodeToken.mint(account, accumulatedInterest);

        emit InterestWithdrawn(account, accumulatedInterest);
    }

    function _setNodeReturnRate(NodeType nodeType, ReturnRate returnRate) internal {
        _nodeReturnRates[nodeType] = returnRate;

        emit NodeReturnRate(nodeType, returnRate);
    }

    function _setNodePrice(NodeType nodeType, NodePrice memory nodePrice) internal {
        _nodePricing[nodeType] = nodePrice;

        emit NodePriceSet(nodeType, nodePrice);
    }

    function _setWithdrawalPeriod(uint256 withdrawalPeriod) internal {
        _withdrawalPeriod = withdrawalPeriod;

        emit WithdrawalPeriodSet(withdrawalPeriod);
    }

    function _calculateInterest(address account)
        internal
        view
        returns (uint256 accumulatedInterest, uint256 rolloverTimeDelta)
    {
        uint256 deltaSinceLastClaim = block.timestamp - Timestamp.unwrap(_lastWithdrawal[account]);
        uint256 claimCount = deltaSinceLastClaim / _withdrawalPeriod;
        for (uint128 i = 0; i < _nodeTypeSet.length(); i++) {
            NodeType nodeType = NodeType.wrap(_nodeTypeSet.at(i));
            OwnedNode storage ownedNode = _nodeStates[account][nodeType];
            ReturnRate returnRate = _nodeReturnRates[nodeType];

            accumulatedInterest += (claimCount * ownedNode.count * ReturnRate.unwrap(returnRate));
        }
        rolloverTimeDelta = accumulatedInterest == 0 ? 0 : deltaSinceLastClaim % _withdrawalPeriod;
    }

    function _getACl() internal virtual override returns (IACL) {
        return acl;
    }

    function _authorizeUpgrade(address newImplementation) internal virtual override onlyAdmin {}
}

