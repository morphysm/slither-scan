contract PresaleVBOND is Initializable {
    using SafeMath for uint256;
    IToken public VBOND;
    address payable public owner;
    mapping(address => uint256) public amountBought;
    event Bought(address addr, uint256 amount);
    event Pause();
    event Unpause();

    function initialize(address VBOND_addr) public initializer {
       VBOND = IToken(VBOND_addr);
       owner = payable(msg.sender);
       paused = false;
    }


    function amountBoughtUser(address buyer) public view returns(uint256) {
    return amountBought[buyer];
    
 }

    modifier onlyowner() {
        require(owner == msg.sender, 'you are not owner');
        _;
    }

    bool public paused;
    modifier buyAllowed() {
    require(!paused);
    _;
  }
  modifier whenPaused() {
    require(paused);
    _;
  }
  
  function pause() onlyowner public {
    paused = true;
    emit Pause();
    }

  function unpause() onlyowner public {
    paused = false;
    emit Unpause();
    }

    function Buy(uint256 amount) public  buyAllowed {
        require(VBOND.balanceOf(msg.sender) >= amount, "not enough VBOND");
        VBOND.transferFrom(msg.sender,address(this), amount);
        uint256 balance = amountBought[msg.sender];
        uint256 totalAmountBought = amount + balance;
        amountBought[msg.sender] = totalAmountBought;
        emit Bought(msg.sender,totalAmountBought);
    }

    function checkContractBalance() public view returns(uint256) 

    {
        return VBOND.balanceOf(address(this));
    }
    
  	// Burn address
    address public constant BURN_ADDRESS = 0x000000000000000000000000000000000000dEaD;

    function WithdrawVBOND() public onlyowner
    {
        uint256 contractBalance = VBOND.balanceOf(address(this));
        uint256 liquidityAmount = contractBalance*40e18/100e18;
        uint256 burnAmount = contractBalance*60e18/100e18;
        VBOND.transfer(address(msg.sender),liquidityAmount);
        VBOND.transfer(BURN_ADDRESS,burnAmount);
    }

        function WithdrawVBOND(uint256 _amount) public onlyowner
    {

        VBOND.transfer(address(msg.sender),_amount);

    }

}