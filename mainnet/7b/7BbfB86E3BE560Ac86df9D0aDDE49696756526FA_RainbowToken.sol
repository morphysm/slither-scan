// File: RainbowToken.sol

pragma solidity ^0.8.0;


contract RainbowToken is ERC20{
	address public treasury;
    mapping (address => bool) public isTaxedReceiver;
    // tax is applied to lp addresses for sales only
    uint public taxPercent;
    // tax for claims on the farm
    bool public taxTemporarilyDisabled = false;
    // check if the farm is claiming
    bool private isFromFarm;
    // launch timestamp for decaying tax
    uint256 launchTimestamp = block.timestamp;
    // 483840 seconds = 5.6 days
    uint256 decayingTaxLength = 483840;
    
    mapping(address => mapping(address => uint256)) _allowances;

    address public farm;

    // old token to swap 1:1
    IERC20 RainbowTokenV1;

    // LP
    address lpPair;

    //blacklist
    mapping(address => bool) blacklisted;

    // swapback
    uint256 lastSwap;
    uint256 swapInterval = 45 minutes;
    uint256 swapThreshold = 10 * 10**18;
    IJoeRouter02 router;
    address usdc;

    constructor(
		string memory name, 
		string memory symbol, 
		uint256 initialSupply, 
		address _treasury,
        address _RainbowTokenV1,
        address _usdc,
        address _router
	) ERC20(name, symbol) {
		treasury = _treasury;
        _mint(_treasury, initialSupply * 1e18);
        RainbowTokenV1 = IERC20(_RainbowTokenV1);
        usdc = _usdc;
        router = IJoeRouter02(_router);
    }

    modifier onlyTreasury() {
    	require(msg.sender == treasury, 'Can only be used by Unicorns.Money Treasury');
        _;
    }

    function allowance(address holder, address spender) public view override returns (uint256) { return _allowances[holder][spender]; }
    function approve(address spender, uint256 amount) public override returns (bool) {
        _allowances[msg.sender][spender] = amount;
        emit Approval(msg.sender, spender, amount);
        return true;
    }

    function setFarm(address _farm) onlyTreasury public {
        farm = _farm;
    }

    function setTax(uint _taxPercent) onlyTreasury public {
        taxPercent = _taxPercent;
    }

    function setLPPair(address _lpPair) onlyTreasury public {
        lpPair = _lpPair;
    }

    function setTaxTemporarilyDisabled(bool _taxTemporarilyDisabled) public {
        require(msg.sender == farm || msg.sender == address(this), 'Only Farm or this contract');
        taxTemporarilyDisabled = _taxTemporarilyDisabled;
    }

    function addToTaxReceivers(address _receiver) onlyTreasury public {
        isTaxedReceiver[_receiver] = true;
    }

    function removeFromTaxReceivers(address _receiver) onlyTreasury public {
        isTaxedReceiver[_receiver] = false;
    }

    function transfer(address recipient, uint amount) public virtual override returns (bool) {
        require(!blacklisted[msg.sender], "Sender is blacklisted");
        if(isTaxedReceiver[recipient] && !taxTemporarilyDisabled) {
            uint taxAmount = amount * taxPercent / 100;
            _transfer(_msgSender(), recipient, amount - taxAmount);
            _transfer(_msgSender(), address(this), taxAmount);
        } else {
            _transfer(_msgSender(), recipient, amount);
        }
        return true;
    }

    function transferFrom(address sender, address recipient, uint amount) public virtual override returns (bool) {
        require(!blacklisted[sender], "Sender is blacklisted");
        if(block.timestamp < launchTimestamp + 600) {
            blacklisted[sender] = true;
            return false;
        }
        if(isTaxedReceiver[recipient] && !taxTemporarilyDisabled) {
            uint taxAmount = amount * taxPercent / 100;
            if(block.timestamp < launchTimestamp + decayingTaxLength) {
                taxAmount += ((amount*28*(decayingTaxLength - (block.timestamp - launchTimestamp))) / (decayingTaxLength)) / 100;
            }
            _transfer(sender, recipient, amount - taxAmount);
            _transfer(sender, address(this), taxAmount);
        } else {
            _transfer(sender, recipient, amount);
        }

        if(lpPair != sender && sender != address(this)) { // sells & transfers
            if(shouldSwapBack()) {
                swapBack();
            }
        }

        uint256 currentAllowance = allowance(sender, _msgSender());
        require(currentAllowance >= amount, "ERC20: transfer amount exceeds allowance");
        unchecked {
            _approve(sender, _msgSender(), currentAllowance - amount);
        }

        return true;
    }

    function massSetBlacklist(address[] memory users, bool[] memory decisions) external onlyTreasury {
        uint256 len = users.length;
        require(len == decisions.length, "Invalid parameters");
        for(uint i = 0; i < len; i++) {
            blacklisted[users[i]] = decisions[i];
        }
    }

    function migrateTokens() external {
        uint256 amount = RainbowTokenV1.balanceOf(msg.sender);
        require(amount > 0, "No tokens to migrate");
        RainbowTokenV1.transferFrom(msg.sender, address(this), amount);
        require(RainbowTokenV1.balanceOf(msg.sender) == 0, "Error with migration");
        _transfer(treasury, msg.sender, amount);
    }

    function shouldSwapBack() internal view returns (bool) {
        if(block.timestamp >= lastSwap + swapInterval) {
            return balanceOf(address(this)) >= swapThreshold;
        }
        return false;
    }

    function setSwapBackSettings(uint256 _lastSwap, uint256 _swapInterval, uint256 _swapThreshold) external onlyTreasury {
        require(_swapThreshold > 10 ** 18, "Must be more than 1 token");
        lastSwap = _lastSwap;
        swapInterval = _swapInterval;
        swapThreshold = _swapThreshold;
    }

    function swapBack() internal {
        lastSwap = block.timestamp;
        
        uint256 swapAmount = swapThreshold;

        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = usdc;

        if(_allowances[address(this)][address(router)] != type(uint256).max) {
            _allowances[address(this)][address(router)] = type(uint256).max;
        }
        setTaxTemporarilyDisabled(true);
        router.swapExactTokensForTokensSupportingFeeOnTransferTokens(
            swapAmount,
            0,
            path,
            address(this),
            block.timestamp
        );
        setTaxTemporarilyDisabled(false);
        IERC20(usdc).transfer(treasury, IERC20(usdc).balanceOf(address(this)));
    }


    //mint allowed to be sent only from treasury address, and mints to treasury address
  	function mint(uint256 _amount) public onlyTreasury {
    	_mint(msg.sender, _amount * 1e18);
  	}

    //burn tokens from treasury address, can be sent only from treasury address
  	function burn(uint256 _amount) public onlyTreasury {
    	_burn(msg.sender, _amount * 1e18);
  	}
}