        _owner = msg.sender;
        emit OwnershipTransferred(address(0), msg.sender);
    }

    /**
     * @dev Returns the address of the current owner.
     */
    function owner() public view returns (address) {
        return _owner;
    }

    /**
     * @dev Throws if called by any account other than the owner.
     */
    modifier onlyOwner() {
        require(_owner == msg.sender, "Ownable: caller is not the owner");
        _;
    }

    /**
     * @dev Leaves the contract without owner. It will not be possible to call
     * `onlyOwner` functions anymore. Can only be called by the current owner.
     *
     * NOTE: Renouncing ownership will leave the contract without an owner,
     * thereby removing any functionality that is only available to the owner.
     */
    function renounceOwnership() public virtual onlyOwner {
        emit OwnershipTransferred(_owner, address(0));
        _owner = address(0);
    }

    /**
     * @dev Transfers ownership of the contract to a new account (`newOwner`).
     * Can only be called by the current owner.
     */
    function transferOwnership(address newOwner) public virtual onlyOwner {
        require(
            newOwner != address(0),
            "Ownable: new owner is the zero address"
        );
        emit OwnershipTransferred(_owner, newOwner);
        _owner = newOwner;
    }
}

contract OPEC is IOPEC, ERC20, Ownable {

    using SafeMath for uint256;

    // Managers
    mapping(address => bool) public _isBlacklisted; // ok
    mapping(address => bool) public _isExcludedFromFee; // ok
    mapping(address => bool) public _managers; // ok

    IJoeRouter02 public _uniswapV2Router; // ok
    IUniswapV2Pair public _uniswapV2Pair; // ok
    address public _treasury; // ok
    address public _policy;

    bool public _isTaxable = true; // ok
    bool public _isSwapping = false; // ok
    bool public _isMaxLimit = true; // ok
    bool public _isLaunched = false; // ok

    uint256 public _launchTime; // ok

    uint256 public _balanceLimit = 500 * 1e18; // ok
    uint256 public _maxBuyLimit = 500 * 1e18; // ok
    uint256 public _liquidity_sell_tax = 40; // ok

    modifier onlyManager() {
        require(_managers[msg.sender] == true, "NOT MANAGER");
        _;
    }

    modifier onlyTreasury() {
        require(msg.sender == _treasury, "ONLY TREASURY ALLOWED");
        _;
    }

    modifier onlyPolicy() {
        require(msg.sender == _policy, "NOT ALLOWED");
        _;
    }

    constructor (
        address[] memory addresses,
        uint256[] memory balances,
        address uniV2Router,
        address treasury
    ) ERC20("Opulence", "OPEC", 5000000 * 1e18) {

        require(addresses.length == balances.length, "OPEC: CONST INDEX ERROR");

        for (uint256 i = 0; i < addresses.length; i ++) {
            _isExcludedFromFee[addresses[i]] = true;

            _mint(addresses[i], balances[i] * 1e18);
        }

        require(uniV2Router != address(0), "OPEC: CONST ROUTER CANNOT BE ZERO");
        _uniswapV2Router = IJoeRouter02(uniV2Router);

        _uniswapV2Pair = IUniswapV2Pair(IJoeFactory(_uniswapV2Router.factory()).createPair(address(this), _uniswapV2Router.WAVAX()));

        _isExcludedFromFee[address(_uniswapV2Pair)] = true;

        _treasury = treasury;

        _managers[msg.sender] = true;
    }

    receive() external payable {}

    function mint(address account_, uint256 amount_) external onlyPolicy {
        _mint(account_, amount_);
    }

    function setPolicy (address _address) external onlyOwner {
        _policy = _address;
    }

    function setManager(address _address, bool _value) external onlyOwner {
        _managers[_address] = _value;
    }

    function setCheckParameter(PROTOCOL_PARAMETER _parameter, address _address, bool _value) external onlyManager {
        if (_parameter == PROTOCOL_PARAMETER.BLACKLIST_ADDRESS) {
            _isBlacklisted[_address] = _value;
        } else if (_parameter == PROTOCOL_PARAMETER.EXCLUDE_ADDRESS) {
            _isExcludedFromFee[_address] = _value;
        }
    }

    function setBooleanParameter(PROTOCOL_PARAMETER _parameter, bool _value) external onlyManager {
        if (_parameter == PROTOCOL_PARAMETER.TAXABLE) {
            _isTaxable = _value;
        } else if (_parameter == PROTOCOL_PARAMETER.MAX_BUY_LIMIT) {
            _isMaxLimit  = _value;
        }
    }

    function setUintParameter(PROTOCOL_PARAMETER _parameter, uint256 _value) external onlyManager {
        if (_parameter == PROTOCOL_PARAMETER.SELL_TAX) {
            _liquidity_sell_tax = _value;
        } else if (_parameter == PROTOCOL_PARAMETER.LIMIT_AMOUNT) {
            _balanceLimit = _value;
        } else if (_parameter == PROTOCOL_PARAMETER.LIMIT_BUY) {
            _maxBuyLimit = _value;
        }
    }

    function setAddressParameter(PROTOCOL_PARAMETER _parameter, address _value) external onlyManager {
        if (_parameter == PROTOCOL_PARAMETER.TREASURY) {
            _treasury = payable(_value);
        }
    }

    function setLaunch() external onlyManager {
        _launchTime = block.timestamp;
        _isLaunched = true;
    }

    function _transfer(address from, address to, uint256 amount) internal override virtual {
        require(_isLaunched || _isExcludedFromFee[from], "OPEC: TRANSFER NOT LAUCHED YET");
        require(!_isBlacklisted[from] && !_isBlacklisted[to], "OPEC: TRANSFER BLACKLIST");
        require(from != address(0), "OPEC: TRANSFER FROM ZERO ADDRESS");
        require(to != address(0), "OPEC: TRANSFER TO ZERO ADDRESS");

        if (!_isExcludedFromFee[to] && _isMaxLimit) {
            require(amount + balanceOf(to) <= _balanceLimit, "OPEC: TRANSFER RECIPIENT BALANCE LIMIT");
        }

        super._transfer(from, address(this), amount);

        if (_isTaxable) {
            uint256 fees = 0;
            uint256 passedDays = (block.timestamp - _launchTime) / 1 days;
            if (passedDays > 4) {
                passedDays = 4;
            }
            if (!_isExcludedFromFee[from] && to == address(_uniswapV2Pair)) {
                fees = (amount / 100) * (_liquidity_sell_tax - passedDays * 10);
                amount = amount.sub(fees);
            } else if (_isMaxLimit && from == address(_uniswapV2Pair) && !_isExcludedFromFee[to]) {
                if (block.timestamp - _launchTime < 10 minutes) {
                    require(amount <= 10 * 1e18, "OPEC: TRANSFER BUY LIMIT ERROR");
                } else {
                    require(amount <= _maxBuyLimit, "OPEC: TRANSFER BUY LIMIT ERROR");
                }
            }
        }

        super._transfer(address(this), to, amount);

        uint256 contractTokenBalance = balanceOf(address(this));

        if (contractTokenBalance > 0) {
            super._transfer(address(this), _treasury, contractTokenBalance);
        }

        emit Transfer(from, to, amount);
    }
}