import "./JoeLibrary.sol";
import "hardhat/console.sol";

contract ArbitrageFlashLoaner {
    //+-We connect the S.C. with the UniSwap(Factory) tools and Info:_
    IFactory constant joeV2Factory =
        IFactory(0x9Ad6C38BE94206cA50bb0d90783181662f0Cfa10); // same for all networks
    IPangolinFactory constant pangolinV2Factory =
        IPangolinFactory(0xefa94DE7a4656D787667C749f7E1223D71E9FD88);

    //+-We connect the S.C. with the SushiSwap(Factory) tools and Info(Its Code is very similar to the Code of UniSwap, for that reason we use the UniSwap Interface):_
    IJoeRouter02 public traderjoeRouter;
    IPangolinRouter public pangolinRouter;
    address private owner;

    constructor(address _traderjoeRouter, address _pangolinRouter) public {
        traderjoeRouter = IJoeRouter02(_traderjoeRouter);
        pangolinRouter = IPangolinRouter(_pangolinRouter);
        owner = msg.sender;
    }

    fallback() external payable {}

    function getBal(address token) public view returns (uint256) {
        return IERC20(token).balanceOf(msg.sender);
    }

    function withdraw(address _token, uint256 _amount) external {
        require(msg.sender == owner, "Not owner");
        IERC20(_token).transfer(owner, _amount);
    }

    //+-The Trader has to monitor Price Differences between UniSwap and SushiSwap and when finds one, Call This Function:_
    function startArbitrage(
        address token0, /**+-Asset Token S.Contract Address. Ex:_ WBTC.*/
        address token1, /**+-StableCoin Token S.Contract Address. Ex:_ DAI.*/
        uint256 amount0, /**+-ZERO Amount of the Asset "token0" that we want to use in the Transaction.*/
        uint256 amount1, /**+-Amount of Money in "token1" that we are going to borrow with a FlashLoan.*/
        bool borrowPangolin
    ) external {
        if (borrowPangolin) {
            //+-It looks for the Coin Pair in UniSwap:_
            address pairAddress = pangolinV2Factory.getPair(token0, token1);
            //+-You have to make sure that the Coin Pair actually exists in UniSwap:_
            require(pairAddress != address(0), "This pool does not exist");
            //+-Initiates the FlashLoan(To Better Understanding, see UniSwap A.P.I. Documentation):_
            IPangolinPair(pairAddress).swap(
                amount0,
                amount1,
                address(this), /**+-Address where we are going to receive the Token that we borrowed(This S.C.).*/
                bytes("1") /**+-Makes sure that this is not empty so it will Trigger the FlashLoan.(IGNORE THIS).*/
            );
        } else {
            address pairAddress = joeV2Factory.getPair(token0, token1);
            //+-You have to make sure that the Coin Pair actually exists in UniSwap:_
            require(pairAddress != address(0), "This pool does not exist");
            console.log("made it here, getting the loan from Joe");
            //+-Initiates the FlashLoan(To Better Understanding, see UniSwap A.P.I. Documentation):_
            IJoePair(pairAddress).swap(
                amount0,
                amount1,
                address(this), /**+-Address where we are going to receive the Token that we borrowed(This S.C.).*/
                bytes("1") /**+-Makes sure that this is not empty so it will Trigger the FlashLoan.(IGNORE THIS).*/
            );
        }
    }

    // @notice Function is called by the Uniswap V2 pair's `swap` function
    function joeCall(
        address _sender,
        uint256 _amount0,
        uint256 _amount1,
        bytes calldata _data
    ) external {
        console.log("joe callback");
        uint256 deadline = block.timestamp + 10 seconds;
        //+-Array of the 2 Tokens Addresses:_
        address[] memory path = new address[](2);
        //+-Ammount of Token that we borrowed:_
        uint256 amountToken = _amount0 == 0 ? _amount1 : _amount0;
        console.log("amount0", _amount0);
        console.log("amount1", _amount1);
        console.log("amount token", amountToken);
        //+-Addresses of the 2 Tokens in the Liquidity Pool of UniSwap:_
        address token0 = IJoePair(msg.sender).token0();
        address token1 = IJoePair(msg.sender).token1();
        console.log("token0", token0);
        console.log("token1", token1);
        console.log(
            "I am caller",
            JoeLibrary.pairFor(address(joeV2Factory), token0, token1)
        );

        require(
            msg.sender ==
                JoeLibrary.pairFor(address(joeV2Factory), token0, token1),
            "Unauthorized"
        );

        path[0] = _amount0 == 0 ? token1 : token0;
        path[1] = _amount0 == 0 ? token0 : token1;
        console.log(path[0]);
        //+-Pointer to the Token that we are going to Sell on SushiSwap:_
        IERC20 token = IERC20(_amount0 == 0 ? token1 : token0);

        //+-We Allow the Router of SushiSwap to Spendig all our Tokens that are Neccesary to doing the Trade:_
        token.approve(address(pangolinRouter), amountToken);
        uint256 amountRequired = repayAmount(
            path[0],
            path[1],
            msg.sender,
            amountToken
        );
        console.log("amount to be repaid", amountRequired);
        //+-We Sell in SushiSwap the Tokens we Borrowed from UniSwap:_
        uint256 amountReceived = pangolinRouter.swapExactTokensForTokens(
            amountToken, /**+-Ammount of Tokens we are going to Sell.*/
            amountRequired, /**+-Minimum Ammount of Tokens that we expect to receive in exchange for our Tokens.*/
            path, /**+-We tell SushiSwap what Token to Sell and what Token to Buy.*/
            address(this), /**+-Address of this S.C. where the Output Tokens are going to be received.*/
            deadline /**+-Time Limit after which an order will be rejected by SushiSwap(It is mainly useful if you send an Order directly from your wallet).*/
        )[1];

        console.log("Amount received", amountReceived);
        uint256 balanceAfter = IERC20(token0).balanceOf(address(this));
        console.log("Joe balance after swap", balanceAfter);

        uint256 balance1After = IERC20(token1).balanceOf(address(this));
        console.log("WAVAX balance after swap", balance1After);
        //+-Pointer to the other Token that we get as an output from SushiSwap:_
        IERC20 otherToken = IERC20(_amount0 == 0 ? token0 : token1);

        //+-A portion of these Tokens are going to be used for reimburse the FlashLoan of UniSwap:_
        IERC20(token0).transfer(msg.sender, amountRequired);
        //+-We take our Profit:_
    }

    function pangolinCall(
        address _sender,
        uint256 _amount0,
        uint256 _amount1,
        bytes calldata _data /**+-Makes sure that this is not empty so it will Trigger the FlashLoan.(IGNORE THIS).*/
    ) external {
        uint256 deadline = block.timestamp + 10 seconds;
        //+-Array of the 2 Tokens Addresses:_
        address[] memory path = new address[](2);
        //+-Ammount of Token that we borrowed:_
        uint256 amountToken = _amount0 == 0 ? _amount1 : _amount0;
        //+-Addresses of the 2 Tokens in the Liquidity Pool of UniSwap:_
        address token0 = IPangolinPair(msg.sender).token0();
        address token1 = IPangolinPair(msg.sender).token1();

        console.log(
            "I am caller)",
            PangolinLibrary.pairFor(address(pangolinV2Factory), token0, token1)
        );
        //+-Makes sure that the Call comes from one of the Pair S.C.s of UniSwap:_
        require(
            msg.sender ==
                PangolinLibrary.pairFor(
                    address(pangolinV2Factory),
                    token0,
                    token1
                ),
            "Unauthorized"
        );

        //+-Makes sure that ONE of the Amounts is == ZERO:_
        require(_amount0 == 0 || _amount1 == 0);

        //+-Defines the Direction of the Trade (From Token0 to Token1 or vice versa):_
        path[0] = _amount0 == 0 ? token1 : token0;
        path[1] = _amount0 == 0 ? token0 : token1;
        console.log(path[0]);
        //+-Pointer to the Token that we are going to Sell on SushiSwap:_
        IERC20 token = IERC20(_amount0 == 0 ? token1 : token0);

        //+-We Allow the Router of SushiSwap to Spendig all our Tokens that are Neccesary to doing the Trade:_
        token.approve(address(traderjoeRouter), amountToken);

        //+-We Calculate the Ammount of Tokens that we will need to reimburse to the FlashLoan in UniSwap:_
        // uint256 amountRequired =
        //     PangolinLibrary.getAmountsIn(address(pangolinV2Factory), amountToken, path)[0];
        //     console.log('Amount required to return to pangolin', amountRequired);
        uint256 amountRequired = repayAmount(
            path[0],
            path[1],
            msg.sender,
            amountToken
        );
        console.log("amount to be repaid", amountRequired);
        //+-We Sell in SushiSwap the Tokens we Borrowed from UniSwap:_
        uint256 amountReceived = traderjoeRouter.swapExactTokensForTokens(
            amountToken, /**+-Ammount of Tokens we are going to Sell.*/
            amountRequired, /**+-Minimum Ammount of Tokens that we expect to receive in exchange for our Tokens.*/
            path, /**+-We tell SushiSwap what Token to Sell and what Token to Buy.*/
            address(this), /**+-Address of this S.C. where the Output Tokens are going to be received.*/
            deadline /**+-Time Limit after which an order will be rejected by SushiSwap(It is mainly useful if you send an Order directly from your wallet).*/
        )[1];

        console.log("Amount received", amountReceived);
        uint256 balanceAfter = IERC20(token0).balanceOf(address(this));
        console.log("Joe balance after swap", balanceAfter);

        uint256 balance1After = IERC20(token1).balanceOf(address(this));
        console.log("WAVAX balance after swap", balance1After);
        //+-Pointer to the other Token that we get as an output from SushiSwap:_
        IERC20 otherToken = IERC20(_amount0 == 0 ? token0 : token1);

        //+-A portion of these Tokens are going to be used for reimburse the FlashLoan of UniSwap:_
        IERC20(token0).transfer(msg.sender, amountRequired);
        //+-We take our Profit:_
    }

    function repayAmount(
        address _tokenBorrow,
        address _tokenPay,
        address pairAddress,
        uint256 _amount
    ) internal view returns (uint256) {
        uint256 pairBalanceTokenBorrow = IERC20(_tokenBorrow).balanceOf(
            pairAddress
        );
        // uint256 pairBalanceTokenPay = IERC20(_tokenPay).balanceOf(pairAddress);
        return
            ((1000 * IERC20(_tokenPay).balanceOf(pairAddress) * _amount) /
                (997 * pairBalanceTokenBorrow)) + 1;
    }

    // @notice Simple getter for convenience while testing
    function getBalanceOf(address _input, bool user)
        external
        view
        returns (uint256)
    {
        if (_input == address(0)) {
            return address(this).balance;
        }
        if (user) {
            return IERC20(_input).balanceOf(msg.sender);
        } else {
            return IERC20(_input).balanceOf(address(this));
        }
    }
}

pragma solidity >=0.5.0;

import "./interfaces/IJoePair.sol";

import "@openzeppelin/contracts/math/SafeMath.sol";

library JoeLibrary {
    using SafeMath for uint256;

    // returns sorted token addresses, used to handle return values from pairs sorted in this order
    function sortTokens(address tokenA, address tokenB) internal pure returns (address token0, address token1) {
        require(tokenA != tokenB, "JoeLibrary: IDENTICAL_ADDRESSES");
        (token0, token1) = tokenA < tokenB ? (tokenA, tokenB) : (tokenB, tokenA);
        require(token0 != address(0), "JoeLibrary: ZERO_ADDRESS");
    }

    // calculates the CREATE2 address for a pair without making any external calls
    function pairFor(
        address factory,
        address tokenA,
        address tokenB
    ) internal pure returns (address pair) {
        (address token0, address token1) = sortTokens(tokenA, tokenB);
        pair = address(
            uint256(
                keccak256(
                    abi.encodePacked(
                        hex"ff",
                        factory,
                        keccak256(abi.encodePacked(token0, token1)),
                        hex"0bbca9af0511ad1a1da383135cf3a8d2ac620e549ef9f6ae3a4c33c2fed0af91" // init code fuji
                    )
                )
            )
        );
    }

    // fetches and sorts the reserves for a pair
    function getReserves(
        address factory,
        address tokenA,
        address tokenB
    ) internal view returns (uint256 reserveA, uint256 reserveB) {
        (address token0, ) = sortTokens(tokenA, tokenB);
        (uint256 reserve0, uint256 reserve1, ) = IJoePair(pairFor(factory, tokenA, tokenB)).getReserves();
        (reserveA, reserveB) = tokenA == token0 ? (reserve0, reserve1) : (reserve1, reserve0);
    }

    // given some amount of an asset and pair reserves, returns an equivalent amount of the other asset
    function quote(
        uint256 amountA,
        uint256 reserveA,
        uint256 reserveB
    ) internal pure returns (uint256 amountB) {
        require(amountA > 0, "JoeLibrary: INSUFFICIENT_AMOUNT");
        require(reserveA > 0 && reserveB > 0, "JoeLibrary: INSUFFICIENT_LIQUIDITY");
        amountB = amountA.mul(reserveB) / reserveA;
    }

    // given an input amount of an asset and pair reserves, returns the maximum output amount of the other asset
    function getAmountOut(
        uint256 amountIn,
        uint256 reserveIn,
        uint256 reserveOut
    ) internal pure returns (uint256 amountOut) {
        require(amountIn > 0, "JoeLibrary: INSUFFICIENT_INPUT_AMOUNT");
        require(reserveIn > 0 && reserveOut > 0, "JoeLibrary: INSUFFICIENT_LIQUIDITY");
        uint256 amountInWithFee = amountIn.mul(997);
        uint256 numerator = amountInWithFee.mul(reserveOut);
        uint256 denominator = reserveIn.mul(1000).add(amountInWithFee);
        amountOut = numerator / denominator;
    }

    // given an output amount of an asset and pair reserves, returns a required input amount of the other asset
    function getAmountIn(
        uint256 amountOut,
        uint256 reserveIn,
        uint256 reserveOut
    ) internal pure returns (uint256 amountIn) {
        require(amountOut > 0, "JoeLibrary: INSUFFICIENT_OUTPUT_AMOUNT");
        require(reserveIn > 0 && reserveOut > 0, "JoeLibrary: INSUFFICIENT_LIQUIDITY");
        uint256 numerator = reserveIn.mul(amountOut).mul(1000);
        uint256 denominator = reserveOut.sub(amountOut).mul(997);
        amountIn = (numerator / denominator).add(1);
    }

    // performs chained getAmountOut calculations on any number of pairs
    function getAmountsOut(
        address factory,
        uint256 amountIn,
        address[] memory path
    ) internal view returns (uint256[] memory amounts) {
        require(path.length >= 2, "JoeLibrary: INVALID_PATH");
        amounts = new uint256[](path.length);
        amounts[0] = amountIn;
        for (uint256 i; i < path.length - 1; i++) {
            (uint256 reserveIn, uint256 reserveOut) = getReserves(factory, path[i], path[i + 1]);
            amounts[i + 1] = getAmountOut(amounts[i], reserveIn, reserveOut);
        }
    }

    // performs chained getAmountIn calculations on any number of pairs
    function getAmountsIn(
        address factory,
        uint256 amountOut,
        address[] memory path
    ) internal view returns (uint256[] memory amounts) {
        require(path.length >= 2, "JoeLibrary: INVALID_PATH");
        amounts = new uint256[](path.length);
        amounts[amounts.length - 1] = amountOut;
        for (uint256 i = path.length - 1; i > 0; i--) {
            (uint256 reserveIn, uint256 reserveOut) = getReserves(factory, path[i - 1], path[i]);
            amounts[i - 1] = getAmountIn(amounts[i], reserveIn, reserveOut);
        }
    }
}

