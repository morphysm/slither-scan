pragma solidity 0.8.13;

import "./SafeMath.sol";
import "./SafeERC20.sol";
import "../interfaces/IPair.sol";
import "../interfaces/IWAVAX.sol";

library DexLibrary {
    using SafeMath for uint256;
    using SafeERC20 for IERC20;

    bytes private constant zeroBytes = new bytes(0);
