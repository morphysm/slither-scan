	}

  event Test(uint256 shortfall, uint256 liquidity);


  function min(uint x, uint y) internal pure returns (uint z) {
    z = x < y ? x : y;
  }

	function liquidateOneAccount(address account) onlyGov public {
		// 1. get all borrowed token infos
		AccountLimits memory vars = joeLensView.getAccountLimits(address(joetroller), account);
    // if (vars.shortfall == 0) {
    //   return;
    // }

		// 2. get all supply token infos
    address[] memory jTokens = joetroller.getAssetsIn(account);
    JTokenBalances[] memory bals = joeLensView.jTokenBalancesAll(jTokens, account);

    // 3. find max borrow and max supply
    /**
    *  the final repayAmount = min(maxBorrowed * 0.5, maxSupplied)
     */
    address maxBorrowToken;
    uint256 maxBorrowValue;

    address maxSupplyToken;
    uint256 maxSupplyValue;

    for (uint256 i = 0; i < bals.length; i++) {
      JTokenBalances memory b = bals[i];
      if (maxBorrowValue < b.borrowValueUSD) {
        maxBorrowValue = b.borrowValueUSD;
        maxBorrowToken = b.jToken;
      } 

      if (maxSupplyValue < b.supplyValueUSD) {
        maxSupplyValue = b.supplyValueUSD;
        maxSupplyToken = b.jToken;
      }

    }

    if (maxBorrowToken == maxSupplyToken) {
      // can not handle token both supply and borrow
      return;
    }

    // for precision * 0.999
    uint256 maxLiquidateValue = min(maxBorrowValue.mul(5).div(10), maxSupplyValue).mul(999).div(1000);
		uint256 borrowUTokenPrice = oracle.getUnderlyingPrice(maxBorrowToken);
		uint256 decimals = IERC20Ex(IJToken(maxBorrowToken).underlying()).decimals();
    uint256 maxBorrowAmt = maxLiquidateValue.mul(1e18).mul(10**(18-decimals)).div(borrowUTokenPrice);


		// 4. call flashLoan
    // pass args to callback
		bytes memory data = abi.encode(maxBorrowToken, maxBorrowAmt, maxSupplyToken, account);
		address receiverAddress = address(this);
		address[] memory assets = new address[](1);
		assets[0] = IJToken(maxBorrowToken).underlying(); 

		uint256[] memory amounts = new uint256[](1);
		amounts[0] = maxBorrowAmt;
		uint256[] memory modes = new uint256[](1);
		modes[0] = 0;
		address onBehalfOf = address(this);
		uint16 referralCode = 0;
		LENDING_POOL.flashLoan(
			receiverAddress,
			assets,
			amounts,
			modes,
			onBehalfOf,
			data,
			referralCode
		);

		// 6. send remain
		uint balOfBorrow = IERC20(assets[0]).balanceOf(address(this));
		IERC20(assets[0]).transfer(vault, balOfBorrow);	
	}

  function set(uint256 a, uint256 b) internal{
    uint256 c = a + b;
  }

	function executeOperation(
			address[] calldata assets,
			uint256[] calldata amounts,
			uint256[] calldata premiums,
			address initiator,
			bytes calldata params
	)
			external
			override
			returns (bool)
	{
		//
		// This contract now has the funds requested.
		// Your logic goes here.
		//
		liquidateInternal(assets,amounts,premiums,initiator,params);
	
		// 5. auto repay
		// At the end of your logic above, this contract owes
			// the flashloaned amounts + premiums.
			// Therefore ensure your contract has enough to repay
			// these amounts.

		// Approve the LendingPool contract allowance to *pull* the owed amount
		for (uint i = 0; i < assets.length; i++) {
				uint amountOwing = amounts[i].add(premiums[i]);
				uint balOfAsset = IERC20(assets[i]).balanceOf(address(this));
				IERC20(assets[i]).approve(address(LENDING_POOL), amountOwing);
				require(balOfAsset > amountOwing, 'amountOwing');
		}
		return true;
	}

	function liquidateInternal(
			address[] calldata assets,
			uint256[] calldata amounts,
			uint256[] calldata premiums,
			address initiator,
			bytes calldata params
	) internal {	
		(address borrowJToken, uint256 maxBorrowAmt, address supplyJToken, address borrower) = abi.decode(params, (address, uint256, address, address));
    address borrowUToken = IJToken(borrowJToken).underlying();
    require(borrowUToken == assets[0], 'borrow token');
		require(IERC20(assets[0]).balanceOf(address(this)) > 0, 'borrowed > 0');
		

    // 2. liquidateBorrow
		uint balOfSupplyJTokenBefore = IJToken(supplyJToken).balanceOf(address(this));
		// !!!!!!!!!!!!!!!!!!mock
		if (mockLiquidate) {
			borrowJToken = 0x8fE40E834065A55D64348cb26F3d2a4D5458eE47;
		}
    if (borrowUToken == wavax) {
			// unwrap wavax -> avax
			IWETH(wavax).withdraw(maxBorrowAmt);
			require(address(this).balance > maxBorrowAmt, 'withdraw');
      IJEther(borrowJToken).liquidateBorrow{value:maxBorrowAmt}(borrower, supplyJToken);
    } else {
      IERC20(borrowUToken).approve(borrowJToken, maxBorrowAmt);
      IJErc20(borrowJToken).liquidateBorrow(borrower, maxBorrowAmt, supplyJToken);
    }
		uint balOfSupplyJTokenAfter = IJToken(supplyJToken).balanceOf(address(this));
		require(balOfSupplyJTokenAfter > balOfSupplyJTokenBefore, 'seized');

		// 3. calteral redeem to underlying token
		IJToken(supplyJToken).redeem(balOfSupplyJTokenAfter);
		address supplyUToken = IJToken(supplyJToken).underlying();
		uint balOfSupplyUToken = IJToken(supplyUToken).balanceOf(address(this));
		require(balOfSupplyUToken > 0, 'balOfSupplyUToken > 0');

		// 4. swap to borrow token
		IERC20(balOfSupplyUToken).approve(address(router), balOfSupplyUToken);
		router.swapExactTokensForTokens(balOfSupplyUToken, 0, getToken2TokenPath(supplyUToken, borrowUToken), address(this), block.timestamp);
	}

	function getToken2TokenPath(address tokenSrc, address tokenDest) public view returns (address[] memory path) {
		if (tokenSrc == wavax || tokenDest == wavax) {
			address[] memory path = new address[](2);
			path[0] = tokenSrc;
			path[1] = tokenDest;
		} else {
			address[] memory path = new address[](3);
			path[0] = tokenSrc;
			path[1] = wavax;
			path[2] = tokenDest;
		}
	}
