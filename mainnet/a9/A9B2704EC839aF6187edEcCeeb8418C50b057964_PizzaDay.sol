pragma solidity ^0.8.2;

// SPDX-License-Identifier: MIT


import "SafeERC20.sol";
import "ReentrancyGuard.sol";
import "IERC20.sol";
import "ERC20.sol";
import "SignatureChecker.sol";

import "AccessControlEnumerable.sol";
import "SafeMath.sol";
import "Strings.sol";
import "Counters.sol";


import "IFactory.sol";
import "IPair.sol";
import "IRouter.sol";
import "IWrapped.sol";
import "IPizzaDay.sol";


contract PizzaDay is IPizzaDay, ReentrancyGuard, AccessControlEnumerable{
    using SafeERC20 for IERC20;
    using SafeMath for uint256;
    uint256 nonce = 0;
    using Counters for Counters.Counter;
    Counters.Counter public pizzeriaCounters;

    address routerAddress;
    address factoryAddress;
    address usdAddress;
    uint256 fee = 7;
    uint256 fundFeeDec = 5;   
    string public usdCoinStr;
    string public blockchainCoinStr;
    uint256 fixedPrice;
    bool fixedStatus = false;


    bytes32 public constant OWNER_ADMIN_ROLE = keccak256("OWNER_ADMIN_ROLE");
    constructor(address _router, address _factory, address _stableCoin, string memory _usdCoinStr, string memory _blockchainCoinStr)
    {
        usdCoinStr = _usdCoinStr;
        blockchainCoinStr = _blockchainCoinStr;
        routerAddress = _router;
        factoryAddress = _factory;
        usdAddress = _stableCoin; 
        _setRoleAdmin(OWNER_ADMIN_ROLE, OWNER_ADMIN_ROLE);

        _setupRole(OWNER_ADMIN_ROLE, 0x8e90811763b5f4ff0205Ba5Ced08F2b1e3632842);        
        _setupRole(OWNER_ADMIN_ROLE, 0xF88FB8106eaf53AFcd9E814567E30Da2Fe036c1c);
        _setupRole(OWNER_ADMIN_ROLE, 0x851Df03a35272100D048C268C4670af3DA353266);
        _setupRole(OWNER_ADMIN_ROLE, 0x3b0137598449B34c3a8332fCfA2bb489Bc2fed0a);

        _setupRole(OWNER_ADMIN_ROLE, address(this));
    }
    
    string [] public pizzeriaName;   
    mapping (string => uint256) public pizzeriaId;
    mapping (uint256 => bool) public statusRegister;
    mapping(uint256 => address) private pizzeriaAddress;
    mapping(uint256 => string []) public pizzariaFlavors;
    mapping(uint256 => mapping (string => bool)) public pizzariaFlavorRegistered;
    mapping(uint256 => mapping (string => bool)) public pizzariaFlavorEnabled;

    mapping(uint256 => mapping(string => mapping (size => uint256))) public flavorPrice;

    /**
     * @dev Register Pizzeria
     *
     * Requirements:
     *
     */
    function register_pizzeria(string memory _pizzeriaName, flavor [] memory _flavors, address _pizzeriaAccount) public onlyRole(OWNER_ADMIN_ROLE){
        require(pizzeriaAddress[pizzeriaId[_pizzeriaName]] == address(0), "This Pizzeria Name Already Exists");
        pizzeriaName.push(_pizzeriaName);          
        pizzeriaId[_pizzeriaName] = pizzeriaName.length;
        pizzeriaAddress[pizzeriaId[_pizzeriaName]] = _pizzeriaAccount;
        
        for (uint256 i = 0; i < _flavors.length; i++){     
            pizzariaFlavors[pizzeriaId[_pizzeriaName]].push(_flavors[i].name);
            pizzariaFlavorRegistered[pizzeriaId[_pizzeriaName]][_flavors[i].name] = true;
            pizzariaFlavorEnabled[pizzeriaId[_pizzeriaName]][_flavors[i].name] = true;              
            flavorPrice[pizzeriaId[_pizzeriaName]][_flavors[i].name][_flavors[i].size] = _flavors[i].price; 
        }     
        enable_pizzeria(_pizzeriaName);
    }

    /**
     * @dev Manage the prizzeria's flavors
     *
     * Requirements:
     *
     */
    function manage_pizzeria_flavor(string memory _pizzeriaName, flavor [] memory _flavors, bool enable) public onlyRole(OWNER_ADMIN_ROLE){
        require(pizzeriaAddress[pizzeriaId[_pizzeriaName]] != address(0), "This Pizzeria Does Not Exists");
        require(_flavors.length > 0, "no flavor informed");

        for(uint256 i =0; i < _flavors.length; i++){       
            if(pizzariaFlavorRegistered[pizzeriaId[_pizzeriaName]][_flavors[i].name] == false){
                pizzariaFlavors[pizzeriaId[_pizzeriaName]].push(_flavors[i].name);
                pizzariaFlavorRegistered[pizzeriaId[_pizzeriaName]][_flavors[i].name] = true; 
            }     
            flavorPrice[pizzeriaId[_pizzeriaName]][_flavors[i].name][_flavors[i].size] = _flavors[i].price;
            pizzariaFlavorEnabled[pizzeriaId[_pizzeriaName]][_flavors[i].name] = enable;  
        }
    }

    /**
     * @dev Disable a pizzeria to sell using this smartcontract
     *
     * Requirements:
     *
     */
    function disable_pizzeria(string memory _pizzeriaName) public onlyRole(OWNER_ADMIN_ROLE){
        require(pizzeriaAddress[pizzeriaId[_pizzeriaName]] != address(0), "This Pizzeria Does Not Exists");
        statusRegister[pizzeriaId[_pizzeriaName]] = false;
        pizzeriaCounters.decrement();
    }

    /**
     * @dev Enable a pizzeria to sell using this smartcontract
     *
     * Requirements:
     *
     */
    function enable_pizzeria(string memory _pizzeriaName) public onlyRole(OWNER_ADMIN_ROLE){
        require(pizzeriaAddress[pizzeriaId[_pizzeriaName]] != address(0), "This Pizzeria Does Not Exists");
        statusRegister[pizzeriaId[_pizzeriaName]] = true;
        pizzeriaCounters.increment();
    }

    /**
     * @dev Buy or donate pizza using base blockchain coin
     *
     * Requirements:
     *
     */
    function buy_pizza(string memory _pizzeriaName, pizzaOrder [] memory pizzas, bool isDonation, string memory toDonated) external payable nonReentrant() {
        require(msg.value > 0,"No Fund has been sended");
        require(statusRegister[pizzeriaId[_pizzeriaName]],"This pizzeria is not available to sell");
        address pizzeriaAdd = pizzeriaAddress[pizzeriaId[_pizzeriaName]];
        uint256 totalPrice = 0;
        
        for(uint256 i = 0; i < pizzas.length; i++){
            require(pizzariaFlavorEnabled[pizzeriaId[_pizzeriaName]][pizzas[i].flavourName], "This pizza is not available at the pizzeria");
            totalPrice += flavorPrice[pizzeriaId[_pizzeriaName]][pizzas[i].flavourName][pizzas[i].size] * pizzas[i].qty;
        }
        uint256 rate = exchange_rate_coin_usd();
        ERC20 tokenUSD = ERC20(usdAddress);
        uint256 decimals = tokenUSD.decimals();
        uint256 usdAmoutCalc = (msg.value * rate) / 10**18;

        require(usdAmoutCalc >= (totalPrice * 10**(18-decimals)), "Insuficient payement fund");

        uint256 roleCount = getRoleMemberCount(OWNER_ADMIN_ROLE);
        roleCount = roleCount.sub(2);
        uint256 amountFee = (msg.value * fee /100)/roleCount;

        payable(pizzeriaAdd).transfer(msg.value - (amountFee * roleCount) - (fundFeeDec * msg.value/1000));
        for(uint256 i = 0; i < roleCount; i++){
            address account = getRoleMember(OWNER_ADMIN_ROLE, i);
            payable(account).transfer(amountFee);
        }
        if(isDonation){
            emit DonatePizza(_pizzeriaName, toDonated, msg.sender, pizzas, blockchainCoinStr , msg.value, 18);
        }
        else{
            emit BuyPizza(_pizzeriaName, msg.sender, pizzas, blockchainCoinStr, msg.value, 18);
        }
    }

    /**
     * @dev Buy or Donate pizza(s) using stablecoin based on USD, another token can be use too.
     *
     * Requirements:
     *
     */
    function buy_pizza_usd(string memory _pizzeriaName, pizzaOrder [] memory pizzas, uint256 totalValue, bool isDonation, string memory toDonated) external nonReentrant() {
        
        require(totalValue > 0,"No Fund has been sended");
        require(statusRegister[pizzeriaId[_pizzeriaName]],"This pizzeria is not available");

        address pizzeriaAdd = pizzeriaAddress[pizzeriaId[_pizzeriaName]];
        uint256 totalPrice = 0;
        
        for(uint256 i = 0; i < pizzas.length; i++){
            require(pizzariaFlavorEnabled[pizzeriaId[_pizzeriaName]][pizzas[i].flavourName], "This flavor is not available at the pizzeria");
            totalPrice += flavorPrice[pizzeriaId[_pizzeriaName]][pizzas[i].flavourName][pizzas[i].size] * pizzas[i].qty;
        }

        require(totalPrice == totalValue, "Invalid fund sent");

        ERC20 tokenUSD = ERC20(usdAddress);
        uint256 decimals = tokenUSD.decimals();
        try tokenUSD.transferFrom(msg.sender, address(this), totalValue){                
            uint256 roleCount = getRoleMemberCount(OWNER_ADMIN_ROLE);
            roleCount = roleCount.sub(2);
            uint256 amountFee = (totalValue * fee /100)/(roleCount);
            tokenUSD.transfer(pizzeriaAdd, totalValue - (amountFee * roleCount) - (fundFeeDec * totalValue/1000));                
            for(uint256 i = 0; i < roleCount ; i++){
                address account = getRoleMember(OWNER_ADMIN_ROLE, i);
                tokenUSD.transfer(account, amountFee);
            }
        }catch Error (string memory _reason){
            revert(_reason);
        }
        if(isDonation){
            emit DonatePizza(_pizzeriaName, toDonated, msg.sender, pizzas, usdCoinStr, totalValue, decimals);
        }
        else{
            emit BuyPizza(_pizzeriaName, msg.sender, pizzas, usdCoinStr, totalValue, decimals);
        }        
    }

    /**
     * @dev Check current exchange price for the coin
     *
     * Requirements:
     *
     */
     function exchange_rate_coin_usd() public view returns(uint256){ 
        uint256 price = fixedPrice; 
        if(fixedStatus == false)
        {
            IRouter02 Router = IRouter02(routerAddress);        
            address wrappedAddress = Router.WETH();
            ERC20 tokenUSD = ERC20(usdAddress);
            uint8 decimals = tokenUSD.decimals();
            IFactory Factory = IFactory(factoryAddress);
            address pairAddress = Factory.getPair(wrappedAddress, usdAddress);
            IPair Pair = IPair(pairAddress);
            (uint112 reserve0, uint112 reserve1, uint32 blockTimestampLast) = Pair.getReserves();            
            price = 10**12 *(((1005 * 10**3 * (10 **(18 - decimals) * reserve1))/reserve0));
        }
        return price;
    }    

    /**
     * @dev Set Swap Router Address
     *
     * Requirements:
     *
     */
    function set_router_address(address _routerAddress) external onlyRole(OWNER_ADMIN_ROLE){
        routerAddress = _routerAddress;
        emit ChangeConfig(msg.sender, block.timestamp, configEnum.ROUTER);
    }

    /**
     * @dev Set a base price in USD for the coin
     *
     * Requirements:
     *
     */
    function set_fixed_price_status(bool _fixedStatus, uint256 _fixedPrice)external onlyRole(OWNER_ADMIN_ROLE){
        fixedStatus = _fixedStatus;
        fixedPrice = _fixedPrice;
    }

    /**
     * @dev Set Swap Factory Address
     *
     * Requirements:
     *
     */
    function set_factory_address(address _factoryAddress) external onlyRole(OWNER_ADMIN_ROLE){
        factoryAddress = _factoryAddress;
        emit ChangeConfig(msg.sender, block.timestamp, configEnum.FACTORY);
    }
    
    /**
     * @dev Set busd tokenAddress
     *
     * Requirements:
     *
     */
    function set_usd_address(address _usdAddress) external onlyRole(OWNER_ADMIN_ROLE){
        usdAddress = _usdAddress;
        emit ChangeConfig(msg.sender, block.timestamp, configEnum.TOKEN);
    }
    
    /**
     * @dev Withdrawal balance for DAO
     *
     * Requirements:
     *
     */
    function withdrawal_balance(address _to, bytes [] memory signatures)external onlyRole(OWNER_ADMIN_ROLE) nonReentrant() returns(bytes32){
        uint256 roleCount = getRoleMemberCount(OWNER_ADMIN_ROLE);
        require(signatures.length >= (roleCount - 2), "Not Enough signatures");

        ERC20 tokenUSD = ERC20(usdAddress);
        uint256 balanceCoin = address(this).balance;
        uint256 balanceUsd = tokenUSD.balanceOf(address(this));
        bytes32 hashTransfer = keccak256(abi.encodePacked(_to, balanceCoin, balanceUsd, nonce));
        bytes32 hashPrefixed = keccak256(abi.encodePacked("\x19Ethereum Signed Message:\n32", hashTransfer));
        for(uint256 i = 0; i < signatures.length; i++){
            address account = getRoleMember(OWNER_ADMIN_ROLE, i);
            if(SignatureChecker.isValidSignatureNow(account, hashPrefixed, signatures[i]) == false)
            {
                revert("Invalid Transfer");
            } 
        }
        if(balanceCoin > 0){
            payable(_to).transfer(balanceCoin);
        }
        if(balanceUsd > 0){
            tokenUSD.transfer(_to, balanceUsd);
        }
        nonce = nonce.add(1);
    }

    receive() external payable
    {

    }
}

