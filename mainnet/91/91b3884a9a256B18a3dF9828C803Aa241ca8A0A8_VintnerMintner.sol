import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "./OpsReady.sol";

// getting ready: Fund this account with AVAX first
// just like fill it up with 10 AVAX just for gelato
contract VintnerMintner is OpsReady, Ownable, ReentrancyGuard, IERC721Receiver {
    mapping(address => uint256) public toMint;

    using SafeERC20 for IERC20;

    Vintner public immutable VINTNER;
    IERC20 public immutable GRAPE;

    address public currentPurchase;

    event BuyVintner(address indexed buyer, uint256 vintners);
    event RefundVintner(address indexed refunder, uint256 vintners);
    event ClaimVintner(address indexed buyer, uint256 vintners);

    constructor(
        address vintner,
        address grape,
        address payable _ops
    ) OpsReady(_ops) {
        VINTNER = Vintner(vintner);
        GRAPE = IERC20(grape);
        GRAPE.approve(vintner, ~uint256(0));
    }

    function mintOne() public {
        GRAPE.safeTransferFrom(msg.sender, address(this), 55 ether); // 10% fee
        _mint(1);
    }

    function mintTwo() public {
        GRAPE.safeTransferFrom(msg.sender, address(this), 110 ether); // 10% fee
        _mint(2);
    }

    function mintFive() public {
        GRAPE.safeTransferFrom(msg.sender, address(this), 270 ether); // 7% fee
        _mint(5);
    }

    function mintTen() public {
        GRAPE.safeTransferFrom(msg.sender, address(this), 525 ether); // 5% fee
        _mint(10);
    }

    function mintTwenty() public {
        GRAPE.safeTransferFrom(msg.sender, address(this), 1050 ether); // 5% fee
        _mint(20);
    }

    function mintFifty() public {
        GRAPE.safeTransferFrom(msg.sender, address(this), 2550 ether); // 2% fee
        _mint(50);
    }

    receive() external payable {}

    function _mint(uint256 amountToMint) internal nonReentrant {
        require(amountToMint <= 20, "20 cannot be minted in one go");
        emit BuyVintner(msg.sender, amountToMint);

        if (toMint[msg.sender] == 0) {
            // abi encode the vintner purchase thing
            bytes memory encodedPurchase = abi.encodeWithSelector(
                this.purchaseVintner.selector,
                msg.sender
            );

            // create new task to purchase vintners
            IOps(ops).createTaskNoPrepayment(
                address(this),
                this.purchaseVintner.selector,
                address(this),
                encodedPurchase,
                ETH
            );
        }
        toMint[msg.sender] += amountToMint;
    }

    function refund() public nonReentrant {
        emit RefundVintner(msg.sender, toMint[msg.sender]);
        toMint[msg.sender] = 0;
        GRAPE.safeTransfer(msg.sender, toMint[msg.sender] * 50 * 10**18);
    }

    // just an "anything" resolver
    function forwarder(bytes memory execData)
        external
        pure
        returns (bool canExec, bytes memory execPayload)
    {
        canExec = true;
        execPayload = execData;
    }

    function purchaseVintner(address account) public nonReentrant {
        currentPurchase = account;
        require(toMint[account] != 0, "nothing to mint");

        uint256 amountMinted = 0;

        // if the user minted more than 20, loop
        for (uint256 i = 0; i <= (toMint[account] / 20) + 1; i++) {
            if (amountMinted == toMint[account]) {
                break;
            }

            if (toMint[account] - amountMinted <= 20) {
                // mint whatever's left
                VINTNER.mintVintnerWithGrape(toMint[account] - amountMinted);
                amountMinted = toMint[account];
            } else {
                VINTNER.mintVintnerWithGrape(20);
                amountMinted += 20;
            }
        }

        emit ClaimVintner(account, toMint[account]);

        // This should always be true
        assert(amountMinted == toMint[account]);

        toMint[account] = 0;
        currentPurchase = address(0);

        uint256 fee;
        address feeToken;

        (fee, feeToken) = IOps(ops).getFeeDetails();

        _transfer(fee, feeToken);
    }

    function collectGrape() public onlyOwner {
        GRAPE.safeTransfer(msg.sender, GRAPE.balanceOf(address(this)));
    }

    function collectAvax() public onlyOwner {
        payable(msg.sender).transfer(address(this).balance);
    }

    function rescueToken(address token) public onlyOwner {
        IERC20(token).safeTransfer(
            msg.sender,
            IERC20(token).balanceOf(address(this))
        );
    }

    function onERC721Received(
        address operator, // 0x0
        address from, // 0x0
        uint256 tokenId, // important bit
        bytes memory data // N/A
    ) public returns (bytes4) {
        require(msg.sender == address(VINTNER), "Not vintner");
        require(currentPurchase != address(0), "No current user");

        VINTNER.transferFrom(address(this), currentPurchase, tokenId);
        return this.onERC721Received.selector;
    }
}

