{
    struct MintedDetails {
        uint16 totalMinted;
        uint16 kindMinted;
        uint16 alphaMinted;
        uint16 unknownMinted;
        uint16 alienKindMinted;
        uint16 alienAlphaMinted;
        uint16 alienUnknownMinted;
    }

