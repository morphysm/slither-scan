        address owner;
        uint256 timestamp;
    }

    struct Wallet {
        uint16 percentage;
        uint256 balance;
    }

    event MintedBy(
        address owner,
        uint256 tokenId,
        uint8 generation,
        uint8 tokenType,
        uint8 alienType
    );
    event TransferNFT(address from, address to, uint256 tokenId);

    // PunkToken contract address used to burn $PUNKZ when minting new Gen1/Gen2/Gen3 NFTs
    IPunkToken private token;
    // PunkLaboratory contract used to stake the minted NFTs
    IPunkLaboratory private punkLaboratory;

    bytes32 public ADMIN_ROLE;
    bytes32 public PAUSER_ROLE;
    bytes32 public MINTER_ROLE;
    bytes32 public UPGRADER_ROLE;
    bytes32 public SET_URI_ROLE;

    // current generation
    uint8 public generation;
    // number of maximum number of NFTs that can be minted in one transaction
    uint8 private MAX_TOKENS_PER_TRANSACTION;
    // chance of minted NFT with $PUNKZ tokens to be stolen
    uint8 public mintedStoleChance;
    // decrease chance of mitned NFT with $PUNKZ tokens to be stolen
    uint8 public mintedStoleChanceKindDecrease;
    // generation => tokentType => count stolen NFTs
    mapping(uint8 => mapping(uint8 => uint16)) private stolen;
    // steal details for a given NFT
    mapping(uint16 => StolenTokenDetails) public stolenDetails;
    // number of maximum number of NFTs on this contract
    uint16 private MAX_TOKENS;
    // number of tokens per generation
    uint16 private TOKENS_PER_GENERATION;
    // nonce to be used on generating the random token id
    uint16 private nonce;
    // indeces used for picking random ids when NFTs are minted
    uint16[10000][] private indices;
    // mint details for each generation
    MintedDetails[4] public minted;
    // details of a given NFT
    mapping(uint256 => TokenDetail) public tokenDetails;
    // number of punks that a given has including those that are staked
    mapping(address => uint16) public hasPunks;
    // amount of AVAX required to mint one GEN 0 Apex Punk
    uint256 public MINT_PRICE;
    // amount of AVAX added to the mint price after each 1000 Apex Punks on GEN 0
    uint256 private MINT_PRICE_INCREASE;
    // number of $PUNKZ required to mint a GEN 1 NFT
    uint256 private MINT_PRICE_TOKENS_G1;
    // number of $PUNKZ required to mint a GEN 2 NFT
    uint256 private MINT_PRICE_TOKENS_G2;
    // number of $PUNKZ required to mint a GEN 3 NFT
    uint256 private MINT_PRICE_TOKENS_G3;
    // base token URI pointed to the token's metadata
    string private _baseTokenURI;
    // receiver address wallets
    address[5] public receiverAddresses;
    // details of funds received by a specific wallet
    mapping(address => Wallet) public wallets;

    function initialize(
        string memory name_,
        string memory symbol_,
        uint8 MAX_TOKENS_PER_TRANSACTION_,
        address[5] memory receiverAddresses_,
        uint16[5] memory receiverPercentages_
    ) public initializer {
        __ERC721_init(name_, symbol_);
        __ERC721Enumerable_init();
        __ERC721URIStorage_init();
        __Pausable_init();
        __AccessControl_init();
        __UUPSUpgradeable_init();
        __ReentrancyGuard_init();

        receiverAddresses = receiverAddresses_;
        for (uint256 i; i < receiverAddresses_.length; i++) {
            wallets[receiverAddresses_[i]] = Wallet(receiverPercentages_[i], 0);
        }

        for (uint8 i = 0; i < 4; i++) {
            indices.push();
        }

        ADMIN_ROLE = keccak256("ADMIN_ROLE");
        PAUSER_ROLE = keccak256("PAUSER_ROLE");
        MINTER_ROLE = keccak256("MINTER_ROLE");
        UPGRADER_ROLE = keccak256("UPGRADER_ROLE");
        SET_URI_ROLE = keccak256("SET_URI_ROLE");

        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(ADMIN_ROLE, msg.sender);

        _grantRole(PAUSER_ROLE, msg.sender);
        _grantRole(MINTER_ROLE, msg.sender);
        _grantRole(UPGRADER_ROLE, msg.sender);
        _grantRole(SET_URI_ROLE, msg.sender);

        MAX_TOKENS = 40000;
        MAX_TOKENS_PER_TRANSACTION = MAX_TOKENS_PER_TRANSACTION_;
        TOKENS_PER_GENERATION = MAX_TOKENS / 4; // 40000 tokens in total - there will be 4 generations
        MINT_PRICE = .5 ether; // mint price 0.5 AVAX
        MINT_PRICE_INCREASE = .1 ether; // after each 1000 NFTs the price increases with 0.1 AVAX

        MINT_PRICE_TOKENS_G1 = 1000 ether; // meaning 1000 $PUNKZ
        MINT_PRICE_TOKENS_G2 = 5000 ether; // meaning 5000 $PUNKZ
        MINT_PRICE_TOKENS_G3 = 15000 ether; // meaning 15000 $PUNKZ

        // 10% chance of minted NFT with $PUNKZ tokens to be stolen
        mintedStoleChance = 100;
        // 0.2% decrease chance of mitned NFT with $PUNKZ tokens to be stolen
        mintedStoleChanceKindDecrease = 2;
    }

    /**
     * Mint an amount of NFTs to a given list of address
     * @param tos which to mint the NFTs for
     * @param amounts of NFTs to mint
     * @param stake the minted NFTs if is the case
     */
    function airdrops(
        address[] memory tos,
        uint8[] memory amounts,
        bool stake
    ) public payable nonReentrant onlyRole(MINTER_ROLE) {
        if (tos.length != amounts.length) revert PunkNFT_NotTheSameLength();
        for (uint256 i = 0; i < amounts.length; i++) {
            if (
                minted[generation].totalMinted + amounts[i] >
                TOKENS_PER_GENERATION
            ) revert PunkNFT_TooManyToMintForCurrentGeneration();
            _mintTokens(stake, amounts[i], tos[i], generation);
        }
    }

    /**
     * Mint an amount of NFTs with AVAX tokens and stake them if wanted
     * @param to which to mint the NFTs for
     * @param amount of NFTs to mint
     * @param stake the minted NFTs if is the case
     */
    function safeMint(
        address to,
        uint8 amount,
        bool stake
    ) public payable nonReentrant {
        if (minted[0].totalMinted + amount > TOKENS_PER_GENERATION)
            revert PunkNFT_TooManyToMintForGen0();
        if (amount == 0 || amount > MAX_TOKENS_PER_TRANSACTION)
            revert PunkNFT_InvalidMintAmount();
        if (amount * MINT_PRICE != msg.value)
            revert PunkNFT_InvalidPaymentAmount();

        // increase the mint price after each 1000 NFTs minted
        if (
            minted[0].totalMinted / 1000 !=
            (minted[0].totalMinted + amount) / 1000
        ) {
            MINT_PRICE += MINT_PRICE_INCREASE;
        }

        // split the received amount of AVAX to the configured wallets
        splitFunds();

        // mint an amount of NFTs to a specific address
        _mintTokens(stake, amount, to, 0);
    }

    /**
     * Mint an amount of NFTs with $PUNKZ tokens and stake them if wanted
     * @param to which to mint the NFTs for
     * @param amount of NFTs to mint
     * @param stake the minted NFTs if is the case
     */
    function safeMintWithTokens(
        address to,
        uint8 amount,
        bool stake
    ) public payable nonReentrant {
        if (
            generation == 0 ||
            minted[generation].totalMinted == TOKENS_PER_GENERATION
        ) {
            generation++;
        }
        if (generation >= 4) revert PunkNFT_TheFinalGenerationIsMinted();

        uint256 amountToBurn = amount;
        // if the sender doesn't have a kind/alpha/enlightened Punk then they will pay double amount of tokens
        if (hasPunks[msg.sender] == 0) {
            amountToBurn *= 2;
        }

        if (
            minted[generation].totalMinted + amountToBurn >
            TOKENS_PER_GENERATION
        ) revert PunkNFT_TooManyToMintForCurrentGeneration();

        token.burn(_msgSender(), getMintPriceInTokens() * amountToBurn);
        _mintTokens(stake, amount, to, generation);
    }

    function _mintTokens(
        bool stake,
        uint8 amount,
        address to,
        uint8 generation_
    ) internal {
        TokenDetail[] memory tokenIdsDetails = stake
            ? new TokenDetail[](amount)
            : new TokenDetail[](0);
        uint16[] memory tokenIds = stake
            ? new uint16[](amount)
            : new uint16[](0);
        uint8 countPunks;
        for (uint256 i = 0; i < amount; i++) {
            uint16 tokenId = randomId(generation_);
            uint8 tokenType = getTokenType(tokenId, generation_);
            uint8 alienTokenType;
            if (tokenType == 3) {
                alienTokenType = getAlienTokenType(tokenId, generation_);
            }
            TokenDetail memory tokenDetail = TokenDetail(
                generation_,
                tokenType,
                alienTokenType
            );
            tokenDetails[tokenId] = tokenDetail;

            minted[generation_].totalMinted++;

            // check if is minted with tokens and see if the NFT is stolenDetails
            if (generation_ > 0) {
                uint16 numberOfStakedKindAliens = punkLaboratory
                    .aliensStakedByOwner(_msgSender(), 0);
                uint16 finalMintedStoleChance = mintedStoleChanceKindDecrease *
                    numberOfStakedKindAliens;
                if (finalMintedStoleChance > mintedStoleChance) {
                    uint256 index = getRandomNumber(1000);
                    if (index < mintedStoleChance - finalMintedStoleChance) {
                        address winner = punkLaboratory.selectWinner();
                        if (winner != address(0)) {
                            // check that is not alien
                            if (tokenType < 3) {
                                hasPunks[winner]++;
                            }
                            stolenDetails[tokenId] = StolenTokenDetails(
                                winner,
                                block.timestamp
                            );
                            stolen[generation_][tokenType + alienTokenType]++;
                            _safeMint(winner, tokenId);
                            emit MintedBy(
                                winner,
                                tokenId,
                                generation_,
                                tokenType,
                                alienTokenType
                            );
                            continue;
                        }
                    }
                }
            }

            // check that is not alien
            if (tokenType < 3) {
                countPunks++;
            }
            if (stake) {
                tokenIds[i] = tokenId;
                tokenIdsDetails[i] = tokenDetail;
            }
            _safeMint(stake ? address(punkLaboratory) : to, tokenId);
            emit MintedBy(
                _msgSender(),
                tokenId,
                generation_,
                tokenType,
                alienTokenType
            );
        }
        if (countPunks > 0) {
            hasPunks[to] += countPunks;
        }
        if (stake && tokenIds.length > 0) {
            punkLaboratory.stakeOf(to, tokenIds, tokenIdsDetails);
        }
    }

    /**
     * Get the generation of a specific NFT
