    function getRandomNumber(uint16 n) internal view returns (uint16) {
        return
            uint16(
                uint256(
                    keccak256(
                        abi.encodePacked(
                            nonce,
                            block.coinbase,
                            block.difficulty,
                            block.timestamp
                        )
                    )
                ) % n
            );
    }

    /**
     * Get random index and save it
     */
    function randomId(uint8 generation_) internal returns (uint16) {
        uint16 totalSize = TOKENS_PER_GENERATION -
            minted[generation_].totalMinted;
        uint16 index = getRandomNumber(totalSize);

        totalSize--;
        uint16 value;

        uint16 currentValue = indices[generation_][index];
        if (currentValue != 0) {
            value = currentValue;
        } else {
            value = index + TOKENS_PER_GENERATION * generation_;
        }
        uint16 currentLastValue = indices[generation_][totalSize];
        // Move last value to selected position
        if (currentLastValue == 0) {
            // Array position not initialized, so use position
            indices[generation_][index] =
                totalSize +
                TOKENS_PER_GENERATION *
                generation_;
        } else {
            // Array position holds a value so use that
            indices[generation_][index] = currentLastValue;
        }
        nonce++;
        // Don't allow a zero index, start counting at 1
        return value + 1;
    }

