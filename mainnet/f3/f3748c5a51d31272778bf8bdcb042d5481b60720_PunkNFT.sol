    /**
     * Get reward power for a given tokenId
     * @param tokenId for which to get the reward power
     */
    function getRewardPower(uint256 tokenId) public view returns (uint256) {
        TokenDetail memory tokenDetail = tokenDetails[tokenId];
        uint16 reward;
        if (tokenDetail.tokenType == 0) {
            reward = 300;
        } else if (tokenDetail.tokenType == 2) {
            reward = 500;
        } else {
            return 0;
        }

        return reward - (reward * tokenDetail.generation) / 10;
    }

    /**
     * Get punks token type
     * 0 - KIND Punks
     * 1 - Alpha Punks
     * 2 - Enlightened Punks
     * 3 - Aliens
     */
    function getTokenType(uint256 tokenId, uint8 generation_)
        internal
        returns (uint8)
    {
        if (generation_ == 0) {
            if (tokenId <= 8500) {
                minted[generation_].kindMinted++;
                return 0;
            } else if (tokenId > 8500 && tokenId <= 9500) {
                minted[generation_].alphaMinted++;
                return 1;
            } else if (tokenId > 9500 && tokenId <= 10000) {
                minted[generation_].unknownMinted++;
                return 2;
            }
        } else if (generation_ == 1) {
            if (tokenId > 10000 && tokenId <= 11136) {
                minted[generation_].kindMinted++;
                return 0;
            } else if (tokenId > 11136 && tokenId <= 11337) {
                minted[generation_].alphaMinted++;
                return 1;
            } else if (tokenId > 11337 && tokenId <= 11437) {
                minted[generation_].unknownMinted++;
                return 2;
            } else if (tokenId > 11437 && tokenId <= 20000) {
                return 3;
            }
        } else if (generation_ == 2) {
            if (tokenId > 20000 && tokenId <= 20267) {
                minted[generation_].kindMinted++;
                return 0;
            } else if (tokenId > 20267 && tokenId <= 20307) {
                minted[generation_].alphaMinted++;
                return 1;
            } else if (tokenId > 20307 && tokenId <= 20327) {
                minted[generation_].unknownMinted++;
                return 2;
            } else if (tokenId > 20327 && tokenId <= 30000) {
                return 3;
            }
        } else if (generation_ == 3) {
            if (tokenId > 30000 && tokenId <= 30028) {
                minted[generation_].kindMinted++;
                return 0;
            }
            if (tokenId > 30028 && tokenId <= 30035) {
                minted[generation_].alphaMinted++;
                return 1;
            }
            if (tokenId >= 30035 && tokenId <= 30039) {
                minted[generation_].unknownMinted++;
                return 2;
            }
            if (tokenId >= 30039 && tokenId <= 40000) {
                return 3;
            }
        }
        return 4; // invalid value
    }

    /**
     * Get alien token type
     * 0 - Kind Aliens
     * 1 - Alpha Aliens
     * 2 - Enlightened Aliens
     */
    function getAlienTokenType(uint256 tokenId, uint8 generation_)
        internal
        returns (uint8)
    {
        if (generation_ == 1) {
            if (tokenId > 11437 && tokenId <= 18425) {
                minted[generation_].alienKindMinted++;
                return 0;
            } else if (tokenId > 18425 && tokenId <= 19413) {
                minted[generation_].alienAlphaMinted++;
                return 1;
            } else if (tokenId > 19413 && tokenId <= 20000) {
                minted[generation_].alienUnknownMinted++;
                return 2;
            }
        } else if (generation_ == 2) {
            if (tokenId > 20327 && tokenId <= 28510) {
                minted[generation_].alienKindMinted++;
                return 0;
            } else if (tokenId > 28510 && tokenId <= 29495) {
                minted[generation_].alienAlphaMinted++;
                return 1;
            } else if (tokenId > 29495 && tokenId <= 30000) {
                minted[generation_].alienUnknownMinted++;
                return 2;
            }
        } else if (generation_ == 3) {
            if (tokenId > 30039 && tokenId <= 38485) {
                minted[generation_].alienKindMinted++;
                return 1;
            } else if (tokenId > 38485 && tokenId <= 39491) {
                minted[generation_].alienAlphaMinted++;
