        if (
            from != address(0) &&
            from != address(punkLaboratory) &&
            to != address(punkLaboratory)
        ) {
            if (hasPunks[from] > 0) {
                hasPunks[from]--;
            }
            hasPunks[to]++;
            emit TransferNFT(from, to, tokenId);
        }

        super._beforeTokenTransfer(from, to, tokenId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
        returns (string memory)
    {
        return super.tokenURI(tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(
            ERC721Upgradeable,
            ERC721EnumerableUpgradeable,
            AccessControlUpgradeable
        )
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        override
        onlyRole(UPGRADER_ROLE)
    {}

    /**
     * @dev split the received AVAX on configured wallets
     */
    function splitFunds() internal {
        for (uint256 i; i < receiverAddresses.length; i++) {
            address receiverAddress = receiverAddresses[i];
            uint256 percentage = wallets[receiverAddress].percentage;
            wallets[receiverAddress].balance += (msg.value * percentage) / 1000;
        }
    }

    /**
     * @dev send the collected AVAX to the configured wallets
     */
    function sendFunds() public {
        for (uint256 i; i < receiverAddresses.length; i++) {
            address receiverAddress = receiverAddresses[i];
            uint256 balance = wallets[receiverAddress].balance;
            wallets[receiverAddress].balance = 0;
            _sendValueTo(receiverAddress, balance);
        }
    }

    function changeTeamMemberAddress(address newAddress) external {
        for (uint256 i; i < receiverAddresses.length; i++) {
            if (receiverAddresses[i] == _msgSender()) {
                receiverAddresses[i] = newAddress;
                wallets[newAddress] = wallets[_msgSender()];
                delete wallets[_msgSender()];
                break;
            }
        }
    }
}