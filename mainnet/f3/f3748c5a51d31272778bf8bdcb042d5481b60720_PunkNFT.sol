    function getMintPriceInTokens() private view returns (uint256 amount) {
        if (generation == 1) {
            return MINT_PRICE_TOKENS_G1;
        } else if (generation == 2) {
            return MINT_PRICE_TOKENS_G2;
        } else if (generation == 3) {
            return MINT_PRICE_TOKENS_G3;
