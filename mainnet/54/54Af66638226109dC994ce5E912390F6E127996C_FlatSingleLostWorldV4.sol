     * @dev See {FlatLostWorldBase-__FlatLostWorldBase_init}
     *
     * @dev Responsibilities:
     * - Call supers
     * - Emit
     *
     * @param initializationParams_ - Params used for base initialization
     * @param maxSupply_ - The `_maxSupply` for the LostWorld
     * @param price_ - Flat price for the LostWorld
     */
    function initialize(
        InitializationParams calldata initializationParams_,
        uint256 maxSupply_,
        uint256 price_
    ) public initializer {
        // Call super to set initialization params, and max supply
        __SingleLostWorldBase_init(initializationParams_, maxSupply_);
        __FlatLostWorldBase_init(price_);

        emit FlatSingleLostWorldInitialized(
            initializationParams_,
            maxSupply_,
            price_
        );
    }

    /**
     * @notice Implements pricing mechanism by calling `super`
     *
     * @dev Bug fix found here by changing inheritance order
     *
     * @param count - The amount of LostWorlds desired
     * @return uint256 - The price based upon flat pricing. See {FlatLostWorldBase-getCostToMint}.
     */
    function getCostToMint(uint256 count)
        public
        view
        virtual
        override(FlatLostWorldBase, LostWorldBaseV2)
        returns (uint256)
    {
        return super.getCostToMint(count);
    }

    /**
     * @dev Principal function to both mint and update any pricing information
     *
     * @param contextData - The context data to store for future metadata use
     * @param count - The desired number of tokens
     * @return uint256[] - All token id's that were just minted
     *
     * Requirements:
     *
     * - The desired number stays under the `_maxSupply`
     */
    function _mintAndGetTokenIds(string calldata contextData, uint256 count)
        internal
        override
        returns (uint256[] memory)
    {
        require(
            _currentSupply + count <= _maxSupply,
            "MAGTI::not-enough-tokens"
        );
        uint256[] memory tokenIds = new uint256[](count);
        uint256 tokenId = _currentSupply;
        for (uint256 i; i < count; i++) {
            // Mint the token
            _mint(msg.sender, tokenId);

            // Grant Royalty
            _tokenRoyaltyOwner[tokenId] = _royaltyReciever == address(0)
                ? msg.sender
                : _royaltyReciever;

            // Update the context
            _tokenMintContext[tokenId] = contextData;

            // Add to list of tokens that were minted
            tokenIds[i] = tokenId;

            // Update the tokenId
            tokenId++;
        }

        return tokenIds;
    }
}


