pragma solidity ^0.8.4;

import "@openzeppelin/contracts-upgradeable/token/ERC721/ERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721URIStorageUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721BurnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/cryptography/draft-EIP712Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/utils/CountersUpgradeable.sol";
import {SafeMathUpgradeable as SafeMath} from "@openzeppelin/contracts-upgradeable/utils/math/SafeMathUpgradeable.sol";
import {StringsUpgradeable as Strings} from "@openzeppelin/contracts-upgradeable/utils/StringsUpgradeable.sol";

contract CaptainV4 is Initializable, ERC721Upgradeable, ERC721EnumerableUpgradeable, ERC721URIStorageUpgradeable, PausableUpgradeable, ERC721BurnableUpgradeable, EIP712Upgradeable  {
    using CountersUpgradeable for CountersUpgradeable.Counter;
      using SafeMath for uint256;
      using Strings for uint256;
    
    CountersUpgradeable.Counter private _tokenIdCounter;

    address private _owner;
    address private _royaltiesAddr;
    uint256 public royaltyPercentage;
    
    mapping(address => bool) private _presaleList;
    mapping(address => uint256) private _presaleListClaimed;

    
    uint256 public presaleMaxMint = 2;
    uint256 public devReserve = 64;

    uint256 public tokenPrice;





    string public baseURI = "ipfs://QmRAjs3kUhSGd3YySAnwoakZDCUHuTiubu2b2F4j4X1rW4/"; // IPFS URI WILL BE SET AFTER ALL TOKENS SOLD OUT
    uint256 public constant maxSupply = 10000;
    uint256 public constant maxTokenPurchase = 12;


    bool public saleIsActive = true;
    bool public presaleIsActive = true;
    bool public isRevealed = true;

    mapping(address => bool) public authorized;
    mapping(uint256 => string) public cns;

 


      struct CaptainMetadata {
        uint256 tokenId;
        address mintedBy;
        address currentOwner;
        uint256 previousPrice;
        uint256 price;
        uint256 numberOfTransfers;
        bool forSale;
        uint256 kg;
    }

       // map id to ChickenRun obj
    mapping(uint256 => CaptainMetadata) public allCaptainMetadata;

      //  implement the IERC721Enumerable which no longer come by default in openzeppelin 4.x
    // Mapping from owner to list of owned token IDs
    mapping(address => mapping(uint256 => uint256)) private _ownedTokens;

        // Mapping from token ID to index of the owner tokens list
    mapping(uint256 => uint256) private _ownedTokensIndex;

    // Array with all token ids, used for enumeration
    uint256[] private _allTokens;

    // Mapping from token id to position in the allTokens array
    mapping(uint256 => uint256) private _allTokensIndex;


    event NameChange(uint256 chiknNumber, string name);

    function initialize(
             address _contractOwner,
             uint256 _tokenPrice 
    ) initializer public {
        __ERC721_init("CaptainV3", "CPT");
        __ERC721Enumerable_init();
        __ERC721URIStorage_init();
        __Pausable_init();
        __ERC721Burnable_init();
        __EIP712_init("CaptainV3", "1");
        _owner = 0xFf6037e5F26CE054aF50856036f2D07e1Bd61AC2;
        tokenPrice = 0;
    }


         modifier onlyAuthorized() {
        require(
            authorized[msg.sender] || msg.sender == _owner,
            "Not authorized"
        );
        _;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
      }
    
      function setBaseURI(string memory _newBaseURI) public  {
        require(msg.sender == _owner, "Only owner");
        baseURI = _newBaseURI;
      }
    



      function reserveTokens(address _to, uint256 _reserveAmount)
        external
        
      {
        require(msg.sender == _owner, "Only owner");
        require(
          _reserveAmount > 0 && _reserveAmount <= devReserve,
          "Not enough reserve left for team"
        );
        for (uint256 i = 0; i < _reserveAmount; i++) {
          uint256 id = totalSupply();
          _safeMint(_to, id);
        }
        devReserve = devReserve.sub(_reserveAmount);
      }

       function changeName(uint256 _tokenId, string memory name)
        external
        onlyAuthorized
    {
        cns[_tokenId] = name;
        emit NameChange(_tokenId, name);
    }

/*
    function _baseURI() internal pure override returns (string memory) {
        return "https://arweave.net/klgsynaLg1GfBONET2W7jogH7RoUMzeYVPOxPCWwmHM";
    } */

    

    function addAuthorized(address _toAdd) public  {
        require(msg.sender == _owner, "Only owner");
        require(_toAdd != address(0));
        authorized[_toAdd] = true;
    }

        function removeAuthorized(address _toRemove) public  {
              require(msg.sender == _owner, "Only owner");
        require(_toRemove != address(0));
        require(_toRemove != msg.sender);
        authorized[_toRemove] = false;
    }



    
    
  
          
      function toggleSaleState() external  {
            require(msg.sender == _owner, "Only owner");
        saleIsActive = !saleIsActive;
      }
    
      function togglePresaleState() external  {
            require(msg.sender == _owner, "Only owner");
        presaleIsActive = !presaleIsActive;
      }

    function tokensOfOwner(address _tokenOwner)
        external
        view
        returns (uint256[] memory)
      {
        uint256 tokenCount = balanceOf(_tokenOwner);
        if (tokenCount == 0) {
          // Return an empty array
          return new uint256[](0);
        } else {
          uint256[] memory result = new uint256[](tokenCount);
          uint256 index;
          for (index = 0; index < tokenCount; index++) {
            result[index] = tokenOfOwnerByIndex(_tokenOwner, index);
          }
          return result;
        }
      }


     function publicMint(uint256 numberOfToken) public payable {
        require(saleIsActive, "Sale must be active to mint Token");
        require(numberOfToken > 0 && numberOfToken <= maxTokenPurchase,"Can only mint one or 12 tokens at a time");
        require(msg.sender != address(0), "not first address"); 
        require(totalSupply().add(numberOfToken) <= maxSupply,"Purchase would exceed max supply of tokens");
        require( msg.value >= tokenPrice.mul(numberOfToken),"Ether value sent is not correct");

        payable(_royaltiesAddr).transfer(msg.value);

        for (uint256 i = 0; i < numberOfToken; i++) {
         
            _tokenIdCounter.increment();
            uint256 newItemId = _tokenIdCounter.current();
            _safeMint(msg.sender, newItemId);
            CaptainMetadata memory newCaptainMetadata = CaptainMetadata(
                newItemId,
                msg.sender,
                msg.sender,
                0,
                tokenPrice,
                0,
                false,
                100
            );
               allCaptainMetadata[newItemId] = newCaptainMetadata;

        }
   

    }


       function presaleMint(uint256 numberOfToken) public payable {
        require(presaleIsActive, "Presale is not active");
        require(_presaleList[msg.sender], "You are not on the Presale List");
        require(
          totalSupply().add(numberOfToken) <= maxSupply,
          "Purchase would exceed max supply of token"
        );
        require(
          numberOfToken > 0 && numberOfToken <= presaleMaxMint,
          "Cannot purchase this many tokens"
        );

        require(
          _presaleListClaimed[msg.sender].add(numberOfToken) <= presaleMaxMint,
          "Purchase exceeds max allowed"
        );


        require(msg.sender != address(0), "not first address"); 


        payable(msg.sender).transfer(msg.value);

        for (uint256 i = 0; i < numberOfToken; i++) {
         
            _tokenIdCounter.increment();
            uint256 newItemId = _tokenIdCounter.current();
            _presaleListClaimed[msg.sender] += 1;
            _safeMint(msg.sender, newItemId);
            CaptainMetadata memory newCaptainMetadata = CaptainMetadata(
                newItemId,
                msg.sender,
                msg.sender,
                0,
                0,
                0,
                false,
                100
            );
               allCaptainMetadata[newItemId] = newCaptainMetadata;

        }
   

    }

       function addToPresaleList(address[] calldata addresses) external  {
                 require(msg.sender == _owner, "Only owner");
        for (uint256 i = 0; i < addresses.length; i++) {
          require(addresses[i] != address(0), "Can't add the null address");
    
          _presaleList[addresses[i]] = true;
        }
      }

           function removeFromPresaleList(address[] calldata addresses)
        external
        
      {
                require(msg.sender == _owner, "Only owner");
        for (uint256 i = 0; i < addresses.length; i++) {
          require(addresses[i] != address(0), "Can't add the null address");
    
          _presaleList[addresses[i]] = false;
        }
      }

        function setPresaleMaxMint(uint256 maxMint) external  {
                  require(msg.sender == _owner, "Only owner");
        presaleMaxMint = maxMint;
      }

          function onPreSaleList(address addr) external view returns (bool) {
        return _presaleList[addr];
      }
    

         function reveal() public  {
        require(msg.sender == _owner, "Only owner");
        isRevealed = true;
      }


function airdropTokens(uint256 numberOfToken, address toAddress) external  {

          require(msg.sender == _owner, "Only owner");
          require(
          totalSupply().add(numberOfToken) <= maxSupply,
          "Purchase would exceed max supply of tokens"
        );

        for (uint256 i = 1; i <= numberOfToken; i++) {
            _tokenIdCounter.increment();
            uint256 newItemId = _tokenIdCounter.current();
            _safeMint(toAddress, newItemId);
            CaptainMetadata memory newCaptainMetadata = CaptainMetadata(
                newItemId,
                msg.sender,
                toAddress,
                0,
                0,
                0,
                false,
                100
            );
            // add the token id to the allChickenRun
            allCaptainMetadata[newItemId] = newCaptainMetadata;
        }
    }

      function setPriceForSale(
        uint256 _tokenId,
        uint256 _newPrice,
        bool isForSale
    ) external {
        require(_exists(_tokenId));
        address tokenOwner = ownerOf(_tokenId);
        require(tokenOwner == msg.sender);
        CaptainMetadata memory captain = allCaptainMetadata[_tokenId];
        captain.price = _newPrice;
        captain.forSale = isForSale;
        allCaptainMetadata[_tokenId] = captain;
    }

        function getAllSaleTokens() public view returns (uint256[] memory) {
        uint256 _totalSupply = totalSupply();
        uint256[] memory _tokenForSales = new uint256[](_totalSupply);
        uint256 counter = 0;
        for (uint256 i = 1; i <= _totalSupply; i++) {
            if (allCaptainMetadata[i].forSale == true) {
                _tokenForSales[counter] = allCaptainMetadata[i].tokenId;
                counter++;
            }
        }
        return _tokenForSales;
    }

      function buyToken(uint256 _tokenId) public payable {
        // check if the token id of the token being bought exists or not
        require(_exists(_tokenId),"not exist token");
        // get the token's owner
        address tokenOwner = ownerOf(_tokenId);
        // token's owner should not be an zero address account
        require(tokenOwner != address(0),"address(0) problem");
        // the one who wants to buy the token should not be the token's owner
        require(tokenOwner != msg.sender,"msg.sender problem");
        // get that token from all ChickenRun mapping and create a memory of it defined as (struct => ChickenRun)
        CaptainMetadata memory captain = allCaptainMetadata[_tokenId];
        // price sent in to buy should be equal to or more than the token's price
        require(msg.value >= captain.price,"msg.value >= captain.price problem");
        // token should be for sale
        require(captain.forSale,"forSale is false");
        uint256 amount = msg.value;
        uint256 _royaltiesAmount = (amount * royaltyPercentage) / 100;
        uint256 payOwnerAmount = amount - _royaltiesAmount;
        payable(_royaltiesAddr).transfer(_royaltiesAmount);
        payable(captain.currentOwner).transfer(payOwnerAmount);
        captain.previousPrice = captain.price;
        captain.currentOwner = msg.sender;
        allCaptainMetadata[_tokenId] = captain;
        _transfer(tokenOwner, msg.sender, _tokenId);
    }

 

    function setKg(uint256 _tokenId, uint256 _newKg) external  {
        require(msg.sender == _owner, "Only owner");
        CaptainMetadata memory captain = allCaptainMetadata[_tokenId];
        captain.kg = _newKg;
        // set and update that token in the mapping
        allCaptainMetadata[_tokenId] = captain;
    }




    function pause() public  {
            require(msg.sender == _owner, "Only owner");
        _pause();
    }

    function unpause() public  {
            require(msg.sender == _owner, "Only owner");
        _unpause();
    }

 
    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        whenNotPaused
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable)
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    // The following functions are overrides required by Solidity.

    function _afterTokenTransfer(address from, address to, uint256 tokenId)
        internal
        override(ERC721Upgradeable)
    {
        super._afterTokenTransfer(from, to, tokenId);
    }

    function _burn(uint256 tokenId)
        internal
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
    {
        super._burn(tokenId);
    }



    function tokenURI(uint256 tokenId)
        public
        view
        override(ERC721Upgradeable, ERC721URIStorageUpgradeable)
        returns (string memory)
    {
       

            require(
          _exists(tokenId),
          "ERC721Metadata: URI query for nonexistent token"
        );
    
        string memory currentBaseURI = _baseURI();
    
        if (isRevealed == false) {
          return
           "ipfs://Qmd4MVVFmq6cZVWUofqUmHsaioLWDuYxs7KjvuHJyUHQcK/hidden.json";
           // "https://arweave.net/sgPDWrP9Aqw1nJCC-KZcnVfJcdgzqgtfzROoUHi7oAA/hidden.jpg";
        }
    
        return
          bytes(currentBaseURI).length > 0
            ? string(abi.encodePacked(currentBaseURI, tokenId.toString(), ".json"))
            : "";
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}

