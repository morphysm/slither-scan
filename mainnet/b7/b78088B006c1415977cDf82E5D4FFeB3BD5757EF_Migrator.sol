abstract contract ReentrancyGuard {
   
    uint256 private constant _NOT_ENTERED = 1;
    uint256 private constant _ENTERED = 2;

    uint256 private _status;

    constructor() {
        _status = _NOT_ENTERED;
    }

   
    modifier nonReentrant() {
        // On the first call to nonReentrant, _notEntered will be true
        require(_status != _ENTERED, "ReentrancyGuard: reentrant call");

        // Any calls to nonReentrant after this point will fail
        _status = _ENTERED;

        _;

        // By storing the original value once again, a refund is triggered (see
        // https://eips.ethereum.org/EIPS/eip-2200)
        _status = _NOT_ENTERED;
    }
}

contract Migrator is Context, Ownable, ReentrancyGuard {
    using SafeMath for uint256;

    IERC20 public oldToken; // ok

    IERC20 public newToken; // ok

    address public _burnAddress = 0x000000000000000000000000000000000000dEaD; // ok

    constructor () {
    }

    //Start Pre-Sale
    function init(
        address _newToken,
        address _oldToken
    ) external onlyOwner {
        oldToken = IERC20(_oldToken);

        newToken = IERC20(_newToken);
    }

    function migrate() public {
        uint256 oldBalance;
        oldBalance = oldToken.balanceOf(address(msg.sender));

        oldToken.transferFrom(msg.sender, address(this), oldBalance);
        newToken.transfer(msg.sender, oldBalance);
    }

    function burnToken() public onlyOwner {
        uint256 remainOldAmount = oldToken.balanceOf(address(this));
        uint256 remainNewAmount = newToken.balanceOf(address(this));

        oldToken.transfer(_burnAddress, remainOldAmount);
        newToken.transfer(_burnAddress, remainNewAmount);
    }
}