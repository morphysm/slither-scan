    function _setAutomatedMarketMakerPair(address pair, bool value) private {
        require(automatedMarketMakerPairs[pair] != value, "TKN:3");

        automatedMarketMakerPairs[pair] = value;
        emit SetAutomatedMarketMakerPair(pair, value);
    }

    function _transfer(address from, address to, uint256 amount) internal  override antiWhale(from, to, amount) {
        require(!isBlacklisted[from] && !isBlacklisted[to], "BLACKLISTED");
        require(from != address(0), "ERC20:1");
        require(to != address(0), "ERC20:2");

        uint256 remainingAmount = amount;

        if (automatedMarketMakerPairs[to] && from != address(this) && to != address(this) && !swapping) {
            if (nodeManager.isNodeOwner(from)) {

                if (sellFeeWithNodesRewards > 0) {
                    uint256 sellFeeRewardsTokens = amount.mul(sellFeeWithNodesRewards).div(100);
                    super._transfer(from, rewardsWallet, sellFeeRewardsTokens);
                    remainingAmount -= sellFeeRewardsTokens;
                }
                
                if (sellFeeWithNodesDev > 0) {
                    uint256 sellFeeDevTokens = amount.mul(sellFeeWithNodesDev).div(100);
                    super._transfer(from, devWallet, sellFeeDevTokens);
                    remainingAmount -= sellFeeDevTokens;
                }

                if (sellFeeWithNodesLiquidity > 0) {
                    uint256 sellFeeLiquidityTokens = amount.mul(sellFeeWithNodesLiquidity).div(100);
                    super._transfer(from, liquidityWallet, sellFeeLiquidityTokens);
                    remainingAmount -= sellFeeLiquidityTokens;
                }
                
                if (sellFeeWithNodesCharity > 0) {
                    uint256 sellFeeCharityTokens = amount.mul(sellFeeWithNodesCharity).div(100);
                    super._transfer(from, charityWallet, sellFeeCharityTokens);
                    remainingAmount -= sellFeeCharityTokens;
                }

            } else {

                if (sellFeeNoNodesRewards > 0) {
                    uint256 sellFeeRewardsTokens = amount.mul(sellFeeNoNodesRewards).div(100);
                    super._transfer(from, rewardsWallet, sellFeeRewardsTokens);
                    remainingAmount -= sellFeeRewardsTokens;
                }
                
                if (sellFeeNoNodesDev > 0) {
                    uint256 sellFeeDevTokens = amount.mul(sellFeeNoNodesDev).div(100);
                    super._transfer(from, devWallet, sellFeeDevTokens);
                    remainingAmount -= sellFeeDevTokens;
                }

                if (sellFeeNoNodesLiquidity > 0) {
                    uint256 sellFeeLiquidityTokens = amount.mul(sellFeeNoNodesLiquidity).div(100);
                    super._transfer(from, liquidityWallet, sellFeeLiquidityTokens);
                    remainingAmount -= sellFeeLiquidityTokens;
                }
                
                if (sellFeeNoNodesCharity > 0) {
                    uint256 sellFeeCharityTokens = amount.mul(sellFeeNoNodesCharity).div(100);
                    super._transfer(from, charityWallet, sellFeeCharityTokens);
                    remainingAmount -= sellFeeCharityTokens;
                }
            }
        }

        super._transfer(from, to, remainingAmount);
    }

    // External node methods

    function createNodeWithTokens() external {
        address sender = _msgSender();

        require(sender != address(0), "CREATENODE:1");
        require(!isBlacklisted[sender], "BLACKLISTED");
        require(sender != devWallet && sender != rewardsWallet && sender != charityWallet, "CREATENODE:2");

        uint256 nodePrice = nodeManager.getNodePrice().mul(10**18);
        require(balanceOf(sender) >= nodePrice, "Not enough tokens");

        uint256 buyFeeTokens = nodePrice.mul(buyNodeFee).div(100);
        super._transfer(sender, devWallet, buyFeeTokens);

        uint256 rewardsWalletTokens = nodePrice.sub(buyFeeTokens);
        super._transfer(sender, rewardsWallet, rewardsWalletTokens);

        nodeManager.createNode(sender);
    }

    function cashoutReward(uint256 blocktime) external {
        address sender = _msgSender();
        require(sender != address(0), "CASHOUT:1");
        require(!isBlacklisted[sender], "BLACKLISTED");
        require(sender != devWallet && sender != rewardsWallet && sender != charityWallet, "CASHOUT:2");

        uint256 rewardAmount = nodeManager.getNodeReward(sender, blocktime);
        require(rewardAmount > 0, "No rewards available");

        uint256 remainingAmount = rewardAmount;

        uint256 rewardsFeeDevTokens = rewardAmount.mul(claimRewardsFeeDev).div(100);
        super._transfer(rewardsWallet, devWallet, rewardsFeeDevTokens);
        remainingAmount -= rewardsFeeDevTokens;

        uint256 rewardsFeeCharityTokens = rewardAmount.mul(claimRewardsFeeCharity).div(100);
        super._transfer(rewardsWallet, charityWallet, rewardsFeeCharityTokens);
        remainingAmount -= rewardsFeeCharityTokens;

        uint256 rewardsFeeLiquidityTokens = rewardAmount.mul(claimRewardsFeeLiquidity).div(100);
        super._transfer(rewardsWallet, liquidityWallet, rewardsFeeLiquidityTokens);
        remainingAmount -= rewardsFeeLiquidityTokens;

        super._transfer(rewardsWallet, sender, remainingAmount);
        nodeManager.cashoutNodeReward(sender, blocktime);
        totalClaimed += rewardAmount;

        emit Cashout(sender, rewardAmount, blocktime);
    }

    function cashoutAll() external {
        address sender = _msgSender();
        require(sender != address(0), "CASHOUTA:1");
        require(!isBlacklisted[sender], "BLACKLISTED");
        require(sender != devWallet && sender != rewardsWallet && sender != charityWallet, "CASHOUTA:2");

        uint256 rewardAmount = nodeManager.getAllNodesRewards(sender);
        require(rewardAmount > 0, "No rewards available");

        uint256 remainingAmount = rewardAmount;

        uint256 rewardsFeeDevTokens = rewardAmount.mul(claimRewardsFeeDev).div(100);
        super._transfer(rewardsWallet, devWallet, rewardsFeeDevTokens);
        remainingAmount -= rewardsFeeDevTokens;

        uint256 rewardsFeeCharityTokens = rewardAmount.mul(claimRewardsFeeCharity).div(100);
        super._transfer(rewardsWallet, charityWallet, rewardsFeeCharityTokens);
        remainingAmount -= rewardsFeeCharityTokens;

        uint256 rewardsFeeLiquidityTokens = rewardAmount.mul(claimRewardsFeeLiquidity).div(100);
        super._transfer(rewardsWallet, liquidityWallet, rewardsFeeLiquidityTokens);
        remainingAmount -= rewardsFeeLiquidityTokens;

        super._transfer(rewardsWallet, sender, remainingAmount);
        nodeManager.cashoutAllNodesRewards(sender);
        totalClaimed += rewardAmount;

        emit Cashout(sender, rewardAmount, 0);
    }
}