pragma solidity ^0.8.4;

contract HeroKitty is ERC20, Ownable {
    using SafeMath for uint256;

    address public constant BURN_ADDRESS = 0x000000000000000000000000000000000000dEaD;
    address public joeRouterAddress = 0x60aE616a2155Ee3d9A68541Ba4544862310933d4;

    address public rewardsWallet;
    address public devWallet;
    address public charityWallet;
    address public liquidityWallet;

    uint8 public sellFeeNoNodesRewards;
    uint8 public sellFeeNoNodesDev;
    uint8 public sellFeeNoNodesLiquidity;
    uint8 public sellFeeNoNodesCharity;
    uint256 public sellFeeNoNodesTotal;

    uint8 public sellFeeWithNodesRewards;
    uint8 public sellFeeWithNodesDev;
    uint8 public sellFeeWithNodesLiquidity;
    uint8 public sellFeeWithNodesCharity;
    uint256 public sellFeeWithNodesTotal;

    uint8 public buyNodeFee;

    uint8 public claimRewardsFeeDev;
    uint8 public claimRewardsFeeCharity;
    uint8 public claimRewardsFeeLiquidity;
    uint256 public claimRewardsFeeTotal;

    uint16 public maxTransferAmountRate = 5;
    uint16 public maxBalanceAmountRate = 20;

    uint256 public totalClaimed = 0;

    bool public isTradingEnabled = true;
    bool public swapLiquifyEnabled = true;

    IPangolinRouter private joeRouter;
    INodeManager private nodeManager;
    bool private swapping = false;

    mapping(address => bool) public isBlacklisted;
    mapping(address => bool) public automatedMarketMakerPairs;
    mapping(address => bool) private _excludedFromAntiWhale;

    event UpdateJoeRouter(address indexed newAddress, address indexed oldAddress);

    event MaxTransferAmountRateUpdated(address indexed operator, uint256 previousRate, uint256 newRate);
    event MaxBalanceAmountRateUpdated(address indexed operator, uint256 previousRate, uint256 newRate);

    event SetAutomatedMarketMakerPair(address indexed pair, bool indexed value);

    event Cashout(address indexed account, uint256 amount, uint256 indexed blockTime);

    modifier antiWhale(address sender, address recipient, uint256 amount) {
        if (maxTransferAmount() > 0) {
            if (_excludedFromAntiWhale[sender] == false && _excludedFromAntiWhale[recipient] == false) {
                require(amount <= maxTransferAmount(), "Hero Kitty::antiWhale: Transfer amount exceeds the maxTransferAmount");
                require(balanceOf(recipient).add(amount) <= maxBalanceAmount(), "Hero Kitty::antiWhale: Transfer would exceed the maxBalanceAmount of the recipient");
            }
        }
        _;
    }

    constructor(
        address[] memory _addresses,
        uint8[] memory sellFeesNoNodes_,
        uint8[] memory sellFeesWithNodes_,
        uint8 buyNodeFee_,
        uint8[] memory claimRewardsFees_,
        uint256 totalSupply_
    ) ERC20("Hero Kitty", "HRKT")
    {
        _excludedFromAntiWhale[msg.sender] = true;
        _excludedFromAntiWhale[address(0)] = true;
        _excludedFromAntiWhale[address(this)] = true;
        _excludedFromAntiWhale[BURN_ADDRESS] = true;

        require(_addresses[0] != address(0) && _addresses[1] != address(0) && _addresses[2] != address(0) && _addresses[3] != address(0), "CONSTR:1");

        rewardsWallet = _addresses[0];
        devWallet = _addresses[1];
        charityWallet = _addresses[2];
        liquidityWallet = _addresses[3];

        _excludedFromAntiWhale[rewardsWallet] = true;
        _excludedFromAntiWhale[devWallet] = true;
        _excludedFromAntiWhale[charityWallet] = true;
        _excludedFromAntiWhale[liquidityWallet] = true;

        nodeManager = INodeManager(_addresses[4]);

        require(joeRouterAddress != address(0), "CONSTR:2");
        IPangolinRouter _joeRouter = IPangolinRouter(joeRouterAddress);
        joeRouter = _joeRouter;

        address _joePair = IPangolinFactory(_joeRouter.factory()).createPair(address(this), _joeRouter.WAVAX());
        _setAutomatedMarketMakerPair(_joePair, true);
        
        sellFeeNoNodesRewards = sellFeesNoNodes_[0];
        sellFeeNoNodesDev = sellFeesNoNodes_[1];
        sellFeeNoNodesLiquidity = sellFeesNoNodes_[2];
        sellFeeNoNodesCharity = sellFeesNoNodes_[3];
        sellFeeNoNodesTotal = uint256(sellFeeNoNodesRewards)
            .add(sellFeeNoNodesDev)
            .add(sellFeeNoNodesLiquidity)
            .add(sellFeeNoNodesCharity);

        sellFeeWithNodesRewards = sellFeesWithNodes_[0];
        sellFeeWithNodesDev = sellFeesWithNodes_[1];
        sellFeeWithNodesLiquidity = sellFeesWithNodes_[2];
        sellFeeWithNodesCharity = sellFeesWithNodes_[3];
        sellFeeWithNodesTotal = uint256(sellFeeWithNodesRewards)
            .add(sellFeeWithNodesDev)
            .add(sellFeeWithNodesLiquidity)
            .add(sellFeeWithNodesCharity);

        buyNodeFee = buyNodeFee_;

        claimRewardsFeeDev = claimRewardsFees_[0];
        claimRewardsFeeCharity = claimRewardsFees_[1];
        claimRewardsFeeLiquidity = claimRewardsFees_[2];
        claimRewardsFeeTotal = uint256(claimRewardsFeeDev)
            .add(claimRewardsFeeCharity)
            .add(claimRewardsFeeLiquidity);

        _mint(msg.sender, totalSupply_ * (10**18));
    }

    receive() external payable { }
    fallback() external payable { }

    function burn(address account, uint256 amount) external onlyOwner {
        _burn(account, amount);
    }

    function updateJoeRouterAddress(address newAddress) external onlyOwner {
        require(newAddress != address(joeRouter), "HKJoe:1");

        IPangolinRouter	 _joeRouter = IPangolinRouter(newAddress);
        joeRouterAddress = newAddress;
        emit UpdateJoeRouter(newAddress, address(joeRouter));

        address _joePair = IPangolinFactory(joeRouter.factory()).createPair(address(this), _joeRouter.WAVAX());
        _setAutomatedMarketMakerPair(_joePair, true);
    }

    function updateNodeManager(address newVal) external onlyOwner {
        nodeManager = INodeManager(newVal);
    }

    function updateDevWallet(address payable newVal) external onlyOwner {
        devWallet = newVal;
    }

    function updateRewardsWallet(address payable newVal) external onlyOwner {
        rewardsWallet = newVal;
    }

    function updateCharityWallet(address payable newVal) external onlyOwner {
        charityWallet = newVal;
    }

    function updateLiquidityWallet(address payable newVal) external onlyOwner {
        charityWallet = newVal;
    }

    // Update Fees

    function updateSellFeeNoNodes(uint8[] memory sellFeesNoNodes_) external onlyOwner {
        sellFeeNoNodesRewards = sellFeesNoNodes_[0];
        sellFeeNoNodesDev = sellFeesNoNodes_[1];
        sellFeeNoNodesLiquidity = sellFeesNoNodes_[2];
        sellFeeNoNodesCharity = sellFeesNoNodes_[3];
        sellFeeNoNodesTotal = uint256(sellFeeNoNodesRewards)
            .add(sellFeeNoNodesDev)
            .add(sellFeeNoNodesLiquidity)
            .add(sellFeeNoNodesCharity);
    }
    
    function updateSellFeeWithNodes(uint8[] memory sellFeesWithNodes_) external onlyOwner {
        sellFeeWithNodesRewards = sellFeesWithNodes_[0];
        sellFeeWithNodesDev = sellFeesWithNodes_[1];
        sellFeeWithNodesLiquidity = sellFeesWithNodes_[2];
        sellFeeWithNodesCharity = sellFeesWithNodes_[3];
        sellFeeWithNodesTotal = uint256(sellFeeWithNodesRewards)
            .add(sellFeeWithNodesDev)
            .add(sellFeeWithNodesLiquidity)
            .add(sellFeeWithNodesCharity);

    }
    
    function updateBuyNodeFee(uint8 buyNodeFee_) external onlyOwner {
        buyNodeFee = buyNodeFee_;
    }
    
    function updateClaimRewardsFee(uint8[] memory claimRewardsFees_) external onlyOwner {
        claimRewardsFeeDev = claimRewardsFees_[0];
        claimRewardsFeeCharity = claimRewardsFees_[1];
        claimRewardsFeeLiquidity = claimRewardsFees_[2];
        claimRewardsFeeTotal = uint256(claimRewardsFeeDev)
            .add(claimRewardsFeeCharity)
            .add(claimRewardsFeeLiquidity);
    }

    function setAutomatedMarketMakerPair(address pair, bool value) external onlyOwner {
        _setAutomatedMarketMakerPair(pair, value);
    }

    function blacklistAddress(address account, bool value) external onlyOwner {
        isBlacklisted[account] = value;
    }

    function isExcludedFromAntiWhale(address _account) public view returns (bool) {
        return _excludedFromAntiWhale[_account];
    }

    /**
     * @dev Returns the max wallet amount.
     */
    function maxBalanceAmount() public view returns (uint256) {
        return totalSupply().mul(maxBalanceAmountRate).div(1000);
    }

    /**
     * @dev Returns the max transfer amount.
     */
    function maxTransferAmount() public view returns (uint256) {
        return totalSupply().mul(maxTransferAmountRate).div(1000);
    }

    function setExcludedFromAntiWhale(address _account, bool _excluded) public onlyOwner {
        _excludedFromAntiWhale[_account] = _excluded;
    }

    /**
     * @dev Update the max balance amount rate.
     * Can only be called by the current operator.
     */
    function updateMaxBalanceAmountRate(uint16 _maxBalanceAmountRate) external onlyOwner {
        require(_maxBalanceAmountRate <= 1000, "SEED::updatemaxBalanceAmountRate: Max transfer amount rate must not exceed the maximum rate.");
        require(_maxBalanceAmountRate >= 1, "SEED::updatemaxBalanceAmountRate: Max transfer amount rate must  exceed the minimum rate.");

        maxBalanceAmountRate = _maxBalanceAmountRate;
        emit MaxBalanceAmountRateUpdated(msg.sender, maxBalanceAmountRate, _maxBalanceAmountRate);
    }

    /**
     * @dev Update the max transfer amount rate.
     * Can only be called by the current operator.
     */
    function updateMaxTransferAmountRate(uint16 _maxTransferAmountRate) public onlyOwner {
        require(_maxTransferAmountRate <= 1000, "SEED::updateMaxTransferAmountRate: Max transfer amount rate must not exceed the maximum rate.");
        require(_maxTransferAmountRate >= 1, "SEED::updateMaxTransferAmountRate: Max transfer amount rate must exceed the minimum rate.");

        maxTransferAmountRate = _maxTransferAmountRate;
        emit MaxTransferAmountRateUpdated(msg.sender, maxTransferAmountRate, _maxTransferAmountRate);
