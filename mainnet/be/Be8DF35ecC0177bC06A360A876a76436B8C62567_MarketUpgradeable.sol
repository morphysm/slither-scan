        require(maxRoyaltyBps <= MAX_BPS, "MarketRoyaltyUpgradeable: Royalty > 10%");

        _maxRoyaltyBps = maxRoyaltyBps;

        emit MarketRoyaltyUpdated(maxRoyaltyBps);
    }

