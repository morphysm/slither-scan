        require(royalties[contractAddress][tokenId].recipient == address(0), "MarketRoyaltyUpgradeable: Royalty already set");
        require(royaltyBps > 0 && royaltyBps <= _maxRoyaltyBps, "MarketRoyaltyUpgradeable: Invalid royalty");
