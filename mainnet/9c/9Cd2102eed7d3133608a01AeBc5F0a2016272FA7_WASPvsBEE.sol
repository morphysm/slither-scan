pragma solidity ^0.8.13;

// / @title WASPvsBEE Contract
// / @author WASPvsBEE dev team
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./IHONEY.sol";

contract WASPvsBEE is ERC721Enumerable, Ownable, ReentrancyGuard {
    // To increment the id of the NFTs
    using Counters for Counters.Counter;

    // To concatenate the URL of an NFT
    using Strings for uint256;

    // To check the addresses in the whitelist
    bytes32 public merkleRoot;

    // Id of the next NFT to mint
    Counters.Counter private _nftIdCounter;

    // MULTISIG adress
    address public MULTISIG = 0xBeC3b9bF3e1498C97272E9a5f001D2875B0d5026;

    // Number of NFTs in the collection
    uint256 public constant MAX_SUPPLY = 20000; //  5000 per gen. There are 4 gen

    // Price of one NFT in whitelist
    uint256 public WHITELIST_MINT_PRICE = 1.2 ether; // 1.2 Avax

    // Price of one NFT in public sale gen 0
    uint256 public MINT_PRICE = 1.5 ether; // 1.5 Avax

    // Price of one NFT in public sale gen 0
    uint256 public MINT_PRICE_GEN_1 = 8000 ether; // $HONEY

    // Price of one NFT in public sale gen 0
    uint256 public MINT_PRICE_GEN_2 = 16000 ether; // $HONEY

    // Price of one NFT in public sale gen 0
    uint256 public MINT_PRICE_GEN_3 = 24000 ether; // $HONEY

    // Base URI of the bee NFTs when revealed
    string public baseBeeURI;

    // Base URI of the wasp NFTs when revealed
    string public baseWaspURI;

    // Reference to $LOOT for burning on mint
    IHONEY public honey;

    // URI of the NFTs when not revealed
    string public notRevealedURI;

    // Number of assets in each race collection
    uint256 public maxAsset = 125;

    // The extension of the file containing the Metadatas of the NFTs
    string public baseExtension = ".json";

    // Are the NFTs revealed yet ?
    bool public revealed = false;

    // The different stages of selling the collection
    enum Steps {
        Pause,
        WhitelistSale,
        PublicSale,
        SoldOut
    }
    //  Race of NFT
    enum Race {
        BEE, //  90%
        WASP //  10%
    }

    Steps public sellingStep;

    // Owner of the smart contract
    address private _owner;

    // Keep a track of the number of tokens per address
    mapping(address => uint256) public nftsPerWallet;

    // Mapping from tokenId to a struct containing the token's traits
    mapping(uint256 => Race) private _tokenTraits;

    // Failed mint event
    event failedMint(address _account, uint256 _amount);

    // Constructor of the collection
    constructor(
        IHONEY _honey,
        string memory _theBaseBeeURI,
        string memory _theBaseWaspURI,
        string memory _notRevealedURI,
        bytes32 _merkleRoot
    ) ERC721("WASPvsBEE", "WvsB") {
        _nftIdCounter.increment();
        transferOwnership(msg.sender);
        sellingStep = Steps.Pause;
        honey = _honey;
        baseBeeURI = _theBaseBeeURI;
        baseWaspURI = _theBaseWaspURI;
        notRevealedURI = _notRevealedURI;
        merkleRoot = _merkleRoot;
    }

    /**
     * @notice Edit the Merkle Root
     *
     * @param _newMerkleRoot The new Merkle Root
     **/
    function changeMerkleRoot(bytes32 _newMerkleRoot) external onlyOwner {
        merkleRoot = _newMerkleRoot;
    }

    /**
     * @notice Change the gen 3 price
     *
     * @param _gen3_price The new price
     **/
    function setGen3Price(uint256 _gen3_price) external onlyOwner {
        MINT_PRICE_GEN_3 = _gen3_price;
    }

    /**
     * @notice Change the gen 2 price
     *
     * @param _gen2_price The new price
     **/
    function setGen2Price(uint256 _gen2_price) external onlyOwner {
        MINT_PRICE_GEN_2 = _gen2_price;
    }

    /**
     * @notice Change the gen 1 price
     *
     * @param _gen1_price The new price
     **/
    function setGen1Price(uint256 _gen1_price) external onlyOwner {
        MINT_PRICE_GEN_1 = _gen1_price;
    }

    /**
     * @notice Change the public sale price
     *
     * @param _public_sale_price The new price
     **/
    function setPublicSaleMintPrice(uint256 _public_sale_price)
        external
        onlyOwner
    {
        MINT_PRICE = _public_sale_price;
    }

    /**
     * @notice Update the multisig address
     *
     * @param _new_account The new address
     **/
    function updateMultisigAddress(address _new_account) external onlyOwner {
        MULTISIG = _new_account;
    }

    /**
     * @notice Change the whitelist price
     *
     * @param _wl_price The new price
     **/
    function setWhiteListMintPrice(uint256 _wl_price) external onlyOwner {
        WHITELIST_MINT_PRICE = _wl_price;
    }

    /**
     * @notice Change the base Bee URI
     *
     * @param _newBaseBeeURI The new base URI
     **/
    function setBaseBeeURI(string memory _newBaseBeeURI) external onlyOwner {
        baseBeeURI = _newBaseBeeURI;
    }

    /**
     * @notice Change the base Wasp URI
     *
     * @param _newBaseWaspURI The new base URI
     **/
    function setBaseWaspURI(string memory _newBaseWaspURI) external onlyOwner {
        baseWaspURI = _newBaseWaspURI;
    }

    /**
     * @notice Change the not revealed URI
     *
     * @param _notRevealedURI The new not revealed URI
     **/
    function setNotRevealURI(string memory _notRevealedURI) external onlyOwner {
        notRevealedURI = _notRevealedURI;
    }

    /**
     * @notice Allows to set the revealed variable to true
     **/
    function reveal() external onlyOwner {
        revealed = true;
    }

    /**
     * @notice Get the tokenTraits if NFTs are revealed
     *
     * @param _nftId The token id of the NFT
     **/
    function getTokenTraits(uint256 _nftId) external view returns (Race) {
        require(_exists(_nftId), "This NFT doesn't exist.");
        require(revealed == true, "NFTs still unrevealed");
        return _tokenTraits[_nftId];
    }

    /**
     * allows owner to withdraw funds from minting
     */
    function withdraw() external onlyOwner {
        payable(MULTISIG).transfer(address(this).balance);
    }

    /**
     * @notice Allows to change the base extension of the metadatas files
     *
     * @param _baseExtension the new extension of the metadatas files
     **/
    function setBaseExtension(string memory _baseExtension) external onlyOwner {
        baseExtension = _baseExtension;
    }

    /**
     * @notice Allows to change the number of assets
     **/
    function setMaxAsset(uint256 _maxAsset) external onlyOwner {
        maxAsset = _maxAsset;
    }

    /**
     * @notice Allows to change the sellinStep to Pause
     **/
    function setPause() external onlyOwner {
        sellingStep = Steps.Pause;
    }

    /**
     * @notice Allows to change the sellinStep to WhitelistSale
     **/
    function setUpWhitelistSale() external onlyOwner {
        sellingStep = Steps.WhitelistSale;
    }

    /**
     * @notice Allows to change the sellinStep to PublicSale
     **/
    function setUpPublicSale() external onlyOwner {
        require(
            sellingStep == Steps.WhitelistSale,
            "First the whitelist Sale, then the public sale."
        );
        sellingStep = Steps.PublicSale;
    }

    /**
     * @notice Allows to mint NFTs if whitelisted
     *
     * @param _amount The amount of NFTs the user wants to mint
     * @param _proof The Merkle Proof
     **/
    function whitelistMint(uint256 _amount, bytes32[] calldata _proof)
        external
        payable
        nonReentrant
    {
        // Are we in WhitelistSale ?
        require(
            sellingStep == Steps.WhitelistSale,
            "WhitelistSale has not started yet."
        );
        // Did this account already mint an NFT ?
        require(
            nftsPerWallet[msg.sender] + _amount <= 5,
            "Your wallet in the Whitelist step should not exceed 5 NFTs"
        );
        // Is this user on the whitelist ?
        require(isWhiteListed(msg.sender, _proof), "Not on the whitelist");
        // Get the price of one NFT during the WhitelistSale
        uint256 price = WHITELIST_MINT_PRICE;
        // Did the user send enough AVAX ?
        require(msg.value >= price * _amount, "Not enough AVAX.");
        // Increment the number of NFTs this user has minted
        nftsPerWallet[msg.sender] += _amount;
        // Minting the NFTs
        _mintNFT(msg.sender, _amount);
    }

    /**
     * @notice Allows to mint NFTs - 90% Bees, 10% Wasps & in gen 1,2,3 : 10% to fail the mint
     *
     * @param _amount The amount of NFTs the user wants to mint
     **/
    function publicSaleMint(uint256 _amount) external payable nonReentrant {
        // Get the number of NFTs sold
        uint256 numberNftSold = totalSupply();
        // Get the price of one NFT in PublicSale
        uint256 price = MINT_PRICE; //  1.5 eth
        // If everything has been bought
        require(sellingStep != Steps.SoldOut, "Sorry, no NFTs left.");
        // If PublicSale didn't start yet
        require(
            sellingStep == Steps.PublicSale,
            "Sorry, public sale has not started yet."
        );
        // If the user try to mint any non-existent token
        require(
            numberNftSold + _amount <= MAX_SUPPLY,
            "Public sale is almost done and we don't have enough NFTs left."
        );
        if (numberNftSold <= 5000) {
            require(
                numberNftSold + _amount <= 5000,
                "Public sale with AVAX is almost done and we don't have enough NFTs left."
            );
            // Did the user have enough AVAX ?
            require(msg.value >= price * _amount, "Not enough AVAX.");
            // Add the amount of NFTs minted by the user to the total he minted
            nftsPerWallet[msg.sender] += _amount;
            // Minting the NFTs with 0 risk to fail
            _mintNFT(msg.sender, _amount);
        } else {
            //  minting g 1,2,3
            price = mintCost(_nftIdCounter.current());
            //  hotfix
            require(
                price == mintCost(_nftIdCounter.current() + _amount),
                "Public sale of the current gen is almost done and we don't have enough NFTs left."
            );
            require(
                honey.balanceOf(_msgSender()) >= price * _amount,
                "Not enough $HONEY."
            );
            // The price of NFTs will be burned
            honey.burn(_msgSender(), price * _amount);
            // Minting the NFTs
            for (uint256 i = 1; i <= _amount; i++) {
                //  Pseudo random
                uint256 random = uint256(
                    keccak256(
                        abi.encodePacked(
                            block.difficulty,
                            block.timestamp,
                            msg.sender,
                            _nftIdCounter.current()
                        )
                    )
                ) % 100;
                if (random < 10) {
                    // Emit event to inform the sender that the mint failed
                    emit failedMint(msg.sender, price);
                } else {
                    // Add the amount of NFTs minted by the user to the total of what he minted
                    nftsPerWallet[msg.sender]++;
                    // If this account minted the last NFTs available
                    if (numberNftSold + 1 == MAX_SUPPLY) {
                        sellingStep = Steps.SoldOut;
                    }
                    if (random >= 90) {
                        _tokenTraits[_nftIdCounter.current()] = Race.WASP;
                    } else {
                        _tokenTraits[_nftIdCounter.current()] = Race.BEE;
                    }
                    // Mint the user NFT
                    _safeMint(msg.sender, _nftIdCounter.current());
                    // Increment the Id of the next NFT to mint
                    _nftIdCounter.increment();
                }
            }
        }
    }

    /**
     * @notice Allows to gift one NFT to an address
     *
     * @param _account The account of the happy new owner of one NFT
     **/
    function gift(address _account, uint256 _amount) external onlyOwner {
        // Get the number of NFTs sold
        uint256 numberNftSold = totalSupply();
        // If everything has been bought
        require(sellingStep != Steps.SoldOut, "Sorry, no NFTs left.");
        // If the user try to mint any non-existent token
        require(
            numberNftSold + _amount <= MAX_SUPPLY,
            "Public sale is almost done and we don't have enough NFTs left."
        );
        // If this account minted the last NFTs available
        if (numberNftSold + _amount == MAX_SUPPLY) {
            sellingStep = Steps.SoldOut;
        }
        // Add the amount of NFTs minted by the user to the total he minted
        nftsPerWallet[_account] += _amount;
        // Minting the NFTs with 0 risk to fail
        _mintNFT(_account, _amount);
    }

    /**
     * @notice Mint without risk
     *
     * @param account Address which will recieve the NFTs
     * @param amount Amount of NFTS
     *
     **/
    function _mintNFT(address account, uint256 amount) private {
        // Minting the NFTs with 0 risk to fail
        for (uint256 i = 1; i <= amount; i++) {
            //  Pseudo random
            uint256 random = uint256(
                keccak256(
                    abi.encodePacked(
                        block.difficulty,
                        block.timestamp,
                        msg.sender,
                        _nftIdCounter.current()
                    )
                )
            ) % 100;
            if (random < 90) {
                _tokenTraits[_nftIdCounter.current()] = Race.BEE;
            } else {
                _tokenTraits[_nftIdCounter.current()] = Race.WASP;
            }
            // Mint the user NFT
            _safeMint(account, _nftIdCounter.current());
            // Increment the Id of the next NFT to mint
            _nftIdCounter.increment();
        }
    }

    /**
     * the first 25% are paid in AVAX
     * the next  25% are 8000 $HONEY
     * the next  25% are 16000 $HONEY
     * the final 25% are 24000 $HONEY
     * @param tokenId the ID to check the cost of to mint
     * @return the cost of the given token ID
     */
    function mintCost(uint256 tokenId) public view returns (uint256) {
        if (tokenId <= 10000) return MINT_PRICE_GEN_1;
        if (tokenId <= 15000) return MINT_PRICE_GEN_2;
        if (tokenId <= 20000) return MINT_PRICE_GEN_3;
        return MINT_PRICE_GEN_3 + 1 ether;
    }

    /**
     * @notice Return true or false if the account is whitelisted or not
     *
     * @param account The account of the user
     * @param proof The Merkle Proof
     *
     * @return true or false if the account is whitelisted or not
     **/
    function isWhiteListed(address account, bytes32[] calldata proof)
        internal
        view
        returns (bool)
    {
        return _verify(_leaf(account), proof);
    }

    /**
     * @notice Return the account hashed
     *
     * @param account The account to hash
     *
     * @return The account hashed
     **/
    function _leaf(address account) internal pure returns (bytes32) {
        return keccak256(abi.encodePacked(account));
    }

    /**
     * @notice Returns true if a leaf can be proved to be a part of a Merkle tree defined by root
     *
     * @param leaf The leaf
     * @param proof The Merkle Proof
     *
     * @return True if a leaf can be provded to be a part of a Merkle tree defined by root
     **/
    function _verify(bytes32 leaf, bytes32[] memory proof)
        internal
        view
        returns (bool)
    {
        return MerkleProof.verify(proof, merkleRoot, leaf);
    }

    /**
     * @notice Allows to get the complete URI of a specific NFT by his ID
     *
     * @param _nftId The id of the NFT
     *
     * @return The token URI of the NFT which has _nftId Id
     **/
    function tokenURI(uint256 _nftId)
        public
        view
        override(ERC721)
        returns (string memory)
    {
        require(_exists(_nftId), "This NFT doesn't exist.");
        if (revealed == false) {
            return notRevealedURI;
        }
        string memory currentBaseURI = notRevealedURI;

        if (_tokenTraits[_nftId] == Race.BEE) {
            currentBaseURI = baseBeeURI;
        } else {
            currentBaseURI = baseWaspURI;
        }

        //  string memory currentBaseURI = _baseURI();
        return
            bytes(currentBaseURI).length > 0
                ? string(
                    abi.encodePacked(
                        currentBaseURI,
                        (_nftId % maxAsset).toString(),
                        baseExtension
                    )
                )
                : "";
    }
}

