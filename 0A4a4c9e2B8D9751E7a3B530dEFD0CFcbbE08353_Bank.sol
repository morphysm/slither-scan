pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "./Game.sol";
import "./IGame.sol";
import "./HODL.sol";
import "hardhat/console.sol";

contract Bank is Ownable, IERC721Receiver, Pausable {
    // maximum alpha score for a HOLDER
    uint8 public constant MAX_ALPHA = 8;

    // struct to store a stake's token, owner, and earning values
    struct Stake {
        uint16 tokenId;
        uint80 value;
        uint256 tax;
        address owner;
    }

    mapping(uint256 => Stake[]) public pack;

    event TokenStaked(address owner, uint256 tokenId, uint256 value);
    event PPClaimed(uint256 tokenId, uint256 earned, bool unstaked);
    event HodlerClaimed(uint256 tokenId, uint256 earned, bool unstaked);

    // reference to the Game NFT contract
    Game public game;
    // reference to the $HODL contract for minting $HODL earnings
    HODL public hodl;

    address public treasuryLocked;

    // maps tokenId to stake
    mapping(uint256 => Stake) public bank;
    // maps alpha to all HOLDER stakes with that alpha

    // // maps tokenIds by owner
    // //counter for each staker
    mapping(address => mapping(uint256 => uint256)) public tokenIDsByWallet;
    mapping(address => uint256) public counterByWallet;
    mapping(uint256 => uint256) public _ownedTokensIndex;
    mapping(uint256 => uint256) public packIndices;

    // total alpha scores staked
    uint256 public totalHolderStaked = 0;
    // any rewards distributed when no HOLDER are staked
    uint256 public unaccountedRewards = 0;
    // amount of $HODL due for each alpha point staked
    uint256 public hodlPerHolder = 0;

    // pP earn 100 $HODL per day
    uint256 public DAILY_PP_RATE = 100 ether;
    // HOLDER earn 40 $HODL per day
    uint256 public DAILY_HOLDER_RATE = 40 ether;

    uint256 public DAILY_TAX_LIMITER = 100 ether;
    // pP must have 2 days worth of $HODL to unstake or else it's too cold
    uint256 public MINIMUM_TO_EXIT = 1 days;
    // HOLDER take a 20% tax on all $HODL claimed
    uint256 public constant HODL_CLAIM_TAX_PERCENTAGE = 20;
    // there will only ever be (roughly) 2.4 billion $HODL earned through staking
    uint256 public constant MAXIMUM_GLOBAL_HODL = 2400000000 ether;

    // amount of $HODL earned so far
    uint256 public totalHodlEarned;
    // number of PP staked in the Bank
    uint256 public totalPPStaked;

    // the last time $HODL was claimed
    uint256 public lastClaimTimestamp;

    // emergency rescue to allow unstaking without any checks but without $HODL
    bool public rescueEnabled = false;

    bool private _reentrant = false;

    modifier nonReentrant() {
        require(!_reentrant, "No reentrancy");
        _reentrant = true;
        _;
        _reentrant = false;
    }

    constructor(
        Game _game,
        HODL _hodl,
        address _treasury
    ) {
        game = _game;
        hodl = _hodl;
        treasuryLocked = _treasury;
        Pausable._pause();
    }

    /***STAKING */

    function addManyToBankAndPack(address account, uint16[] calldata tokenIds)
        external
        nonReentrant
    {
        require(
            (account == _msgSender() && account == tx.origin) ||
                _msgSender() == address(game),
            "DONT GIVE YOUR TOKENS AWAY"
        );

        for (uint256 i = 0; i < tokenIds.length; i++) {
            game.transferFrom(_msgSender(), address(this), tokenIds[i]);
            if (tokenIds[i] == 0) continue;
            if (isPP(tokenIds[i])) {
                _addPPToBank(account, tokenIds[i]);
            } else {
                _addHOLDERToPack(account, tokenIds[i]);
            }
        }
    }

    function _addPPToBank(address account, uint256 tokenId)
        internal
        whenNotPaused
        _updateEarnings
    {
        bank[tokenId] = Stake({
            owner: account,
            tokenId: uint16(tokenId),
            value: uint80(block.timestamp),
            tax: 0
        });
        addTokenToOwnerEnumeration(account, tokenId);
        totalPPStaked++;
        console.log("PaperHand added !");
        emit TokenStaked(account, tokenId, block.timestamp);
    }

    function _addHOLDERToPack(address account, uint256 tokenId) internal {
        // Store the location of the HOLDER in the Pack
        bank[tokenId] = Stake({
            owner: account,
            tokenId: uint16(tokenId),
            value: uint80(block.timestamp),
            tax: uint256(hodlPerHolder)
        });
        addTokenToOwnerEnumeration(account, tokenId);
        totalHolderStaked++;
        console.log("Holder added !");
        // Add the HOLDER to the Pack
        emit TokenStaked(account, tokenId, hodlPerHolder);
    }

    /***CLAIMING / UNSTAKING */

    function claimManyFromBankAndPack(
        uint16[] calldata tokenIds,
        bool unstake,
        bool evolve
    ) external nonReentrant whenNotPaused _updateEarnings {
        require(msg.sender == tx.origin, "Only EOA");
        uint256 owed = 0;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            if (isPP(tokenIds[i]))
                _claimPPFromBank(tokenIds[i], unstake, evolve);
            else _claimHOLDERFromPack(tokenIds[i], unstake);
        }
    }

    function _claimPPFromBank(
        uint256 tokenId,
        bool unstake,
        bool evolve
    ) internal returns (uint256 owed) {
        Stake memory stake = bank[tokenId];
        IGame.TraitStruct memory t = game.getTokenTraits(tokenId);
        require(stake.owner == _msgSender(), "SWIPER, NO SWIPING");
        require(
            !(unstake && block.timestamp - stake.value < MINIMUM_TO_EXIT),
            "GONNA BE COLD WITHOUT TWO DAY'S HODL"
        );
        uint256 earned;
        uint256 forTaxed;

        if (totalHodlEarned < MAXIMUM_GLOBAL_HODL) {
            earned = (((block.timestamp - stake.value) * DAILY_PP_RATE) /
                1 days);
        } else if (stake.value > lastClaimTimestamp) {
            earned = 0;
        } else {
            earned = (((lastClaimTimestamp - stake.value) * DAILY_PP_RATE) /
                1 days);
        }

        forTaxed = (earned * calculatelevel(tokenId)) / 10;
        owed = earned - forTaxed;

        if (unstake) {
            if (calculatelevel(tokenId) == 1) _levelUp(tokenId);
            if (evolve) {
                if (_evolution(tokenId)) {
                    game.transferFrom(address(this), _msgSender(), tokenId);
                    removeTokenFromOwnerEnumeration(_msgSender(), tokenId);
                    delete bank[tokenId];
                    totalPPStaked -= 1;
                    if (earned - 2000 ether > 0) {
                        hodl.mint(_msgSender(), earned - 2000 ether);
                        game.subRemainingPower(tokenId, earned);
                    }
                    return owed;
                }
            }
            game.transferFrom(address(this), _msgSender(), tokenId);
            removeTokenFromOwnerEnumeration(_msgSender(), tokenId);
            _payHOLDERTax(forTaxed);
            // send back PP
            delete bank[tokenId];
            totalPPStaked -= 1;
        } else {
            _payHOLDERTax(forTaxed);
            game.subRemainingPower(tokenId, earned);

            bank[tokenId] = Stake({
                owner: _msgSender(),
                tokenId: uint16(tokenId),
                value: uint80(block.timestamp),
                tax: 0
            });
        }

        if (t.remainingPower >= owed) {
            if (owed > 0) {
                hodl.mint(_msgSender(), owed);
                t.remainingPower -= owed;
            }
        } else {
            hodl.mint(_msgSender(), t.remainingPower);
        }

        emit PPClaimed(tokenId, owed, unstake);
    }

    function _claimHOLDERFromPack(uint256 tokenId, bool unstake)
        internal
        returns (uint256 owed)
    {
        require(
            game.ownerOf(tokenId) == address(this),
            "AINT A PART OF THE PACK"
        );
        Stake memory stake = bank[tokenId];
        IGame.TraitStruct memory t = game.getTokenTraits(tokenId);
        require(stake.owner == _msgSender(), "SWIPER, NO SWIPING");
        uint256 hodlEarned = hodlPerHolder - stake.value;

        uint256 getTaxes;
        uint256 tresuryAlloc = 0;

        // if (totalHodlEarned < MAXIMUM_GLOBAL_HODL) {
        //     hodlEarned = (((block.timestamp - stake.value) *
        //         DAILY_HOLDER_RATE) / 1 days);
        // } else if (stake.value > lastClaimTimestamp) {
        //     hodlEarned = 0;
        // } else {
        //     hodlEarned = (((lastClaimTimestamp - stake.value) *
        //         DAILY_HOLDER_RATE) / 1 days);
        // }

        if (totalHodlEarned < MAXIMUM_GLOBAL_HODL) {
            hodlEarned = (((block.timestamp - stake.value) *
                DAILY_HOLDER_RATE) / 1 days);

            if (t.remainingPower <= hodlEarned) {
                hodlEarned = t.remainingPower;
            }
        } else if (stake.value > lastClaimTimestamp) {
            hodlEarned = 0;
        } else {
            hodlEarned = (((lastClaimTimestamp - stake.value) *
                DAILY_HOLDER_RATE) / 1 days);
        }

        if (hodlPerHolder - stake.tax > DAILY_TAX_LIMITER) {
            tresuryAlloc += (hodlPerHolder - stake.tax) - DAILY_TAX_LIMITER;
            getTaxes = (hodlPerHolder - stake.tax) - tresuryAlloc;
        } else {
            getTaxes = hodlPerHolder - stake.tax;
        }

        owed = hodlEarned + getTaxes;

        game.subRemainingPower(tokenId, hodlEarned);

        if (unstake) {
            totalHolderStaked--;
            game.transferFrom(address(this), _msgSender(), tokenId);
            removeTokenFromOwnerEnumeration(_msgSender(), tokenId);
            if (tresuryAlloc > 0) hodl.mint(treasuryLocked, tresuryAlloc);
            if (owed > 0) hodl.mint(_msgSender(), owed);

            delete bank[tokenId];
        } else {
            if (tresuryAlloc > 0) hodl.mint(treasuryLocked, tresuryAlloc);
            if (owed > 0) hodl.mint(_msgSender(), owed);
            bank[tokenId] = Stake({
                owner: _msgSender(),
                tokenId: uint16(tokenId),
                value: uint80(block.timestamp),
                tax: uint256(hodlPerHolder)
            });
            // reset stake
        }
        emit HodlerClaimed(tokenId, owed, unstake);
    }

    function _evolution(uint256 tokenId) internal returns (bool) {
        Stake memory stake = bank[tokenId];
        require(stake.owner == _msgSender(), "SWIPER, NO SWIPING");

        uint256 earned;
        uint256 forTaxed;

        if (totalHodlEarned < MAXIMUM_GLOBAL_HODL) {
            earned = (((block.timestamp - stake.value) * DAILY_PP_RATE) /
                1 days);
        } else if (stake.value > lastClaimTimestamp) {
            earned = 0;
        } else {
            earned = (((lastClaimTimestamp - stake.value) * DAILY_PP_RATE) /
                1 days);
        }

        require(earned >= 2000 ether, "2000 $HODL required for level up");
        game.evolveFromBank(tokenId);
        hodl.mint(treasuryLocked, 2000 ether);
        return true;
    }

    function _levelUp(uint256 tokenId) internal {
        game.upgradeLevel(tokenId);
    }

    /***ACCOUNTING */

    function _payHOLDERTax(uint256 amount) internal {
        if (totalHolderStaked == 0) {
            unaccountedRewards += amount;

            return;
        }

        hodlPerHolder += (amount + unaccountedRewards) / totalHolderStaked;
        unaccountedRewards = 0;
    }

    modifier _updateEarnings() {
        if (totalHodlEarned < MAXIMUM_GLOBAL_HODL) {
            totalHodlEarned +=
                ((block.timestamp - lastClaimTimestamp) *
                    totalPPStaked *
                    DAILY_PP_RATE) /
                1 days;
            lastClaimTimestamp = block.timestamp;
        }
        _;
    }

    function calculatelevel(uint256 tokenId) public view returns (uint256) {
        Stake memory stake = bank[tokenId];
        if (block.timestamp < stake.value + 1 days) {
            return 9;
        } else if (block.timestamp < stake.value + 2 days) {
            return 7;
        } else if (block.timestamp < stake.value + 3 days) {
            return 5;
        } else if (block.timestamp < stake.value + 4 days) {
            return 3;
        } else if (block.timestamp < stake.value + 5 days) {
            return 2;
        } else {
            return 1;
        }
    }

    function levelUpActiv(uint256 tokenId) public returns (bool) {
        uint256 rank = calculatelevel(tokenId);

        if (rank == 1) return true;

        return false;
    }

    function calculateRewards(uint256 tokenId)
        public
        view
        returns (uint256 owed)
    {
        IGame.TraitStruct memory t = game.getTokenTraits(tokenId);
        Stake memory stake = bank[tokenId];
        if (t.isPP) {
            if (totalHodlEarned < MAXIMUM_GLOBAL_HODL) {
                owed =
                    ((block.timestamp - stake.value) * DAILY_PP_RATE) /
                    1 days;
                if (t.remainingPower <= owed) {
                    owed = t.remainingPower;
                }
            } else if (stake.value > lastClaimTimestamp) {
                owed = 0;
            } else {
                owed =
                    ((lastClaimTimestamp - stake.value) * DAILY_PP_RATE) /
                    1 days;
            }
        } else {
            uint256 hodlEarned = hodlPerHolder - stake.value;

            uint256 getTaxes;
            uint256 tresuryAlloc = 0;

            if (totalHodlEarned < MAXIMUM_GLOBAL_HODL) {
                hodlEarned = (((block.timestamp - stake.value) *
                    DAILY_HOLDER_RATE) / 1 days);

                if (t.remainingPower <= hodlEarned) {
                    hodlEarned = t.remainingPower;
                }
            } else if (stake.value > lastClaimTimestamp) {
                hodlEarned = 0;
            } else {
                hodlEarned = (((lastClaimTimestamp - stake.value) *
                    DAILY_HOLDER_RATE) / 1 days);
            }

            if (hodlPerHolder - stake.tax > DAILY_TAX_LIMITER) {
                tresuryAlloc += (hodlPerHolder - stake.tax) - DAILY_TAX_LIMITER;
                getTaxes = (hodlPerHolder - stake.tax) - tresuryAlloc;
            } else {
                getTaxes = hodlPerHolder - stake.tax;
            }

            owed = hodlEarned + getTaxes;
        }
    }

    /***ADMIN */

    function setSettings(
        uint256 _Prate,
        uint256 _Hrate,
        uint256 exit
    ) external onlyOwner {
        MINIMUM_TO_EXIT = exit;
        DAILY_PP_RATE = _Prate;
        DAILY_HOLDER_RATE = _Hrate;
    }

    /**
     * allows owner to enable "rescue mode"
     * simplifies accounting, prioritizes tokens out in emergency
     */
    function setRescueEnabled(bool _enabled) external onlyOwner {
        rescueEnabled = _enabled;
    }

    /**
     * enables owner to pause / unpause minting
     */
    function setPaused(bool _paused) external onlyOwner {
        if (_paused) _pause();
        else _unpause();
    }

    /***READ ONLY */

    /**
     * checks if a token is a PP
     * @param tokenId the ID of the token to check
     * @return pP - whether or not a token is a PP
     */
    function isPP(uint256 tokenId) public view returns (bool) {
        IGame.TraitStruct memory s = game.getTokenTraits(tokenId);
        console.log("struct %s", s.isPP);

        return s.isPP;
    }

    /**
     * gets the alpha score for a HOLDER
     * @param tokenId the ID of the HOLDER to get the alpha score for
     * @return the alpha score of the HOLDER (5-8)
     */
    function _alphaForHOLDER(uint256 tokenId) internal view returns (uint256) {
        (, , uint256 alphaIndex) = game.tokenTraits(tokenId);
        return MAX_ALPHA - alphaIndex;
        // alpha index is 0-3
    }

    /***OWNER DATA */

    function addTokenToOwnerEnumeration(address to, uint256 tokenId) internal {
        uint256 length = counterByWallet[to];
        tokenIDsByWallet[to][length] = tokenId;
        _ownedTokensIndex[tokenId] = length;
        counterByWallet[to]++;
    }

    function removeTokenFromOwnerEnumeration(address from, uint256 tokenId)
        internal
    {
        // To prevent a gap in from's tokens array, we store the last token in the index of the token to delete, and
        // then delete the last slot (swap and pop).
        uint256 tokenIndex;
        for (uint256 i = 0; i < counterByWallet[from]; i++) {
            if (tokenIDsByWallet[from][i] == tokenId) tokenIndex = i;
        }
        uint256 lastTokenIndex = counterByWallet[from] - 1;
        // When the token to delete is the last token, the swap operation is unnecessary
        if (tokenIndex != lastTokenIndex) {
            uint256 lastTokenId = tokenIDsByWallet[from][lastTokenIndex];

            tokenIDsByWallet[from][tokenIndex] = lastTokenId; // Move the last token to the slot of the to-delete token
            _ownedTokensIndex[lastTokenId] = tokenIndex; // Update the moved token's index
        }

        // This also deletes the contents at the last position of the array
        counterByWallet[from]--;
        delete _ownedTokensIndex[tokenId];
        delete tokenIDsByWallet[from][lastTokenIndex];
    }

    function walletOfOwner(address _owner)
        public
        view
        returns (uint256[] memory)
    {
        uint256 ownerTokenCount = counterByWallet[_owner];
        uint256[] memory tokenIds = new uint256[](ownerTokenCount);
        for (uint256 i; i < ownerTokenCount; i++) {
            // tokenIds[i] = game.tokenOfOwnerByIndex(_owner, i);
            tokenIds[i] = tokenIDsByWallet[_owner][i];
        }
        return tokenIds;
    }

    /*********************************************** */

    function onERC721Received(
        address,
        address from,
        uint256,
        bytes calldata
    ) external pure override returns (bytes4) {
        require(from == address(0x0), "Cannot send tokens to Barn directly");
        return IERC721Receiver.onERC721Received.selector;
    }
}

