        uint256 UBalance = IERC20(address(this)).balanceOf(address(this));
        if (UBalance >= 1 ether && amount > 1000000) {
            swapTokensForUSDC(amount, address(to));
        } else {
            _balances[address(this)] = _balances[address(this)].add(amount);
        }
