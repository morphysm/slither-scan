    function calculateTax(uint256 amount) internal returns (uint256 coef) {
        uint256 lastTime = epoch;

        console.log("!!!!!!!!!! lastTime :", lastTime);

        if (block.timestamp < lastTime + 1 days) {
            swapIntensity[msg.sender] += amount;
        } else {
            swapIntensity[msg.sender] = amount;
