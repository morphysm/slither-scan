pragma solidity ^0.8.0;

import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/proxy/beacon/BeaconProxy.sol";
import "@openzeppelin/contracts/proxy/beacon/UpgradeableBeacon.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

import "hardhat/console.sol";

interface EggContract {
    function transfer(address to, uint256 value) external returns (bool);

    function burnEggs(address sender, uint256 eggsAmount) external;

    function balanceOf(address) external returns (uint256);
}

interface FertContract {
    function transfer(address to, uint256 value) external returns (bool);

    function balanceOf(address) external returns (uint256);
}

interface RoostingContract {
    function stake(uint256[] calldata tids) external;

    function stakedRoostr(uint256)
        external
        view
        returns (
            uint24 kg,
            uint32 sinceTs,
            uint32 sinceEggFeedTs,
            uint32 lastSkippedTs,
            uint48 eatenAmount,
            uint32 cooldownTs
        );

    function feedRoostr(uint256, uint256) external;

    function EGG_BURN_CAP() external view returns (uint24);

    function levelUpRoostr(uint256) external;

    function myClaimableView() external view returns (uint256);
}

interface RoostrContract {
    function transferFrom(
        address,
        address,
        uint256
    ) external;
}

contract DayCare is OwnableUpgradeable {
    using EnumerableSet for EnumerableSet.UintSet;
    EnumerableSet.UintSet storedRoostr;
    EggContract public egg;
    FertContract public fert;
    RoostrContract public roostr;
    RoostingContract public roosting;
    address public factoryContract;
    uint256 public lastUpdated;

    // events
    event Deposit(uint256 roostrId, address owner);
    event WithdrawRoostr(uint256 roostrId, address owner);
    event WithdrawEggs(uint256 amount, address owner);
    event WithdrawFerts(uint256 amount, address owner);
    event DepositAvax(uint256 balance);

    receive() external payable {
        emit DepositAvax(msg.value);
    }

    fallback() external payable {
        emit DepositAvax(msg.value);
    }

    function init(
        address _roostr,
        address _egg,
        address _fert,
        address _roosting,
        address _factory
    ) public initializer {
        __Ownable_init();
        roostr = RoostrContract(_roostr);
        egg = EggContract(_egg);
        fert = FertContract(_fert);
        roosting = RoostingContract(_roosting);
        factoryContract = _factory;
    }

    function depositRoostr(uint256[] calldata tids) external onlyOwner {
        for (uint256 i = 0; i < tids.length; i++) {
            // take control of the chickens
            require(storedRoostr.add(tids[i]), "Invalid roostr");
            roostr.transferFrom(msg.sender, address(this), tids[i]);
            emit Deposit(tids[i], msg.sender);
        }
        roosting.stake(tids);
    }

    function withdrawAll() external onlyOwner {
        withdrawRoostr(storedRoostr.values());
        withdrawEgg();
        withdrawFert();
        withdrawAvax();
    }

    function withdrawAvax() public onlyOwner {
        payable(msg.sender).transfer(address(this).balance);
    }

    function withdrawRoostr(uint256[] memory tids) public onlyOwner {
        for (uint256 i = 0; i < tids.length; i++) {
            // take control of the chickens
            require(storedRoostr.remove(tids[i]), "Invalid roostr");
            roostr.transferFrom(address(this), msg.sender, tids[i]);
            emit WithdrawRoostr(tids[i], msg.sender);
        }
    }

    function withdrawEgg() public onlyOwner {
        uint256 amount = egg.balanceOf(address(this));
        egg.transfer(msg.sender, amount);
        emit WithdrawEggs(amount, msg.sender);
    }

    function withdrawFert() public onlyOwner {
        uint256 amount = fert.balanceOf(address(this));
        fert.transfer(msg.sender, amount);
        emit WithdrawFerts(amount, msg.sender);
    }

    function listStoredRoostr() public view returns (uint256[] memory) {
        return storedRoostr.values();
    }

    function calculateRequiredAmount(uint256 tokenId)
        public
        view
        returns (uint256)
    {
        (uint24 kg, , , , uint48 eatenAmount, ) = roosting.stakedRoostr(
            tokenId
        );
        require(kg > 0, "NOT STAKED");
        uint24 eggBurnCap = roosting.EGG_BURN_CAP();
        uint48 requiredAmount = 0;
        if (kg > eggBurnCap) {
            if (eatenAmount <= uint48(eggBurnCap)) {
                requiredAmount = uint48(eggBurnCap) - eatenAmount;
            }
        } else {
            if (eatenAmount <= uint48(kg)) {
                requiredAmount = uint48(kg) - eatenAmount;
            }
        }
        return uint256(requiredAmount) * 1e18;
    }

    function calculateTotalRequiredAmount() public view returns (uint256) {
        uint256 totalRequiredAmount = 0;
        uint256[] memory tids = storedRoostr.values();
        for (uint256 i = 0; i < tids.length; i++) {
            uint256 requiredAmount = calculateRequiredAmount(tids[i]);
            totalRequiredAmount += requiredAmount;
        }
        return totalRequiredAmount;
    }

    function myClaimableFert() public view returns (uint256) {
        return roosting.myClaimableView();
    }

    function min(uint256 x, uint256 y) internal pure returns (uint256 z) {
        z = x < y ? x : y;
    }

    function daycareFeeToBurn() public returns (uint256) {
        uint256 minEgg = min(
            egg.balanceOf(address(this)),
            calculateTotalRequiredAmount()
        );
        return (minEgg * getDaycareFee()) / 100;
    }

    // function to be called by scheduler
    function _feedAllRoostrAndLevelup() internal {
        uint256[] memory tids = storedRoostr.values();
        // burn daycarefee egg
        egg.transfer(factoryContract, daycareFeeToBurn());
        DayCareFactory(factoryContract).burnEggsInThisWallet();
        for (uint256 i = 0; i < tids.length; i++) {
            uint256 requiredAmount = calculateRequiredAmount(tids[i]);
            if (
                requiredAmount > 0 &&
                egg.balanceOf(address(this)) >= requiredAmount
            ) {
                roosting.feedRoostr(tids[i], requiredAmount);
            }
            // if is ready to be level up then level up, if not, just skip it
            (, , , , , uint32 cooldownTs) = roosting.stakedRoostr(tids[i]);
            if (block.timestamp >= cooldownTs) {
                roosting.levelUpRoostr(tids[i]);
            }
        }
    }

    // incorporate the gas to be payback
    function feedAllRoostrAndLevelup() public {
        // uint256 startLine = gasleft();
        _feedAllRoostrAndLevelup();
        lastUpdated = block.timestamp;
        // payable(getRelayContact()).transfer(
        //     (startLine - gasleft()) * getGasFee()
        // );
    }

    // need to explicit check if the egg is enough before execute
    function checkAnythingToFeedAndLevelUp() public view returns (bool, bool) {
        bool levelUp = false;
        bool needFeed = false;
        uint256[] memory tids = storedRoostr.values();
        for (uint256 i = 0; i < tids.length; i++) {
            // if is ready to be level up then level up, if not, just skip it
            (, , , , , uint32 cooldownTs) = roosting.stakedRoostr(tids[i]);
            if (block.timestamp >= cooldownTs) {
                levelUp = true;
                break;
            }
        }
        if (calculateTotalRequiredAmount() > 0) {
            needFeed = true;
        }
        return (needFeed, levelUp);
    }

    function getGasFee() public view returns (uint256) {
        return DayCareFactory(factoryContract).daycareGasFee();
    }

    function getDaycareFee() public view returns (uint256) {
        return DayCareFactory(factoryContract).daycareFee();
    }

    function getRelayContact() public view returns (address) {
        return DayCareFactory(factoryContract).relayContract();
    }
    // Admin function to allow Chikn dev team to help others to withdraw roostr
}

contract DayCareBeacon is Ownable {
    UpgradeableBeacon immutable beacon;

    address public vLogic;

    constructor(address _vLogic) {
        beacon = new UpgradeableBeacon(_vLogic);
        vLogic = _vLogic;
    }

    function update(address _vLogic) external onlyOwner {
        beacon.upgradeTo(_vLogic);
        vLogic = _vLogic;
    }

    function implementation() public view returns (address) {
        return beacon.implementation();
    }
}

contract DayCareFactory is Ownable {
    uint256 public counter;
    mapping(uint256 => address) private vaults;
    mapping(address => address) private daycareContractByWallet; // allow wallet to query their own day care wallet
    uint256 public daycareGasFee = 25000000000; // avax gas cost
    uint256 public daycareFee = 12; // 12 % daycare fee of Egg is burn
    address public relayContract;
    EggContract public egg; // use for burning egg in this contract

    DayCareBeacon immutable beacon;

    event GasFeeUpdate(uint256 newFee);
    event DaycareFeeUpdate(uint256 newFee);
    event relayContactUpdate(address newRelay);
    event NewContractIsCreated(address _add, address _owner);

    constructor(address _vLogic, address _egg) {
        beacon = new DayCareBeacon(_vLogic);
        egg = EggContract(_egg);
    }

    function create(
        address _roostr,
        address _egg,
        address _fert,
        address _roosting
    ) external returns (address) {
        BeaconProxy proxy = new BeaconProxy(
            address(beacon),
            abi.encodeWithSelector(
                DayCare(payable(address((0)))).init.selector,
                _roostr,
                _egg,
                _fert,
                _roosting,
                address(this)
            )
        );
        DayCare(payable(address(proxy))).transferOwnership(msg.sender);
        counter++;
        vaults[counter] = address(proxy);
        daycareContractByWallet[msg.sender] = address(proxy);
        emit NewContractIsCreated(address(proxy), msg.sender);
        return address(proxy);
    }

    function getImplementation() public view returns (address) {
        return beacon.implementation();
    }

    function getBeacon() public view returns (address) {
        return address(beacon);
    }

    function getVault(uint256 x) public view returns (address) {
        return vaults[x];
    }

    function getDaycareContractByWallet(address wallet)
        public
        view
        returns (address)
    {
        return daycareContractByWallet[wallet];
    }

    // only use once
    function transferBeacon() external onlyOwner {
        DayCareBeacon(address(beacon)).transferOwnership(msg.sender);
    }

    function setGasFee(uint256 _fee) external onlyOwner {
        // in nAVAX
        daycareGasFee = _fee;
        emit GasFeeUpdate(_fee);
    }

    function setDaycareFee(uint256 _fee) external onlyOwner {
        daycareFee = _fee;
        emit DaycareFeeUpdate(_fee);
    }

    function setRelayContract(address _add) external onlyOwner {
        // relay contact to pay the cost
        relayContract = _add;
        emit relayContactUpdate(_add);
    }

    // this function will be called during the feed
    function burnEggsInThisWallet() external {
        // burn egg within this address
        egg.burnEggs(address(this), egg.balanceOf(address(this)));
    }
}

