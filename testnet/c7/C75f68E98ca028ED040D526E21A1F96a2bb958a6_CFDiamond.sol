pragma solidity ^0.8.0;

import "hardhat/console.sol";

/// @title Bonding Curve Algorithm
/// @author abhi3700, hosokawa-zen
library FundingUtils {

    function getWeeksInCP(uint256 conversionPeriod) internal pure returns(uint256) {
        // 604800 = 7 * 24 * 3600 (seconds of a week)
        uint256 w = conversionPeriod / 604800;
        if(conversionPeriod > w * 604800){
            return w + 1;
        }
        return w;
    }

    function getCurWeek(uint256 curCPTime) internal pure returns(uint256) {
        return curCPTime / 604800;
    }

    // @dev get rand number between 10000000 ~ 20000000
    // @param nonce random number nonce
    function random(bytes32 hash, uint256 nonce) internal pure returns (uint256) {
        return 1e7 + uint256(keccak256(abi.encodePacked(hash, nonce))) % 1e7;
    }

    // @dev selection sort uint256 array in increasing order
    function sortArray(uint256[] memory arr) internal pure {
        uint256 mi;
        uint256 mv;
        for(uint256 i = 0; i < arr.length - 1; i++){
            mv = arr[i];
            mi = i;
            for(uint256 j = i + 1; j < arr.length; j++){
                if(arr[j] < mv){
                    mi = j;
                    mv = arr[j];
                }
            }
            (arr[i], arr[mi]) = (arr[mi], arr[i]);
        }
    }
}

