import {LibCFDiamond} from "./libraries/LibCFDiamond.sol";
import { IDiamondCut } from "./interfaces/IDiamondCut.sol";
import './libraries/FundingUtils.sol';
import "./libraries/Strings.sol";
import "./UniqueStorage.sol";

import "hardhat/console.sol";

contract CFDiamond is UniqueStorage, IDiamondCut {

    constructor(string memory _pName, address _coinAddr, address _stAddr, uint16 _pOFeePt, uint256 _bufferPeriod, uint256 _conversionPeriod, uint256 _minTh, address _treasuryAddr, address _analyticMathAddr) UniqueStorage(keccak256(abi.encodePacked(_pName, msg.sender, block.timestamp))) {
        LibCFDiamond.setContractOwner(_storagePos, msg.sender);
        LibCFDiamond.setCrowdfunding(_storagePos, _pName, _coinAddr, _stAddr, _pOFeePt, _bufferPeriod, _conversionPeriod, _minTh, _treasuryAddr);
        LibCFDiamond.setAnalyticMath(_storagePos, _analyticMathAddr);
    }

    function diamondCut(
        FacetCut[] calldata _diamondCut,
        address _init,
        bytes calldata _calldata
    ) external override {
        LibCFDiamond.enforceIsContractOwner(_storagePos);
        LibCFDiamond.diamondCut(_storagePos, _diamondCut, _init, _calldata);
    }

    // Get Storage Position As Bytes32
    function getStoragePosition() external view returns (bytes32 pos){
        LibCFDiamond.enforceIsContractOwner(_storagePos);
        return _storagePos;
    }

    // Find facet for function that is called and execute the
    // function if a facet is found and return any value.
    fallback() external payable {
        LibCFDiamond.DiamondStorage storage ds;

        bytes32 position = _storagePos;
        // get diamond storage
        assembly {
            ds.slot := position
        }
        // get facet from function selector
        address facet = ds.selectorToFacetAndPosition[msg.sig].facetAddress;
        require(facet != address(0), "Diamond: Function does not exist");

        // Execute external function from facet using delegatecall and return any value.
        assembly {
            // copy function selector and any arguments
            calldatacopy(0, 0, calldatasize())
            // execute function call using the facet
            let result := delegatecall(gas(), facet, 0, calldatasize(), 0, 0)
            // get any return value
            returndatacopy(0, 0, returndatasize())
            // return any return value or error back to the caller
            switch result
                case 0 {
                    revert(0, returndatasize())
                }
                default {
                    return(0, returndatasize())
                }
        }
    }

    receive() external payable {}
}

