pragma solidity 0.8.10;

// inheritance
import "./EnvFactoryBase.sol";

// libs
import "@openzeppelin/contracts-upgradeable/utils/AddressUpgradeable.sol";

// external interfaces
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "../interface/IGameFiCore.sol";
import "../interface/IGameFiCore.sol";
import "../interface/IGameFiMarketplace.sol";
import "../interface/type/ITokenTypes.sol";
import "../interface/IGameFiShops.sol";
import "../interface/IProfileVault.sol";
import "@openzeppelin/contracts-upgradeable/access/IAccessControlEnumerableUpgradeable.sol";
// import "../interface/router/IZoaRouter.sol";

contract EnvFactoryV1 is EnvFactoryBase {

    using AddressUpgradeable for address;

    bytes32 public constant OPERATOR_ROLE = keccak256("OPERATOR_ROLE");

    modifier onlyAdminOrOperator() {
        require(
            hasRole(DEFAULT_ADMIN_ROLE, _msgSender()) || hasRole(OPERATOR_ROLE, _msgSender()),
            "EnvFactoryV1: caller is not the admin/operator"
        );
        _;
    }

    modifier onlyOperator() {
        _checkRole(OPERATOR_ROLE, _msgSender());
        _;
    }

    /// @custom:oz-upgrades-unsafe-allow constructor
    constructor() initializer {}

    function initialize() external initializer {
        __EnvFactoryBase_init();
    }

    struct RokojEnvSettings {
        // general
        address envAdmin;
        address envOperator;
        string environmentName;
        string environmentTag;
        // gameficore
        address gameFiCoreImpl;
        bytes gameFiCoreInitializeData;
        bytes createProperty1Data;
        bytes createProperty2Data;
        // avatars
        bytes avatarsCreateCollectionData;
        // boxes
        bytes boxesCreateCollectionData;
        // gamefi shops
        address gameFiShopsImpl;
        // gamefi marketplace
        address gameFiMarketplaceImpl;
        // gamefi router
        address gameFiRouterImpl;
    }

    function deployEnvironment(
        RokojEnvSettings memory envSettings
    ) external returns (uint256 environmentId, Environment memory environment) {

        // create new env
        (Environment memory env, uint256 environmentId) = _createEnv(
            envSettings.environmentName,
            envSettings.environmentTag
        );

        // deploy gameficore
        (EnvInstance memory gameFiCoreInst, uint256 gameFiCoreInstanceId) = _createInstance(
            environmentId,
            EnvInstanceType.GAMEFI_CORE,
            envSettings.gameFiCoreImpl,
            envSettings.gameFiCoreInitializeData
        );

        // deploy shops
        (EnvInstance memory gameFiShopsInst, uint256 gameFiShopsInstanceId) = _createInstance(
            environmentId,
            EnvInstanceType.GAMEFI_SHOPS,
            envSettings.gameFiShopsImpl,
            abi.encodeWithSelector(IGameFiShops.initialize.selector, gameFiCoreInst.instanceContract)
        );

        // deploy marketplace
        (EnvInstance memory gameFiMarketplaceInst, uint256 gameFiMarketplaceInstanceId) = _createInstance(
            environmentId,
            EnvInstanceType.GAMEFI_MARKETPLACE,
            envSettings.gameFiMarketplaceImpl,
            abi.encodeWithSelector(IGameFiMarketplace.initialize.selector, gameFiCoreInst.instanceContract)
        );

        // deploy marketplace
        (EnvInstance memory gameFiRouterInst, uint256 gameFiRouterInstanceId) = _createInstance(
            environmentId,
            EnvInstanceType.GAMEFI_ROUTER,
            envSettings.gameFiRouterImpl,
            abi.encodeWithSelector(IGameFiMarketplace.initialize.selector, gameFiCoreInst.instanceContract)
        );

        // setup gameficore
        {
            // create property 1
            gameFiCoreInst.instanceContract.functionCall(
                abi.encodeWithSelector(IProperty.createProperty.selector, envSettings.createProperty1Data)
            );

            // create property 2
            gameFiCoreInst.instanceContract.functionCall(
                abi.encodeWithSelector(IProperty.createProperty.selector, envSettings.createProperty2Data)
            );

            // create avatar collection
            gameFiCoreInst.instanceContract.functionCall(
                abi.encodeWithSelector(ICollection.createCollection.selector, envSettings.avatarsCreateCollectionData)
            );
            // setup avatar collection
            // bytes memory addOperatorMessage = 
            // uint256 avatarCollectionId = ICollection(gameFiCoreInst.instanceContract).callToCollection(0, data);

            // create boxes collection
            gameFiCoreInst.instanceContract.functionCall(
                abi.encodeWithSelector(ICollection.createCollection.selector, envSettings.boxesCreateCollectionData)
            );


            // reset ownership
            IAccessControlEnumerableUpgradeable(gameFiCoreInst.instanceContract).grantRole(bytes32(0), envSettings.envAdmin);
            IAccessControlEnumerableUpgradeable(gameFiCoreInst.instanceContract).grantRole(keccak256("OPERATOR_ROLE"), envSettings.envOperator);
            IAccessControlEnumerableUpgradeable(gameFiCoreInst.instanceContract).renounceRole(bytes32(0), address(this));
        }

    }

}

