interface IHelper {
    function claimAll(uint256[] calldata _nodes) external;
}

contract Proxy is Ownable {
    IERC721 public manager;
    IHelper public helper;
    IERC20 public token;

    uint[] nfts;

    constructor(address _manager, address _helper, address _token) {
        manager = IERC721(_manager);
        helper = IHelper(_helper);
        token = IERC20(_token);
    }

    function deposit(uint[] memory _nfts) external onlyOwner {
        for (uint256 i = 0; i < _nfts.length; i++) {
            manager.transferFrom(owner(), address(this), _nfts[i]);
        }
        nfts = _nfts;
    }

    function withdraw(address to) external onlyOwner {
        for (uint256 i = 0; i < nfts.length; i++) {
            manager.transferFrom(address(this), to, nfts[i]);
        }
    }

    function claim(address to) external onlyOwner {
        helper.claimAll(nfts);
        token.transfer(to, token.balanceOf(address(this)));
    }
}

contract Gate is Ownable {
    IERC721 public manager;
    IHelper public helper;
    IERC20 public token;

    mapping(address => Proxy[]) public _proxies;

    constructor(address _manager, address _helper, address _token) {
        manager = IERC721(_manager);
        helper = IHelper(_helper);
        token = IERC20(_token);
    }

    function _grab(uint[] memory _nfts) internal {
        for (uint256 i = 0; i < _nfts.length; i++) {
            manager.transferFrom(msg.sender, address(this), _nfts[i]);
        }
    }
    
    function _createAnFill(uint[] memory _nfts) internal {
        Proxy _proxy = new Proxy(address(manager),address(helper),address(token));
        _proxies[msg.sender].push(_proxy);
        manager.setApprovalForAll(address(_proxy), true);
        _proxy.deposit(_nfts);
    }

    function _deposit(uint[] memory _nfts) internal {
        uint toCreate = _nfts.length / 9;
        uint rest = _nfts.length % 9;
        uint c = 0;
        _grab(_nfts);
        for (uint256 i = 0; i < toCreate; i++) {
            uint[] storage nfts;
            for (uint256 j = 0; j < 9; j++) {
                nfts.push(_nfts[c]);
                c += 1;
            }
            _createAnFill(nfts);
        }
        uint[] storage nfts2;
        for (uint256 i = 0; i < rest; i++) {
            nfts2.push(_nfts[c]);
            c += 1;
        }
        _createAnFill(nfts2);
    }

    function _claim() internal {
        Proxy[] memory proxies = _proxies[msg.sender];
        for (uint256 i = 0; i < proxies.length; i++) {
            Proxy proxy = proxies[i];
            proxy.claim(msg.sender);
        }
    }

    function deposit(uint[] memory _nfts) external {
        _deposit(_nfts);
    }

    function claim() external {
        _claim();
    }

    function depositAndClaim(uint[] memory _nfts) external {
        _deposit(_nfts);
        _claim();
    }

    function withdraw(uint proxieAmount) external {
        Proxy[] storage proxies = _proxies[msg.sender];
        uint long = proxies.length - proxieAmount;
        for (uint256 i = proxies.length; i > long; i--) {
            proxies[i].withdraw(msg.sender);
            proxies.pop();
        }
    }
}