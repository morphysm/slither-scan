pragma solidity >=0.8.0;
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "./Authorizable.sol";
import "hardhat/console.sol";

interface RoostrRootingV2Interace {
    function stakedRoostr(uint256)
        external
        view
        returns (
            uint24 kg,
            uint32 sinceTs,
            uint32 sinceEggFeedTs,
            uint32 lastSkippedTs,
            uint48 eatenAmount,
            uint32 cooldownTs
        );

    function feedRoostr(uint256, uint256) external;

    function EGG_BURN_CAP() external view returns (uint24);

    function levelUpRoostr(uint256) external;
}

interface EggInterface {
    function burnEggs(address, uint256) external;

    function balanceOf(address) external returns (uint256);

    function transferFrom(
        address,
        address,
        uint256
    ) external returns (bool);

    function transfer(address, uint256) external returns (bool);
}

contract BulkRoosting is Authorizable {
    using SafeMath for uint256;

    address public EGG_CONTRACT;
    address public ROOSTR_CONTRACT;
    address public ROOSTINGROOSTR_CONTRACT;

    mapping(address => uint256) public userCredit;

    event DepositEgg(address indexed user, uint256 amount);
    event WithdrawEgg(address indexed user, uint256 amount);

    constructor(
        address _roostrContract,
        address _eggContract,
        address _roostingRoostrV2Contract
    ) {
        EGG_CONTRACT = _eggContract;
        ROOSTR_CONTRACT = _roostrContract;
        ROOSTINGROOSTR_CONTRACT = _roostingRoostrV2Contract;
    }

    function deposit(uint256 amount) external {
        require(amount > 0, "MUST BE MORE THAN 0");
        EggInterface eggContract = EggInterface(EGG_CONTRACT);
        uint256 available = eggContract.balanceOf(msg.sender);
        require(available >= amount, "NOT ENOUGH EGG");
        bool success = eggContract.transferFrom(
            msg.sender,
            address(this),
            amount
        );
        require(success, "Unable to transfer eggs");
        userCredit[msg.sender] += amount;
        emit DepositEgg(msg.sender, amount);
    }

    function withdraw(uint256 amount) external {
        require(amount > 0, "MUST BE MORE THAN 0");
        require(
            userCredit[msg.sender] >= amount,
            "Can't withdraw more than deposited"
        );
        EggInterface eggContract = EggInterface(EGG_CONTRACT);
        bool success = eggContract.transfer(msg.sender, amount);
        require(success, "Unable to transfer eggs");
        userCredit[msg.sender] -= amount;
        emit WithdrawEgg(msg.sender, amount);
    }

    function calculateRequiredAmount(uint256 tokenId)
        public
        view
        returns (uint256)
    {
        RoostrRootingV2Interace y = RoostrRootingV2Interace(ROOSTINGROOSTR_CONTRACT);
        (uint24 kg, , , , uint48 eatenAmount, ) = y.stakedRoostr(tokenId);
        require(kg > 0, "NOT STAKED");
        uint24 eggBurnCap = y.EGG_BURN_CAP();
        uint48 requiredAmount = 0;
        if (kg > eggBurnCap) {
            requiredAmount = uint48(eggBurnCap) - eatenAmount;
        } else {
            requiredAmount = uint48(kg) - eatenAmount;
        }
        return uint256(requiredAmount) * 1e18;
    }

    function calculateTotalRequiredAmount(uint256[] calldata tokenIds)
        public
        view
        returns (uint256)
    {
        uint256 totalRequiredAmount = 0;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            uint256 requiredAmount = calculateRequiredAmount(tokenIds[i]);
            totalRequiredAmount += requiredAmount;
        }
        return totalRequiredAmount;
    }

    /**
     * Feed all roostr owned by the message sender to next level, user will only send staked roostr
     */
    function feedAllRoostr(uint256[] calldata tokenIds) external {
        RoostrRootingV2Interace y = RoostrRootingV2Interace(ROOSTINGROOSTR_CONTRACT);
        for (uint256 i = 0; i < tokenIds.length; i++) {
            uint256 requiredAmount = calculateRequiredAmount(tokenIds[i]);
            if (requiredAmount > 0) {
                require(
                    userCredit[msg.sender] >= requiredAmount,
                    "NOT ENOUGH EGG"
                );
                y.feedRoostr(tokenIds[i], requiredAmount);
                userCredit[msg.sender] -= requiredAmount;
            }
        }
    }

    /**
     * Level up all staked roostr
     */
    function levelUpAllRoostr(uint256[] calldata tokenIds) external {
        RoostrRootingV2Interace y = RoostrRootingV2Interace(ROOSTINGROOSTR_CONTRACT);
        for (uint256 i = 0; i < tokenIds.length; i++) {
            y.levelUpRoostr(tokenIds[i]);
        }
    }

    function feedAllRoostrAndLevelup(uint256[] calldata tokenIds) external {
        RoostrRootingV2Interace y = RoostrRootingV2Interace(ROOSTINGROOSTR_CONTRACT);
        for (uint256 i = 0; i < tokenIds.length; i++) {
            uint256 requiredAmount = calculateRequiredAmount(tokenIds[i]);
            if (requiredAmount > 0) {
                require(
                    userCredit[msg.sender] >= requiredAmount,
                    "NOT ENOUGH EGG"
                );
                y.feedRoostr(tokenIds[i], requiredAmount);
                userCredit[msg.sender] -= requiredAmount;
            }
            // if is ready to be level up then level up, if not, just skip it
            (, , , , , uint32 cooldownTs) = y.stakedRoostr(tokenIds[i]);
            if (block.timestamp >= cooldownTs) {
                y.levelUpRoostr(tokenIds[i]);
            }
        }
    }
}

