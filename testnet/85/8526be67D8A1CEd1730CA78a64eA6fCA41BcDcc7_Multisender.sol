import "./base/Multicall.sol";
import "./libraries/TransferHelper.sol";

interface IERC721 {
    /**
     * @dev Safely transfers `tokenId` token from `from` to `to`, checking first that contract recipients
     * are aware of the ERC721 protocol to prevent tokens from being forever locked.
     *
     * Requirements:
     *
     * - `from` cannot be the zero address.
     * - `to` cannot be the zero address.
     * - `tokenId` token must exist and be owned by `from`.
     * - If the caller is not `from`, it must be have been allowed to move this token by either {approve} or {setApprovalForAll}.
     * - If `to` refers to a smart contract, it must implement {IERC721Receiver-onERC721Received}, which is called upon a safe transfer.
     *
     * Emits a {Transfer} event.
     */
    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    ) external;
}

interface IERC20 {
    /**
     * @dev Returns the amount of tokens owned by `account`.
     */
    function balanceOf(address account) external view returns (uint256);
}

contract Multisender is Multicall, ReentrancyGuard {
    address private admin;

    struct DataReceiver {
        address reiciver;
        uint256 value; // amount or tokenId
        uint256 id;
    }

    constructor() {
        admin = msg.sender;
    }

    modifier onlyAdmin() {
        require(msg.sender == admin, "Only admin");
        _;
    }

    receive() external payable {
        (bool success, ) = admin.call{value: msg.value}("");
        require(success, "transfer failed");
    }

    event LogMultiSendToken(address indexed token, DataReceiver[] data);

    function multiSenderToken(
        address token,
        uint256 amount,
        DataReceiver[] memory data
    ) external payable onlyAdmin nonReentrant {
        require(amount > 0, "amount is 0");
        require(data.length > 0, "data is null");
        if (token != address(0)) {
            TransferHelper.safeTransferFrom(
                token,
                msg.sender,
                address(this),
                amount
            );
            for (uint256 i = 0; i < data.length; i++) {
                TransferHelper.safeTransfer(
                    token,
                    data[i].reiciver,
                    data[i].value
                );
            }
        } else {
            for (uint256 i = 0; i < data.length; i++) {
                TransferHelper.safeTransferETH(data[i].reiciver, data[i].value);
            }
        }
        uint256 amountResidual = getBalance(token);
        if (amountResidual > 0) {
            if (token != address(0)) {
                TransferHelper.safeTransfer(token, msg.sender, amountResidual);
            } else {
                TransferHelper.safeTransferETH(msg.sender, amountResidual);
            }
        }
        emit LogMultiSendToken(token, data);
    }

    event LogMultiSendNfts(address indexed collection, DataReceiver[] data);

    function multiSenderNFTs(address collection, DataReceiver[] memory data)
        external
        onlyAdmin
        nonReentrant
    {
        require(data.length > 0, "data is null");
        for (uint256 i = 0; i < data.length; i++) {
            IERC721(collection).safeTransferFrom(
                msg.sender,
                data[i].reiciver,
                data[i].value
            );
        }
        emit LogMultiSendNfts(collection, data);
    }

    function getBalance(address _token) internal view returns (uint256) {
        return
            _token == address(0)
                ? address(this).balance
                : IERC20(_token).balanceOf(address(this));
    }
}

