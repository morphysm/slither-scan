contract Eighty80StakingContractSigner is EIP712Upgradeable{

    string public SIGNING_DOMAIN; //= "Eighty80Staker";
    string public SIGNATURE_VERSION;// = "1";

    struct Staker{
        address userAddress;
        uint nonce;
        bytes signature;
    }

    function __Eighty80StakingContractSigner_init(string memory domain, string memory version) internal initializer {
        SIGNATURE_VERSION = version;
        SIGNING_DOMAIN = domain;
        __EIP712_init(domain, version);
    }

    function getSigner(Staker memory staker) public view returns(address){
        return _verify(staker);
    }

    /// @notice Returns a hash of the given rarity, prepared using EIP712 typed data hashing rules.

    function _hash(Staker memory staker) internal view returns (bytes32) {
        return _hashTypedDataV4(keccak256(abi.encode(
                keccak256("Staker(address userAddress,uint256 nonce)"),
                staker.userAddress,
                staker.nonce
            )));
    }

    function _verify(Staker memory staker) internal view returns (address) {
        bytes32 digest = _hash(staker);
        return ECDSAUpgradeable.recover(digest, staker.signature);
    }

}

