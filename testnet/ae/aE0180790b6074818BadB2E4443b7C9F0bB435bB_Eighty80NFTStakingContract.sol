pragma solidity ^0.8.0;
import "./utils/Ownable.sol";
import "./utils/Pausable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721ReceiverUpgradeable.sol";
import "./interfaces/IERC721UpgradeableEighty80.sol";
import "./interfaces/IERC20UpgradeableEighty80.sol";
import "./signers/Eighty80StakingContractSignature.sol";
import "hardhat/console.sol";

contract Eighty80NFTStakingContract is Ownable, Pausable, Eighty80StakingContractSigner {

    IERC20UpgradeableEighty80 public rewardToken;

    bytes4 private constant _ERC721_RECEIVED = 0x150b7a02;
    event Stake(address _user, address _nftAddress, uint tokenId);
    event Lock(address _user, address _nftAddress, uint tokenId, uint lockPeriod);
    event Unstake(address _user, address _nftAddress, uint tokenId);
    event Claim(address _user, address _nftAddress, uint[] tokenId, uint rewardAmount, uint taxAmount);

    ///@notice contain the details of the token i.e locked or staked in the contract
    struct tokenDetails {
        address currentOwner;
        bool isLocked;
        uint lockTime;
        uint lockPeriod;
        uint stakeTime;
        uint totalRewardClaimedTillDate;
        uint lastClaimTime;
    }

    ///@notice  maps whether an user has staked or locked a token of a particular nftAddress or not
    mapping (address => mapping(address => bool)) public userToNftContract;
    ///@notice  maps the user and tokenId to the tokenDetails struct
    mapping (address => mapping (uint => tokenDetails)) public details;
    ///@notice  gives the total array of tokens of a particular tokenAddress staked by an user
    mapping (address => mapping (address => uint[])) public tokensStaked;
    ///@notice  gives the total array of tokens of a particular tokenAddress locked by an user
    mapping (address => mapping (address => uint[])) public tokensLocked;
    ///@notice  keeps an internal mapping of staked tokenIds position
    mapping (address => mapping (uint => uint)) tokensStakedPosition;
    ///@notice  keeps an internal mapping of locked tokenIds position
    mapping (address => mapping (uint => uint)) tokensLockedPosition;
    ///@notice  returns the total nftAddresses the user used to interact with this contract
    mapping (address => address[]) public userCollection;
    ///@notice  used once while initialising a contract
    bool public isInitialised;

    ///
    function initialize (string memory domain, string memory version, address _owner) external  {
        require(!isInitialised,'Already initialised');
        isInitialised = true;
        Pausableinitialize();
        Ownableinitialize();
        transferOwnership(_owner);
        __Eighty80StakingContractSigner_init(domain, version);
    }

    function stakeTokens(Staker memory staker, address _nftAddress, uint[] memory tokenIds) external onlyOwner {
//        require (getSigner(staker)==staker.userAddress,'Wrong Signer');
//        require (!nonce[staker.userAddress][staker.nonce],'Nonce Used');
//        require (staker.nonce + 5 minutes > block.timestamp,'Signature Expired');
        nonce[staker.userAddress][staker.nonce] = true;
        for (uint i=0; i<tokenIds.length;i++)
        {
            require (IERC721UpgradeableEighty80(_nftAddress).ownerOf(tokenIds[i]) == staker.userAddress,'!Owner Of Token');
            IERC721UpgradeableEighty80(_nftAddress).safeTransferFrom(staker.userAddress,address(this),tokenIds[i],'');
            tokenDetails memory data = details[_nftAddress][tokenIds[i]];
            data.currentOwner = staker.userAddress;
            data.isLocked = false; // rarity.userAddress;
            data.stakeTime = block.timestamp; // rarity.userAddress;
            data.lastClaimTime = block.timestamp; // rarity.userAddress;
            details[_nftAddress][tokenIds[i]] = data;
            pushToCollection(staker.userAddress,_nftAddress);
            tokensStakedPosition[_nftAddress][tokenIds[i]] = tokensStaked[staker.userAddress][_nftAddress].length;
            tokensStaked[staker.userAddress][_nftAddress].push(tokenIds[i]);
            emit Stake(staker.userAddress,_nftAddress,tokenIds[i]);
        }
    }


   function lockTokens(Staker memory staker, address _nftAddress, uint[] memory tokenIds, uint lockDays) external onlyOwner {
//       require (getSigner(staker)==staker.userAddress,'!Valid User');
//       require (!nonce[staker.userAddress][staker.nonce],'Nonce Used');
//       require (staker.nonce + 5 minutes > block.timestamp,'Signature Expired');
       nonce[staker.userAddress][staker.nonce] = true;
       for (uint i=0; i<tokenIds.length;i++){
           require (IERC721UpgradeableEighty80(_nftAddress).ownerOf(tokenIds[i]) == staker.userAddress,'Token Not Of User');
           IERC721UpgradeableEighty80(_nftAddress).safeTransferFrom(staker.userAddress,address(this),tokenIds[i],'');
           tokenDetails memory data = details[_nftAddress][tokenIds[i]];
           data.currentOwner = staker.userAddress;
           data.isLocked = true; // rarity.userAddress;
           data.lockTime = block.timestamp;
           data.lockPeriod = lockDays; // rarity.lockingTime;
           data.stakeTime = block.timestamp; // rarity.userAddress;
           data.lastClaimTime = block.timestamp; // rarity.userAddress;
           details[_nftAddress][tokenIds[i]] = data;
           pushToCollection(staker.userAddress, _nftAddress);
           tokensLockedPosition[_nftAddress][tokenIds[i]] = tokensLocked[staker.userAddress][_nftAddress].length;
           tokensLocked[staker.userAddress][_nftAddress].push(tokenIds[i]);
           emit Lock(staker.userAddress,_nftAddress,tokenIds[i],lockDays);
        }
   }

   function lockStakedTokens(Staker memory staker, address _nftAddress, uint[] memory tokenIds, uint lockDays) external onlyOwner {
//       require (getSigner(staker)==staker.userAddress);
//       require (!nonce[staker.userAddress][staker.nonce],'Nonce Used');
//       require (staker.nonce + 5 minutes > block.timestamp,'Signature Expired');
       nonce[staker.userAddress][staker.nonce] = true;
       for (uint i=0; i < tokenIds.length; i++) {
           require(details[_nftAddress][tokenIds[i]].currentOwner == staker.userAddress,'Token Either Locked Or Not Staked');
           if (!details[_nftAddress][tokenIds[i]].isLocked) {
               popStakedToken(tokenIds[i], staker.userAddress, _nftAddress); // popping the token from stake list
               details[_nftAddress][tokenIds[i]].isLocked = true; // changing the locked status
               tokensLockedPosition[_nftAddress][tokenIds[i]] = tokensLocked[staker.userAddress][_nftAddress].length;
               tokensLocked[staker.userAddress][_nftAddress].push(tokenIds[i]); //pushing the token to lock list
           }
           else {
               require (block.timestamp > details[_nftAddress][tokenIds[i]].lockTime + details[_nftAddress][tokenIds[i]].lockPeriod, "Error: Token already locked");
           }
           details[_nftAddress][tokenIds[i]].lockTime = block.timestamp;
           details[_nftAddress][tokenIds[i]].lockPeriod = lockDays; //setting the locking period
           emit Lock(staker.userAddress, _nftAddress, tokenIds[i], lockDays);
       }
   }

   function claimRewardTokens(
       uint[] memory tokenIds,
       Staker memory staker,
       address _nftAddress,
       uint minimumTimeQuantum,
       uint[] memory yield,
       uint[] memory lockBonusPercentage,
       uint[] memory multiHolderBonusPercentage,
       uint[] memory taxPercentage ) public onlyOwner {
//       require (getSigner(staker) == staker.userAddress,'!User');
//       require (!nonce[staker.userAddress][staker.nonce],'Nonce Used');
//       require (staker.nonce + 5 minutes > block.timestamp,'Signature Expired');
       nonce[staker.userAddress][staker.nonce] = true;
       uint totalRewardAmount;
       uint totalTaxAmount;
           for (uint i = 0; i< tokenIds.length; i++) {
               (uint rewardAmount, uint taxAmount)= calculateReward(staker.userAddress,tokenIds[i],_nftAddress,minimumTimeQuantum,yield[i],lockBonusPercentage[i],multiHolderBonusPercentage[i],taxPercentage[i]);
               tokenDetails storage data =  details[_nftAddress][tokenIds[i]];
               data.totalRewardClaimedTillDate += rewardAmount;
               data.lastClaimTime = block.timestamp;
               totalRewardAmount+=rewardAmount;
               totalTaxAmount+=taxAmount;
           }
       require (totalRewardAmount > 0,'Zero balance');
       rewardToken.mintExactToken(owner(), totalTaxAmount);
       rewardToken.mintExactToken(staker.userAddress, totalRewardAmount);
       emit Claim(staker.userAddress,_nftAddress,tokenIds,totalRewardAmount,totalTaxAmount);
   }

   function calculateReward(
       address _userAddress,
       uint tokenId,
       address _nftAddress,
       uint minimumTimeQuantum,
       uint yield,
       uint lockBonusPercentage,
       uint multiHolderBonusPercentage,
       uint taxPercentage )
   public view returns(uint,uint) {
           tokenDetails memory data = details[_nftAddress][tokenId];
           uint delta = block.timestamp - data.lastClaimTime;
           uint finalTimeDelta = delta / minimumTimeQuantum;
           yield=yield * 1 ether;
           uint amount = finalTimeDelta * yield;
           if (data.isLocked && finalTimeDelta > 0)
               amount = amount + ((yield * lockBonusPercentage) / 1000);
           if (viewInteractingTokensOfUsers(_userAddress,_nftAddress) > 1 && finalTimeDelta > 0)
               amount = amount + ((yield * multiHolderBonusPercentage) / 1000);
           uint taxAmount = (amount * taxPercentage) / 1000;
           amount = amount - taxAmount;
           return (amount,taxAmount);
   }


   function claimAndUnstakeToken(
       uint[] memory tokenIds,
       Staker memory staker,
       address _nftAddress,
       uint minimumTimeQuantum,
       uint[] memory yield,
       uint[] memory lockBonusPercentage,
       uint[] memory multiHolderBonusPercentage,
       uint[] memory taxPercentage
   ) external onlyOwner  {
      uint amount;
       uint fees;
       for (uint i=0;i<tokenIds.length;i++) {
         (uint amountOf,uint feesOf) = calculateReward(staker.userAddress, tokenIds[i],  _nftAddress, minimumTimeQuantum, yield[i], lockBonusPercentage[i], multiHolderBonusPercentage[i], taxPercentage[i]);
           amount += amountOf;
           fees += feesOf;
       }
       if (amount > 0 )
       rewardToken.mintExactToken(owner(),fees);
       rewardToken.mintExactToken(staker.userAddress,amount);
//       require (!unstakeNonce[staker.userAddress][staker.nonce],'Error: Nonce Used');
//       require (staker.nonce + 10 minutes > block.timestamp,'Signature Expired');
//       unstakeNonce[staker.userAddress][staker.nonce]= true;

       for (uint i = 0; i< tokenIds.length; i++) {
            tokenDetails memory data = details[_nftAddress][tokenIds[i]];
           if (data.isLocked) {
               require (block.timestamp >= data.lockTime + data.lockPeriod,"Error: Locking Period Is Not Over");
               popLockedToken(tokenIds[i],staker.userAddress,_nftAddress);

           } else {
               require (block.timestamp > data.stakeTime + minimumTimeQuantum,'Error: Minimum Quantum Not Reached');
               popStakedToken(tokenIds[i],staker.userAddress,_nftAddress);
           }
            delete details[_nftAddress][tokenIds[i]];
            IERC721UpgradeableEighty80(_nftAddress).safeTransferFrom(address(this), staker.userAddress, tokenIds[i],'');
           emit Unstake(staker.userAddress, _nftAddress, tokenIds[i]);
       }
   }

    function popLockedToken(uint tokenId, address _user, address _nftAddress) internal {
        uint[] storage currentMap = tokensLocked[_user][_nftAddress];
        uint lastToken = currentMap[currentMap.length-1];
        uint currentPosition = tokensLockedPosition[_nftAddress][tokenId];
        tokensLocked[_user][_nftAddress][currentPosition] = lastToken;
        tokensLockedPosition[_nftAddress][lastToken] = currentPosition;
        tokensLocked[_user][_nftAddress].pop();
    }

    function popStakedToken(uint tokenId, address _user, address _nftAddress) internal {
        uint[] storage currentMap = tokensStaked[_user][_nftAddress];
        uint lastToken = currentMap[currentMap.length-1];
        uint currentPosition =  tokensStakedPosition[_nftAddress][tokenId];
        tokensStaked[_user][_nftAddress][currentPosition] = lastToken;
        tokensStakedPosition[_nftAddress][lastToken] = currentPosition;
        tokensStaked[_user][_nftAddress].pop();
    }

    function pushToCollection(address _user, address _nft) internal returns(bool){
        if (userToNftContract[_user][_nft]){
            return false;
        }
        else {
            userToNftContract[_user][_nft] = true;
            userCollection[_user].push(_nft);
            return true;
        }
    }

    function viewStakedTokensOfUser(address _user, address _nftAddress) public view returns (uint[] memory) {
        return tokensStaked[_user][_nftAddress];
    }

    function viewLockedTokensOfUser(address _user, address _nftAddress) public view returns (uint[] memory) {
        return tokensLocked[_user][_nftAddress];
    }

    function viewInteractingTokensOfUsers(address _user, address _nftAddress) public view returns (uint) {
        uint stakedAmount = tokensStaked[_user][_nftAddress].length;
        uint lockedAmount = tokensLocked[_user][_nftAddress].length;
        return stakedAmount+lockedAmount;
    }

    function viewUserCollection(address _user) external view returns (uint, address[] memory) {
        uint total = userCollection[_user].length;
        return (total, userCollection[_user]);
    }

    ///@dev Helpers
    //@dev Receive the tokens
    function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata
    ) public pure returns (bytes4) {
        return _ERC721_RECEIVED;
    }

    function setRewardToken(address _tokenAddress) external onlyOwner {
        rewardToken = IERC20UpgradeableEighty80(_tokenAddress);
    }

    function getTime() public view returns (uint) {
        return block.timestamp;
    }

    ///@notice  keeps an internal mapping of nonce to stop users from doing replay attacks
    mapping (address => mapping (uint => bool)) nonce;
    ///@notice  keeps an internal mapping of nonce to stop users from doing replay attacks
    mapping (address => mapping (uint => bool)) unstakeNonce;
}

