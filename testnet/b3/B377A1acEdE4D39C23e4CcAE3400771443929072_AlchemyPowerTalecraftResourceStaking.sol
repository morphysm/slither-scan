pragma solidity ^0.8.0;

import "/OpenZeppelin/[email protected]/contracts/access/Ownable.sol";
import "/OpenZeppelin/[email protected]/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "../interfaces/ITalecraftResource.sol";
import "../interfaces/IAlchemyPower.sol";
import "../interfaces/ITimeProvider.sol";
import "../utils/CustomEnumerableMap.sol";

contract AlchemyPowerTalecraftResourceStaking is Ownable, ERC1155Holder {
    using CustomEnumerableMap for CustomEnumerableMap.UintToUintMap;

    struct BalanceItem {
        uint256 tokenId;
        uint256 amount;
    }

    struct UserPayout {
        uint256 startingValue;
        uint256 targetValue;
        uint256 startingTime;
        uint256 targetTime;
        uint256 paidOut;
    }

    IAlchemyPower public immutable alchemyPower;
    ITalecraftResource public immutable talecraftResource;
    ITimeProvider public immutable timeProvider;

    mapping (address => CustomEnumerableMap.UintToUintMap) private _stakedTokens;
    mapping (address => UserPayout) public userPayouts;
    uint256 public constant payoutDuration = 7 days;

    event TalecraftResourceStaked(address indexed account, uint256 tokenId, uint256 amount);
    event TalecraftResourceUnstaked(address indexed account, uint256 tokenId, uint256 amount);

    constructor(IAlchemyPower alchemyPower_, ITalecraftResource talecraftResource_, ITimeProvider timeProvider_) {
        alchemyPower = alchemyPower_;
        talecraftResource = talecraftResource_;
        timeProvider = timeProvider_;
    }

    function _getTalecraftMultiplier(uint256 tokenId) private view returns (uint256) {
        uint256[] memory ids = new uint256[](1);
        ids[0] = tokenId;
        ITalecraftResource.TalecraftResourceType[] memory resource = talecraftResource.getResourceTypes(ids);
        uint256 tier = resource[0].tier;
        uint256 multiplier = 0;
        if (tier == 1)
            multiplier = .5e18;
        else if (tier == 2)
            multiplier = .75e18;
        else if (tier == 3)
            multiplier = 1e18;
        else if (tier == 4)
            multiplier = 1.5e18;
        else if (tier == 5)
            multiplier = 2e18;
        return multiplier;
    }

    function stake(uint256 tokenId, uint256 amount) external {
        uint256 weight = talecraftResource.getResourceWeight(tokenId) * amount;
        uint256 multiplier = _getTalecraftMultiplier(tokenId);
        require(multiplier > 0, "tokens without tier cannot be staked");
        require(amount > 0, "cannot stake 0");

        uint256 apAmount = weight * multiplier;
        UserPayout storage payout = userPayouts[msg.sender];
        _claimAlchemyPower(msg.sender);
        uint256 timestamp = timeProvider.getTimestamp();
        payout.startingValue = payout.paidOut;
        payout.startingTime = timestamp;
        payout.targetValue += apAmount;
        payout.targetTime = timestamp + payoutDuration;
        (/*bool success*/, uint256 initialAmount) = _stakedTokens[msg.sender].tryGet(tokenId);
        _stakedTokens[msg.sender].set(tokenId, initialAmount + amount);

        talecraftResource.safeTransferFrom(msg.sender, address(this), tokenId, amount, "");
        emit TalecraftResourceStaked(msg.sender, tokenId, amount);
    }

    function unstake(uint256 tokenId, uint256 amount) external {
        (/*bool success*/, uint256 currentBalance) = _stakedTokens[msg.sender].tryGet(tokenId);
        require(currentBalance >= amount, "insufficient staked tokens");
        require(amount > 0, "cannot unstake 0");
        uint256 weight = talecraftResource.getResourceWeight(tokenId) * amount;
        uint256 multiplier = _getTalecraftMultiplier(tokenId);

        uint256 apAmount = weight * multiplier;
        UserPayout storage payout = userPayouts[msg.sender];
        uint256 newTargetValue = payout.targetValue - apAmount;
        _claimAlchemyPower(msg.sender);
        if (payout.paidOut > newTargetValue) {
            payout.startingValue = 0;
            payout.startingTime = 0;
            payout.targetValue = newTargetValue;
            payout.targetTime = 0;
            alchemyPower.burn(msg.sender, payout.paidOut - newTargetValue);
            payout.paidOut = newTargetValue;
        } else {
            uint256 timestamp = timeProvider.getTimestamp();
            payout.startingValue = payout.paidOut;
            payout.startingTime = timestamp;
            payout.targetValue = newTargetValue;
            payout.targetTime = timestamp + payoutDuration;
        }
        if (currentBalance == amount)
            _stakedTokens[msg.sender].remove(tokenId);
        else
            _stakedTokens[msg.sender].set(tokenId, currentBalance - amount);

        talecraftResource.safeTransferFrom(address(this), msg.sender, tokenId, amount, "");
        emit TalecraftResourceUnstaked(msg.sender, tokenId, amount);
    }

    function pendingAlchemyPower(address account) public view returns (uint256) {
        uint256 timestamp = timeProvider.getTimestamp();
        UserPayout memory payout = userPayouts[account];
        uint256 valuesDelta = payout.targetValue - payout.startingValue;
        uint256 pendingDelta;
        if (timestamp >= payout.targetTime) {
            pendingDelta = valuesDelta;
        } else {
            uint256 timesDelta = payout.targetTime - payout.startingTime;
            pendingDelta = valuesDelta * (timestamp - payout.startingTime) / timesDelta;
        }
        return payout.startingValue + pendingDelta - payout.paidOut;
    }

    function _claimAlchemyPower(address account) private {
        UserPayout storage payout = userPayouts[account];
        uint256 pendingAmount = pendingAlchemyPower(account);
        if (pendingAmount > 0) {
            payout.paidOut += pendingAmount;
            alchemyPower.mint(account, pendingAmount);
        }
    }

    function claimAlchemyPower() external {
        _claimAlchemyPower(msg.sender);
    }

    function stakedTokens(address account) external view returns (BalanceItem[] memory) {
        CustomEnumerableMap.UintToUintMap storage values = _stakedTokens[account];
        uint256 valuesLength = values.length();
        BalanceItem[] memory result = new BalanceItem[](valuesLength);
        for (uint256 i; i < valuesLength; i++) {
            (uint256 tokenId, uint256 amount) = values.at(i);
            result[i] = BalanceItem({
                tokenId: tokenId,
                amount: amount
            });
        }
        return result;
    }
}

