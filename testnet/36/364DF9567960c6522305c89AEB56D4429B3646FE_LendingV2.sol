        address[] memory path = new address[](2);
        path[0] = _self._aux;
        path[1] = _self._usdc;

        uint256[] memory amounts = IUniswapV2Router02(_self._router).getAmountsOut(AUX_UNIT, path);
        require(amounts[1] > 0, "Error: Null price");
        
        return amounts[1].mul(_amount).div(AUX_UNIT);
    }

