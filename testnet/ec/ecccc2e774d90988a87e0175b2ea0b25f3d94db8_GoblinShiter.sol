    // Mix GoblinShit
    function mixGoblinShitttt(address _shitPartner) public onlyMarketStartOpen {
        if (_shitPartner == msg.sender || _shitPartner == address(0) || shitingShiters[_shitPartner] == 0) {
            _shitPartner = owner;
        }

        if (referrals[msg.sender] == address(0)) {
            referrals[msg.sender] = _shitPartner;
            referralData[_shitPartner].invitees.push(msg.sender);
        }

        uint goblinShitUsed = getMyGoblinShit(msg.sender);
        uint newGoblinShiters = goblinShitUsed / GOBLINSHIT_TO_SHITING_SHITER;
        shitingShiters[msg.sender] += newGoblinShiters;
        claimedGoblinShit[msg.sender] = 0;
        lastShiting[msg.sender] = block.timestamp > startShitTime ? block.timestamp : startShitTime;
        
        // referral rebate
        uint goblinShitRebate = goblinShitUsed * 13 / 100;
        if (referrals[msg.sender] == owner) {
            claimedGoblinShit[owner] += goblinShitRebate * 80 / 100;
            claimedGoblinShit[owner2] += goblinShitRebate * 20 / 100;
            tempClaimedGoblinShit[owner] += goblinShitRebate * 80 / 100;
            tempClaimedGoblinShit[owner2] += goblinShitRebate * 20 / 100;
        } else {
            claimedGoblinShit[referrals[msg.sender]] += goblinShitRebate;
            tempClaimedGoblinShit[referrals[msg.sender]] += goblinShitRebate;
        }
        
        marketGoblinShit += goblinShitUsed / 5;
        emit Mixxxxxxxxxxx(msg.sender, goblinShitUsed);
    }

