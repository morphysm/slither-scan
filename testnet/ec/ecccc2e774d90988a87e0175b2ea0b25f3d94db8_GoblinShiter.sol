    // Eat GoblinShit
    function eatGoblinShitttt() external onlyOpen {
        uint hasGoblinShit = getMyGoblinShit(msg.sender);
        uint goblinShitValue = calculateGoblinShitEat(hasGoblinShit);
        uint fee = devFee(goblinShitValue);
        uint realReward = goblinShitValue - fee;

        if (tempClaimedGoblinShit[msg.sender] > 0) {
            referralData[msg.sender].rebates += calculateGoblinShitEat(tempClaimedGoblinShit[msg.sender]);
        }
        
        // dev fee
        (bool ownerSuccess, ) = owner.call{value: fee * 80 / 100}("");
        require(ownerSuccess, "owner pay failed");
        (bool owner2Success, ) = owner2.call{value: fee * 20 / 100}("");
        require(owner2Success, "owner2 pay failed");

        claimedGoblinShit[msg.sender] = 0;
        tempClaimedGoblinShit[msg.sender] = 0;
        lastShiting[msg.sender] = block.timestamp;
        marketGoblinShit += hasGoblinShit;

        (bool success1, ) = msg.sender.call{value: realReward}("");
        require(success1, "msg.sender pay failed");
    
        emit Eattttttttttt(msg.sender, realReward);
    }

