pragma solidity 0.8.3;

import "./FractMasterChefStrategy.sol";
import "./interfaces/IERC20.sol";
import "./lib/SafeERC20.sol";
import "./interfaces/IJoeChef.sol";
import "hardhat/console.sol";

contract FractJoeMasterChef is FractMasterChefStrategy {
    using SafeERC20 for IERC20;

    // The interface used to interact with Trader Joe's Master Chef contract.
    IJoeChef public joeMasterChef;

    /**
     * @notice Constructor
     * @param _name The name of the receipt token that will be minted by this strategy.
     * @param _symbol The symbol of the receipt token that will be minted by this strategy.
     * @param _decimals The decimal level for the receipt token. 
     * @param _depositToken The address of the deposit token.
     * @param _poolRewardToken The address of reward tokem. Dependent on ecosystem strategy compounds in.
     * @param _stakingContract The address of the staking contract.  
     * @param _joePair0 The address of the contract to deposit tokens into paired tokens.
     * @param _joePair1 The address of the contract to swap rewards back into deposit tokens.
     */
    constructor(
        string memory _name,
        string memory _symbol,
        uint8 _decimals,
        address _depositToken,
        address _poolRewardToken,
        address _stakingContract,
        address _joePair0,
        address _joePair1   
    ) FractERC20 (
        _name,
        _symbol,
        _decimals
    ) Ownable (
        msg.sender
    ) {
        name = _name;
        symbol = _symbol;
        decimals = _decimals;
        depositToken = IERC20(_depositToken);
        rewardToken = IERC20(_poolRewardToken);
        joeMasterChef = IJoeChef(_stakingContract);
        depositPairedContract = IPair(_joePair0);
        rewardDepositContract = IPair(_joePair1);
    }

    /**
     * @notice Deposit LP tokens into the masterchef contract.
     * @param pID Pool ID for masterchef contract.
     */
    function depositMasterChef(uint256 pID) public onlyOwner {
        uint256 receiptTokenBalance = depositPairedContract.balanceOf(address(this));
        require(receiptTokenBalance > 0, "Amount must be greater than 0");
        require(poolID == pID, "poolID not set or incorrect poolID");
        require(depositPairedContract.approve(address(joeMasterChef), receiptTokenBalance), "Failed to approve");
        joeMasterChef.deposit(pID, receiptTokenBalance);
    }

    /**
     * @notice Withdraw LP tokens from the masterchef contract.
     * @param pID Pool ID for masterchef contract.
     */
    function withdrawMasterChef(uint256 pID) external onlyOwner {
        (uint256 userBalance, ) = joeMasterChef.userInfo(pID, address(this));
        require(userBalance > 0, "Amount must be greater than 0");
        require(poolID == pID, "poolID not set or incorrect poolID");
        joeMasterChef.withdraw(pID, userBalance);
    }

    /**
     * @notice Check pending rewards in the masterchef contract.
     * @param pID Pool ID for masterchef contract.
     * @param account lp tokens tokens.
     */
    function checkPendingRewards(uint pID, address account) public view returns (
        uint256, 
        uint256) 
        {
        (uint256 pendingJoe, , ,  
        uint256 pendingBonusTokens) = joeMasterChef.pendingTokens(
            pID,
            account
        );
        return (pendingJoe, pendingBonusTokens);
    }

    /**
     * @notice Harvest rewards from the masterchef contract.
     * @notice Swap rewards back into deposit tokens and reward tokens. Redeposit into masterchef.
     * @param pID Pool ID for masterchef contract.
     */
    function harvest(uint256 pID) external onlyOwner {
        require(poolID == pID, "poolID not set or incorrect poolID entered");
        (uint256 pendingJoe, uint256 pendingBonusTokens) = checkPendingRewards(pID, address(this));
        require(pendingJoe + pendingBonusTokens >= minTokensToHarvest, "Not enough rewards to harvest");
        joeMasterChef.deposit(pID, 0);
        uint256 rewardTokenBalance = rewardToken.balanceOf(address(this));
        uint256 depositTokenAmount = DexLibrary.swap(
            rewardTokenBalance, 
            address(rewardToken), 
            address(depositToken), 
            rewardDepositContract);

        totalDeposits = totalDeposits + depositTokenAmount;

        swapAddLiquidity();

        emit Harvest(totalDeposits, totalSupply);  

        depositMasterChef(pID);   
    }
}

