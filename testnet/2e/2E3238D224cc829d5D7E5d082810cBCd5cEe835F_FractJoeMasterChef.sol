import "./FractStrategyV1.sol";
import "hardhat/console.sol";

/**
 * @notice FractLPStrategy inherits from FractStrategtyV1
 * @notice FractLPStrategy should be inherited by new lp strategies.
 */

 abstract contract FractMasterChefStrategy is FractStrategyV1 {
     using SafeERC20 for IERC20;

     // Address of swap pair contract to swap deposit token into paired token. 
    IPair internal depositPairedContract;

    // Address of swap pair contract to swap rewards back into deposit token.
    IPair internal rewardDepositContract;

    // Address of stable token paired to deposit token.
    address public pairedToken;

    // Pool ID for depositing LP tokens via Master Chef contract. 
    uint256 internal poolID;

    /**
     * @notice This event is fired when the the strategy adds liquidity to a pool.
     * @param poolAddress Specifies the pool liquidity was added to.
     */
    event AddLiquidity(address indexed poolAddress);

    /**
     * @notice This event is fired when the the strategy withdraws liquidity from a pool.
     * @param poolAddress Specifies the pool liquidity was added to.
     */
    event RemoveLiquidity(address indexed poolAddress);

    /**
     * @notice This event is fired when the the strategy initiates a swap.
     * @param toToken Specified the token swapped to.  
     * @param amount Specifies the amount swapped.
     */
    event SwapToPairedToken(address indexed toToken, uint256 amount);

    /**
     * @notice This event is fired when the pool contract address is set. 
     * @param poolAddress Specifies the address of the poolContract. 
     */
    event SetPool(address indexed poolAddress);


    /**
     * @notice This event is fired when the pool ID is set. 
     * @param pID Specifies the poolID number. 
     */
    event SetPoolID(uint256 pID);

    /**
     * @notice This event is fired when the pool ID is set. 
     * @param pairedTokenAddress The address of the paired token.  
     */
    event SetPairedToken(address indexed pairedTokenAddress);

    /**
     * @notice Set the pool ID, used when depositng into masterchef contract.
     * @param pID The pool ID used for a masterchef contract.
     */
    function setPoolID(uint256 pID) external onlyOwner {
        poolID = pID;
        emit SetPoolID(poolID);
    }

    /**
     * @notice the pool ID, used when depositng into masterchef contract.
     */
    function checkPoolID() external view returns (uint256) {
        return poolID;
    }

    /**
     * @notice Set the paired token the deposit token will swap into before depositing into lp pool.
     * @param pairedTokenAddress The address of the paired token.
     */
    function setPairedToken(address pairedTokenAddress) external onlyOwner {
        require(pairedTokenAddress != address(0), "Address cannot be a 0 address");
        pairedToken = pairedTokenAddress;

        emit SetPairedToken(pairedTokenAddress);
    }

    /**
     * @notice Deposit into the strategy. Can only be called by the fractVault.
     * @dev Must mint receipt tokens to `msg.sender`
     * @param amount deposit tokens
     */
    function deposit(uint256 amount) public override onlyVault {
        require(amount > 0, "Amount must be greater than 0");
        uint256 depositAmount = getSharesForDepositTokens(amount);
        require(depositAmount > 0, "Deposit Amount must be greater than 0");
        totalDeposits = totalDeposits + amount;

        _mint(msg.sender, depositAmount);

        emit Deposit(msg.sender, amount);

        depositToken.safeTransferFrom(msg.sender, address(this), amount);
    }


    /**
     * @notice Withdraw from the strategy. Can only be called by the fractVault.
     * @dev Must burn receipt tokens from `msg.sender`
     * @param amount receipt tokens.
     */
    function withdraw(uint256 amount) public override onlyVault {
        require(amount > 0, "Amount must be greater than 0");
        uint256 withdrawalAmount = getDepositTokensForShares(amount);
        require(withdrawalAmount > 0, "Withdrawal Amount must be greater than 0");
        totalDeposits = totalDeposits - withdrawalAmount;

        _burn(msg.sender, amount);

        emit Withdraw(msg.sender, withdrawalAmount);

        depositToken.safeTransfer(msg.sender, withdrawalAmount);

    }

    /**
     * @notice Swaps the deposit token into the pairedToken, and adds liquidity to pool contract. 
     */
    function swapAddLiquidity() public onlyOwner {
        uint256 swapAmount = depositToken.balanceOf(address(this)) / 2;
        require(swapAmount > 0, "Amount must be greater than 0");
        require(pairedToken != address(0), "Must set Paired Token before calling this function");

        emit SwapToPairedToken(pairedToken, swapAmount);
        emit AddLiquidity(address(depositPairedContract));

        uint256 pairedTokenAmount = 0;
        pairedTokenAmount = DexLibrary.swap(
            swapAmount, 
            address(depositToken), 
            pairedToken, 
            depositPairedContract);

        uint256 poolReceiptTokens = 0;
        poolReceiptTokens = DexLibrary.addLiquidity(address(depositPairedContract), swapAmount, pairedTokenAmount);
        
    }

    /**
     * @notice Removes liquidity from the poolContract. Swaps pairedTokenAmount back into depositTokenAmount. 
     */
    function swapRemoveLiquidity() external onlyOwner {

        uint256 amount0 = 0;
        uint256 amount1 = 0;
        uint256 swappedTokens = 0;
        uint256 pairedTokenAmount = 0;
        uint256 depositTokenAmount = 0;

        emit RemoveLiquidity(address(depositPairedContract));

        (amount0, amount1) = DexLibrary.removeLiquidity(address(depositPairedContract));

        pairedTokenAmount = IERC20(pairedToken).balanceOf(address(this));

        swappedTokens = DexLibrary.swap(
            pairedTokenAmount,
            pairedToken,
            address(depositToken),
            depositPairedContract);

        pairedTokenAmount = IERC20(pairedToken).balanceOf(address(this));
        depositTokenAmount = depositToken.balanceOf(address(this));

        uint256 depositDifference = totalDeposits - depositTokenAmount;

        totalDeposits = totalDeposits - depositDifference;

        console.log(totalDeposits);

        require(depositTokenAmount > pairedTokenAmount, "Swap Failed");
    }
}

