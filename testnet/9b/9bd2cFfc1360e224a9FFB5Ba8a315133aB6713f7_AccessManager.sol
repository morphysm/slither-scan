	/*///////////////////////////////////////////////////////////////
                            CONSTRUCTOR
  //////////////////////////////////////////////////////////////*/

	constructor(
		string memory name_,
		string memory symbol_,
		IAccessPassStore accessPassStore_
	) ERC1155() {
		owner = msg.sender;

		name = name_;

		symbol = symbol_;

		accessPassStore = accessPassStore_;

		itemList[itemCount] = Item({
			live: true,
			tokenId: itemCount,
			price: 0,
			maxSupply: type(uint256).max,
			currentSupply: 0,
			accessLevel: 0,
			resaleRoyalty: 100, // TODO correct this
			description: 'Basic level access',
			name: 'Basic Pass'
		});

		++itemCount;
	}

	// ============ OWNER INTERFACE ============

	function collectFees() external {
		if (msg.sender != owner) revert Unauthorised();

		(bool success, ) = payable(msg.sender).call{value: address(this).balance}(new bytes(0));
		require(success, 'AccessManager: ether transfer failed');
	}

	function setItemWhitelist(
		address user,
		uint256 itemId,
		bool setTo
	) public {
		if (msg.sender != owner) revert Unauthorised();

		// prevents setting uninitilised item
		if (itemList[itemId].maxSupply == 0) revert InvalidItem();

		roundtableWhitelist[user][itemId] = setTo;
	}

	function addItem(
		uint256 price,
		uint256 itemSupply,
		uint256 accessLevel,
		string calldata itemName,
		string calldata desc,
		string calldata svg
	) external {
		if (msg.sender != owner) revert Unauthorised();

		itemList[itemCount] = Item({
			live: false,
			tokenId: itemCount,
			price: price,
			maxSupply: itemSupply,
			currentSupply: 0,
			accessLevel: accessLevel,
			resaleRoyalty: 100, // TODO correct this
			description: desc,
			name: itemName
		});

		accessPassStore.addItemSvg(msg.sender, itemCount, svg);
		++itemCount;

		emit ItemAdded(itemCount, accessLevel);
	}

	function setItemMintLive(uint256 itemId, bool setting) external {
		if (msg.sender != owner) revert Unauthorised();

		itemList[itemId].live = setting;

		emit ItemMintLive(itemCount, setting);
	}

	function transferOwnership(address newOwner) public virtual {
		if (msg.sender != owner) revert Unauthorised();

		address oldOwner = owner;
		owner = newOwner;
		emit OwnershipTransferred(oldOwner, newOwner);
	}

	// function setTokenURI(string memory _uri) external {
	// 	_setURI(_uri);
	// 	emit URI(_uri);
	// }

	// ============ PUBLIC INTERFACE ============

	function whitelistMintItem(uint256 itemId) external {
		if (!roundtableWhitelist[msg.sender][itemId]) revert Unauthorised();

		if (itemList[itemId].maxSupply == 0) revert InvalidItem();

		if (itemList[itemId].currentSupply >= itemList[itemId].maxSupply) revert ItemUnavailable();

		if (balanceOf[msg.sender][itemId] > 0) revert AlreadyOwner();

		// If item is a level pass
		if (itemId <= 3) {
			// Update users global level if new level is higher than current level
			if (memberLevel[msg.sender] < itemId) memberLevel[msg.sender] = itemId;

			// Mint the basic pass as well as special level pass as this indicates app-wide access
			if (balanceOf[msg.sender][0] == 0) {
				_mint(msg.sender, 0, 1, '');
				++itemList[0].currentSupply;
			}
		}

		_mint(msg.sender, itemId, 1, '');
		_setItemWhitelist(msg.sender, itemId, false);
		++itemList[itemId].currentSupply;

		emit ItemMinted(msg.sender, itemId, itemList[itemId].accessLevel);
	}

	function mintItem(uint256 itemId) external payable {
		// prevents unregestered user minting any item but an access pass
		if (itemId > 3 && balanceOf[msg.sender][0] == 0) revert Unauthorised();

		if (itemList[itemId].live == false) revert MintingClosed();

		if (itemList[itemId].currentSupply >= itemList[itemId].maxSupply) revert ItemUnavailable();

		if (balanceOf[msg.sender][itemId] > 0) revert AlreadyOwner();

		// TODO calculate discount for higher level members
		if (msg.value != itemList[itemId].price) revert IncorrectValue();

		// If item is a level pass
		if (itemId <= 3) {
			// Update users global level if new level is higher than current level
			if (memberLevel[msg.sender] < itemId) memberLevel[msg.sender] = itemId;

			// If special level pass is minted, mint the basic pass anyway as this indicates app-wide access
			if (itemId != 0 && balanceOf[msg.sender][0] == 0) {
				_mint(msg.sender, 0, 1, '');
				++itemList[0].currentSupply;
			}
		}

		if (memberLevel[msg.sender] < itemList[itemId].accessLevel) revert InsufficientLevel();

		_mint(msg.sender, itemId, 1, '');
		++itemList[itemId].currentSupply;

		emit ItemMinted(msg.sender, itemId, itemList[itemId].accessLevel);
	}

	function upgradeLevel(uint256 itemId) external payable {
		if (itemId > 3) revert InvalidItem();

		if (itemId <= memberLevel[msg.sender]) revert InvalidItem();

		if (itemList[itemId].currentSupply >= itemList[itemId].maxSupply) revert ItemUnavailable();

		if (msg.value != itemList[itemId].price - itemList[memberLevel[msg.sender]].price)
			revert IncorrectValue();

		// Burn and adjust supply for any non BasicLevel token
		if (memberLevel[msg.sender] != 0) {
			_burn(msg.sender, itemList[memberLevel[msg.sender]].tokenId, 1);
			--itemList[memberLevel[msg.sender]].currentSupply;
		}

		_mint(msg.sender, itemId, 1, '');
		++itemList[itemId].currentSupply;

		memberLevel[msg.sender] = itemId;

		emit ItemMinted(msg.sender, itemId, itemList[itemId].accessLevel);
	}

