		// update user level
		//update item and owner
		// emit event
	}

	function safeTransferFrom(
		address from,
		address to,
		uint256 id,
		uint256 amount,
		bytes memory data
	) public virtual override {
		// consider intermeiate function to hold items until accepted

		if (!(msg.sender == from || isApprovedForAll[from][msg.sender])) revert Unauthorised();

		if (balanceOf[from][id] == 0) revert InvalidItem();

		// If item is a membership level
		if (id <= 3)
			if (
				memberLevel[to] < id // New level must be higher than old to prevent forced transfer to lower level.
			) {
				// Update count of token which was replaced (unless 0 since basic token is required)
				if (memberLevel[to] != 0) --itemList[memberLevel[to]].currentSupply;

				memberLevel[to] = id;

				memberLevel[from] = 0;
			} else {
				revert InvalidItem();
			}

		if (memberLevel[to] < itemList[id].accessLevel) revert InsufficientLevel();

		balanceOf[from][id] -= amount;
		balanceOf[to][id] += amount;

		emit TransferSingle(msg.sender, from, to, id, amount);

		require(
			to.code.length == 0
				? to != address(0)
				: ERC1155TokenReceiver(to).onERC1155Received(msg.sender, from, id, amount, data) ==
					ERC1155TokenReceiver.onERC1155Received.selector,
			'UNSAFE_RECIPIENT'
		);

		// super.safeTransferFrom(from, to, id, amount, data);
		// require(msg.sender == from || isApprovedForAll[from][msg.sender], 'NOT_AUTHORIZED');

		// balanceOf[from][id] -= amount;
		// balanceOf[to][id] += amount;

		// emit TransferSingle(msg.sender, from, to, id, amount);

		// require(
		// 	to.code.length == 0
		// 		? to != address(0)
		// 		: ERC1155TokenReceiver(to).onERC1155Received(msg.sender, from, id, amount, data) ==
		// 			ERC1155TokenReceiver.onERC1155Received.selector,
		// 	'UNSAFE_RECIPIENT'
		// );
	}

	// ============ PUBLIC VIEW FUNCTIONS ============

	function uri(uint256 id) public view override returns (string memory) {
		uri(id);
	}

	function maxSupply() public view returns (uint256) {
