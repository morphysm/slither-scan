library LibStorage {
    // Storage are structs where the data gets updated throughout the lifespan of the game
    bytes32 constant GAME_STORAGE_POSITION = keccak256("mafiastars.storage.game");
    bytes32 constant WHITELIST_STORAGE_POSITION = keccak256("mafiastars.storage.whitelist");
    bytes32 constant BATCH_STORAGE_POSITION = keccak256("mafiastars.storage.batches");
    bytes32 constant ADMIN_STORAGE_POSITION = keccak256("mafiastars.storage.admin");
    bytes32 constant ADMIN_STATS_STORAGE_POSITION = keccak256("mafiastars.storage.adminstats");
    // Constants are structs where the data gets configured on game initialization
    bytes32 constant GAME_CONSTANTS_POSITION = keccak256("mafiastars.constants.game");

    function gameStorage() internal pure returns (GameStorage storage gs) {
        bytes32 position = GAME_STORAGE_POSITION;
        assembly {
            gs.slot := position
        }
    }
    
    function whitelistStorage() internal pure returns (WhitelistStorage storage ws) {
        bytes32 position = WHITELIST_STORAGE_POSITION;
        assembly {
            ws.slot := position
        }
    }

    function batchStorage() internal pure returns (BatchStorage storage bs) {
        bytes32 position = BATCH_STORAGE_POSITION;
        assembly {
            bs.slot := position
        }
    }

    function adminStatsStorage() internal pure returns (AdminStatsStorage storage adss) {
        bytes32 position = ADMIN_STATS_STORAGE_POSITION;
        assembly {
            adss.slot := position
        }
    }

    function adminStorage() internal pure returns (AdminStorage storage ads) {
        bytes32 position = ADMIN_STORAGE_POSITION;
        assembly {
            ads.slot := position
        }
    }

     function gameConstants() internal pure returns (GameConstants storage gc) {
        bytes32 position = GAME_CONSTANTS_POSITION;
        assembly {
            gc.slot := position
        }
    }
}


contract WithStorage {
    function gs() internal pure returns (GameStorage storage) {
        return LibStorage.gameStorage();
    }

    function ws() internal pure returns (WhitelistStorage storage) {
        return LibStorage.whitelistStorage();
    }

    function bs() internal pure returns (BatchStorage storage) {
        return LibStorage.batchStorage();
    }

    function ads() internal pure returns (AdminStorage storage) {
        return LibStorage.adminStorage();
    }

    function adss() internal pure returns (AdminStatsStorage storage) {
        return LibStorage.adminStatsStorage();
    }

    function gameConstants() internal pure returns (GameConstants storage) {
        return LibStorage.gameConstants();
    }
}

