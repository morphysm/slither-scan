interface IERC4907 {

    // Logged when the user of a NFT is changed or expires is changed
    /// @notice Emitted when the `user` of an NFT or the `expires` of the `user` is changed
    /// The zero address for user indicates that there is no user address
    event UpdateUser(uint256 indexed tokenId, address indexed user, uint64 expires);

    /// @notice set the user and expires of a NFT
    /// @dev The zero address indicates there is no user
    /// Throws if `tokenId` is not valid NFT
    /// @param user  The new user of the NFT
    /// @param expires  UNIX timestamp, The new user could use the NFT before expires
    function setUser(uint256 tokenId, address user, uint64 expires) external;

    /// @notice Get the user address of an NFT
    /// @dev The zero address indicates that there is no user or the user is expired
    /// @param tokenId The NFT to get the user address for
    /// @return The user address for this NFT
    function userOf(uint256 tokenId) external view returns(address);

    /// @notice Get the user expires of an NFT
    /// @dev The zero value indicates that there is no user
    /// @param tokenId The NFT to get the user expires for
    /// @return The user expires for this NFT
    function userExpires(uint256 tokenId) external view returns(uint256);
}

contract ERC4907 is ERC721, IERC4907 {
    string private constant REVERT_NOT_HAVE_POSSESSION = "ERC4907: transfer caller is not owner nor approved";
    string private constant REVERT_NOT_VALID_TRANSFER = "ERC4907: Cannot transfer NFTs that are in rented";

    struct UserInfo 
    {
        address user;   // address of user role
        uint64 expires; // unix timestamp, user expires
    }

    mapping (uint256  => UserInfo) internal _users;

    constructor(string memory name_, string memory symbol_) ERC721(name_, symbol_) {}
    
    function setUser(uint256 tokenId, address user, uint64 expires) public virtual{
        require(_isApprovedOrOwner(msg.sender, tokenId), REVERT_NOT_HAVE_POSSESSION);
        UserInfo storage info =  _users[tokenId];
        info.user = user;
        info.expires = expires;
        emit UpdateUser(tokenId, user, expires);
    }

    function userOf(uint256 tokenId) public view virtual returns(address){
        if( uint256(_users[tokenId].expires) >=  block.timestamp){
            return  _users[tokenId].user;
        }
        else{
            return address(0);
        }
    }
    function userExpires(uint256 tokenId) public view virtual returns(uint256){
        return _users[tokenId].expires;
    }

    function supportsInterface(bytes4 interfaceId) public view virtual override returns (bool) {
        return interfaceId == type(IERC4907).interfaceId || super.supportsInterface(interfaceId);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override{
        super._beforeTokenTransfer(from, to, tokenId);
        require(userExpires(tokenId) < block.timestamp, REVERT_NOT_VALID_TRANSFER);
        if (from != to && _users[tokenId].user != address(0)) {
            delete _users[tokenId];
            emit UpdateUser(tokenId, address(0), 0);
        }
    }
} 

contract TestNFT4907V2 is ERC4907, ERC721Burnable, MinterRole, Business, TransferFee, Lockable {
    event SetCreator(address creator);
    event SetTypeNFT(uint256 typeId, uint256 typeNFT, uint256 priceUsd, uint256 numItems);
    event Register(address indexed user, uint256 tokenId, uint256 numItems, uint256 typeNFT);
    event UpgradeNftLevel(address indexed user, uint256 tokenId, uint8 nftLevel);

    using SafeMath for uint256;
    using Counters for Counters.Counter;

    Counters.Counter private _tokenIdCounter;

    struct Meta {
        address creator;
        uint256 typeNFT;
        uint8 nftLevel;
    }

    struct TypeNft{
      uint256 typeNFT;
      uint256 price; // token
      uint256 numItems;
    }

    mapping(uint256 => TypeNft) public typeNFTs;
    mapping(uint256 => Meta) public metadata;
    address public creator;
    address public upgradeToken;

    constructor() ERC4907("TestNFT4907", "TN4907") {
        _setFeeTransfer(0x2F7265b97F6655F78AbBf13168e1FB4749A03bd0, 0, _msgSender());
        _addMinter(_msgSender());
        creator = _msgSender();
        upgradeToken = 0x2F7265b97F6655F78AbBf13168e1FB4749A03bd0;
    }

    modifier onlyTokenOwner(uint256 tokenId){
        require(_msgSender() == ownerOf(tokenId));
        _;
    }

    function getNFTMeta(uint256 tokenId) public view returns(Meta memory){
        return metadata[tokenId];
    }

    function setUpgradeToken(address _upgradeToken) public onlyOwner{
       upgradeToken = _upgradeToken;
    }

    function setType(uint256 typeId, uint256 typeNFT, uint256 price, uint256 numItems) external onlyOwner {
      require(price > 0, "Invalid price!");
      typeNFTs[typeId] = TypeNft(typeNFT, price, numItems);
      emit SetTypeNFT(typeId, typeNFT, price, numItems);
    }

    function upgradeLevel(uint256 tokenId) public payable onlyTokenOwner(tokenId){
        Meta memory nftMeta = metadata[tokenId];
        require(nftMeta.nftLevel < 5, "Invalid level!");
        address account = _msgSender();
        // Tạm thời lấy giá trị upgrade = giá trị nft x level mới
        uint256 price = typeNFTs[nftMeta.typeNFT].price * (nftMeta.nftLevel + 1);
        IERC20(upgradeToken).transferFrom(account, feeBeneficiary, price);        
        metadata[tokenId].nftLevel += 1;

        emit UpgradeNftLevel(account, tokenId, metadata[tokenId].nftLevel);
    }

    function safeMint(address to, uint256 typeNFT) private returns(uint256) {
        _tokenIdCounter.increment();
        uint256 tokenId = _tokenIdCounter.current();
        _safeMint(to, tokenId);
        // Default NFT level: 1
        metadata[tokenId] = Meta(creator, typeNFT, 1);
        return tokenId;
    }

    function multipleMint(address user, uint256 typeNFT, uint256 numItems) private {
        uint256 tokenId;
        for (uint256 j = 0; j < numItems; j++) {
            tokenId = safeMint(msg.sender, typeNFT);
        }
        emit Register(user, tokenId, numItems, typeNFT);
    }

    function buyNFT(uint256 typeId) public payable{
        uint256 price = typeNFTs[typeId].price;
        require(msg.value >= price, "Insufficient balance!");
        payable(feeBeneficiary).transfer(msg.value);
        
        multipleMint(msg.sender, typeNFTs[typeId].typeNFT, typeNFTs[typeId].numItems);
    }

    function transferFrom(address from, address to, uint256 tokenId) public override {
        require(isBusiness(msg.sender) || IERC20(feeToken).transferFrom(from, feeBeneficiary, feeAmount), "Manager: caller is not in the manager");
        return super.transferFrom(from, to, tokenId);
    }

    function safeTransferFrom(address from, address to, uint256 tokenId) public override {
        require(isBusiness(msg.sender) || IERC20(feeToken).transferFrom(from, feeBeneficiary, feeAmount), "Manager: caller is not in the manager");
        return super.safeTransferFrom(from, to, tokenId);
    }

    function safeTransferFrom(address from, address to, uint256 tokenId, bytes memory _data) public override {
        require(isBusiness(msg.sender) || IERC20(feeToken).transferFrom(from, feeBeneficiary, feeAmount), "Manager: caller is not in the manager");
        super.safeTransferFrom(from, to, tokenId, _data);
    }

    function withdraw(address token, uint256 amount) external onlyOwner {
        IERC20 erc20 = IERC20(token);
        require(erc20.balanceOf(address(this)) > amount, "Insufficient balance!");
        erc20.transfer(_msgSender(), amount);
    }

    function setCreator(address account) external onlyOwner {
        creator = account;
        emit SetCreator(account);
    }


    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        override(ERC721, ERC4907)
        onlyUnLocked(msg.sender, from, to)
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

     // The following functions are overrides required by Solidity.

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, ERC4907)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}