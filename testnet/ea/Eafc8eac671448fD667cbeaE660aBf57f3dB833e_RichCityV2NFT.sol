        uint8 mask;
        uint8 weapon;
        uint8 gen;
    }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) external;

    function ownerOf(uint256 tokenId) external view returns (address owner);

    function getTokenTraits(uint256 tokenId)
        external
        view
        returns (RichCity memory);
}

contract RichCityV2NFT is ERC721Enumerable, Ownable, IERC721Receiver, Pausable {
    using SafeERC20 for IERC20;
    using SafeMath for uint256;

    event TokenMinted(address owner, uint256 tokenId, uint256 timestamp);

    event TokenUpgrade(address owner, uint256 tokenId, uint256 timestamp);

    address payable public immutable feeAccount1;
    address payable public immutable feeAccount2;
    // mint price
    uint256 public PUBLIC_PRICE = 100;
    uint16 public constant maxFirstMinted = 1500;
    uint16 public constant maxOwnerMinted = 1500;
    // number of tokens have been minted so far
    uint32 public minted;
    uint16 public firstMinted;
    uint16 public ownerMinted;
    uint8 public constant whiteOneMaxMinted = 10;
    uint8 public constant whiteTwoMaxMinted = 10;
    mapping(address => bool) public whiteListedOne;
    mapping(address => bool) public whiteListedTwo;
    mapping(address => uint8) public whiteMinted;
    struct TokenTrait {
        uint8 body;
        uint8 clothes;
        uint8 hair;
        uint8 jewelry;
        uint8 tatto;
        uint8 belt;
        uint8 sunglass;
        uint8 hat;
        uint8 mask;
        uint8 weapon;
    }

    struct TokenType {
        uint8 gen;
        uint8 level;
        uint8 kind; //  1 2 3  => Ganster swat eva  Lead(level 4 5)
        uint8 mintCount; // have a baby Count
    }

    mapping(uint256 => TokenTrait) public tokenTraits;
    mapping(uint256 => TokenType) public tokenTypes;

    IERC20 public CASH;
    IERC20 public DRUG;
    IERC20 public USDC;

    ITraits public traits;

    IRandomSource public randomSource;
    IRichcityNFT public RichcityNFT;
    bool private _reentrant = false;

    modifier nonReentrant() {
        require(!_reentrant, "No reentrancy");
        _reentrant = true;
        _;
        _reentrant = false;
    }

    constructor(
        IERC20 _USDC,
        IERC20 _CASH,
        IERC20 _DRUG,
        ITraits _traits,
        IRandomSource _randomSource,
        IRichcityNFT _RichcityNFT,
        address _feeAccount1,
        address _feeAccount2
    ) ERC721("Rich City Game", "RCG") {
        USDC = _USDC;
        CASH = _CASH;
        DRUG = _DRUG;
        traits = _traits;
        randomSource = _randomSource;
        RichcityNFT = _RichcityNFT;
        feeAccount1 = payable(_feeAccount1);
        feeAccount2 = payable(_feeAccount2);
    }

    function migrateOldNFT(uint16[] calldata tokenIds)
        external
        nonReentrant
        whenNotPaused
    {
        require(tx.origin == _msgSender(), "Only EOA");
        for (uint256 i = 0; i < tokenIds.length; i++) {
            require(
                RichcityNFT.ownerOf(tokenIds[i]) == msg.sender,
                "not owner"
            );
            RichcityNFT.transferFrom(msg.sender, address(this), tokenIds[i]);
            uint256 costDRUG = (tokenIds[i] / 1001 + 1) * 1e18;
            DRUG.safeTransferFrom(msg.sender, feeAccount2, costDRUG);
            uint256 seed = random(tokenIds[i]);
            if (uint16(seed & 0xFFFF) % 1000 > 4) {
                minted++;
                IRichcityNFT.RichCity memory oldT = RichcityNFT.getTokenTraits(
                    tokenIds[i]
                );
                tokenTypes[minted] = TokenType({
                    kind: oldT.isGangster ? 1 : 2,
                    gen: 0,
                    level: oldT.level,
                    mintCount: 0
                });
                tokenTraits[minted] = TokenTrait({
                    body: oldT.body,
                    clothes: oldT.clothes,
                    hair: oldT.hair,
                    jewelry: oldT.jewelry,
                    tatto: oldT.tatto,
                    belt: oldT.belt,
                    sunglass: oldT.sunglass,
                    hat: oldT.hat,
                    mask: oldT.mask,
                    weapon: oldT.weapon
                });
                _safeMint(msg.sender, minted);
                emit TokenMinted(msg.sender, minted, block.timestamp);
            }
        }
    }

    //todo:delete
    function mintOldNFT()
        external
        onlyOwner
        nonReentrant
        whenNotPaused
        returns (TokenTrait memory t1, TokenType memory t2)
    {
        minted++;
        uint256 seed = random(minted);
        t2.gen = 0;
        t2.kind = uint8((uint16(seed & 0xFFFF) & 1) + 1);
        t2.level = 1;
        t2.mintCount = 0;
        tokenTypes[minted] = t2;
        (t1, ) = selectTraits(seed, false); //todo: select T1
        tokenTraits[minted] = t1;
        _safeMint(msg.sender, minted);
        emit TokenMinted(msg.sender, minted, block.timestamp);
    }

    function mintByOwner(uint256 amount) external onlyOwner {
        require(ownerMinted <= maxOwnerMinted, "Exceed amount");
        uint256 seed;
        for (uint256 i = 0; i < amount; i++) {
            ownerMinted++;
            minted++;
            seed = random(minted);
            randomSource.update(minted ^ seed);
            generate(minted, seed);
            _safeMint(msg.sender, minted);
            emit TokenMinted(msg.sender, minted, block.timestamp);
        }
    }

    function mintFirst(uint256 amount) external nonReentrant whenNotPaused {
        require(tx.origin == _msgSender(), "Only EOA");
        require(amount > 0 && amount <= 3, "Invalid mint amount");
        require(firstMinted <= maxFirstMinted, "Exceed amount");
        require(
            whiteListedOne[msg.sender] || whiteListedTwo[msg.sender],
            "No Whitelist"
        );
        if (whiteListedTwo[msg.sender] && whiteListedOne[msg.sender]) {
            require(
                whiteMinted[msg.sender] + amount <= whiteOneMaxMinted,
                "Exceed Purchase Times"
            );
        } else if (whiteListedTwo[msg.sender]) {
            require(
                whiteMinted[msg.sender] + amount <= whiteTwoMaxMinted,
                "Exceed Purchase Times"
            );
        } else {
            require(
                whiteMinted[msg.sender] + amount <= whiteOneMaxMinted,
                "Exceed Purchase Times"
            );
        }
        uint256 totalCost = 0;
        uint256 seed;
        for (uint256 i = 0; i < amount; i++) {
            firstMinted++;
            minted++;
            whiteMinted[msg.sender]++;
            seed = random(minted);
            randomSource.update(minted ^ seed);
            generate(minted, seed);
            totalCost += PUBLIC_PRICE;
            _safeMint(msg.sender, minted);
            emit TokenMinted(msg.sender, minted, block.timestamp);
        }
        if (totalCost > 0) {
            USDC.safeTransferFrom(
                msg.sender,
                address(this),
                totalCost.mul(1e6)
            );
        }
    }

    function mintPublic(uint256 evaId, uint256 tokenId)
        external
        nonReentrant
        whenNotPaused
    {
        require(ownerOf(evaId) == msg.sender && ownerOf(tokenId) == msg.sender);
        TokenType memory et = tokenTypes[evaId];
        TokenType memory tt = tokenTypes[tokenId];
        require(et.kind == 3 && tt.kind != 3);
        require(et.mintCount < 7 && tt.mintCount < 7, "Over mint times");
        require(et.level >= 3);
        if (tt.kind == 2) require(tt.level >= 2);
        if (tt.kind == 1) require(tt.level >= 3);
        minted++;
        tokenTypes[evaId].mintCount++;
        tokenTypes[tokenId].mintCount++;
        uint256 seed = random(minted);
        randomSource.update(minted ^ seed);
        (uint256 cost1, uint256 cost2) = mintPublicCost(evaId);
        generatePublic(minted, seed, tokenId);
        _safeMint(msg.sender, minted);
        pay(msg.sender, cost2 * 1e18, cost1 * 1e18);
        emit TokenMinted(msg.sender, minted, block.timestamp);
    }

    function upgrade(uint16[] calldata tokenIds) external {
        for (uint256 id = 0; id < tokenIds.length; id++) {
            require(ownerOf(tokenIds[id]) == msg.sender);
            TokenType storage t = tokenTypes[tokenIds[id]];
            require((t.kind != 2 && t.level < 5) || t.level < 3, "Max Level");
            if (t.kind == 1 && t.level == 3) {
                generate(tokenIds[id], random(minted));
            }
            uint256 costValue = upgradeCost(tokenIds[id]);
            t.level++;
            if (t.kind == 3) {
                pay(msg.sender, costValue * 1e18, 0);
            } else {
                CASH.burnFrom(msg.sender, costValue * 1e18);
            }
            emit TokenUpgrade(msg.sender, tokenIds[id], block.timestamp);
        }
    }

    /***INTERNAL */

    function pay(
        address _from,
        uint256 _DRUGamount,
        uint256 _CASHamount
    ) internal {
        if (_DRUGamount > 0) {
            DRUG.safeTransferFrom(
                _from,
                feeAccount1,
                _DRUGamount.mul(7).div(10)
            );
            DRUG.safeTransferFrom(
                _from,
                feeAccount2,
                _DRUGamount.mul(3).div(10)
            );
        }
        if (_CASHamount > 0) {
            CASH.safeTransferFrom(
                _from,
                feeAccount1,
                _CASHamount.mul(7).div(10)
            );
            CASH.safeTransferFrom(
                _from,
                feeAccount2,
                _CASHamount.mul(3).div(10)
            );
        }
    }

    function generate(uint256 tokenId, uint256 seed)
        internal
        returns (TokenTrait memory t1, TokenType memory t2)
    {
        if (tokenTypes[tokenId].kind == 1) {
            t1 = upgradeTraits(tokenId, seed);
        } else {
            (t1, t2) = selectTraits(seed, true);
            tokenTraits[tokenId] = t1;
            tokenTypes[tokenId] = t2;
        }
    }

    function generatePublic(
        uint256 tokenId,
        uint256 seed,
        uint256 atokenId
    ) internal returns (TokenTrait memory t1, TokenType memory t2) {
        t2.gen = tokenTypes[atokenId].mintCount;
        uint8 p = getMintPossibility(atokenId, seed);
        t2.kind = p;
        t2.level = 1;
        if (p == 4) {
            t2.kind = 1;
            t2.level = 4;
        }
        t2.mintCount = 0;
        tokenTypes[tokenId] = t2;
        (t1, ) = selectTraits(seed, false);
        tokenTraits[tokenId] = t1;
    }

    /**
     * uses A.J. Walker's Alias algorithm for O(1) rarity table lookup
     * ensuring O(1) instead of O(n) reduces mint cost by more than 50%
     * probability & alias tables are generated off-chain beforehand
     * @param seed portion of the 256 bit seed to remove trait correlation
     * @param traitType the trait type to select a trait for
     * @return the ID of the randomly selected trait
     */
    function selectTrait(uint16 seed, uint8 traitType)
        internal
        view
        returns (uint8)
    {
        return traits.selectTrait(seed, traitType);
    }

    function upgradeTraits(uint256 tokenId, uint256 seed)
        internal
        returns (TokenTrait memory)
    {
        TokenTrait storage t = tokenTraits[tokenId];
        t.jewelry = 0;
        t.tatto = 0;
        t.body = selectTrait(uint16(seed & 0xFFFF), 45);
        seed >>= 16;
        t.clothes = selectTrait(uint16(seed & 0xFFFF), 46);
        seed >>= 16;
        t.belt = selectTrait(uint16(seed & 0xFFFF), 49);
        seed >>= 16;
        t.hair = selectTrait(uint16(seed & 0xFFFF), 48);
        seed >>= 16;
        t.sunglass = selectTrait(uint16(seed & 0xFFFF), 50);
        return t;
    }

    function selectTraits(uint256 seed, bool flag)
        internal
        view
        returns (TokenTrait memory t1, TokenType memory t2)
    {
        if (flag) {
            t2.gen = 0;
            t2.kind = 3;
            t2.level = 1;
            t2.mintCount = 0;
        } else {
            t2.kind = tokenTypes[minted].kind;
        }
        uint8 shift = t2.kind * 10; //  10 20 30 40
        if (t2.kind == 1 && t2.level > 3) shift = 40;
        seed >>= 16;
        if (t2.kind == 1 && t2.level <= 3) {
            t1.body = selectTrait(uint16(seed & 0xFFFF), 5 + shift);
            seed >>= 16;
            t1.clothes = selectTrait(uint16(seed & 0xFFFF), 6 + shift);
            seed >>= 16;
            t1.hair = selectTrait(uint16(seed & 0xFFFF), 8 + shift);
            seed >>= 16;
            t1.jewelry = selectTrait(uint16(seed & 0xFFFF), 9 + shift);
            seed >>= 16;
            t1.tatto = selectTrait(uint16(seed & 0xFFFF), 10 + shift);
        } else if (t2.kind == 2) {
            t1.body = selectTrait(uint16(seed & 0xFFFF), 5 + shift);
            seed >>= 16;
            t1.clothes = selectTrait(uint16(seed & 0xFFFF), 6 + shift);
            seed >>= 16;
            t1.mask = selectTrait(uint16(seed & 0xFFFF), 8 + shift);
            seed >>= 16;
            t1.weapon = selectTrait(uint16(seed & 0xFFFF), 9 + shift);
            seed >>= 16;
            t1.hat = selectTrait(uint16(seed & 0xFFFF), 10 + shift);
        } else if (t2.kind == 3) {
            t1.body = selectTrait(uint16(seed & 0xFFFF), 5 + shift);
            seed >>= 16;
            t1.jewelry = selectTrait(uint16(seed & 0xFFFF), 8 + shift);
            seed >>= 16;
            t1.hair = selectTrait(uint16(seed & 0xFFFF), 9 + shift);
            seed >>= 16;
            t1.tatto = selectTrait(uint16(seed & 0xFFFF), 10 + shift);
        } else {
            t1.body = selectTrait(uint16(seed & 0xFFFF), 5 + shift);
            seed >>= 16;
            t1.clothes = selectTrait(uint16(seed & 0xFFFF), 6 + shift);
            seed >>= 16;
            t1.belt = selectTrait(uint16(seed & 0xFFFF), 8 + shift);
            seed >>= 16;
            t1.hair = selectTrait(uint16(seed & 0xFFFF), 9 + shift);
            seed >>= 16;
            t1.sunglass = selectTrait(uint16(seed & 0xFFFF), 10 + shift);
        }
    }

    /**
     * generates a pseudorandom number
     * @param seed a value ensure different outcomes for different sources in the same block
     * @return a pseudorandom value
     */
    function random(uint256 seed) internal view returns (uint256) {
        return
            uint256(
                keccak256(
                    abi.encodePacked(
                        tx.origin,
                        blockhash(block.number - 1),
                        block.timestamp,
                        seed
                    )
                )
            ) ^ randomSource.seed();
    }

    /***READ */
    function upgradeCost(uint256 tokenId) public view returns (uint256 cost) {
        TokenType memory t = tokenTypes[tokenId];
        if (t.kind == 2) {
            cost = t.level == 1 ? 900 : 2700;
        } else if (t.kind == 1) {
            cost = 100 * 3**(t.level - 1);
        } else {
            cost = 20 * 3**(t.level - 1);
        }
        if (t.level >= 5) {
            cost = 0;
        }
    }

    function getTokenTraits(uint256 tokenId)
        external
        view
        returns (TokenTrait memory, TokenType memory)
    {
        return (tokenTraits[tokenId], tokenTypes[tokenId]);
    }

    function getMintPossibility(uint256 tokenId, uint256 seed)
        public
        view
        returns (uint8 p)
    {
        uint256 k = tokenTypes[tokenId].kind;
        seed = seed & 0xFFFF;
        if (k == 1 && tokenTypes[tokenId].level < 4) {
            if (seed % 100 < 82) {
                p = 1;
            } else if (seed % 100 < 87) {
                p = 4;
            } else if (seed % 100 < 90) {
                p = 2;
            } else {
                p = 3;
            }
        } else if (k == 2) {
            if (seed % 1000 < 750) {
                p = 1;
            } else if (seed % 1000 < 825) {
                p = 4;
            } else if (seed % 1000 < 870) {
                p = 2;
            } else {
                p = 3;
            }
        } else {
            if (seed % 100 < 65) {
                p = 1;
            } else if (seed % 100 < 75) {
                p = 4;
            } else if (seed % 100 < 82) {
                p = 2;
            } else {
                p = 3;
            }
        }
    }

    //cost1 cash cost2 drug
    function mintPublicCost(uint256 evaId)
        public
        view
        returns (uint256 cost1, uint256 cost2)
    {
        uint256 count = tokenTypes[evaId].mintCount;
        if (count == 1) {
            cost2 = 20;
            cost1 = 40;
        } else {
            cost2 = 40 * (count - 1);
            cost1 = 80 * (count - 1);
        }
    }

    /***ADMIN */
    function setRandomSource(address _randomSource) external onlyOwner {
        randomSource = IRandomSource(_randomSource);
    }

    function withdraw(IERC20 _token) external onlyOwner {
        IERC20(_token).transfer(
            msg.sender,
            IERC20(_token).balanceOf(address(this))
        );
    }

    function setTraits(ITraits addr) external onlyOwner {
        traits = addr;
    }

    function setPaused(bool _paused) external onlyOwner {
        if (_paused) _pause();
        else _unpause();
    }

    function setRichcityNFT(address _addr) external onlyOwner {
        RichcityNFT = IRichcityNFT(_addr);
    }

    function whiteListBuyers(
        address[] memory _buyersOne,
        address[] memory _buyersTwo
    ) external onlyOwner {
        for (uint256 i; i < _buyersOne.length; i++) {
            whiteListedOne[_buyersOne[i]] = true;
        }
        for (uint256 i; i < _buyersTwo.length; i++) {
            whiteListedTwo[_buyersTwo[i]] = true;
        }
    }

    /***RENDER */

    function tokenURI(uint256 tokenId)
        public
        view
        override
        returns (string memory)
    {
        require(_exists(tokenId), "ERC721Metadata: nonexistent token");
        return traits.tokenURI(tokenId);
    }

    function onERC721Received(
        address,
        address from,
        uint256,
        bytes calldata
    ) external pure override returns (bytes4) {
        require(from == address(0x0), "Cannot send tokens to Barn directly");
        return IERC721Receiver.onERC721Received.selector;
    }
}