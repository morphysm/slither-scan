contract DynamicCUBE is ERC721, ERC721Enumerable, ERC2981, Pausable, Ownable  {
    using Strings for uint256;
    using Counters for Counters.Counter;

    Counters.Counter private _tokenIdCounter;

    uint256 public constant cost = 0 ether;
    uint256 private constant maxMintsPerTxn = 5;
    uint256 public maxSupply = 1000;
    uint96 public constant royaltyPercentage = 1000; //10% royalty 
    string public baseURI;
    bool public publicSaleStarted = true;
    bool public pauseMinting = false;
    mapping(uint256 => string) public tokenURIs;
    mapping(address => bool) public whitelistedMap;
    mapping(address => uint256) public addressMintedBalance;
    address game;
    constructor(
        string memory name_,
        string memory symbol_,
        string memory baseURI_

    ) ERC721(name_, symbol_) {
        baseURI = baseURI_;
        _tokenIdCounter.increment();
        _setDefaultRoyalty(owner(),royaltyPercentage);
    }

    //Owner can mint unlimited amount, no fee
    function adminMint(uint256 _mintAmount) public onlyOwner {
        for (uint256 i = 1; i <= _mintAmount; i++) {
            require(_tokenIdCounter.current() <= maxSupply, "Transaction exceeds max mint amount");
            _safeMint(_msgSender(), _tokenIdCounter.current());
            _tokenIdCounter.increment();
        }
    }
    function walletOfOwner(address _owner) public view returns (uint256[] memory) {
        uint256 ownerTokenCount = balanceOf(_owner);
        uint256[] memory tokenIds = new uint256[](ownerTokenCount);
        for (uint256 i; i < ownerTokenCount; i++) {
            tokenIds[i] = tokenOfOwnerByIndex(_owner, i);
        }
        return tokenIds;
    }
    function publicMint(uint256 _mintAmount) public {
        require(addressMintedBalance[msg.sender] + _mintAmount <= 5,"Max 5 mint");
        require(pauseMinting == false, "Cannot mint, minting paused");
        require(publicSaleStarted == true, "Public Sale not started yet");
        require(_mintAmount <= maxMintsPerTxn, "Exceeds max amount per mint");
        for (uint256 i = 1; i <= _mintAmount; i++) {
            require(_tokenIdCounter.current() <= maxSupply, "Transaction exceeds max mint amount");
            _mint(_msgSender(), _tokenIdCounter.current());
            //_setTokenRoyalty(_tokenIdCounter.current(), owner(), royaltyPercentage);
            tokenURIs[_tokenIdCounter.current()] = string(abi.encodePacked(_baseURI(),"11111",".json"));
            _tokenIdCounter.increment();
            addressMintedBalance[msg.sender]++;
            
        }
    }
    function setGameContract(address _game) public onlyOwner {
        game = _game;
    }
    function levelUpCube(uint tokenId, uint a, uint b,uint c,uint d,uint e) public {
        require(msg.sender == game);
        string memory newCubeUri = string(bytes.concat(bytes(_baseURI()),bytes(Strings.toString(a)),bytes(Strings.toString(b)),bytes(Strings.toString(c)),bytes(Strings.toString(d)),bytes(Strings.toString(e)),".json"));
        tokenURIs[tokenId] = newCubeUri;
    }

    function withdraw() public payable onlyOwner {
        require(address(this).balance > 0, "No contract balance");

        (bool os, ) = payable(owner()).call{value: address(this).balance}("");
        require(os);
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    function setBaseURI(string memory baseURI_) public onlyOwner {
        baseURI = baseURI_;
    }

    function setTotalSupply (uint256 totalSupply_) public onlyOwner {
        maxSupply = totalSupply_;
    }

    /*
    *   Start public sale
    */
    function startPublicSale() public onlyOwner {
        require(publicSaleStarted == false, "Public sale already set true");
        publicSaleStarted = true;
    }

    /*
    *   stop public sale
    */
    function stopPublicSale() public onlyOwner {
        require(publicSaleStarted == true, "Public sale already set false");
        publicSaleStarted = false;
    }

    /*
    *   Pause Minting
    */
    function PauseMinting() public onlyOwner {
        require(pauseMinting == false, "Pause minting already set true");
        pauseMinting = true;
    }

    /*
    *   unPause Minting
    */
    function unPauseMinting() public onlyOwner {
        require(pauseMinting == true, "Pause minting already set false");
        pauseMinting = false;
    }

    /*
    *   Pause the transfer of tokens
    */
    function pause() public onlyOwner whenNotPaused  {
        _pause();
    }

    /*
    *   UnPause the transfer of tokens
    */
    function unpause() public onlyOwner whenPaused {
        _unpause();
    }

    // The following functions are overrides required by Solidity.
    function _beforeTokenTransfer(address from, address to, uint256 tokenId)
        internal
        whenNotPaused
        override(ERC721, ERC721Enumerable)
    {
        super._beforeTokenTransfer(from, to, tokenId);
    }

    function tokenURI(uint256 tokenId_)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
            _exists(tokenId_),
            "ERC721Metadata: URI query for nonexistent token"
        );
        return tokenURIs[tokenId_];
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721, ERC721Enumerable, ERC2981)
        returns (bool)
    {
        return super.supportsInterface(interfaceId);
    }
}
