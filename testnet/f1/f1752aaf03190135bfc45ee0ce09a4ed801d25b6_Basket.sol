import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import "./interfaces/IBasket.sol";
import "./interfaces/ISeaport.sol";
import {BasicOrderParameters, AdditionalRecipient} from "./interfaces/ConsiderationStructs.sol";
import {BasicOrderType} from "./interfaces/ConsiderationEnums.sol";
import "./token/BasketToken.sol";
import "./BasketMandate.sol";

// import "./BasketManager.sol";

contract Basket is IBasket, BasketMandate {
    uint256 public immutable startPrice;
    uint256 public initialAssetValue;
    address public immutable paymentToken;

    BasketToken public immutable basketToken;

    constructor(string memory name,
        string memory symbol,
        uint256 _startPrice, address _paymentToken) 
        
        BasketMandate(msg.sender, msg.sender) {
        startPrice = _startPrice;
        paymentToken = _paymentToken;

        basketToken = new BasketToken(name, symbol, address(this));
    }

    function initializeBasket(uint256 _initialAssetValue) external  {
        initialAssetValue = _initialAssetValue;

        //tokens only minted on initialization
        BasketToken(basketToken).mint(
            (initialAssetValue * 1 ether / startPrice), // may need to mul by 1e18 depending on decimals
            address(this)
        );
    }

    function transferNFT(
        address token,
        uint256 tokenId,
        address receiver
    ) external onlyRole(EMERGENCY_ADMIN_ROLE) {
        IERC721(token).transferFrom(address(this), receiver, tokenId);
    }

    function transferNFTs(
        address token,
        uint256[] memory tokenIds,
        address receiver
    ) external onlyRole(EMERGENCY_ADMIN_ROLE) {
        for (uint8 i = 0; i < tokenIds.length; i++) {
            IERC721(token).transferFrom(address(this), receiver, tokenIds[i]);
        }
    }
    // User entry point to the bagssssss
    function buy(uint256 amount, address receiver) external {
        IERC20(paymentToken).transferFrom(msg.sender, address(this), amount);
        basketToken.buyTransfer(address(this), receiver, amount  * 1 ether / startPrice);
    }

    //////////// Fund Manager Functions ////////////

    // struct BasicOrderParameters {
    // // calldata offset
    // address considerationToken; // 0x24
    // uint256 considerationIdentifier; // 0x44
    // uint256 considerationAmount; // 0x64
    // address payable offerer; // 0x84
    // address zone; // 0xa4
    // address offerToken; // 0xc4
    // uint256 offerIdentifier; // 0xe4
    // uint256 offerAmount; // 0x104
    // BasicOrderType basicOrderType; // 0x124
    // uint256 startTime; // 0x144
    // uint256 endTime; // 0x164
    // bytes32 zoneHash; // 0x184
    // uint256 salt; // 0x1a4
    // bytes32 offererConduitKey; // 0x1c4
    // bytes32 fulfillerConduitKey; // 0x1e4
    // uint256 totalOriginalAdditionalRecipients; // 0x204
    // AdditionalRecipient[] additionalRecipients; // 0x224
    // bytes signature; // 0x244
    // // Total length, excluding dynamic array data: 0x264 (580)
    //}

    //     struct AdditionalRecipient {
    //     uint256 amount;
    //     address payable recipient;
    // }

    function buyNFT(
        address token,
        uint256 tokenId,
        uint256 amount,
        AdditionalRecipient[] memory additionalRecipients
    ) external onlyRole(FUND_MANAGER_ROLE) isAConstituent(token) {
        SeaportInterface(address(0)).fulfillBasicOrder(
            BasicOrderParameters({
                considerationToken: token,
                considerationIdentifier: tokenId,
                considerationAmount: amount,
                offerer: payable(msg.sender),
                zone: address(0),
                offerToken: address(paymentToken),
                offerIdentifier: 0,
                offerAmount: amount,
                basicOrderType: BasicOrderType.ERC20_TO_ERC721_FULL_OPEN,
                startTime: 0,
                endTime: 0,
                zoneHash: "",
                salt: 0,
                offererConduitKey: "",
                fulfillerConduitKey: "",
                totalOriginalAdditionalRecipients: 0,
                additionalRecipients: // AdditionalRecipient({
                //     amount: 0,
                //     recipient: payable(address(0))
                // })
                additionalRecipients,
                signature: ""
            })
        );
    }

    function onERC721Received(
        address, // operator,
        address, //from,
        uint256, //tokenId,
        bytes calldata // data
    ) external pure returns (bytes4) {
        return bytes4(keccak256("onERC721Received(address,address,uint256,bytes)"));
    }
}

