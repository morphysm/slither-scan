pragma solidity 0.8.15;

import "@openzeppelin/contracts/access/AccessControlEnumerable.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";

contract BasketMandate is AccessControlEnumerable {
    /* 
    constituents
    rebalance period = quarterly?
    min holding of constituents
    functions to rebalance
    */

    // constituentId
    uint256 public totalConstituent;

    struct ConstituentComponents {
        uint32 minAllocation;
        uint32 initialAllocation;
        uint32 currentAllocation;
        uint256 initialTotalValue;
    }

    address[] constituentAddresses;

    mapping(address => ConstituentComponents) public constituents;

    bytes32 EMERGENCY_ADMIN_ROLE = keccak256("EMERGENCY_ADMIN_ROLE");
    bytes32 FUND_MANAGER_ROLE = keccak256("FUND_MANAGER_ROLE");

    constructor(address fundManager, address emergencyAdmin) {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(EMERGENCY_ADMIN_ROLE, emergencyAdmin);
        _grantRole(FUND_MANAGER_ROLE, fundManager); // eg set this as compound governance module
    }

    error InitialMandateNotMet(
        address basket,
        address token,
        uint32 minAllocation
    );
    modifier initialMandateIsMet() {
        require(constituentAddresses.length > 0, "no constituents added");
        for (uint32 i = 0; i < constituentAddresses.length; i++) {
            address token = constituentAddresses[i];
            uint32 minAlloc = constituents[token].minAllocation;
            uint256 currentAllocation = IERC721(token).balanceOf(address(this));
            if (currentAllocation < minAlloc) {
                revert InitialMandateNotMet(address(this), token, minAlloc);
            }
            constituents[token].currentAllocation = uint32(currentAllocation);
        }
        _;
    }

    error NotAConstituent(address token);

    modifier isAConstituent(address token) {
        if (!(constituents[token].initialAllocation == 0)) {
            revert NotAConstituent({token: token});
        }
        _;
    }

    function addConstituent(
        address token,
        uint32 minAllocation,
        uint32 initialAllocation,
        uint256 initialValueOfConstituentAssets
    ) external onlyRole(FUND_MANAGER_ROLE) {
        constituents[token] = ConstituentComponents({
            minAllocation: minAllocation,
            initialAllocation: initialAllocation,
            currentAllocation: 0,
            initialTotalValue: initialValueOfConstituentAssets
        });

        constituentAddresses.push(token);
    }
}

