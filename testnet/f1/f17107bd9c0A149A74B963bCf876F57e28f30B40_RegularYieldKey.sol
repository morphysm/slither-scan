pragma solidity ^0.8.10;

import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721BurnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/extensions/IERC20MetadataUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "./lib/Random.sol";

/**
 * @title YieldKey Contract / NFT Smart Contract ERC721
 * @dev Implementation of the YieldKey NFT ERC721.
 * @custom:a ValiFi
 */
contract RegularYieldKey is
    ERC721EnumerableUpgradeable,
    ERC721BurnableUpgradeable,
    PausableUpgradeable,
    ReentrancyGuardUpgradeable,
    Random
{
    using StringsUpgradeable for uint256;
    using SafeMathUpgradeable for uint256;
    using AddressUpgradeable for address;

    string public baseURI;
    string public baseExtension;
    uint256 public constant TOTAL_SUPPLY = ~(uint256(0)) >> 1;
    /** @dev Enable or Disable Whitelist */
    bool private isActiveWhitelist;

    address private addressD;
    address private addressP;
    /** @dev ValiFi Treasury Address */
    address private treasuryAddress;
    /** @dev Max Amount Yield Key per Wallet*/
    uint256 public maxAmountYieldKey;

    uint256 private DValue;
    uint256 private PValue;

    uint32 public publicSaleStartTime; // Sunday, 26 Jun 2022 00:00:00 GMT UTC+0

    /** @dev Mint Pause */
    bool public mintPause;

    // the administrator mappings
    mapping(address => bool) _administrators;

    /**@dev Mapping of ERC20 tokens that are accepted */
    mapping(address => uint256) public amountCoin;

    /** @dev Amount of YieldKeys that can be mintable in the current phase */
    uint256 public batchSize;

    /** @dev Mapping of YieldKeys minted by an address */
    mapping(address => uint256) public totalMinted;

    /** @dev Total YieldKeys Minted */
    uint256 public totalMintCnt;

    /** @dev Mapping storing whether addresses are whitelisted or not */
    mapping(address => bool) public whitelist;

    /** @dev Mapping for storing Activation Capacity per token */
    mapping(uint256 => uint256) public capacityAmount;

    /**
     * @dev Event when setting the BASE URI
     * @param baseURI new value of the Base URI
     */
    event SetBaseURI(string baseURI);

    /**
     * @dev Event when setting a new Batch Size
     * @param batchSize new value of the Batch Size
     */
    event SetBatchSize(uint256 batchSize);

    /**
     * @dev Event when setting the Cost per Token
     * @param tokenAddress address or ERC20 Token
     * @param costPerToken new value of the Cost per ERC20 Token
     */
    event SetCostPerToken(address tokenAddress, uint256 costPerToken);

    /**
     * @dev Event when setting the Treasury Address (ValiFi ERC20)
     * @param treasuryAddress new value of Treasury Address (ValiFi ERC20)
     */
    event SetTreasuryAddress(address treasuryAddress);

    function initialize(
        string memory _name,
        string memory _symbol,
        string memory _initBaseURI,
        address _treasuryAddress
    ) public initializer {
        __ERC721_init_unchained(_name, _symbol);
        __Ownable_init();
        __Pausable_init();
        __ReentrancyGuard_init();
        setBaseURI(_initBaseURI);
        batchSize = uint256(500);
        maxAmountYieldKey = 10;
        isActiveWhitelist = true;
        DValue = 15;
        PValue = 5;
        publicSaleStartTime = 1656201600; // Sunday, 11 July 2022 00:00:00 GMT UTC+0
        treasuryAddress = _treasuryAddress;
        baseExtension = ".json";
    }

    /**
     * @dev Modifier to permit only the address added in the _administrators mapping to call the function
     */
    modifier onlyAdmin() {
        require(
            _administrators[_msgSender()] || owner() == _msgSender(),
            "Caller is not an admin!"
        );
        _;
    }

    /**
     * @dev Override Standard Method to set Base URI of the NFT Token
     * @return String Base URI of the NFT Token
     */
    function _baseURI() internal view override returns (string memory) {
        return baseURI;
    }

    /**
     * @dev Contract owner can set timestamp of the Whitelist/Public Sale Start
     * @param timestamp Timestamp of the PublicSale start time
     */
    function setPublicSaleStartTime(uint32 timestamp) public onlyOwner {
        publicSaleStartTime = timestamp;
    }

    /**
     * @dev Validate if the current time is before or after the PublicSale start time
     * @return bool True if the current time is after the PublicSale start time
     */
    function publicSaleIsActive() public view returns (bool) {
        return publicSaleStartTime <= block.timestamp;
    }

    /**
     * @dev Add address to Whilelist while Whitelist is Active.
     * @param _address Address to be whitelisted
     */
    function addToWhitelist(address _address)
        external
        validAddress(_address)
        onlyAdmin
    {
        require(isActiveWhitelist, "Whitelist is not active");

        whitelist[_address] = true;
    }

    /**
     * @dev Verify if the address is in the Whitelist
     * @param _address Address to verify
     */
    function isWhitelisted(address _address)
        public
        view
        validAddress(_address)
        returns (bool)
    {
        if (isActiveWhitelist) {
            return whitelist[_address];
        } else {
            return true;
        }
    }

    /**
     * @dev Activate or Deactivate the Whitelist
     * @param _isActive Boolean to activate or deactivate the whitelist
     */
    function setWhitelistActive(bool _isActive) external onlyOwner {
        isActiveWhitelist = _isActive;
    }

    /**
     * @dev Method to Mint YieldKey
     * @param _currency Address of ERC20 Token used to pay the cost of the each YieldKey
     * @param _to Address that will own the tokens
     * @param _mintAmount Amount of YieldKeys to mint
     * @return _capacityAmount Returns an array of the capacity amounts minted
     */
    function mint(
        address _currency,
        address _to,
        uint256 _mintAmount
    )
        external
        validAddress(_to)
        whenNotPaused
        nonReentrant
        returns (uint256[] memory)
    {
        require(!mintPause, "Minting is paused");
        require(publicSaleIsActive(), "Public sale is not active");
        require(isWhitelisted(_to), "Wallet is not whitelisted");
        require(
            totalMinted[_to] + _mintAmount <= maxAmountYieldKey,
            "Wallet has reached the maximum amount of YieldKeys"
        );
        require(isValidToken(_currency), "ERC721 ValiFi: Token not valid");
        require(
            batchSize > uint256(0),
            "ERC721 ValiFi: Can't create any YieldKeys until a new batch is available"
        );

        uint256 supply = totalMintCnt;
        require(
            ((_mintAmount > 0) && (_mintAmount <= batchSize)),
            "ERC721 ValiFi: Invalid amount"
        );
        require(
            supply + _mintAmount <= TOTAL_SUPPLY,
            "ERC721 ValiFi: Mint exceeds supply"
        );
        uint256 totalPrice = amountCoin[_currency].mul(_mintAmount) *
            (10**IERC20MetadataUpgradeable(_currency).decimals());
        require(
            IERC20Upgradeable(_currency).balanceOf(_msgSender()) >= totalPrice,
            "ERC721 ValiFi: Insufficient Funds"
        );
        // Transfer the tokens to mint the YieldKey
        bool success = IERC20Upgradeable(_currency).transferFrom(
            _msgSender(),
            addressD,
            totalPrice.mul(DValue).div(100)
        );
        require(
            success,
            "ERC721 ValiFi: Can't create YieldKey, funds transfer failed (1)"
        );
        success = IERC20Upgradeable(_currency).transferFrom(
            _msgSender(),
            addressP,
            totalPrice.mul(PValue).div(100)
        );
        require(
            success,
            "ERC721 ValiFi: Can't create YieldKey, funds transfer failed (2)"
        );
        success = IERC20Upgradeable(_currency).transferFrom(
            _msgSender(),
            treasuryAddress,
            totalPrice.mul(100 - (DValue + PValue)).div(100)
        );
        require(
            success,
            "ERC721 ValiFi: Can't create YieldKey, funds transfer failed (3)"
        );

        uint256[] memory _capacityAmount = new uint256[](_mintAmount);
        for (uint256 i = 0; i < _mintAmount; i++) {
            _safeMint(_to, supply + i + 1); // tokenId starts at 1
            capacityAmount[supply + i + 1] = getCapacityAmount();
            _capacityAmount[i] = capacityAmount[supply + i + 1];
            batchSize = batchSize.sub(uint256(1));
        }
        totalMinted[_to] += _mintAmount;
        totalMintCnt += _mintAmount;
        return _capacityAmount;
    }

    /**
     * @dev Verify if the address is owner of one or several YieldKeys
     * @param _wallet Wallet address of the owner
     * @return tokenIds Array of token Ids of YieldKeys owned by the wallet
     */
    function tokenHoldings(address _wallet)
        public
        view
        returns (uint256[] memory)
    {
        uint256 ownerTokenCount = balanceOf(_wallet);
        uint256[] memory tokenIds = new uint256[](ownerTokenCount);
        for (uint256 i = 0; i < ownerTokenCount; i++) {
            tokenIds[i] = tokenOfOwnerByIndex(_wallet, i);
        }
        return tokenIds;
    }

    function tokenURI(uint256 _tokenId)
        public
        view
        override
        returns (string memory)
    {
        require(
            _exists(_tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );

        string memory currentBaseURI = _baseURI();
        return
            bytes(currentBaseURI).length > 0
                ? string(
                    abi.encodePacked(
                        currentBaseURI,
                        _tokenId.toString(),
                        baseExtension
                    )
                )
                : "";
    }

    /**
     * @dev Function to Set Base URI of the NFT Token
     * @param _newBaseURI New URL of Metadata of the NFT Token
     */
    function setBaseURI(string memory _newBaseURI) public onlyOwner {
        baseURI = _newBaseURI;
        emit SetBaseURI(baseURI);
    }

    /**
     * @dev Function to Set Base Extension of the NFT Token Metadata
     * @param _newBaseExtension string of extension for the URI metadata files
     */
    function setBaseExtension(string memory _newBaseExtension)
        external
        onlyOwner
    {
        baseExtension = _newBaseExtension;
    }

    /**
     * @dev Implementation / Instance of paused methods() in the ERC721.
     * @param status Setting the status boolean (True for paused, or False for unpaused)
     * See {ERC721Pausable}.
     */
    function pause(bool status) external onlyAdmin {
        if (status) {
            _pause();
        } else {
            _unpause();
        }
    }

    /**
     * @dev Implementation / Instance of paused methods() for Pause only the Mint Method.
     * @param status Setting the status boolean (True for paused, or False for unpaused)
     * See {ERC721Pausable}.
     */
    function pauseMint(bool status) external onlyAdmin {
        if (status) {
            mintPause = true;
        } else {
            mintPause = false;
        }
    }

    /**
     * @dev Helpers Funtions.
     */

    /**
     * @dev This function allows getting the Treasury Address.
     * @return Treasury Address
     */
    function getTreasuryAddress() external view returns (address) {
        return treasuryAddress;
    }

    /**
     * @dev This function allows getting a random capacity amount to be used by the mint funtion
     * @return _amountKeys The capacity amount for the token Id
     */
    function getCapacityAmount() internal view returns (uint256 _amountKeys) {
        uint256 randomNum = randrange(1, 100);
        if (randomNum <= 70) {
            _amountKeys = 4;
        } else if ((randomNum > 70) && (randomNum <= 92)) {
            _amountKeys = 6;
        } else if ((randomNum > 92) && (randomNum <= 100)) {
            _amountKeys = 8;
        } else {
            revert("Error: Random Number is not in range");
        }
    }

    /**
     * @dev This function allows to get the Batch Size available for minting.
     * @return _amount Amount of YieldKeys mintable in batch
     */
    function getBatchSize() external view returns (uint256 _amount) {
        return batchSize;
    }

    /**
     * @dev This function allows getting the currency (cost) value to mint a YieldKey.
     * @param _token Token Address of currency used to create YieldKeys
     * @return Value currency (cost) value to mint a YieldKey
     */
    function getTokenValue(address _token)
        external
        view
        validAddress(_token)
        returns (uint256)
    {
        return amountCoin[_token];
    }

    /**
     * @dev This function is to verify if a Token Address is valid to create YieldKeys.
     * @param _token Token Address of currency used to create YieldKeys
     * @return Status true if value is more then 0, or false if value is 0
     */
    function isValidToken(address _token)
        public
        view
        validAddress(_token)
        returns (bool)
    {
        return amountCoin[_token] > 0;
    }

    /**
     * @dev This function allows to set the Treasury Wallet Address
     * @param _treasuryAddress Treasury Address
     */
    function setTreasuryAddress(address _treasuryAddress)
        external
        validAddress(_treasuryAddress)
        onlyOwner
    {
        treasuryAddress = _treasuryAddress;
        emit SetTreasuryAddress(_treasuryAddress);
    }

    /**
     * @dev This function allows to set the token address and currency value (YieldKey cost).
     * @param _token Address of the token in the Avalanche Network
     * @param _value Value in the currency of the chosen token
     */
    function setTokenValue(address _token, uint256 _value)
        external
        validAddress(_token)
        onlyOwner
    {
        require(_token.isContract(), "ERC721 ValiFi: Token is not a contract");
        require(
            _value >= 0,
            "ERC721 ValiFi: Can't set value, value must be greater than 0"
        );
        amountCoin[_token] = _value;
        emit SetCostPerToken(_token, _value);
    }

    /**
     * @dev This function allows to set the minting Batch Size.
     * @param _amount Amount of YieldKeys to Mint per Batches
     */
    function setBatchSize(uint256 _amount, uint256 _maxAmountPerWallet)
        external
        onlyOwner
    {
        batchSize = _amount;
        maxAmountYieldKey = _maxAmountPerWallet;
        emit SetBatchSize(_amount);
    }

    /**
     * @dev function to revoke Admin role
     * @param addr Address of the admin to revoke
     */
    function revokeAdmin(address addr) external onlyOwner {
        _administrators[addr] = false;
    }

    /**
     * @dev function to Add Admin role
     * @param addr Address of the admin to add
     */
    function addAdmin(address addr) external onlyOwner {
        _administrators[addr] = true;
    }

    /**
     * @dev function to Set Distribution Addresses
     */
    function setDistributionAddresses(
        address addrD,
        address addrP,
        uint256 valueD,
        uint256 valueP
    ) external onlyOwner {
        require(
            valueD + valueP <= 100,
            "ERC721 ValiFi: Value must be less than or equal to 100"
        );
        addressD = addrD;
        addressP = addrP;
        DValue = valueD;
        PValue = valueP;
    }

    // The following functions are overrides required by Solidity.

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 _tokenId
    )
        internal
        override(ERC721EnumerableUpgradeable, ERC721Upgradeable)
        notBlacklisted(from)
    {
        require(
            !paused(),
            "ERC721 ValiFi: can't create or transfer any YieldKey while paused"
        );
        require(
            !isBlacklisted(to),
            "ERC721 ValiFi: can't transfer to blacklisted address"
        );
        super._beforeTokenTransfer(from, to, _tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721EnumerableUpgradeable, ERC721Upgradeable)
        returns (bool)
    {
        return
            interfaceId == type(IERC721Upgradeable).interfaceId ||
            interfaceId == type(IERC721EnumerableUpgradeable).interfaceId ||
            interfaceId == type(IERC721MetadataUpgradeable).interfaceId ||
            super.supportsInterface(interfaceId);
    }
}

