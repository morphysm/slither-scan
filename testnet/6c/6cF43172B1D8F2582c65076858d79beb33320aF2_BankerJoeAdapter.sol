pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "../../common/Basic.sol";
import "../base/AdapterBase.sol";
import {IJToken} from "../../interfaces/traderJoe/IJoeLens/IJToken.sol";
import {IJoetroller} from "../../interfaces/traderJoe/IJoeLens/IJoetroller.sol";
import {IJWrappedNativeDelegator} from "../../interfaces/traderJoe/IJoeLens/IJWrappedNativeDelegator.sol";
import {IPriceOracle} from "../../interfaces/traderJoe/IJoeLens/IPriceOracle.sol";
import {IMaximillion} from "../../interfaces/traderJoe/IJoeLens/IMaximillion.sol";
import {IBankerJoeAdapter} from "./IBankerJoeAdapter.sol";
import {IWAVAX} from "../../interfaces/IWAVAX.sol";
import "hardhat/console.sol";

contract BankerJoeAdapter is AdapterBase, Basic, Ownable {
    using SafeERC20 for IERC20;

    address public constant JoetrollerAddr =
        0xdc13687554205E5b89Ac783db14bb5bba4A1eDaC;
    IJoetroller internal Joetroller = IJoetroller(JoetrollerAddr);

    address public constant joeBarAddr =
        0x57319d41F71E81F3c65F2a47CA4e001EbAFd4F33;

    address public constant joeMaximillionAddr =
        0xe5cDdAFd0f7Af3DEAf4bd213bBaee7A5927AB7E7;

    mapping(address => address) public trustJTokenAddr;

    constructor(address _adapterManager)
        AdapterBase(_adapterManager, "BankerJoe")
    {}

    struct JTokenSnapshot {
        uint256 balance;
        uint256 borrow;
        uint256 rate;
        uint256 price;
    }

    event TraderJoeSupplyEvent(
        address token,
        address joeToken,
        uint256 amount,
        address owner
    );

    event TraderJoeWithdrawEvent(
        address token,
        address joeToken,
        uint256 amount,
        address owner
    );

    event TraderJoeBorrowEvent(address joeToken, uint256 amount, address owner);
    event TraderJoeRepayEvent(address joeToken, uint256 amount, address owner);
    event TraderJoeStake(address owner, uint256 stakeAmount);
    event TraderJoeUnStake(address owner, uint256 stakeAmount);

    function initialize(
        address[] calldata tokenAddr,
        address[] calldata jTokenAddr
    ) external onlyOwner {
        require(
            tokenAddr.length > 0 && tokenAddr.length == jTokenAddr.length,
            "Set length mismatch."
        );
        for (uint256 i = 0; i < tokenAddr.length; i++) {
            if (tokenAddr[i] == avaxAddr) {
                require(
                    IJWrappedNativeDelegator(jTokenAddr[i]).underlying() ==
                        wavaxAddr,
                    "Address mismatch."
                );
            } else {
                require(
                    IJToken(jTokenAddr[i]).underlying() == tokenAddr[i],
                    "Address mismatch."
                );
            }
            trustJTokenAddr[tokenAddr[i]] = jTokenAddr[i];
        }
    }

    //todo delete jtokenAddr arg
    function deposit(bytes calldata encodedData) external onlyProxy {
        (address tokenAddr, address jtokenAddr_, uint256 tokenAmount) = abi
            .decode(encodedData, (address, address, uint256));
        address jtokenAddr = IBankerJoeAdapter(ADAPTER_ADDRESS).trustJTokenAddr(
            tokenAddr
        );
        require(jtokenAddr != address(0), "Token invalid.");
        if (tokenAddr == avaxAddr) {
            require(
                IJWrappedNativeDelegator(jtokenAddr).underlying() == wavaxAddr,
                "Not AVAX."
            );
            IJWrappedNativeDelegator(jtokenAddr).mintNative{
                value: tokenAmount
            }();
        } else {
            require(
                tokenAddr == IJToken(jtokenAddr).underlying(),
                "Token invalid."
            );
            IJToken(jtokenAddr).mint(tokenAmount);
        }

        emit TraderJoeSupplyEvent(
            tokenAddr,
            jtokenAddr,
            tokenAmount,
            address(this)
        );
    }

    //todo delete jtokenAddr arg
    function withdraw(bytes calldata encodedData) external onlyAdapterManager {
        (
            address tokenAddr,
            address jtokenAddr_,
            uint256 jtokenAmount,
            address owner
        ) = abi.decode(encodedData, (address, address, uint256, address));
        address jtokenAddr = IBankerJoeAdapter(ADAPTER_ADDRESS).trustJTokenAddr(
            tokenAddr
        );
        require(jtokenAddr != address(0), "Token invalid.");

        uint256 amountDiff;
        if (tokenAddr == avaxAddr || tokenAddr == wavaxAddr) {
            require(
                IJWrappedNativeDelegator(jtokenAddr).underlying() == wavaxAddr,
                "Not AVAX."
            );
            pullAndApprove(jtokenAddr, owner, jtokenAddr, jtokenAmount);
            uint256 amountBefore = address(this).balance;
            IJWrappedNativeDelegator(jtokenAddr).redeemNative(jtokenAmount);
            amountDiff = address(this).balance - amountBefore;
            require(amountDiff >= 0, "amount error");
            if (tokenAddr == avaxAddr) {
                safeTransferAVAX(owner, amountDiff);
            } else {
                IWAVAX(wavaxAddr).deposit{value: amountDiff}();
                IWAVAX(wavaxAddr).transfer(owner, amountDiff);
            }
        } else {
            require(
                IJToken(jtokenAddr).underlying() == tokenAddr,
                "Not token."
            );
            pullAndApprove(jtokenAddr, owner, jtokenAddr, jtokenAmount);
            IERC20 token = IERC20(tokenAddr);
            uint256 amountBefore = token.balanceOf(address(this));

            IJToken(jtokenAddr).redeem(jtokenAmount);
            amountDiff = token.balanceOf(address(this)) - amountBefore;
            require(amountDiff > 0, "amount error");
            token.safeTransfer(owner, amountDiff);
        }
        emit TraderJoeWithdrawEvent(tokenAddr, jtokenAddr, amountDiff, owner);
    }

    function enterMarkets(bytes calldata encodedData) external onlyProxy {
        address[] memory jTokenAddr = abi.decode(encodedData, (address[]));
        uint256[] memory result = IJoetroller(JoetrollerAddr).enterMarkets(
            jTokenAddr
        );
    }

    function exitMarket(bytes calldata encodedData) external onlyProxy {
        address jTokenAddr = abi.decode(encodedData, (address));
        uint256 result = IJoetroller(JoetrollerAddr).exitMarket(jTokenAddr);
    }

    function borrow(bytes calldata encodedData) external onlyProxy {
        (address tokenAddr, address jtokenAddr_, uint256 amount) = abi.decode(
            encodedData,
            (address, address, uint256)
        );
        address jtokenAddr = IBankerJoeAdapter(ADAPTER_ADDRESS).trustJTokenAddr(
            tokenAddr
        );
        require(jtokenAddr != address(0), "Token invalid.");

        if (tokenAddr == avaxAddr) {
            IJWrappedNativeDelegator(jtokenAddr).borrowNative(amount);
        } else {
            uint256 result = IJToken(jtokenAddr).borrow(amount);
        }

        emit TraderJoeBorrowEvent(jtokenAddr, amount, address(this));
    }

    function repay(bytes calldata encodedData) external onlyProxy {
        (address tokenAddr, address jtokenAddr_, uint256 amount) = abi.decode(
            encodedData,
            (address, address, uint256)
        );
        address jtokenAddr = IBankerJoeAdapter(ADAPTER_ADDRESS).trustJTokenAddr(
            tokenAddr
        );
        require(jtokenAddr != address(0), "Token invalid.");
        if (tokenAddr == avaxAddr) {
            if (amount == type(uint256).max) {
                uint256 repayValue = IJWrappedNativeDelegator(jtokenAddr)
                    .borrowBalanceStored(address(this));
                IMaximillion(joeMaximillionAddr).repayBehalf{value: repayValue}(
                    address(this)
                );
            } else {
                IJWrappedNativeDelegator(jtokenAddr).repayBorrowNative{
                    value: amount
                }();
            }
        } else {
            uint256 result = IJToken(jtokenAddr).repayBorrow(amount);
        }

        emit TraderJoeRepayEvent(jtokenAddr, amount, address(this));
    }

    function claimReward(bytes calldata encodedData) external onlyProxy {
        (uint8 rewardType, address holder) = abi.decode(
            encodedData,
            (uint8, address)
        );
        IJoetroller(JoetrollerAddr).claimReward(rewardType, holder);
    }

    /// @dev Get account info for TraderJoe.
    function GetUserAccountData(address user)
        public
        view
        returns (
            uint256, // Err, if error occurred, this will be non zero
            uint256, // Total collateral amount, the amount of currency in the loan market
            uint256 // Total borrow amount, how much you borrow in usd
        )
    {
        IJoetroller joe = IJoetroller(JoetrollerAddr);
        IPriceOracle oracle = IPriceOracle(joe.oracle());
        uint256 sumBorrow;
        uint256 sumCollateral;
        address[] memory assets = joe.getAssetsIn(user);
        for (uint256 i = 0; i < assets.length; i++) {
            uint256 err;
            JTokenSnapshot memory s;
            IJToken jtoken = IJToken(assets[i]);
            (err, s.balance, s.borrow, s.rate) = jtoken.getAccountSnapshot(
                user
            );

            // If error occurred, return error.
            if (err != 0) {
                return (err, 0, 0);
            }

            if (s.balance == 0 && s.borrow == 0) {
                continue;
            }

            s.price = oracle.getUnderlyingPrice(address(jtoken));

            if (s.price == 0) {
                return (0, 0, 0);
            }

            sumBorrow += s.borrow * s.price;
            sumCollateral += s.balance * s.rate * s.price;
        }

        // Set precision to 10^18
        return (0, sumCollateral / (10**36), sumBorrow / (10**18));
    }

    /// @notice This function calculates how much the user can borrow without liquidation.
    /// @param user User to query.
    /// @return The calaculated value.
    function GetUserLiquidateValue(address user) public view returns (uint256) {
        IJoetroller joe = IJoetroller(JoetrollerAddr);
        IPriceOracle oracle = IPriceOracle(joe.oracle());
        uint256 sumCollateral;
        address[] memory assets = joe.getAssetsIn(user);
        for (uint256 i = 0; i < assets.length; i++) {
            uint256 err;
            JTokenSnapshot memory s;
            IJToken jtoken = IJToken(assets[i]);
            (err, s.balance, s.borrow, s.rate) = jtoken.getAccountSnapshot(
                user
            );

            // If error occurred, return error.
            if (err != 0) {
                return 0;
            }

            if (s.balance == 0) {
                continue;
            }

            s.price = oracle.getUnderlyingPrice(address(jtoken));

            if (s.price == 0) {
                return 0;
            }

            uint256 factor;

            (, factor, ) = joe.markets(assets[i]);

            sumCollateral +=
                (((s.balance * s.rate * s.price) / 1e18) * factor) /
                1e36;
        }
        return sumCollateral;
    }

    //bool[] isAsCollateral
    //bool[] isBorrowing
    //address[] reservesList
    function GetUserMarketInfo(address account)
        external
        view
        returns (
            bool[] memory,
            bool[] memory,
            address[] memory
        )
    {
        IJoetroller joe = IJoetroller(JoetrollerAddr);
        address[] memory jTokenAddr = joe.getAllMarkets();
        address[] memory reservesList = new address[](jTokenAddr.length);
        bool[] memory isAsCollateral = new bool[](jTokenAddr.length);
        bool[] memory isBorrowing = new bool[](jTokenAddr.length);

        for (uint256 i = 0; i < jTokenAddr.length; i++) {
            isAsCollateral[i] = joe.checkMembership(account, jTokenAddr[i]);
            IJToken jToken = IJToken(jTokenAddr[i]);
            reservesList[i] = jToken.underlying();
            isBorrowing[i] = jToken.borrowBalanceStored(account) > 0
                ? true
                : false;
        }
        return (isAsCollateral, isBorrowing, reservesList);
    }

    function GetUserDepositPosition(address user)
        external
        view
        returns (uint256)
    {
        IJoetroller bankerJoe = IJoetroller(JoetrollerAddr);
        address[] memory reservesList = bankerJoe.getAllMarkets();
        uint256 assetValue;
        IPriceOracle oracle = IPriceOracle(bankerJoe.oracle());
        for (uint256 i = 0; i < reservesList.length; i++) {
            uint256 tokenBalance = IERC20(reservesList[i]).balanceOf(user);
            if (tokenBalance > 0) {
                uint256 price = oracle.getUnderlyingPrice(reservesList[i]);
                uint256 decimals = IJToken(reservesList[i]).decimals();
                uint256 exchangeRateStored = IJToken(reservesList[i])
                    .exchangeRateStored();
                assetValue += ((tokenBalance * price * exchangeRateStored) /
                    ((10**decimals) * 1e36));
            }
        }
        return (assetValue);
    }
}

