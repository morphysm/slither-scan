pragma solidity ^0.8.10;

import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Enumerable.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721Burnable.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/IERC20Metadata.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "./lib/Random.sol";

/**
 * @title Yield-Key Contract / NFT Smart Contract ERC721
 * @dev Implementation of the Yield-Key NFT ERC721.
 * @custom:a ValiFi
 */
contract YieldKey is ERC721Enumerable, ERC721Burnable, Pausable, Random {
    using Strings for uint256;
    using SafeMath for uint256;
    using Address for address;
    using SafeERC20 for IERC20;

    string public baseURI;
    string public baseExtension = ".json";
    uint256 public maxSupply = ~(uint256(0)) >> 1;
    bool private isActiveWhitelist;

    uint32 public publicSaleStartTime = 1656201600; // Sunday, 26 Jun 2022 00:00:00 GMT UTC+0

    /** @dev ValiFi Treasury Address */
    address private treasuryAddress;

    /** @dev Mint Pause */
    bool public mintPause;

    // Mapping of Staked Yield-Keys
    struct StakeYieldKey {
        bool isStaked;
        uint64[] startstake;
        uint64[] endstake;
    }

		// the administrator mappings
    mapping(address => bool) _administrators;

    /**@dev Mapping of ERC20 tokens that are accepted */
    mapping(address => uint256) public amountCoin;

    /** @dev Amount of Yield-Keys that can be mintable in the current phase */
    uint256 private _batchSize;

    /** @dev Mapping of Yield-Keys minted by an address */
    mapping(address => uint256) public totalMinted;
    /** @dev Total Yield-Keys Minted */
    uint256 public totalMintCnt = 0;
    /** @dev Mapping storing whether addresses are whitelisted or not */
    mapping(address => bool) public whitelist;
    /** @dev Mapping for storage of Activation Capacity per token */
    mapping(uint256 => uint256) public capacityAmount;
    /**
     * @dev Mapping for storage Yield-Keys Staked
     * Address owner => TokenId => StakeYieldKey struct
     */
    mapping(address => mapping(uint256 => StakeYieldKey))
        public stakedYieldKeys;

    /**
     * @dev Event when change the BASE URL
     * @param baseURI new value of the Base URI
     */
    event SetBaseURI(string baseURI);
    /**
     * @dev Event when setting a new Batch Size
     * @param batchSize new value of the Batch Size
     */
    event SetBatchSize(uint256 batchSize);
    /**
     * @dev Event when setting the Cost per Token
     * @param tokenAddress address or ERC20 Token
     * @param costPerToken new value of the Cost per ERC20 Token
     */
    event SetCostPerToken(address tokenAddress, uint256 costPerToken);

    /**
     * @dev Event when setting the Treasury Address (ValiFi ERC20)
     * @param treasuryAddress new value of Treasury Address (ValiFi ERC20)
     */
    event SetTreasuryAddress(address treasuryAddress);

    /**
     * @dev Event when Staking a Yield-Key
     * @param user Address of the NFT Yield-Key user
     * @param tokenIds Token ID of the Yield-Key
     * @param capacityAmount Capacity Amount per NFT Yield-Key
     */
    event StakedYieldKey(
        address indexed user,
        uint256[] tokenIds,
        uint256[] capacityAmount
    );

    /**
     * @dev Event when Unstaked a Yield-Key
     * @param user Address of the NFT Yield-Key
     * @param tokenIds Token ID of the Yield-Key
     * @param capacityAmount Capacity Amount per NFT Yield-Key
     */
    event UnStakedYieldKey(
        address indexed user,
        uint256[] tokenIds,
        uint256[] capacityAmount
    );

    constructor(
        string memory _name,
        string memory _symbol,
        string memory _initBaseURI,
        address _treasuryAddress
    ) ERC721(_name, _symbol) {
        setBaseURI(_initBaseURI);
        _batchSize = uint256(500);
        isActiveWhitelist = true;
				mintPause = false;
        treasuryAddress = _treasuryAddress;
    }

		modifier onlyAdmin() {
        require(_administrators[msg.sender], "Caller is not an admin!");
        _;
    }

    /**
     * @dev Override Standard Method to set Base URI of the NFT Token
     * @return String Base URI of the NFT Token
     */
    function _baseURI() internal view override returns (string memory) {
        return baseURI;
    }

    /**
     * @dev Contract owner can set timestamp of the Whitelist/Public Sale Start
     * @param timestamp Timestamp of the PublicSale start time
     */
    function setPublicSaleStartTime(uint32 timestamp) public onlyOwner {
        publicSaleStartTime = timestamp;
    }

    /**
     * @dev Validate if the current time is before or after the PublicSale start time
     * @return bool True if the current time is after the PublicSale start time
     */
    function publicSaleIsActive() public view returns (bool) {
        return publicSaleStartTime <= block.timestamp;
    }

    /**
     * @dev Add addresses to Whilelist while Whitelist is Active.
     * @param _address Address to be whitelisted
     */
    function addToWhitelist(address _address) external validAddress(_address) onlyAdmin() {
        require(isActiveWhitelist, "Whitelist is not active");

        whitelist[_address] = true;
    }

    /**
     * @dev Verify if the address is in the Whitelist
     * @param _address Address to verify
     */
    function isWhitelisted(address _address)
        public
        view
        validAddress(_address)
        returns (bool)
    {
        if (isActiveWhitelist) {
            return whitelist[_address];
        } else {
            return true;
        }
    }

    /**
     * @dev Activate or Deactivate the Whitelist
     * @param _isActive Boolean to activate or deactivate the whitelist
     */
    function setWhitelistActive(bool _isActive) external onlyOwner {
        isActiveWhitelist = _isActive;
    }

    /**
     * @dev Verify if the address is in the Whitelist
     * @param _to Address that will own the tokens
     * @param _mintAmount Amount of tokens to mint
     * @return _capacityAmount Returns an array of the minted capacity amounts
     */
    function mint(
        address _stable,
        address _to,
        uint256 _mintAmount
    ) external validAddress(_stable) whenNotPaused returns (uint256[] memory) {
        require(!mintPause, "Minting is paused");
        require(publicSaleIsActive(), "Public sale is not active");
        require(isWhitelisted(msg.sender), "You are not whitelisted");
        require(
            isValidToken(_stable),
            "ERC721 ValiFi: Token not valid"
        );
        require(
            _batchSize > uint256(0),
            "ERC721 ValiFi: Can't create any Yield-Keys until a new batch is available"
        );

        uint256 supply = totalMintCnt;
        require(
            ((_mintAmount > 0) && (_mintAmount <= _batchSize)),
            "ERC721 ValiFi: Invalid amount"
        );
        require(
            supply + _mintAmount <= maxSupply,
            "ERC721 ValiFi: Mint exceeds supply"
        );

        require(
            IERC20Metadata(_stable).balanceOf(_msgSender()) >=
                amountCoin[_stable].mul(_mintAmount) *
                    (10**IERC20Metadata(_stable).decimals()),
            "ERC721 ValiFi: Insufficient Funds"
        );
        // Transfer the tokens to mint the Yield-Key
        bool success_treasury = IERC20Metadata(_stable).transferFrom(
            _msgSender(),
            treasuryAddress,
            amountCoin[_stable].mul(_mintAmount) *
                (10**IERC20Metadata(_stable).decimals())
        );
        require(
            success_treasury,
            "ERC721 ValiFi: Can't create Yield-Key, funds transfer failed"
        );

        uint256[] memory _capacityAmount = new uint256[](_mintAmount);
        for (uint256 i = 0; i < _mintAmount; i++) {
            _safeMint(_to, supply + i + 1); // to add 1 to avoid start the tokenId at 0
            capacityAmount[supply + i + 1] = getCapacityAmount();
            _capacityAmount[i] = capacityAmount[supply + i + 1];
            _batchSize = _batchSize.sub(uint256(1));
        }
        totalMinted[_to] += _mintAmount;
        totalMintCnt += _mintAmount;
        return _capacityAmount;
    }

    /**
     * @dev Verify if the address is owner of one or several Yield-Keys
     * @param _owner Wallet address of the owner
     * @return tokenIds Array of token Ids of Yield-Keys owned by the wallet
     */
    function tokenHoldings(address _owner)
        public
        view
        returns (uint256[] memory)
    {
        uint256 ownerTokenCount = balanceOf(_owner);
        uint256[] memory tokenIds = new uint256[](ownerTokenCount);
        for (uint256 i; i < ownerTokenCount; i++) {
            tokenIds[i] = tokenOfOwnerByIndex(_owner, i);
        }
        return tokenIds;
    }

    /**
     * @dev Verify if the address is staking of one or several Yield-Keys
     * @param _owner Wallet address of the owner
     * @return stakeTokenIds Array of token Ids of Yield-Keys owned by the wallet
     */
    function tokenStaking(address _owner)
        external
        view
        returns (uint256[] memory stakeTokenIds)
    {
        uint256[] memory tokenIds = tokenHoldings(_owner);
        if (tokenIds.length == 0) {
            return stakeTokenIds;
        }
        stakeTokenIds = tokenIds;
        for (uint256 i = 0; i < tokenIds.length; i++) {
            if (!isStaked(_owner, tokenIds[i])) {
                stakeTokenIds[i] = stakeTokenIds[stakeTokenIds.length - 1];
                delete stakeTokenIds[stakeTokenIds.length - 1];
            }
        }
    }

    function tokenURI(uint256 _tokenId)
        public
        view
        override
        returns (string memory)
    {
        require(
            _exists(_tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );

        string memory currentBaseURI = _baseURI();
        return
            bytes(currentBaseURI).length > 0
                ? string(
                    abi.encodePacked(
                        currentBaseURI,
                        _tokenId.toString(),
                        baseExtension
                    )
                )
                : "";
    }

    /**
     * @dev Function to Set Base URI of the NFT Token
     * @param _newBaseURI New URL of Metadata of the NFT Token
     */
    function setBaseURI(string memory _newBaseURI) public onlyOwner {
        baseURI = _newBaseURI;
        emit SetBaseURI(baseURI);
    }

    /**
     * @dev Function to Set Base Extension of the NFT Token Metadata
     * @param _newBaseExtension string of extension for the URI metadata files
     */
    function setBaseExtension(string memory _newBaseExtension)
        external
        onlyOwner
    {
        baseExtension = _newBaseExtension;
    }

    /**
     * @dev Implementation / Instance of paused methods() in the ERC721.
     * @param status Setting the status boolean (True for paused, or False for unpaused)
     * See {ERC721Pausable}.
     */
    function pause(bool status) public onlyAdmin() {
        if (status) {
            _pause();
        } else {
            _unpause();
        }
    }

    /**
     * @dev Implementation / Instance of paused methods() in the ERC721.
     * @param status Setting the status boolean (True for paused, or False for unpaused)
     * See {ERC721Pausable}.
     */
    function pauseMint(bool status) public onlyAdmin() {
        if (status) {
            mintPause = true;
        } else {
            mintPause = false;
        }
    }

    /**
     * @dev Helpers Funtions.
     */

    /**
     * @dev This function allows getting the Treasury Address.
     * @return Treasury Address
     */
    function getTreasuryAddress() public view returns (address) {
        return treasuryAddress;
    }

    /**
     * @dev This function allows getting a random capacity amount to be used by the mint funtion
     * @return _amountKeys The capacity amount for the token Id
     */
    function getCapacityAmount() internal view returns (uint256 _amountKeys) {
        uint256 randomNum = randrange(1, 100);
        if (randomNum <= 70) {
            _amountKeys = 4;
        } else if ((randomNum > 70) && (randomNum <= 92)) {
            _amountKeys = 6;
        } else if ((randomNum > 92) && (randomNum <= 100)) {
            _amountKeys = 8;
        } else {
            revert("Error: Random Number is not in range");
        }
    }

    /**
     * @dev This Method permit to get the Batch Size for Mint Yield Box.
     * @return _amount Amount of Yield Box to Mint per Batches
     */
    function getBatchSize() public view returns (uint256 _amount) {
        return _batchSize;
    }

    /**
     * @dev This Method permit getting the Value in Stable Coin and Token ValiFi to create Yield Box.
     * @param _token Token Address of Stable Coin and Token ValiFi to create Yield Box
     * @return Value in Avax and Token ValiFi for create Yield Box
     */
    function getTokenValue(address _token)
        public
        view
        validAddress(_token)
        returns (uint256)
    {
        return amountCoin[_token];
    }

    /**
     * @dev This Method verify if Token Address is Valid to Create Yield Box.
     * @param _token Token Address of Stable Coin and Token ValiFi to create Yield Box
     * @return Status true if value is more then 0, or false if value is 0
     */
    function isValidToken(address _token)
        public
        view
        validAddress(_token)
        returns (bool)
    {
        return amountCoin[_token] > 0;
    }

    /**
     * @dev This method validate is the Yield-Key is Staked
     * @param wallet address of Yield-Key
     * @param _tokenId Token Id of Yield-Key
     */
    function isStaked(address wallet, uint256 _tokenId)
        public
        view
        validAddress(wallet)
        returns (bool)
    {
        require(
            _exists(_tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );
        if (stakedYieldKeys[wallet][_tokenId].startstake.length > 0) {
            return stakedYieldKeys[wallet][_tokenId].isStaked;
        } else {
            return false;
        }
    }

    /**
     * @dev This method to Staked Yield-Key
     * @param wallet address of Yield-Key
     * @param tokenIds Array of Token Id of Yield-Key to Staked
     */
    function stakeYieldKey(address wallet, uint256[] calldata tokenIds)
        external
        validAddress(wallet)
        whenNotPaused
    {
        require(
            wallet == _msgSender(),
            "ERC721 ValiFi: Incorrect wallet, stake Yield-Key failed"
        );
        for (uint256 i; i < tokenIds.length; i++) {
            require(
                _exists(tokenIds[i]),
                "ERC721Metadata: URI query for nonexistent token"
            );
            require(
                ownerOf(tokenIds[i]) == _msgSender(),
                "ERC721 ValiFi: Sender is not owner of Yield-Key"
            );
            require(
                !isStaked(wallet, tokenIds[i]),
                "ERC721 ValiFi: Yield-Key already being staked"
            );
        }
        uint256[] memory AmountKeys = new uint256[](tokenIds.length);
        for (uint256 i; i < tokenIds.length; i++) {
            // addMapping(wallet, tokenIds[i]);
            stakedYieldKeys[wallet][tokenIds[i]].isStaked = true;
            stakedYieldKeys[wallet][tokenIds[i]].startstake.push(
                uint64(block.timestamp)
            );
            stakedYieldKeys[wallet][tokenIds[i]].endstake.push(uint64(0));
            AmountKeys[i] = capacityAmount[tokenIds[i]];
        }
        emit StakedYieldKey(wallet, tokenIds, AmountKeys);
    }

    /**
     * @dev This method to unStaked Yield-Key
     * @param wallet address of Yield-Key
     * @param tokenIds Array of Token Id of Yield-Key to Staked
     */
    function unStakeYieldKey(address wallet, uint256[] calldata tokenIds)
        external
        validAddress(wallet)
        whenNotPaused
    {
        require(
            wallet == _msgSender(),
            "ERC721 ValiFi: Incorrect wallet, unstake Yield-Key failed"
        );
        for (uint256 i; i < tokenIds.length; i++) {
            require(
                _exists(tokenIds[i]),
                "ERC721Metadata: URI query for nonexistent token"
            );
            require(
                ownerOf(tokenIds[i]) == _msgSender(),
                "ERC721 ValiFi: Sender is not owner of Yield-Key"
            );
            require(
                isStaked(wallet, tokenIds[i]),
                "ERC721 ValiFi: Yield-Key is not being staked"
            );
        }
        uint256[] memory AmountKeys = new uint256[](tokenIds.length);
        for (uint256 i; i < tokenIds.length; i++) {
            stakedYieldKeys[wallet][tokenIds[i]].isStaked = false;
            stakedYieldKeys[wallet][tokenIds[i]].endstake[
                stakedYieldKeys[wallet][tokenIds[i]].startstake.length - 1
            ] = uint64(block.timestamp);
            AmountKeys[i] = capacityAmount[tokenIds[i]];
        }
        emit UnStakedYieldKey(wallet, tokenIds, AmountKeys);
    }

    /**
     * @dev This function allows to set the Treasury Wallet Address
     * @param _treasuryAddress Treasury Address
     */
    function setTreasuryAddress(address _treasuryAddress)
        external
        validAddress(_treasuryAddress)
        onlyOwner
    {
        treasuryAddress = _treasuryAddress;
        emit SetTreasuryAddress(_treasuryAddress);
    }

    /**
     * @dev This function allows to set the currency.
     * @param _token Address of the token in the Avalanche Network
     * @param _value Value in the currency of the chosen token
     */
    function setTokenValue(address _token, uint256 _value)
        external
        validAddress(_token)
        onlyOwner
    {
				require(_token.isContract(), "ERC721 ValiFi: Token is not a contract");
        require(
            _value >= 0,
            "ERC721 ValiFi: Can't set value, value must be greater than 0"
        );
        amountCoin[_token] = _value;
        emit SetCostPerToken(_token, _value);
    }

    /**
     * @dev This function allows to set the minting Batch Size.
     * @param _amount Amount of Yield-Keys to Mint per Batches
     */
    function setBatchSize(uint256 _amount) external onlyOwner {
        _batchSize = _amount;
        emit SetBatchSize(_amount);
    }

    /**
     * @dev Get Historical of Staked Yield-Key
     * @param wallet address of Yield-Key owner
     * @param _tokenId Token Id of Yield-Key
     */
    function getHistoricalStakedYieldKey(address wallet, uint256 _tokenId)
        public
        view
        returns (uint64[] memory startstake, uint64[] memory endstake)
    {
        startstake = stakedYieldKeys[wallet][_tokenId].startstake;
        endstake = stakedYieldKeys[wallet][_tokenId].endstake;
    }

		function revokeAdmin(address addr) external onlyOwner {
        _administrators[addr] = false;
    }

    function addAdmin(address addr) external onlyOwner {
        _administrators[addr] = true;
    }

    // The following functions are overrides required by Solidity.

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 _tokenId
    ) internal override(ERC721Enumerable, ERC721) notBlacklisted(from) {
        require(
            !paused(),
            "ERC721 ValiFi: can't create or transfer any Yield-Key while paused"
        );
        require(
            !isBlacklisted(to),
            "ERC721 ValiFi: can't transfer to blacklisted address"
        );
        if (from != address(0)) {
            require(
                !isStaked(from, _tokenId),
                "ERC721 ValiFi: can't transfer staked Yield-Key"
            );
        }
        super._beforeTokenTransfer(from, to, _tokenId);
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(ERC721Enumerable, ERC721)
        returns (bool)
    {
        return
            interfaceId == type(IERC721).interfaceId ||
            interfaceId == type(IERC721Enumerable).interfaceId ||
            interfaceId == type(IERC721Metadata).interfaceId ||
            super.supportsInterface(interfaceId);
    }
}

