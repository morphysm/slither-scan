interface IRewardsClaimerV2 {
    event SignerAuthorityUpdated(
        address indexed oldSignerAuthority,
        address indexed newSignerAuthority
    );

    event RewardsClaimed(
        uint256 indexed vaultId,
        address indexed userAddress,
        uint256 claimPeriodStart,
        uint256 claimPeriodEnd,
        RewardsInfo[] rewardsInfo
    );

    event FeeCreated(
        uint256 indexed vaultId,
        address indexed receiver,
        uint256 amount,
        uint256 percentageBase
    );

    event FeeUpdated(
        uint256 indexed vaultId,
        address indexed newReceiver,
        uint256 newAmount,
        uint256 newPercentageBase
    );

    event FeeRemoved(
        uint256 indexed vaultId,
        address indexed oldReceiver,
        uint256 oldAmount,
        uint256 oldPercentageBase
    );

    event CustodialAccountCreated(uint256 vaultId, address setter, address account);

    event CustodialAccountRemoved(uint256 vaultId, address oldSetter, address oldAccount);

    event CustodialAccountUpdated(uint256 vaultId, address newSetter, address newAccount);

    struct RewardsInfo {
        uint8 tokenType; //0-> ERC20 , 1 -> ERC721)
        address tokenAddress; // Address of token contract.
        uint256 amount; // Number(in the case of ERC721) ot amount of tokens to send
        uint256[] tokenIds; //  Ids of the token to reward
        uint8 withdrawMethod; // 0 -> mint, 1 -> transfer
    }

    struct ClaimInfo {
        uint256 vaultId;
        address userAddress;
        uint256 claimPeriodStart;
        uint256 claimPeriodEnd;
        RewardsInfo[] rewardsInfo;
    }

    /**
        @dev It performs the verification of the claim period based on all the previous periods stored.
        It is not possible to claim from previous periods.
        The signature is then verified against the claim info.
        Depending on the type of token, either erc20 or erc721, different actions are performed.
        At the end, a transfer or minting action is carried out depending on the extraction method. 
        For minting, the contract needs to be allowed to  mint the output token.
        In the case of transfer, the contract needs to receive the allowance of the custodial account that owns the tokens.
        @param claimInfo data of claiming.
        @param authorityECDSA  variables(v, r and s) of the authority signature.
      
     */
    function claim(ClaimInfo memory claimInfo, ECRecoverLib.ECDSAVariables memory authorityECDSA)
        external;

    /**
        @notice Setting up a new signer authority address.
        @param newSignerAuthority address of the new signer authority.
    */
    function setSignerAuthority(address newSignerAuthority) external;

    /**
        @notice Setting up a new fee structure.
        @param vaultId Id of the vault where the new fee value will be created.
        @param receiver address of the account that will receive the new fee.
        @param amount amount of the fees that act as numerator of the fee ratio.
        @param percentageBase percentage base uses to calculate the fee ratio along with the amount.
    */
    function createFee(
        uint256 vaultId,
        address receiver,
        uint256 amount,
        uint256 percentageBase
    ) external;

    /**
        @notice Updating up an existing fee structure.
        @param vaultId Id of the fees that will be updated.
        @param receiver new address of the account that will receive the new fee.
        @param amount  new amount of the fees that act as numerator of the fee ratio.
        @param percentageBase new percentage base uses to calculate the fee ratio along with the amount.
    */
    function updateFee(
        uint256 vaultId,
        address receiver,
        uint256 amount,
        uint256 percentageBase
    ) external;

    function removeFee(uint256 vaultId) external;

    /** 
        @notice Setting up a new custodialAccount structure.
        @param vaultId Id of the vault where the new fee value will be created.
        @param account address account that will act as account that will hold the funds.
    */
    function createCustodialAccount(uint256 vaultId, address account) external;

    /** 
        @notice Updating up an existing CustodialAccount structure.
        @param vaultId Id of the custodialAccount that will be updated.
        @param newAccount new address account that will act as account that will hold the funds.
    */
    function updateCustodialAccount(uint256 vaultId, address newAccount) external;

    function removeCustodialAccount(uint256 vaultId) external;

    function getVaultFeeData(uint256 vaultId)
        external
        view
        returns (
            address receiver,
            uint256 amount,
            uint256 percentageBase,
            bool exists
        );

    function getCustodialAccountByVaultId(uint256 vaultId)
        external
        view
        returns (
            address setter,
            address account,
            bool exists
        );

    function getPeriod(uint256 vaultId, address userAccount) external view returns (uint256 period);
}

