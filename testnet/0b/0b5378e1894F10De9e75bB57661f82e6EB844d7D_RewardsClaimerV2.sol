pragma solidity <0.9.0;

// Libraries
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/utils/introspection/ERC165Checker.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "../libs/ECRecoverLib.sol";
import "../libs/FeesLib.sol";
import "../libs/CustodialAccountsLib.sol";

// Contracts
import "../../px-vaults-contracts/contracts/base/Base.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

// Interfaces
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721Receiver.sol";
import "../tokens/IMintableERC20.sol";
import "../tokens/IMintableERC721.sol";
import "./IRewardsClaimerV2.sol";

import "hardhat/console.sol";

contract RewardsClaimerV2 is IRewardsClaimerV2, IERC721Receiver, Base, ReentrancyGuard {
    using ECRecoverLib for ECRecoverLib.ECDSAVariables;
    using FeesLib for FeesLib.Fees;
    using CustodialAccountsLib for CustodialAccountsLib.CustodialAccount;
    using SafeERC20 for IMintableERC20;
    using SafeMath for uint256;
    using ERC165Checker for address;

    bytes32 public constant CLAIMER_PAUSE = ("ClaimerPause");

    uint256 public constant DEFAULT_FEE_AMOUNT = 3;

    uint256 public constant DEFAULT_FEE_BASE = 100;

    address public defaultFeeReceiver;

    address public signerAuthority;

    modifier onlyStaker(address user) {
        require(user == msg.sender, "CALLER_ISNT_STAKER");
        _;
    }

    /**
        @notice This mapping represents the periods where:
        - The first key is the id of the vault where belongs the period.
        - The second key is the address of the user that claimed.
        - The value is the end period of the last claim.

        vaultId => userAccount => lastEndPeriod
     */
    mapping(uint256 => mapping(address => uint256)) internal periods;

    /**
        @notice This mapping represents the  fees structure where:
        - The key is the id of the vault where belongs the fee.
        - The value is the fee structure. It includes the receiver, amount and base values.
     */
    mapping(uint256 => FeesLib.Fees) internal fees;

    /**
        @notice This mapping represents the  custodialAccounts structure where:
        - The key is the id of the vault where belongs the fee.
        - The value is the custodialAccount structure. It includes the setter, account and exist value.
     */
    mapping(uint256 => CustodialAccountsLib.CustodialAccount) internal custodialAccounts;

    constructor(
        address settingsAddress,
        address signerAuthorityAddress,
        address defaultFeeReceiverAddress
    ) Base(settingsAddress) {
        signerAuthority = signerAuthorityAddress;
        defaultFeeReceiver = defaultFeeReceiverAddress;
    }

    function claim(ClaimInfo memory claimInfo, ECRecoverLib.ECDSAVariables memory authorityECDSA)
        external
        override
        onlyStaker(claimInfo.userAddress)
        whenPlatformIsNotPaused
        nonReentrant
    {
        require(!_isClaimerPaused(), "CLAIMER_CONTRACT_IS_PAUSED");

        custodialAccounts[claimInfo.vaultId].requireExists();

        // Prove to contract that signer authority signed this message.
        authorityECDSA.verifySign(signerAuthority, _getMessageHash(claimInfo));

        uint256 lastClaimPeriod = periods[claimInfo.vaultId][claimInfo.userAddress];

        if (lastClaimPeriod != 0) {
            require(claimInfo.claimPeriodStart >= lastClaimPeriod, "INVALID_CLAIM_PERIOD");
        }

        periods[claimInfo.vaultId][claimInfo.userAddress] = claimInfo.claimPeriodEnd;

        FeesLib.Fees memory fee;
        fee.receiver = _getFeeReceiver(claimInfo.vaultId);
        fee.amount = _getFeeAmount(claimInfo.vaultId);
        fee.percentageBase = _getFeePercentageBase(claimInfo.vaultId);
        address claimer = claimInfo.userAddress;
        address custodialAccount = custodialAccounts[claimInfo.vaultId].account;

        for (uint8 indexAt = 0; indexAt < claimInfo.rewardsInfo.length; indexAt++) {
            address rewardsToken = claimInfo.rewardsInfo[indexAt].tokenAddress;
            uint256 claimedAmount = claimInfo.rewardsInfo[indexAt].amount;
            uint8 withdrawMethod = claimInfo.rewardsInfo[indexAt].withdrawMethod;

            if (claimInfo.rewardsInfo[indexAt].tokenType == uint8(0)) {
                IMintableERC20 erc20RewardsToken;
                try ERC20(rewardsToken).decimals() {
                    erc20RewardsToken = IMintableERC20(rewardsToken);
                } catch {
                    revert("REWARDS_TOKEN_HAS_AN_INVALID_ERC20_IMPLEMENTATION");
                }
                _withdrawERC20(
                    erc20RewardsToken,
                    claimedAmount,
                    claimer,
                    withdrawMethod,
                    fee,
                    custodialAccount
                );
            } else {
                require(
                    rewardsToken.supportsInterface(type(IERC721).interfaceId),
                    "REWARDS_TOKEN_HAS_AN_INVALID_ERC721_IMPLEMENTATION"
                );
                uint256[] memory tokenIds = claimInfo.rewardsInfo[indexAt].tokenIds;
                _withdrawERC721(rewardsToken, tokenIds, claimer, withdrawMethod, custodialAccount);
            }
        }

        emit RewardsClaimed(
            claimInfo.vaultId,
            claimInfo.userAddress,
            claimInfo.claimPeriodStart,
            claimInfo.claimPeriodEnd,
            claimInfo.rewardsInfo
        );
    }

    function setSignerAuthority(address newSignerAuthority)
        external
        override
        onlyConfigurator(msg.sender)
    {
        require(
            newSignerAuthority != address(0x0) && newSignerAuthority != signerAuthority,
            "NEW_SIGNER_AUTHORITY_INVALID"
        );

        address oldSignerAuthority = signerAuthority;
        signerAuthority = newSignerAuthority;
        emit SignerAuthorityUpdated(oldSignerAuthority, newSignerAuthority);
    }

    function createCustodialAccount(uint256 vaultId, address account)
        external
        override
        onlyConfigurator(msg.sender)
    {
        custodialAccounts[vaultId].create(msg.sender, account);
        emit CustodialAccountCreated(vaultId, msg.sender, account);
    }

    function updateCustodialAccount(uint256 vaultId, address newAccount)
        external
        override
        onlyConfigurator(msg.sender)
    {
        custodialAccounts[vaultId].update(msg.sender, newAccount);
        emit CustodialAccountUpdated(vaultId, msg.sender, newAccount);
    }

    function removeCustodialAccount(uint256 vaultId)
        external
        override
        onlyConfigurator(msg.sender)
    {
        address oldSetter = custodialAccounts[vaultId].setter;
        address oldAccount = custodialAccounts[vaultId].account;
        custodialAccounts[vaultId].remove();
        emit CustodialAccountRemoved(vaultId, oldSetter, oldAccount);
    }

    function createFee(
        uint256 vaultId,
        address receiver,
        uint256 amount,
        uint256 percentageBase
    ) external override onlyConfigurator(msg.sender) {
        fees[vaultId].create(receiver, amount, percentageBase);

        emit FeeCreated(vaultId, receiver, amount, percentageBase);
    }

    function updateFee(
        uint256 vaultId,
        address newReceiver,
        uint256 newAmount,
        uint256 newPercentageBase
    ) external override onlyConfigurator(msg.sender) {
        fees[vaultId].update(newReceiver, newAmount, newPercentageBase);

        emit FeeUpdated(vaultId, newReceiver, newAmount, newPercentageBase);
    }

    function removeFee(uint256 vaultId) external override onlyConfigurator(msg.sender) {
        address oldReceiver = fees[vaultId].receiver;
        uint256 oldAmount = fees[vaultId].amount;
        uint256 oldPercentageBase = fees[vaultId].percentageBase;
        fees[vaultId].remove();
        emit FeeRemoved(vaultId, oldReceiver, oldAmount, oldPercentageBase);
    }

    function getVaultFeeData(uint256 vaultId)
        external
        view
        override
        onlyConfigurator(msg.sender)
        returns (
            address receiver,
            uint256 amount,
            uint256 percentageBase,
            bool exists
        )
    {
        receiver = fees[vaultId].receiver;
        amount = fees[vaultId].amount;
        percentageBase = fees[vaultId].percentageBase;
        exists = fees[vaultId].exists;
    }

    function getCustodialAccountByVaultId(uint256 vaultId)
        external
        view
        override
        onlyConfigurator(msg.sender)
        returns (
            address setter,
            address account,
            bool exists
        )
    {
        setter = custodialAccounts[vaultId].setter;
        account = custodialAccounts[vaultId].account;
        exists = custodialAccounts[vaultId].exists;
    }

    function getPeriod(uint256 vaultId, address userAccount)
        external
        view
        override
        onlyConfigurator(msg.sender)
        returns (uint256 period)
    {
        period = periods[vaultId][userAccount];
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata
    ) external pure override returns (bytes4) {
        // It is implemented to support ERC721 transfers.
        return IERC721Receiver.onERC721Received.selector;
    }

    /*
    Internal Functions
     */

    function _withdrawERC20(
        IMintableERC20 erc20RewardsToken,
        uint256 claimedAmount,
        address claimer,
        uint8 withdrawMethod,
        FeesLib.Fees memory fee,
        address custodialAccount
    ) internal {
        uint256 feeRate = (claimedAmount.mul(fee.amount)).div(fee.percentageBase);
        if (withdrawMethod == 0) {
            erc20RewardsToken.mint(fee.receiver, feeRate);
            erc20RewardsToken.mint(claimer, claimedAmount);
        } else {
            erc20RewardsToken.safeTransferFrom(custodialAccount, fee.receiver, feeRate);
            erc20RewardsToken.safeTransferFrom(custodialAccount, claimer, claimedAmount);
        }
    }

    function _withdrawERC721(
        address rewardsToken,
        uint256[] memory tokenIds,
        address claimer,
        uint8 withdrawMethod,
        address custodialAccount
    ) internal {
        if (withdrawMethod == 0) {
            for (uint32 index = 0; index < tokenIds.length; index++) {
                IMintableERC721(rewardsToken).mint(claimer, tokenIds[index]);
            }
        } else {
            for (uint32 index = 0; index < tokenIds.length; index++) {
                IERC721(rewardsToken).safeTransferFrom(custodialAccount, claimer, tokenIds[index]);
            }
        }
    }

    /**
    @return true when the queried marketplace contract is paused.
     */
    function _isClaimerPaused() internal view returns (bool) {
        return _getPlatformSettingsValue(address(this), CLAIMER_PAUSE) != 0;
    }

    function _getFeeReceiver(uint256 vaultId) internal view returns (address) {
        return fees[vaultId].exists ? fees[vaultId].receiver : defaultFeeReceiver;
    }

    function _getFeeAmount(uint256 vaultId) internal view returns (uint256) {
        return fees[vaultId].exists ? fees[vaultId].amount : DEFAULT_FEE_AMOUNT;
    }

    function _getFeePercentageBase(uint256 vaultId) internal view returns (uint256) {
        return fees[vaultId].exists ? fees[vaultId].percentageBase : DEFAULT_FEE_BASE;
    }

    function _getMessageHash(ClaimInfo memory claimInfo) internal pure returns (bytes32) {
        bytes32 messageHash;

        bytes32[] memory rewardsInfoBytes = new bytes32[](claimInfo.rewardsInfo.length);

        for (uint8 index = 0; index < claimInfo.rewardsInfo.length; index++) {
            rewardsInfoBytes[index] = keccak256(
                abi.encodePacked(
                    claimInfo.rewardsInfo[index].tokenType,
                    claimInfo.rewardsInfo[index].tokenAddress,
                    claimInfo.rewardsInfo[index].amount,
                    claimInfo.rewardsInfo[index].tokenIds,
                    claimInfo.rewardsInfo[index].withdrawMethod
                )
            );
        }

        messageHash = keccak256(
            abi.encodePacked(
                claimInfo.vaultId,
                claimInfo.userAddress,
                claimInfo.claimPeriodStart,
                claimInfo.claimPeriodEnd,
                rewardsInfoBytes
            )
        );
        return messageHash;
    }
}

