pragma solidity ^0.8.3;

import "./FyRiseNFT.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "hardhat/console.sol";

contract FyRiseMarketplace is Ownable, ReentrancyGuard {
    using SafeMath for uint256;

    address public fyRiseNFTContract;
    address public benificiaryOneAddress;
    address public benificiaryTwoAddress;

    address public moderatorAddress;
    uint256 public nftSold;
    uint256 public totalVolumeTraded;

    uint256 public saleFee = 100; // 10 % of profit (cannot change it)
    uint256 public TOTAL_PERC = 1000;
    uint256 public benificiaryShare = 500; // 50%

    mapping(uint256=>bool) public nftUpdated;

    bool public isMarketplaceOpen = false;

    uint256[4] public landBuildPrice = [1.5 ether, 3 ether, 5 ether, 8 ether];
    
    string[4] public landBuildImages = [
        "updated land 1",
        "updated land 2",
        "updated land 3",
        "updated land 4"
    ];
    struct Land {
        string name;
        string desc;
        uint256 id;
        string imageURI;
        address ownerAddress;
        uint256 buyPrice;
        uint256 boughtDate;
        uint256 currentPrice; // current calculation price if isForSale is false
        bool isForSale; // false
        uint256 profitPerc; // 10
        uint256 collectionType;
    }

    constructor(
        address _fyRiseNFTContract,
        address _benificiaryOneAddress,
        address _benificiaryTwoAddress,
        address _moderatorAddress
    ) {
        require(
            _fyRiseNFTContract != address(0) &&
                _benificiaryOneAddress != address(0) &&
                _benificiaryTwoAddress != address(0) &&
                _moderatorAddress != address(0),
            "Cannot pass null address for params"
        );

        fyRiseNFTContract = _fyRiseNFTContract;
        benificiaryOneAddress = _benificiaryOneAddress;
        benificiaryTwoAddress = _benificiaryTwoAddress;
        moderatorAddress = _moderatorAddress;
    }

    modifier onlyBenificiaryOne() {
        require(
            msg.sender == benificiaryOneAddress,
            "Only benificiary One can do this operation"
        );
        _;
    }

    modifier onlyBenificiaryTwo() {
        require(
            msg.sender == benificiaryTwoAddress,
            "Only benificiary two can do this operation"
        );
        _;
    }

    modifier anyBenificiary() {
        require(
            msg.sender == benificiaryOneAddress ||
                msg.sender == benificiaryTwoAddress,
            "Only benificiaries are allowed for this operation"
        );
        _;
    }

    modifier onlyModerator() {
        require(
            msg.sender == moderatorAddress || msg.sender == owner(),
            "Only moderator is allowed to do this operation"
        );
        _;
    }

    function setMarketplaceStatus(bool _marketplaceStatus) public onlyModerator{
        require(isMarketplaceOpen!=_marketplaceStatus,"New value cannot be same with previous value");
        isMarketplaceOpen = _marketplaceStatus;
    }

    function fetchAllNfts(uint256 offset,uint256 limit)
        public
        view
        returns (FyRiseNFT.Land[] memory)
    {
        uint256 mintedSupply = FyRiseNFT(fyRiseNFTContract).mintedSupply();
        if(limit>mintedSupply){
            limit = mintedSupply;
        }
        FyRiseNFT.Land[] memory landArr = new FyRiseNFT.Land[](
            limit.sub(offset)
        );
        uint256 index=0;
        
        for (uint256 i = offset; i < limit; i++) {
            landArr[index] = FyRiseNFT(fyRiseNFTContract).getNft(i.add(1));
            index = index.add(1);
        }

        return landArr;
    }

    function buyNft(uint256 tokenId) public payable {
        isNftForSale(tokenId);
        FyRiseNFT.Land memory landData = FyRiseNFT(fyRiseNFTContract)
            .getNft(tokenId);
        uint256 price = landData.currentPrice;
        uint256 boughtPrice = landData.buyPrice;
        require(msg.value == price, "Incorrect amount of Matic sent");
        uint256 profit = price - boughtPrice;

        nftSold = nftSold.add(1);
        totalVolumeTraded = totalVolumeTraded.add(price);

        uint256 fee = profit.mul(saleFee).div(TOTAL_PERC);

        uint256 amountToSend = msg.value - fee;

        address receiver = FyRiseNFT(fyRiseNFTContract).ownerOf(tokenId);
        require(receiver!=msg.sender,"Already owned");
        FyRiseNFT(fyRiseNFTContract).safeTransferFrom(
            receiver,
            msg.sender,
            tokenId
        );
        FyRiseNFT(fyRiseNFTContract).updateNFTAfterSale(tokenId);

        payable(receiver).transfer(amountToSend);
    }

    function getBalance() public view returns (uint256) {
        return address(this).balance;
    }

    function isNftForSale(uint256 tokenId) private view {
        require(
            FyRiseNFT(fyRiseNFTContract).isForSale(tokenId),
            "NFT is not for sale at the moment"
        );
    }

    function setFyRiseNFTContractAddress(address _newAddress)
        public
        onlyModerator
    {
        require(
            _newAddress != fyRiseNFTContract,
            "New FyRise NFT Contract address cannot be same with previous address"
        );
        fyRiseNFTContract = _newAddress;
    }

    function setBenificiaryOneAddress(address _newAddress)
        public
        onlyBenificiaryOne
    {
        require(
            _newAddress != address(0) && _newAddress != benificiaryOneAddress,
            "New address cannot be null or same as previous address"
        );
        benificiaryOneAddress = _newAddress;
    }

    function setBenificiaryTwoAddress(address _newAddress)
        public
        onlyBenificiaryTwo
    {
        require(
            _newAddress != address(0) && _newAddress != benificiaryTwoAddress,
            "New address cannot be null or same as previous address"
        );
        benificiaryTwoAddress = _newAddress;
    }

    function setModeratorAddress(address _newAddress) public onlyModerator {
        require(
            _newAddress != address(0) && _newAddress != moderatorAddress,
            "New address cannot be null or same as previous address"
        );
        moderatorAddress = _newAddress;
    }

    function setLandBuildPrice(uint256 _newPrice, uint256 _index) public onlyModerator {
        require(
            _index < landBuildPrice.length && landBuildPrice[_index] != _newPrice,
            "New price cannot be same with previous price"
        );
        landBuildPrice[_index] = _newPrice;
    }

    function setLandBuildImage(string memory _imageUrl, uint256 _index)
        public
        onlyModerator
    {
        require(
            _index < landBuildImages.length &&
                !compareStrings(landBuildImages[_index], _imageUrl),
            "New Image value cannot be same with previous Image URL"
        );
        landBuildImages[_index] = _imageUrl;
    }

    function buildOnLand(uint256 tokenId) public payable{
        require(!nftUpdated[tokenId],"Already built");
        require(isMarketplaceOpen,"Marketplace is not open at the moment");
        require(FyRiseNFT(fyRiseNFTContract).ownerOf(tokenId)==msg.sender,"Only owner of nft is allowed to build");

        FyRiseNFT.Land memory landData = FyRiseNFT(fyRiseNFTContract).getNft(tokenId);
        uint buildPrice = landBuildPrice[landData.collectionType];
        require(msg.value == buildPrice, "Incorrect value of Avax sent");
        landData.profitPerc = 2;
        landData.imageURI = landBuildImages[landData.collectionType];
        nftUpdated[tokenId] = true;
    }

    function withdraw() public anyBenificiary nonReentrant {
        uint256 balance = address(this).balance;
        uint256 profitFor50Perc = balance.mul(benificiaryShare).div(TOTAL_PERC);
        payable(benificiaryOneAddress).transfer(profitFor50Perc);
        payable(benificiaryTwoAddress).transfer(profitFor50Perc);
    }

    function compareStrings(string memory a, string memory b)
        private
        pure
        returns (bool)
    {
        return (keccak256(abi.encodePacked((a))) ==
            keccak256(abi.encodePacked((b))));
    }
}

// SPDX-License-Identifier: MIT OR Apache-2.0
pragma solidity ^0.8.3;

import "@openzeppelin/contracts/utils/Counters.sol";
import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "./Base64.sol";

contract FyRiseNFT is ERC721, Ownable, ReentrancyGuard {
    using Counters for Counters.Counter;
    using SafeMath for uint256;
    Counters.Counter private _tokenIds;

    string[4] public collectionType = ["The Cottage", "Villa", "Marvellow", "FyRise Museum"];
    string[4] public landDescriptions = [
        "The Cottage is smallest FyRise Land and the most economical way of Investment to get profit for the lifetime.",
        "Villa is here to make your lifestyle stylish and comfortable in FyRise community.",
        "With Marvellow you will be able to get a lot of profit in FyRise lands.",
        "It is the most valuable asset for the FyRise community."
    ];
    uint256 public constant totalSupply = 5000;
    uint256 public mintedSupply = 0;
    uint256[4] public LAND_SUPPLY = [2000, 1500, 1000, 500];
    uint256[4] public LAND_SUPPLY_SOLD = [0, 0, 0, 0];

    uint256[4] public landPrices = [1 ether, 1.75 ether, 3.5 ether, 5 ether];

    uint256 public TOTAL_PERC = 1000;
    uint256 public priceIncrementTimeStep = 1 days; // NO Setter (Cannot change it)
    uint256 public refPerc = 50;

    uint256 public totalRevenue = 0;
    uint256 public refferalReward = 0;

    uint256 public benificiaryOneShare = 400; // 40%
    uint256 public benificiaryTwoShare = 400; // 40%
    uint256 public benificiaryThreeShare = 200; // 20%

    // One image for one type
    string[4] public landImages = [
        "https://ipfs.io/ipfs/bafybeiev7gft5tzoqtu47da5kgzww4xt6ytsmb74it3wjwzw7tabxr47tq",
        "https://ipfs.io/ipfs/bafybeifkvlsjmiur7p5hjjx3wblg4oqgxuan6wx62s66edlblxmksg75yq",
        "https://ipfs.io/ipfs/bafybeih5nfy6wkyzetgq4azw6bjhazzyc56s7pkuocym4ufrxsc54jawwa",
        "https://ipfs.io/ipfs/bafybeie7ykea4bkw5gqve3zmih5mwuk6wgkhdtlfkjvzjk5jcz4qd2tv5y"
    ];

    uint256 public startTime;

    bool public isMarketplaceOpen = false;

    struct Land {
        string name;
        string desc;
        uint256 id;
        string imageURI;
        address ownerAddress;
        uint256 buyPrice;
        uint256 boughtDate;
        uint256 currentPrice; // current calculation price if isForSale is false
        bool isForSale; // false
        uint256 profitPerc; // 10
        uint256 collectionType;
    }

    mapping(uint256 => Land) public nfts;

    address public benificiaryOneAddress;
    address public benificiaryTwoAddress;
    address public benificiaryThreeAddress;

    address public marketplaceAddress; //will be Marketplace address

    constructor(
        address _benificiary1Address,
        address _benificiary2Address,
        address _benificiary3Address,
        address _marketplaceAddress,
        uint256 _startTime
    ) ERC721("FyRise", "FR") {
        require(
            _benificiary1Address != address(0) &&
                _benificiary2Address != address(0) &&
                _benificiary3Address != address(0),
            "Cannot pass null address for params"
        );
        benificiaryOneAddress = _benificiary1Address;
        benificiaryTwoAddress = _benificiary2Address;
        benificiaryThreeAddress = _benificiary3Address;
        marketplaceAddress = _marketplaceAddress;
        startTime = _startTime;
    }

    modifier onlyBenificiaryOne() {
        require(
            msg.sender == benificiaryOneAddress,
            "Only benificiary One can do this operation"
        );
        _;
    }

    modifier onlyBenificiaryTwo() {
        require(
            msg.sender == benificiaryTwoAddress,
            "Only benificiary two can do this operation"
        );
        _;
    }
    
    modifier onlyBenificiaryThree() {
        require(
            msg.sender == benificiaryThreeAddress,
            "Only benificiary three can do this operation"
        );
        _;
    }

    modifier anyBenificiary() {
        require(
            msg.sender == benificiaryOneAddress ||
                msg.sender == benificiaryTwoAddress ||
                msg.sender == benificiaryThreeAddress
                ,
            "Only benificiaries are allowed for this operation"
        );
        _;
    }

    modifier onlyMarketplace() {
        require(
            msg.sender == marketplaceAddress || msg.sender == owner(),
            "Only marketplace is allowed to do this operation"
        );
        _;
    }

    function onlyOwnerOfNft(uint256 id, address _msgSender) private view {
        require(
            ownerOf(id) == _msgSender,
            "You are not allowed for this operation"
        );
    }

    function onlyExistingNFT(uint256 id) private view {
        require(ownerOf(id) != address(0), "NFT Doesn't exists");
    }

    function setMarketplaceStatus(bool _marketplaceStatus) public onlyOwner{
        require(isMarketplaceOpen!=_marketplaceStatus,"New value cannot be same with previous value");
        isMarketplaceOpen = _marketplaceStatus;
    }

    function createLandToken(uint256 landType, address ref)
        public
        payable
        nonReentrant
    {
        require(startTime<=block.timestamp, "Cannot buy at the moment");
        require(_tokenIds.current() < totalSupply, "No Land Available");
        require(landType < collectionType.length, "Invalid Land Type");
        require(
            LAND_SUPPLY_SOLD[landType] < LAND_SUPPLY[landType],
            "Land not available for this type"
        );
        require(ref != msg.sender, "Cannot add your own account as Referral");
        uint256 mintPrice = landPrices[landType];
        require(msg.value == mintPrice, "Incorrect value of  Matic sent");

        string memory name = collectionType[landType];
        string memory imageURI = landImages[landType];
        string memory landDesc = landDescriptions[landType];
        LAND_SUPPLY_SOLD[landType] = LAND_SUPPLY_SOLD[landType].add(1);

        _tokenIds.increment();
        uint256 newItemId = _tokenIds.current();

        _safeMint(msg.sender, newItemId);
        Land memory land = Land({
            name: name,
            desc: landDesc,
            id: newItemId,
            imageURI: imageURI,
            ownerAddress: msg.sender,
            buyPrice: mintPrice,
            boughtDate: block.timestamp,
            currentPrice: mintPrice,
            isForSale: false,
            profitPerc: 10      ,
            collectionType: landType
        });

        nfts[_tokenIds.current()] = land;
        mintedSupply = _tokenIds.current();
        totalRevenue = totalRevenue.add(mintPrice);
        if (ref != msg.sender && ref != address(0)) {
            uint256 refAmount = mintPrice.mul(refPerc).div(TOTAL_PERC);
            refferalReward = refferalReward.add(refAmount);
            payable(ref).transfer(refAmount);
        }
    }

    function getNft(uint256 id) public view returns (Land memory) {
        onlyExistingNFT(id);
        Land memory land = nfts[id];
        land.currentPrice = getNFTPrice(id);
        return land;
    }

    function setName(uint256 id, string calldata _newName) public {
        onlyOwnerOfNft(id, msg.sender);
        require(
            !compareStrings(nfts[id].name, _newName),
            "New name cannot be same with previous name"
        );
        nfts[id].name = _newName;
    }

    function setDescription(uint256 id, string calldata _newDesc) public {
        onlyOwnerOfNft(id, msg.sender);
        require(
            !compareStrings(nfts[id].desc, _newDesc),
            "New description cannot be same with previous description"
        );
        nfts[id].desc = _newDesc;
    }

    function setPercentage(uint256 id, uint256 _newPerc)
        public
        onlyMarketplace
    {
        onlyExistingNFT(id);
        require(
            nfts[id].profitPerc != _newPerc,
            "New profit percentage cannot be same with previous percentage"
        );
        require(
            _newPerc >= 10,
            "Profit percentage cannot be less than 1 percent"
        );
        nfts[id].profitPerc = _newPerc;
    }

    function listForSale(uint256 id) public {
        require(isMarketplaceOpen,"Marketplace is not open at the moment");
        onlyOwnerOfNft(id, msg.sender);
        require(!nfts[id].isForSale, "Nft is already listed for sale");
        onlyExistingNFT(id);
        uint256 _currentPrice = getNFTPrice(id);
        nfts[id].currentPrice = _currentPrice;
        approve(marketplaceAddress, id);
        nfts[id].isForSale = true;
    }

    function updateNFTAfterSale(uint256 id) public onlyMarketplace {
        onlyExistingNFT(id);
        require(nfts[id].isForSale, "Nft is not listed for sale");
        nfts[id].isForSale = false;
        nfts[id].buyPrice = nfts[id].currentPrice;
        nfts[id].boughtDate = block.timestamp;
    }

    function setImageForNft(uint256 id, string calldata newImageUrl)
        public
        onlyMarketplace
    {
        require(
            !compareStrings(nfts[id].imageURI, newImageUrl),
            "New image cannot be same with previous image"
        );
        nfts[id].imageURI = newImageUrl;
    }

    function transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override {
        super.transferFrom(from, to, tokenId);
        nfts[tokenId].ownerAddress = to;
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public virtual override {
        super.safeTransferFrom(from, to, tokenId);
        nfts[tokenId].ownerAddress = to;
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 tokenId,
        bytes memory _data
    ) public virtual override {
        super.safeTransferFrom(from, to, tokenId, _data);
        nfts[tokenId].ownerAddress = to;
    }

    function approve(address to, uint256 tokenId) public virtual override {
        require(!nfts[tokenId].isForSale, "Cannot approve listed item");
        super.approve(to, tokenId);
    }

    function tokenURI(uint256 _id)
        public
        view
        override
        returns (string memory)
    {
        Land memory land = nfts[_id];
        string memory landName = collectionType[land.collectionType];
        return
            string(
                abi.encodePacked(
                    "data:application/json;base64,",
                    Base64.encode(
                        bytes(
                            abi.encodePacked(
                                '{"name":"',
                                land.name,
                                '", "image":"',
                                land.imageURI,
                                '", "attributes":['
                                '{"trait_type":"',
                                "profit Percentage",
                                '", "value":"',
                                Strings.toString(land.profitPerc.div(10)),
                                '"},',
                                '{"trait_type":"',
                                "Buy Price",
                                '", "value":"',
                                Strings.toString(land.buyPrice.div(1 ether)),
                                '"},',
                                '{"trait_type":"',
                                "Collection Type",
                                '", "value":"',
                                landName,
                                '"}'
                                "]"
                                "}"
                            )
                        )
                    )
                )
            );
    }

    function getNFTPrice(uint256 id) public view returns (uint256) {
        Land memory land = nfts[id];
        if (land.id == 0) return 0;

        if (land.isForSale) return land.currentPrice;
        uint256 difference = block.timestamp - land.boughtDate;
        uint256 profit = land.buyPrice.mul(land.profitPerc).mul(difference).div(
            priceIncrementTimeStep.mul(TOTAL_PERC)
        );
        uint256 nftPrice = land.buyPrice.add(profit);
        return nftPrice;
    }

    function isForSale(uint256 id) public view returns (bool) {
        onlyExistingNFT(id);
        return nfts[id].isForSale;
    }

    function setBenificiaryOneAddress(address _newAddress)
        public
        onlyBenificiaryOne
    {
        require(
            _newAddress != address(0) && _newAddress != benificiaryOneAddress,
            "New address cannot be null or same as previous address"
        );
        benificiaryOneAddress = _newAddress;
    }

    function setBenificiaryTwoAddress(address _newAddress)
        public
        onlyBenificiaryTwo
    {
        require(
            _newAddress != address(0) && _newAddress != benificiaryTwoAddress,
            "New address cannot be null or same as previous address"
        );
        benificiaryTwoAddress = _newAddress;
    }

    function setBenificiaryThreeAddress(address _newAddress)
        public
        onlyBenificiaryThree
    {
        require(
            _newAddress != address(0) && _newAddress != benificiaryThreeAddress,
            "New address cannot be null or same as previous address"
        );
        benificiaryThreeAddress = _newAddress;
    }

    function setMarketplaceAddress(address _newAddress) public onlyMarketplace {
        require(
            _newAddress != address(0) && _newAddress != marketplaceAddress,
            "New address cannot be null or same as previous address"
        );
        marketplaceAddress = _newAddress;
    }

    function setLandPrice(uint256 _newPrice, uint256 _index) public onlyOwner {
        require(
            _index < landPrices.length && landPrices[_index] != _newPrice,
            "New price cannot be same with previous price"
        );
        landPrices[_index] = _newPrice;
    }

    function setLandImage(string memory _imageUrl, uint256 _index)
        public
        onlyOwner
    {
        require(
            _index < landImages.length &&
                !compareStrings(landImages[_index], _imageUrl),
            "New Image value cannot be same with previous Image URL"
        );
        landImages[_index] = _imageUrl;
    }

    function setLandTypes(string memory _newName, uint256 _index)
        public
        onlyOwner
    {
        require(
            _index < collectionType.length &&
                !compareStrings(collectionType[_index], _newName),
            "New name value cannot be same with previous name value"
        );
        collectionType[_index] = _newName;
    }

    function setLandDescription(string memory _newDesc, uint256 _index)
        public
        onlyOwner
    {
        require(
            _index < landDescriptions.length &&
                !compareStrings(landDescriptions[_index], _newDesc),
            "New description value cannot be same with previous description value"
        );
        landDescriptions[_index] = _newDesc;
    }

    function withdraw() public anyBenificiary nonReentrant {
        uint256 balance = address(this).balance;
        uint256 profitFor40Perc = balance.mul(benificiaryOneShare).div(TOTAL_PERC);
        uint256 profitFor20Perc = balance.mul(benificiaryThreeShare).div(TOTAL_PERC);
        payable(benificiaryOneAddress).transfer(profitFor40Perc);
        payable(benificiaryTwoAddress).transfer(profitFor40Perc);
        payable(benificiaryThreeAddress).transfer(profitFor20Perc);
    }

    function compareStrings(string memory a, string memory b)
        private
        pure
        returns (bool)
    {
        return (keccak256(abi.encodePacked((a))) ==
            keccak256(abi.encodePacked((b))));
    }
}

