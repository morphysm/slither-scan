pragma solidity ^0.8.14;

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/utils/SafeERC20.sol";
import "@openzeppelin/contracts/token/ERC721/utils/ERC721Holder.sol";
import "@openzeppelin/contracts/token/ERC1155/utils/ERC1155Holder.sol";
import "@openzeppelin/contracts/utils/Counters.sol";
import "../libraries/IRewardReceiver.sol";

contract LoadMarketplace is ReentrancyGuard, Ownable, ERC721Holder, ERC1155Holder {
    event TokenOnSale(
        address indexed seller,
        address indexed nftContract,
        uint256 indexed nftId,
        uint256 price,
        uint256 category,
        uint256 saleIndex,
        bool isERC1155,
        uint256 quantity
    );
    event TokenRemovedFromSale(uint256 indexed saleIndex);
    event TokenSold(address indexed buyer, uint256 saleIndex);
    event NewCategoryAdded(uint256 categoryId, string _categoryName);
    event NewCollectionAdded(address collectionContract, uint256 _category);
    event CollectionRemoved(address collectionContract);
    event RewarderContractSet(address indexed user, address _rewarderContract);

    using EnumerableSet for EnumerableSet.AddressSet;
    using Counters for Counters.Counter;
    using SafeERC20 for IERC20;

    EnumerableSet.AddressSet private collections;
    mapping(uint256 => string) public categories;
    mapping(address => uint256) public collectionCategory;
    mapping(address => bool) private isERC1155;

    struct SaleNFT {
        uint256 tokenId;
        uint256 price;
        address seller;
        uint256 category;
        address tokenContract;
        bool isERC1155;
        uint256 quantity;
    }

    mapping(uint256 => SaleNFT) saleNFTList; // id => SaleNFT
    Counters.Counter private saleIds;

    /* ========== VARIABLES ========== */
    address public immutable DCAU_TOKEN_CONTRACT;
    address public immutable DCAR_TOKEN_CONTRACT;

    address public immutable DCG_EQUIPMENT_CONTRACT;
    address public immutable DCG_RESOURCES_CONTRACT;
    address public immutable DCG_CONSUMABLES_CONTRACT;
    address public immutable DCG_HERO_CONTRACT;

    address public immutable DCAR_BONDS_CONTRACT;
    address public immutable TINY_DRAGONS_CONTRACT;
    address public immutable DRAGON_NEST_CONTRACT;

    uint256 public immutable COMMISSION_RATE;
    uint256 public onSaleNftAmount;
    uint256 public categoryLength;
    uint256 public numCollections = 0;

    address public REWARDER_CONTRACT;

    /* ========== CONSTRUCTOR ========== */
    constructor(
        address _dcauTokenContract,
        address _dcarTokenContract,
        address _equipmentContract,
        address _resourcesContract,
        address _consumablesContract,
        address _heroContract,
        address _dcarBondsContract,
        address _tinyDragonsContract,
        address _dragonNestContract,
        address _rewarderContract,
        uint256 _commissionRate
    ) {
        require(_commissionRate < 10000, "rate too high");

        DCAU_TOKEN_CONTRACT = _dcauTokenContract;
        DCAR_TOKEN_CONTRACT = _dcarTokenContract;
        REWARDER_CONTRACT = _rewarderContract;

        DCG_EQUIPMENT_CONTRACT = _equipmentContract;
        DCG_RESOURCES_CONTRACT = _resourcesContract;
        DCG_CONSUMABLES_CONTRACT = _consumablesContract;
        DCG_HERO_CONTRACT = _heroContract;
        DCAR_BONDS_CONTRACT = _dcarBondsContract;
        TINY_DRAGONS_CONTRACT = _tinyDragonsContract;
        DRAGON_NEST_CONTRACT = _dragonNestContract;

        COMMISSION_RATE = _commissionRate;

        categories[1] = "Heroes";
        categories[2] = "Equipment";
        categories[3] = "Resources";
        categories[4] = "Consumables";
        categories[5] = "Tiny Dragons";
        categories[6] = "Dragon Nest";
        categories[7] = "DCAR Bonds";

        categoryLength = 7;

        addCategoryContract(_heroContract, 1);
        addCategoryContract(_equipmentContract, 2);
        addCategoryContract(_resourcesContract, 3);
        addCategoryContract(_consumablesContract, 4);
        addCategoryContract(_tinyDragonsContract, 5);
        addCategoryContract(_dragonNestContract, 6);
        addCategoryContract(_dcarBondsContract, 7);

        isERC1155[_resourcesContract] = true;
        isERC1155[_consumablesContract] = true;
    }

    /* ========== VIEW FUNCTIONS ========== */

    function isWhitelistedCollection(address stakeContract) public view returns (bool) {
        return collections.contains(stakeContract);
    }

    function getWhitelistedCollection(uint256 index) external view returns (address) {
        return collections.at(index);
    }

    function getSaleNFTLength() external view returns (uint256) {
        return saleIds.current();
    }

    function getSale(uint256 index)
        external
        view
        returns (
            address tokenContract,
            uint256 tokenId,
            uint256 price,
            address seller,
            uint256 category,
            uint256 quantity
        )
    {
        require(index < saleIds.current(), "Out of array");
        require(saleNFTList[index].seller != address(0), "This sale is no longer active");

        uint256 nftQuantity = saleNFTList[index].isERC1155 ? saleNFTList[index].quantity : 1;

        return (
            saleNFTList[index].tokenContract,
            saleNFTList[index].tokenId,
            saleNFTList[index].price,
            saleNFTList[index].seller,
            saleNFTList[index].category,
            nftQuantity
        );
    }

    /* ========== MARKET FUNCTIONS ========== */
    function saleNFT(
        address _tokenContract,
        uint256 _tokenId,
        uint256 _price,
        uint256 _quantity
    ) external nonReentrant {
        require(isWhitelistedCollection(_tokenContract), "Collection not whitelisted!");
        require(_price > 0, "Price cannot be zero");

        uint256 _saleId = saleIds.current();

        bool isERC1155Token = isERC1155[_tokenContract];

        if (!isERC1155Token) {
            IERC721 nftTokenContract = IERC721(_tokenContract);
            nftTokenContract.transferFrom(msg.sender, address(this), _tokenId);
        } else {
            require(_quantity > 0, "Must have a quantity");
            IERC1155 nftTokenContract = IERC1155(_tokenContract);
            nftTokenContract.safeTransferFrom(msg.sender, address(this), _tokenId, _quantity, "");
        }

        uint256 _category = collectionCategory[_tokenContract];
        saleNFTList[_saleId] = SaleNFT(_tokenId, _price, msg.sender, _category, _tokenContract, isERC1155Token, _quantity);
        onSaleNftAmount++;

        saleIds.increment();
        emit TokenOnSale(msg.sender, _tokenContract, _tokenId, _price, _category, _saleId, isERC1155Token, _quantity);
    }

    function removeNFT(uint256 index) external nonReentrant {
        require(saleIds.current() > index, "Out of array length");
        require(saleNFTList[index].seller == msg.sender, "You must be the seller of this NFT");

        if (!saleNFTList[index].isERC1155) {
            IERC721 nftTokenContract = IERC721(saleNFTList[index].tokenContract);
            nftTokenContract.transferFrom(address(this), msg.sender, saleNFTList[index].tokenId);
        } else {
            IERC1155 nftTokenContract = IERC1155(saleNFTList[index].tokenContract);
            nftTokenContract.safeTransferFrom(
                address(this),
                msg.sender,
                saleNFTList[index].tokenId,
                saleNFTList[index].quantity,
                ""
            );
        }

        delete saleNFTList[index];
        onSaleNftAmount--;

        emit TokenRemovedFromSale(index);
    }

    function buyNFT(uint256 index) external nonReentrant {
        require(saleIds.current() > index, "Out of array length");
        require(saleNFTList[index].seller != address(0), "This sale is no longer active");

        SaleNFT memory onSaleNft = saleNFTList[index];

        IRewardReceiver rewardReceiverContract = IRewardReceiver(REWARDER_CONTRACT);

        uint256 commissionAmount = (onSaleNft.price * COMMISSION_RATE) / 10000;
        uint256 sellerAmount = onSaleNft.price - commissionAmount;

        require(sellerAmount > 0, "Seller amount must be bigger than zero");

        if (!onSaleNft.isERC1155) {
            IERC20 dcauContract = IERC20(DCAU_TOKEN_CONTRACT);
            IERC721 nftTokenContract = IERC721(onSaleNft.tokenContract);
            nftTokenContract.transferFrom(address(this), msg.sender, onSaleNft.tokenId);

            if (commissionAmount > 0) {
                dcauContract.safeTransferFrom(msg.sender, REWARDER_CONTRACT, commissionAmount);
                rewardReceiverContract.receiveTokens(DCAU_TOKEN_CONTRACT, commissionAmount);
            }

            dcauContract.safeTransferFrom(msg.sender, onSaleNft.seller, sellerAmount);
        } else {
            IERC20 dcarContract = IERC20(DCAR_TOKEN_CONTRACT);
            IERC1155 nftTokenContract = IERC1155(onSaleNft.tokenContract);
            nftTokenContract.safeTransferFrom(address(this), msg.sender, onSaleNft.tokenId, onSaleNft.quantity, "");

            if (commissionAmount > 0) {
                dcarContract.safeTransferFrom(msg.sender, REWARDER_CONTRACT, commissionAmount);
                rewardReceiverContract.receiveTokens(DCAR_TOKEN_CONTRACT, commissionAmount);
            }

            dcarContract.safeTransferFrom(msg.sender, onSaleNft.seller, sellerAmount);
        }

        delete saleNFTList[index];
        onSaleNftAmount--;

        emit TokenSold(msg.sender, index);
    }

    /* ========== RESTRICTED FUNCTIONS ========== */

    function setRewarderContract(address _rewarderContract) external onlyOwner {
        require(_rewarderContract != REWARDER_CONTRACT, "New commission taker must be different from old one");
        require(_rewarderContract != address(0), "Commissiontaker cannot be zero address");
        REWARDER_CONTRACT = _rewarderContract;

        emit RewarderContractSet(msg.sender, _rewarderContract);
    }

    function addNewCategory(string calldata _categoryName) external onlyOwner {
        categories[categoryLength + 1] = _categoryName;
        categoryLength = categoryLength + 1;

        emit NewCategoryAdded(categoryLength, _categoryName);
    }

    function addWhitelistedCollection(
        address collectionContract,
        uint256 _category,
        bool _isERC1155
    ) external onlyOwner {
        require(collectionContract != address(0), "addWhitelistedCollection: collectionContract cannot be the zero address");
        require(isWhitelistedCollection(collectionContract) == false, "addWhitelistedCollection: collection already whitelisted");
        require(_category <= categoryLength, "addWhitelistedCollection: Not listed category");

        isERC1155[collectionContract] = _isERC1155;
        addCategoryContract(collectionContract, _category);
    }

    function addCategoryContract(address contractAddress, uint256 category) internal {
        collections.add(contractAddress);
        collectionCategory[contractAddress] = category;

        numCollections++;

        emit NewCollectionAdded(contractAddress, category);
    }

    function removeWhitelistedCollection(address collectionContract) external onlyOwner {
        require(isWhitelistedCollection(collectionContract), "removeWhitelistedCollection: collection not whitelisted");

        collections.remove(collectionContract);
        collectionCategory[collectionContract] = 0;

        numCollections--;

        emit CollectionRemoved(collectionContract);
    }
}

