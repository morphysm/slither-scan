contract ERC721aMetadataRoyalty is ERC721aMetadata, IERC721aMetadataRoyalty {
    RoyaltyInfo private _defaultRoyaltyInfo;
    mapping(uint256 => RoyaltyInfo) private _tokenRoyaltyInfo;

    /**
     * @dev See {IERC165-supportsInterface}.
     */
