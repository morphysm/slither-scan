pragma solidity ^0.8.12;

import "./ERC721aUpgradeable.sol";
import "./interface/IERC721aNFTPackage.sol";

contract ERC721aNFTPackage is ERC721aUpgradeable, IERC721aNFTPackage {
    mapping(string => Package) public packages;
    mapping(string => uint256) private _packageCount;
    uint256 private _totalSupply;
    bool private _publicSale;

    function initialize(string memory name_, string memory symbol_, address owner_) public virtual override initializer {
        __ERC721aUpgradeable_init(name_, symbol_, owner_);
    }

    function supportsInterface(bytes4 interfaceId) public view override(IERC165, ERC721aMetadataRoyalty) returns (bool) {
        return interfaceId == type(IERC721aNFTPackage).interfaceId || super.supportsInterface(interfaceId);
    }

    function _setPackage(Package memory pkg) internal {
        require(0 == _packageCount[pkg.name], "400");
        
        _totalSupply -= packages[pkg.name].totalSupply;
        packages[pkg.name] = pkg;
        _totalSupply += pkg.totalSupply;
    }

    function setPublicSale(bool publicSale_) external virtual override onlyOwner {
        _publicSale = publicSale_;
    }

    function setPackages(Package[] memory pkgs) public virtual override onlyOwner {
        for (uint256 index = 0; index < pkgs.length; index++) {
            _setPackage(pkgs[index]);
        }
    }

    function totalSupply() public view virtual override(IERC721aMetadata, ERC721aMetadata) returns (uint256) {
        return _totalSupply;
    }

    function totalMinted() external view returns (uint256) {
        return super._totalMinted();
    }

    function packageCount(string memory pkg) public view override returns (uint256) {
        return _packageCount[pkg];
    }

    function packageUnitPrice(string memory pkg) public view override returns (uint256) {
        return packages[pkg].unitPrice;
    }

    function packageName(uint256 tokenId) public view override returns (string memory) {
        return _ownershipOf(tokenId).metadata;
    }

    function _allowMint(address sender) internal virtual returns (bool) {
        return !_publicSale && hasRole(MINTER_ROLE, sender);
    }

    function _mintPackage(address to, uint256 quantity, string memory pkg) internal virtual {
        require(packageCount(pkg) + quantity <= packages[pkg].totalSupply, "429");
        
        _packageCount[pkg] += quantity;
        super._mint(to, quantity, pkg);
    }

    function mint(address to, uint256 quantity, string memory pkg) external virtual override {
        require(_allowMint(msg.sender), "403");
        
        _mintPackage(to, quantity, pkg);
    }

    function mintBatch(address to, uint256[] memory quantity, string[] memory pkgs) external virtual override {
        require(_allowMint(msg.sender), "403");

        for (uint256 index = 0; index < pkgs.length; index++) {
            _mintPackage(to, quantity[index], pkgs[index]);
        }
    }
}

