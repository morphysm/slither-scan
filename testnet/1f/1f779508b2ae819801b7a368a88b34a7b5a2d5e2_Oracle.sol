pragma solidity ^0.8.0;

// L I B R A R I E S






contract Oracle is Ownable {

  IJoeRouter01 public joeV2Router; //0xe968f7eB387A6a374c1ff58955fa3a53E5B46c31
  address public wavax; //0xd00ae08403B9bbb9124bB305C09058E32C39A48c
  address public thor;  //0xb529782800e4a0feac1A01aE410E99E90c4C28AB
  address public usdc;  //0x5425890298aed601595a70AB815c96711a31Bc65

  constructor(address _wavax, address _thor, address _usdc, address _uniV2Router) {
      joeV2Router = IJoeRouter01(_uniV2Router);
      wavax = _wavax;
      thor = _thor;
      usdc = _usdc;
  }

  function updateTokens(address _wavax, address _thor, address _usdc) external onlyOwner {
      wavax = _wavax;
      thor = _thor;
      usdc = _usdc;
  }

  function updateRouter( address _uniV2Router) external onlyOwner {
    joeV2Router = IJoeRouter01(_uniV2Router);
  }


  function thor2Avax(uint256 _amountIn) external view returns (uint256){
      address[] memory path = new address[](2);
      path[0] = address(thor);
      path[1] = address(wavax);
      return joeV2Router.getAmountsOut(_amountIn, path )[1];
  }

  function avax2thor(uint256 _amountIn) external view returns (uint256){
    address[] memory path = new address[](2);
    path[0] = address(wavax);
    path[1] = address(thor);
    return  joeV2Router.getAmountsOut(_amountIn, path )[1];
  }

  function thor2Usd(uint256 _amountIn) external view returns (uint256){
    address[] memory path = new address[](3);
    path[0] = address(thor);
    path[1] = address(wavax);
    path[2] = address(usdc);
    return joeV2Router.getAmountsOut(_amountIn, path)[2];
  }

}