  function setWhitelistUser(address _user) public onlyOwner {
    whitelisted[_user].whitelisted = true; // winner address;
  }

  function setWhitelistUsers(address[] memory _users) public onlyOwner {
    for (uint256 i = 0; i < _users.length; i++) {
      setWhitelistUser(_users[i]);
    }
  }

  function removeWhitelistUser(address _user) public onlyOwner {
    delete whitelisted[_user];
  }

  function pickWinner() private {
    require(allTransfersArray.length > 0);
    drawWinner(false); 
  }

  function redrawWinner(uint256 round) external onlyOwner {
    require(winners[round].winnerAddress == address(0));
    drawWinner(true);
    redrawRound = round;
  }

  function distribute(address receiver, uint256 balanceAmount) private {
    (bool success,) = address(receiver).call{ value: balanceAmount }("");

    require(success, "Transfer failed.");
  }

  function distributeToDev(uint256 balanceAmount) private {
    (bool success,) = address(_devWalletContract).call{ value: balanceAmount }("");

    require(success, "Transfer failed.");
  }


  function withdrawTokens(IERC20 token) public onlyOwner {
    uint256 funds = token.balanceOf(address(this));
    require(funds != 0, 'Has no token left');
    token.transfer(owner(), funds);
  }

  function withdrawBalance() public onlyOwner {
    require(address(this).balance != 0, 'Has no MATIC left');

    distributeToDev(address(this).balance);
  }

