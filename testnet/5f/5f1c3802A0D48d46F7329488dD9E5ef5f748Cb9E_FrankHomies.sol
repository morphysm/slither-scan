  function mint(uint256 _mintAmount) 
    public
    payable
  {
    uint256 supply = totalSupply();
    require(supply + _mintAmount <= MAX_TOKENS, "Exceeds maximum tokens available for purchase");

    if (msg.sender != owner()) {
        require(_mintAmount > 0 && _mintAmount <= MAX_TOKENS_PER_PURCHASE, "Exceeds maximum tokens you can purchase in a single transaction");

        if(whitelisted[msg.sender].whitelisted != true || (whitelisted[msg.sender].whitelisted == true && whitelisted[msg.sender].claimed == true)) {
          require(msg.value >= price * _mintAmount, "INVALID_AMOUNT");
          require(address(this).balance > 0, "No Balance to send");
        }

        if (whitelisted[msg.sender].whitelisted == true && whitelisted[msg.sender].claimed != true) {
          require(_mintAmount == 1, "WHITELISTED CANNOT MINT MORE THAN ONE");
        }
    }
    _bulkMint(supply, _mintAmount);

    if (whitelisted[msg.sender].whitelisted == true) {
      whitelisted[msg.sender].claimed = true;
    }
  }

  function _bulkMint(uint256 supply, uint256 _mintAmount) private {
    for (uint256 i = 0; i < _mintAmount; i++) {
      uint256 tokenId = supply + i;
      _safeMint(msg.sender, tokenId);
      _setTokenURI(tokenId, 'frankhomies-cover.json'); // show the base version first
      emit TokenMinted(msg.sender, tokenId);

      if (ROUNDS[tokenId + 1]) {
        pickWinner();
      }
    }
    emit TokensMinted(msg.sender, _mintAmount);
  }

