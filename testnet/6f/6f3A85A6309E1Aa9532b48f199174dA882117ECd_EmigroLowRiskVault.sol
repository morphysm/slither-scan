pragma solidity 0.8.10;

import {SafeTransferLib} from "solmate/utils/SafeTransferLib.sol";
import "./abstract/ERC4626.sol";
import "./LowRiskStrategy.sol";

contract EmigroLowRiskVault is ERC4626 {
    using SafeTransferLib for ERC20;

    LowRiskStrategy public immutable strategy;
    uint256 public totalStrategyHoldings;

    constructor(
        ERC20 _underlying,
        string memory _name,
        string memory _symbol,
        LowRiskStrategy _strategy
    ) ERC4626(_underlying, _name, _symbol) {
        strategy = _strategy;
    }

    function totalHoldings() public view override returns (uint256) {
        return underlying.balanceOf(address(this));
    }

    function beforeWithdraw(uint256 underlyingAmount) internal override returns(uint) {
       return withdrawFromStrategy(underlyingAmount);
    }

    function afterDeposit(uint256 underlyingAmount) internal override {
        depositIntoStrategy(underlyingAmount);
    }

    // function setStrategy(address _strategyAddress) {

    // }

    /// @notice Emitted after the Vault deposits into a strategy contract.
    /// @param user The authorized user who triggered the deposit.
    /// @param strategy The strategy that was deposited into.
    /// @param underlyingAmount The amount of underlying tokens that were deposited.
    event StrategyDeposit(
        address indexed user,
        LowRiskStrategy indexed strategy,
        uint256 underlyingAmount
    );

    /// @notice Emitted after the Vault withdraws funds from a strategy contract.
    /// @param user The authorized user who triggered the withdrawal.
    /// @param strategy The strategy that was withdrawn from.
    /// @param underlyingAmount The amount of underlying tokens that were withdrawn.
    event StrategyWithdrawal(
        address indexed user,
        LowRiskStrategy indexed strategy,
        uint256 underlyingAmount
    );

    /// @notice Deposit a specific amount of float into a trusted strategy.
    /// @param underlyingAmount The amount of underlying tokens in float to deposit.
    function depositIntoStrategy(uint256 underlyingAmount) internal {
        // Increase totalStrategyHoldings to account for the deposit.
        //TODO add overflow protection here
        totalStrategyHoldings += underlyingAmount;

        emit StrategyDeposit(msg.sender, strategy, underlyingAmount);

        // Approve underlyingAmount to the strategy so we can deposit.
        underlying.safeApprove(address(strategy), underlyingAmount);

        // Deposit into the strategy and revert if it returns an error code.
        require(
            LowRiskStrategy(address(strategy)).mint(underlyingAmount) == 0,
            "MINT_FAILED"
        );
    }

    /// @notice Withdraw a specific amount of underlying tokens from a strategy.
    /// @param underlyingAmount  The amount of underlying tokens to withdraw.
    /// @dev Withdrawing from a strategy will not remove it from the withdrawal stack.
    function withdrawFromStrategy(uint256 underlyingAmount) internal returns(uint){
        // Decrease totalStrategyHoldings to account for the withdrawal.
        // Cannot underflow as the balance of one strategy will never exceed the sum of all.
        //TODO add underflow protection here
        totalStrategyHoldings -= underlyingAmount;

        emit StrategyWithdrawal(msg.sender, strategy, underlyingAmount);

        // Withdraw from the strategy and revert if it returns an error code.
        (uint errorCode, uint withdrawnUTokens) = LowRiskStrategy(address(strategy)).redeemUnderlying(
                underlyingAmount
            );
        require(
            errorCode == 0,
            "REDEEM_FAILED"
        );
        return withdrawnUTokens;
    }
}

