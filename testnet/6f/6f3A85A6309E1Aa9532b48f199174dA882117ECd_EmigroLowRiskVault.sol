/* For deployment, check out the rari vault/vault factory contract templates and scripts that they use
1. deploy strategy contract with the address of the erc20 we will use
2. use the strategy contract's addres and add it to the underlying vault's trusted strategies
3. we can then interact with that strategy through the vault since all strategies have the same mint (deposit) and redeemUnderlying(withdraw) functions
*/


//TODO discuss what to name the token on below line
contract LowRiskStrategy is ERC20("Emigro Low Risk Strategy", "ELRS", 18), ERC20Strategy {
    using SafeTransferLib for ERC20;
    using FixedPointMathLib for uint256;

    //@dev Benqi QiTokenInterface
    //@dev https://snowtrace.io/token/0x5c0401e81bc07ca70fad469b451682c0d747ef1c
    QiTokenInterface qiToken = QiTokenInterface(0xBEb5d47A3f720Ec0a390d04b4d41ED7d9688bC7F);


    constructor(ERC20 _UNDERLYING) {
        UNDERLYING = _UNDERLYING;

        BASE_UNIT = 10**_UNDERLYING.decimals();
    }


    // Benqi Events
    event BenqiDeposit(address indexed accountAddress, uint uTokenAmount, uint qiTokensReturned);
    event BenqiWithdrawal(address indexed userAddress, uint qiTokensAmount, uint uTokensReturned);

    /*///////////////////////////////////////////////////////////////
                             STRATEGY LOGIC
    //////////////////////////////////////////////////////////////*/

    function isCEther() external pure override returns (bool) {
        return false;
    }

    function underlying() external view override returns (ERC20) {
        return UNDERLYING;
    }

    function mint(uint256 amount) external override returns (uint256) {
        // minting strategy tokens to vault contract anytime we receive a deposit to vault address
         _mint(msg.sender, amount.fdiv(exchangeRate(), BASE_UNIT));

        // transfer tokens to contract from sender
        UNDERLYING.safeTransferFrom(msg.sender, address(this), amount);
        depositBenqiERC20(amount);
        return 0;
    }

    //  Deposit ERC20 uToken into Benqi QI token contract, get back cTokens and store in Savings contract balance for withdrawal later
    function depositBenqiERC20(uint _uTokenAmount) internal {
        //
        //Transfer uTokens to savings contract from the user, then approve for spend by benqi
        // uToken.transferFrom(msg.sender, address(this), _uTokenAmount); dont need this as money is transfered in from mint function
        UNDERLYING.approve(address(qiToken), _uTokenAmount);

        //Deposit to benqi and calculate change in cToken balance to determine what we got back
        uint qiTokenBalanceBefore = qiToken.balanceOf(address(this));
        require(qiToken.mint(_uTokenAmount) == 0, "mint failed");
        uint qiTokenBalanceAfter = qiToken.balanceOf(address(this));
        uint qiTokensReturned = qiTokenBalanceAfter - qiTokenBalanceBefore;
        //TODO to track user qiTokens, we may want to return qiTokensReturned and add that to the value on the vault contract
        emit BenqiDeposit(msg.sender, _uTokenAmount, qiTokensReturned);
    }

    function redeemUnderlying(uint256 amount) external override returns (uint256, uint256) {
        uint withdrawnUTokens = withdrawBenqiERC20(amount);
        // console.log("burn panic code cause: %s", amount.fdiv(exchangeRate(), BASE_UNIT));
        // console.log("withdrawnUSDCe: %s", withdrawnUTokens);
        _burn(msg.sender, amount.fdiv(exchangeRate(), BASE_UNIT));

        UNDERLYING.safeTransfer(msg.sender, withdrawnUTokens);

        return (0, withdrawnUTokens);
    }

    function withdrawBenqiERC20(uint _qiTokenAmount) internal returns(uint) {
        //error handling
        require(_qiTokenAmount > 0, "amount is zero");

        uint redemptionAmount = (_qiTokenAmount.fdiv(totalSupply, BASE_UNIT)).fmul(qiToken.balanceOf(address(this)), BASE_UNIT);
        //TODO: Is this the best way to work out how many uTokens we exchanged our cTokens for?
        uint uBalanceBefore = UNDERLYING.balanceOf(address(this));
        require(qiToken.redeem(redemptionAmount) == 0, "redeem failed");
        uint uBalanceAfter = UNDERLYING.balanceOf(address(this));
        uint uTokensReturned = uBalanceAfter - uBalanceBefore;
        //TODO: error handle transfer
        console.log("uTokensReturned: %s", uTokensReturned);
        emit BenqiWithdrawal(msg.sender, _qiTokenAmount, uTokensReturned);
        return uTokensReturned;
    }

    function balanceOfUnderlying(address user) external view override returns (uint256) {
        return balanceOf[user].fmul(exchangeRate(), BASE_UNIT);
    }


    /*///////////////////////////////////////////////////////////////
                            INTERNAL LOGIC
    //////////////////////////////////////////////////////////////*/

    ERC20 internal immutable UNDERLYING;

    uint256 internal immutable BASE_UNIT;

    function exchangeRate() internal view returns (uint256) {
        uint256 cTokenSupply = totalSupply;

        if (cTokenSupply == 0) return BASE_UNIT;

        return UNDERLYING.balanceOf(address(this)).fdiv(cTokenSupply, BASE_UNIT);
    }
}

