// SPDX-License-Identifier: UNLICENSED
pragma solidity ^0.8.4;

import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "hardhat/console.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

/// @title Stake BNB Contract.
/// @notice This contract accepts BNB and provides interest and perks.
contract StakingBNB is Ownable {
    // DECLARATION
    using SafeMath for uint256;
    using Counters for Counters.Counter;

    // OWNER
    address public treasury;
    bool public isActive;

    // CONSTANTS
    uint256 MINIMUM_CONTRIBUTION = 10000000000000000; // 0.01 BNB
    uint256 MAXIMUM_CONTRIBUTION = 500000000000000000000; // 500 BNB
    //uint256 MAXIMUM_STAKING_PERIOD = 250 days;
    uint256 MAXIMUM_REFERRAL_STACK = 5;
    uint256 PERFORMANCE_FEE_CHECK = 100000000000000000000; // 100 BNB
    uint256 MAX_LEVELS = 25;

    // GLOBAL VERIABLES
    Counters.Counter private stakeId;
    uint256[] public refLvlRate = [10, 5, 3, 2, 1];

    // STRUCTS - It is use to store individual stakes for each user.
    // it sores details of user each deposit.
    struct UserFunds {
        uint256 id;
        uint256 balance; // net balance after deducting fee
        uint256 createdAt;
        uint256 lastWithdrawal;
        // uint256 timeLeft; // not used now, will be removed.
    }

    // stores user details.
    struct UserDetails {
        address refferedBy; // parent node
        ReferalLevels referrals; // child nodes
        bool isRegistered; // checks user registration
        bool isSuspended; // checks user suspension
    }

    // stores user balances.
    struct UserBalance {
        uint256 totalBalance; //cumulative balance
        uint256 balance; // net balance after deducting 10% fees.
        uint256 ownEarnings; // daily interest on staked amount.
        uint256 oeTs; // when last earning has been calculated.
        uint256 totalRefEarnings; // Ref Earnings + Ref Bonus + Withdrawal Commission
        uint256 tRefTs;
        Withdrawals totalWithdrawal;
        uint256 maxWithdrawal;
        uint256 withdrawalCommission; // commission earned when a direct referall withdrawas
        uint256 referralBonus;
    }

    // stores user withdrawals.
    struct Withdrawals {
        uint256[] amountWithdrawan;
        uint256[] twTs;
    }

    // stores user referrals.
    struct ReferalLevels {
        address[][25] refAddrs; // stores all addresses of referrals.
        uint256[25] refBalances; // referral earnings for each level. it gets updated when the lower level referrer deposits some amount.
        uint256[25] rbTs; // Time Stamp get updated when lower level user depoisits some amount.
        uint256[25] referralEarnings;
        uint256[25] reTs;
    }

    // MAPPING
    mapping(address => UserBalance) public stakedBalance; // all user balances.
    mapping(address => UserFunds[]) public userFundDetails; // all deposited/staked funds by user.
    mapping(uint256 => uint256) public levelInfo; // minimum balance for each level to get commission.
    mapping(address => UserDetails) public userDetails;

    // EVENTS
    event STAKED(address indexed staker, uint256 indexed amount);
    event WITHDRAW(address indexed withdrawer, uint256 indexed amount);
    event REGISTERED(address _userAdd, address _referredBy);

    /** @dev Constructor.
     * @param _treasury Account/contract address which receives owner's commission.
     */
    constructor(address _treasury) {
        require(_treasury != address(0), "Treasury add can't be null");
        treasury = _treasury;
        isActive = true;

        levelInfo[1] = 10000000000000000;
        levelInfo[2] = 100000000000000000;
        levelInfo[3] = 1000000000000000000;
        levelInfo[4] = 5000000000000000000;
        levelInfo[5] = 10000000000000000000;

        userDetails[msg.sender].refferedBy = address(0);
        userDetails[msg.sender].isRegistered = true;
        userDetails[msg.sender].isSuspended = false;
    }

    /** @notice Flip the active tag to false and will pause the contract
     * @dev pause the contract.
     */
    function pauseContract() public onlyOwner {
        require(isActive, "Already paused");
        isActive = false;
    }

    /** @notice Flip the active tag to true and will active the contract again.
     * @dev activate the contract.
     */
    function activeContract() public onlyOwner {
        require(!isActive, "Already Actived");
        isActive = true;
    }

    /** @notice user registration
     * @dev -
     * @param _referrer referrer address.
     */
    function registerUser(address _referrer)
        public
        isContractActive
        isNotSuspendedUser
    {
        require(
            userDetails[msg.sender].isRegistered != true,
            "User already registered"
        );
        require(
            userDetails[_referrer].isRegistered,
            "Referrer is not registered"
        );

        _registerUser(_referrer);
    }

    /** @notice staking BNB function.
     * @dev stakes the msg.value.
     */
    function stakeBnb()
        public
        payable
        isContractActive
        isRegisteredUser
        isNotSuspendedUser
    {
        address _user = msg.sender;
        uint256 _value = msg.value;
        require(
            _value >= MINIMUM_CONTRIBUTION,
            "Staking amount should be greater than 0.01 BNB"
        );
        require(
            _value <= MAXIMUM_CONTRIBUTION,
            "Staking amount should be less than 500 BNB"
        );
        if (_value > stakedBalance[_user].maxWithdrawal) {
            stakedBalance[_user].maxWithdrawal = _value;
        }

        UserFunds memory newUserStake;
        uint256 amountAfterDeduction = (_value * 9) / 10;

        calculateDailyEarnings(_user);

        stakedBalance[_user].totalBalance = stakedBalance[_user]
            .totalBalance
            .add(_value);
        stakedBalance[_user].balance = stakedBalance[_user].balance.add(
            amountAfterDeduction
        );

        newUserStake.balance = amountAfterDeduction;
        newUserStake.createdAt = block.timestamp;
        newUserStake.lastWithdrawal = 0 days;
        // newUserStake.timeLeft = 250 days;
        newUserStake.id = stakeId.current();
        stakeId.increment();

        userFundDetails[_user].push(newUserStake);
        _updateRefBalance(_value);
        _deductDepositFees(_value);

        emit STAKED(_user, _value);
    }

    /** @notice update referral balance of all referred user balance. i.e. from bottom to top updates balance for all users.
     * @dev internal function.
     * @param _amount balance to be updated.
     */
    function _updateRefBalance(uint256 _amount) internal {
        // Need to check till max referral allowed and add in refferal level wise
        address _referredBy = msg.sender;
        for (uint256 i = 0; i < MAXIMUM_REFERRAL_STACK; i++) {
            _referredBy = userDetails[_referredBy].refferedBy;
            uint256 _curLvlBalance = userDetails[_referredBy]
                .referrals
                .refBalances[i];
            uint256 _rate;
            if (isActive && !userDetails[_referredBy].isSuspended) {
                _rate = refLvlRate[i];
            } else if (!isActive || userDetails[_referredBy].isSuspended) {
                _rate = 0;
            }
            console.log("_updateRefBalance ~ _rate", _referredBy, _rate);

            uint256 _earnings = (_amount * _rate) / (100);
            userDetails[_referredBy].referrals.refBalances[i] =
                _curLvlBalance +
                _amount;
            userDetails[_referredBy].referrals.rbTs[i] = block.timestamp;

            uint256 _totalRefEar = userDetails[_referredBy]
                .referrals
                .referralEarnings[i];
            userDetails[_referredBy].referrals.referralEarnings[i] =
                _totalRefEar +
                _earnings;
            userDetails[_referredBy].referrals.reTs[i] = block.timestamp;

            if (_isAdminOrNullAddress(_referredBy)) {
                return;
            }
        }
    }

    /** @notice returns daily earning rate of a user as per the user staked balance.
     * @dev internal function.
     * @param _amount user balance.
     * @return _roi daily reate of interest.
     */
    function _getDailyEarningsRate(uint256 _amount)
        internal
        view
        returns (uint256 _roi)
    {
        if (isActive) {
            if (_amount >= 251 * 10**18) {
                return 150;
            } else if (_amount >= 101 * 10**18) {
                return 125;
            } else if (_amount > 0.01 * 10**18) {
                return 100;
            }
        } else if (!isActive || userDetails[msg.sender].isSuspended) {
            return 0;
        }
    }

    /** @notice calculates the daily earnings of a
     * @dev -
     * @param _user user address.
     * @return _earnings user net earnings.
     */
    function calculateDailyEarnings(address _user)
        public
        isContractActive
        isRegisteredUser
        returns (uint256 _earnings)
    {
        // check earnings rate
        UserFunds[] memory uf = userFundDetails[_user];
        uint256 _dailyEarnings = stakedBalance[_user].ownEarnings;

        uint256 _lastIndex = 0;
        uint256 _lastTs = block.timestamp;
        if (uf.length >= 1) {
            _lastIndex = uf.length - 1;
            _lastTs = uf[_lastIndex].createdAt;
        }
        uint256 _lastBalance = stakedBalance[_user].balance;
        // checks for the latest txn i.e. withdrawl or deposit. .oeTs is withdrawl timestamp
        if (stakedBalance[_user].oeTs > _lastTs) {
            _lastTs = stakedBalance[_user].oeTs;
        }
        uint256 _time = block.timestamp - _lastTs; // in seconds
        uint256 _rate = _getDailyEarningsRate(_lastBalance);
        _dailyEarnings += (_lastBalance * _rate * _time) / (10000 * 24 * 3600);

        uint256 _maxEarnings = checkMaxEarnings(_user);

        if (_dailyEarnings >= _maxEarnings) {
            _dailyEarnings = _maxEarnings;
        }

        stakedBalance[_user].ownEarnings = _dailyEarnings;
        stakedBalance[_user].oeTs = block.timestamp;

        uint256 _downlineBalance = getDonwlineBalance(_user);
        uint256 _performanceFee = 0;
        if (_downlineBalance >= PERFORMANCE_FEE_CHECK) {
            _performanceFee = _deductPerformanceFees(_dailyEarnings);
        }
        return _dailyEarnings - _performanceFee;
    }

    /** @notice function to get the user funds
     * @dev -
     * @param _user user address.
     * @return _uf array of user deposits.
     */
    function getUserFunds(address _user)
        public
        view
        returns (UserFunds[] memory _uf)
    {
        _uf = userFundDetails[_user];

        return _uf;
    }

    /** @notice calculates referral earnings from the downline. Also considering eligibile levels as per staked balance.
     * @dev -
     * @param _user user address.
     * @return net referral earnings.
     */
    function calculateReferalEarnings(address _user)
        public
        isContractActive
        isRegisteredUser
        returns (uint256)
    {
        // if suspended, then user can't earn
        if (userDetails[_user].isSuspended) {
            return 0;
        }
        // check earnings

        uint256 _withdrawalCommission = stakedBalance[_user]
            .withdrawalCommission;
        uint256 _referralBonus = stakedBalance[_user].referralBonus;

        uint256[25] memory _curLvlEarnings = userDetails[_user]
            .referrals
            .referralEarnings;
        // get eligible ref level
        uint256 _eligibleLvl = _getMaximumReferralStack(_user);

        console.log("_eligibleLvl: ", _user, _eligibleLvl);

        uint256 _eligibleRefEarnings;
        for (uint256 i = 0; i < _eligibleLvl; i++) {
            _eligibleRefEarnings += _curLvlEarnings[i];

            console.log("_eligibleRefEarnings: ", _user, _eligibleRefEarnings);
        }

        uint256 _totalRefEarr = _eligibleRefEarnings +
            _referralBonus +
            _withdrawalCommission;
        console.log("_totalRefEarr: ", _user, _totalRefEarr);

        uint256 _maxEarnings = checkMaxEarnings(_user);

        if (_totalRefEarr >= _maxEarnings) {
            _totalRefEarr = _maxEarnings;
        }

        stakedBalance[_user].totalRefEarnings += _totalRefEarr;
        stakedBalance[_user].tRefTs = block.timestamp;

        uint256 _refFee = _deductReferralFees(_totalRefEarr);
        return _totalRefEarr - _refFee;
    }

    /** @notice gives the downline balance of a user => total staked balance of the referrals.
     * @dev -
     * @param _user user address.
     * @return _totalDownBalance total pool balance of a user referrals.
     */
    function getDonwlineBalance(address _user)
        public
        view
        returns (uint256 _totalDownBalance)
    {
        // uint256 _eligibleLvl = _getMaximumReferralStack(_user);
        uint256 _eligibleLvl = MAXIMUM_REFERRAL_STACK;
        uint256[25] memory _curLvlBalance = userDetails[_user]
            .referrals
            .refBalances;
        for (uint256 i = 0; i < _eligibleLvl; i++) {
            _totalDownBalance += _curLvlBalance[i];
        }
        console.log("_totalDownBalance: ", _user, _totalDownBalance);
        return _totalDownBalance;
    }

    /** @notice function to withdraw a user's earnings => daily earnings + referral earnings + referral bonus + withdraw commission.
     * @dev it will credit the total staked balance of a user to user's address.
     */
    function withdrawEarnings() public {
        address _user = payable(msg.sender);

        uint256 _grossEarnigns = getTotalEarnings(_user);
        require(
            _grossEarnigns >= MINIMUM_CONTRIBUTION,
            "withdraw atleast 0.01 BNB"
        );
        require(
            checkMaxWithdrawal(_user) >= _grossEarnigns,
            "daily withdrawal limit reached"
        );
        uint256 _taxes = _deductWithdrawalFees(_grossEarnigns) +
            _checkAntiWhaleTaxes(_grossEarnigns);
        uint256 _netEarnings = _grossEarnigns - _taxes;

        stakedBalance[_user].totalRefEarnings = 0;
        stakedBalance[_user].withdrawalCommission = 0;
        stakedBalance[_user].referralBonus = 0;
        stakedBalance[_user].ownEarnings = 0;
        stakedBalance[_user].oeTs = block.timestamp;
        delete userDetails[_user].referrals.referralEarnings;
        stakedBalance[_user].totalWithdrawal.amountWithdrawan.push(
            _grossEarnigns
        );
        stakedBalance[_user].totalWithdrawal.twTs.push(block.timestamp);
        stakedBalance[_user].tRefTs = block.timestamp;

        payable(_user).transfer(_netEarnings);
        emit WITHDRAW(_user, _netEarnings);
    }

    /** @notice function to withdraw a user's staked balance.
     * @dev it will credit the total staked balance of a user to user's address.
     */
    function withdrawStakedBalacne() public {
        address _user = payable(msg.sender);
        uint256 _grossTotalBalance = stakedBalance[_user].balance;

        require(
            _grossTotalBalance >= MINIMUM_CONTRIBUTION,
            "withdraw atleast 0.01 BNB"
        );
        require(
            checkMaxWithdrawal(_user) >= _grossTotalBalance,
            "daily withdrawal limit reached"
        );

        uint256 _taxes = _deductWithdrawalFees(_grossTotalBalance) +
            _checkAntiWhaleTaxes(_grossTotalBalance);
        uint256 _netEarnings = _grossTotalBalance - _taxes;
        stakedBalance[_user].tRefTs = block.timestamp;
        stakedBalance[_user].oeTs = block.timestamp;
        stakedBalance[_user].totalWithdrawal.amountWithdrawan.push(
            _grossTotalBalance
        );
        stakedBalance[_user].totalWithdrawal.twTs.push(block.timestamp);

        stakedBalance[_user].balance = 0;
        stakedBalance[_user].totalBalance = 0;

        payable(_user).transfer(_netEarnings);
        emit WITHDRAW(_user, _netEarnings);
    }

    /** @notice function to withdraw all user earnings and staked balance.
     * @dev it will credit all the user balances back to user's account and setis user's deposit and staked balances to zero.
     */
    function withdrawAllBalances() public {
        withdrawEarnings();
        withdrawStakedBalacne();
    }

    /** @notice funciton to get the total earnings of the user.
     * @dev returns user's earnings => daily earnings + referral earnings + referral bonus + withdraw commission.
     * @param _user user address.
     * @return returns user's earnings.
     */
    function getTotalEarnings(address _user) public returns (uint256) {
        // calculate the daily earnings of all the referral, to update the performance commission of user
        address[][25] memory _refAddrs = userDetails[_user].referrals.refAddrs;
        for (uint256 i = 0; i < MAXIMUM_REFERRAL_STACK; i++) {
            address[] memory _lvlAddrs = _refAddrs[i];
            uint256 _len = _lvlAddrs.length;
            for (uint256 j = 0; j < _len; j++) {
                calculateDailyEarnings(_lvlAddrs[j]);
            }
        }

        uint256 _grossOwnEarnings = calculateDailyEarnings(_user);
        uint256 _grossRefEarnings = calculateReferalEarnings(_user);

        uint256 _grossTotalEarnigns = _grossRefEarnings + _grossOwnEarnings;

        uint256 _maxEarnings = checkMaxEarnings(_user);

        if (_grossTotalEarnigns >= _maxEarnings) {
            _grossTotalEarnigns = _maxEarnings;
        }
        console.log(
            "checkTotalEarnings ~ _grossTotalEarnigns",
            _user,
            _grossTotalEarnigns
        );
        return _grossTotalEarnigns;
    }

    function checkMaxEarnings(address _user) public view returns (uint256) {
        uint256 _currentBalance = stakedBalance[_user].balance;
        console.log(
            "checkMaxEarnings ~ (_currentBalance * 250) / 100",
            _user,
            (_currentBalance * 250) / 100
        );
        return (_currentBalance * 250) / 100;
    }

    // can't withdraw more than the max staked amount in one go.(in 24 hrs cycle)
    function checkMaxWithdrawal(address _user) public view returns (uint256) {
        uint256 _maxAllowed = stakedBalance[_user].maxWithdrawal;
        uint256[] memory _withdrawalAmout = stakedBalance[_user]
            .totalWithdrawal
            .amountWithdrawan;
        uint256[] memory _lastTs = stakedBalance[_user].totalWithdrawal.twTs;
        uint256 _currentTs = block.timestamp;
        if (_lastTs.length >= 1) {
            uint256 _amount = 0;
            for (uint256 i = _lastTs.length; i > 0; i--) {
                uint256 _day = _currentTs - 86400;
                if (_lastTs[i - 1] >= _day) {
                    _amount += _withdrawalAmout[i - 1];
                } else if (_lastTs[i - 1] < _day) {
                    break;
                }
            }
            if (_maxAllowed >= _amount) {
                console.log(
                    "checkMaxWithdrawal ~ _maxAllowed - _amount",
                    _user,
                    _maxAllowed - _amount
                );
                return _maxAllowed - _amount;
            } else {
                return 0;
            }
        } else {
            console.log("checkMaxWithdrawal ~ _maxAllowed", _user, _maxAllowed);
            return _maxAllowed;
        }
    }

    function _checkAntiWhaleTaxes(uint256 _amount)
        public
        view
        returns (uint256)
    {
        uint256 _stakingBalance = address(this).balance;
        uint256 _percentage = _getRelativePercentage(_amount, _stakingBalance);

        for (uint256 i = 1; i <= 10; i++) {
            if (_percentage >= i) {
                return (_amount * i * 5) / 100;
            }
        }

        return 0;
    }

    function _getRelativePercentage(uint256 partialValue, uint256 totalValue)
        internal
        pure
        returns (uint256)
    {
        return (100 * partialValue) / totalValue;
    }

    // MISCELLANEOUS FUNCTIONS
    function _registerUser(address _refferer) internal {
        address _user = msg.sender;
        if (_refferer != owner()) {
            require(_user != _refferer, "Cannot refer to yourself");
        }
        userDetails[_user].refferedBy = _refferer;
        userDetails[_user].isRegistered = true;
        userDetails[_user].isSuspended = false;

        _updateRefAddrs(_refferer);
        emit REGISTERED(_user, _refferer);
    }

    function _updateRefAddrs(address _refAdrs) internal {
        address _user = msg.sender;
        // Need to check till max referral allowed and add in refferal level wise
        address _referredBy = _refAdrs;
        for (uint256 i = 0; i < MAXIMUM_REFERRAL_STACK; i++) {
            if (i == 0) {
                userDetails[_referredBy].referrals.refAddrs[i].push(_user);
            } else {
                _referredBy = userDetails[_referredBy].refferedBy;
                userDetails[_referredBy].referrals.refAddrs[i].push(_user);
                if (_isAdminOrNullAddress(_referredBy)) {
                    return;
                }
            }
        }
    }

    function _deductDepositFees(uint256 _amount) internal returns (uint256) {
        uint256 _amountToDeduct = _amount / 10; // 10%
        // 50% of 10% and the remaining 50% will reaming with this SC
        payable(treasury).transfer(_amountToDeduct / 2);
        // payable(address(this)).transfer(_amountToDeduct / 2);
        return _amountToDeduct;
    }

    function _deductWithdrawalFees(uint256 _amount) internal returns (uint256) {
        // 50% of 10% to treasury and send 10% to direct sponsor and the remaining 40% will reaming with this SC
        uint256 _amountToDeduct = _amount / 10; // 10%
        uint256 _treasuryShare = _amountToDeduct / 2;
        uint256 _directRefShare = _amountToDeduct / 10;

        payable(treasury).transfer(_treasuryShare);

        address _user = userDetails[msg.sender].refferedBy;
        stakedBalance[_user].totalWithdrawal.amountWithdrawan.push(
            _directRefShare
        );
        stakedBalance[_user].totalWithdrawal.twTs.push(block.timestamp);
        stakedBalance[_user].withdrawalCommission += _directRefShare;
        return _amountToDeduct;
    }

    function _deductReferralFees(uint256 _amount) internal returns (uint256) {
        uint256 _amountToDeduct = _amount / 10; // 10%
        // 50% of 10% and the remaining 50% will reaming with this SC
        payable(treasury).transfer(_amountToDeduct / 2);
        return _amountToDeduct;
    }

    function _deductPerformanceFees(uint256 _amount)
        internal
        returns (uint256)
    {
        uint256 _amountToDeduct = _amount / 10; // 10%
        // 10% is split into three parts - 50% to treasury, 35% ramains with this SC
        //15% will be spilt equally between 5 levels.
        payable(treasury).transfer(_amountToDeduct / 2);
        _distributeReferralBonus((_amountToDeduct * 15) / 100);
        return _amountToDeduct;
    }

    function _isAdminOrNullAddress(address _address)
        internal
        view
        returns (bool)
    {
        return owner() == _address || address(0) == _address;
    }

    function _getMaximumReferralStack(address _user)
        internal
        view
        returns (uint256 stack)
    {
        uint256 _balance = stakedBalance[_user].balance;
        require(_balance > 0, "No Staked Balance");
        for (uint256 i = MAXIMUM_REFERRAL_STACK; i > 0; i--) {
            uint256 _lvlBalance = levelInfo[i];
            if (_balance >= _lvlBalance) {
                console.log("eligible ref lvl", _user, i);
                return i;
            }
        }
    }

    // distributes referral Bonus for referrals form bottom to top.
    function _distributeReferralBonus(uint256 _amount) internal {
        uint256 _splitAmount = _amount / 5;
        address _referredBy;

        for (uint256 i = 0; i < MAXIMUM_REFERRAL_STACK; i++) {
            if (i == 1) {
                _referredBy = userDetails[msg.sender].refferedBy;
            } else {
                _referredBy = userDetails[_referredBy].refferedBy;
            }

            if (!isActive || userDetails[_referredBy].isSuspended) {
                _amount = 0;
            }
            stakedBalance[_referredBy].referralBonus += _splitAmount;

            if (_isAdminOrNullAddress(_referredBy)) {
                return;
            }
        }
    }

    function getContractBalance() public view returns (uint256) {
        return address(this).balance;
    }

    function addBalance() public payable {
        require(msg.value > 0, "Send some funds");
    }

    function suspendUser(address _user) public onlyOwner {
        require(!userDetails[_user].isSuspended, "User alredy suspended");
        userDetails[_user].isSuspended = true;
    }

    function revokeSuspension(address _user) public onlyOwner {
        require(userDetails[_user].isSuspended, "User alredy active");
        userDetails[_user].isSuspended = false;
    }

    function updateGlobalParams(
        uint256 _minContribution,
        uint256 _maxContribution,
        uint256 _refLevels,
        uint256[] memory _refLvlRate
    ) public onlyOwner {
        MINIMUM_CONTRIBUTION = _minContribution;
        MAXIMUM_CONTRIBUTION = _maxContribution;
        MAXIMUM_REFERRAL_STACK = _refLevels;

        for (uint256 i = 0; i < _refLevels; i++) {
            refLvlRate[i] = _refLvlRate[i];
        }
    }

    function addLiquidity() public payable {
        require(msg.value > 0, "send some amount");
    }

    // MODIFIERS

    modifier isRegisteredUser() {
        require(userDetails[msg.sender].isRegistered, "User not registered");
        _;
    }

    modifier isNotSuspendedUser() {
        require(!userDetails[msg.sender].isSuspended, "User suspended");
        _;
    }

    modifier isContractActive() {
        require(isActive, "contract paused");
        _;
    }
}

