    *   @param _numTokens the Number of tokens that need to be minted
    */
    function mintWhiteList(uint16 _numTokens) external payable 
    {
        require(tx.origin == _msgSender(), "Only EOA");
        require(whitelistSaleOpen(), "The WL sale is not open");
        require(_numTokens <= _whiteList[msg.sender], "Exceeded max available to purchase");
        require(WL_MINT_PRICE * _numTokens <= msg.value, "Incorrect price value");
        
        payable(lockAddress).transfer(msg.value);
        _whiteList[msg.sender] -= _numTokens;
        _mintTokens(_numTokens, _msgSender());
    }



    /**
    *   Mint a token when the sale is active.
    *   For all PAID_TOKENS you need 1 AVAX
    *   @param _numTokens the Number of tokens that need to be minted
    */
    function mint(uint16 _numTokens) external payable 
    {
        require(tx.origin == _msgSender(), "Only EOA");
        require(saleOpen(), "The sale is not open at this current time");

        if(mintedTokens < PAID_TOKENS){
            require(mintedTokens + _numTokens <= PAID_TOKENS, "All tokens on-sale already sold");
            require(msg.value == _numTokens * MINT_PRICE, "Incorrect amount sent");

            payable(lockAddress).transfer(msg.value);
        } 
        else {
            require(msg.value == 0);
        }
        _mintTokens(_numTokens, _msgSender());
    }



    /**
    *   Calculate the mint cost (In $PULP) for a certain tokenID
    * @param tokenId the ID to check the cost of to mint
    * @return the cost of the given token ID
    */
    function mintCost(uint256 tokenId) public pure returns (uint256) 
    {
        if (tokenId <= PAID_TOKENS) return 0;
        // if (tokenId <= MAX_SUPPLY * 2 / 5) return 20000 ether; //40%
        // if (tokenId <= MAX_SUPPLY * 4 / 5) return 40000 ether; //80%
        if (tokenId <= MAX_SUPPLY / 5) return 20000 ether; //20%
        if (tokenId <= MAX_SUPPLY / 5 * 2) return 4000 ether; //40%
        if (tokenId <= MAX_SUPPLY / 5 * 3 ) return 60000 ether; //60%
        if (tokenId <= MAX_SUPPLY / 10 * 7) return 70000 ether; //70%
        if (tokenId <= MAX_SUPPLY / 5 * 4) return 80000 ether; //80%
        if (tokenId <= MAX_SUPPLY / 10 * 9) return 90000 ether; //90%
        if (tokenId <= MAX_SUPPLY / 100 * 95) return 95000 ether; //95%

        return 100000 ether;
    }



    /**
    *   Function that checks whether the whitelist sale is open
    *   @return Boolean that defines whether the whitelist sale is open
    */
    function whitelistSaleOpen() public view returns (bool) 
    {
        return whitelistStartTime != 0 && block.timestamp >= whitelistStartTime && block.timestamp <= saleStartTime;
    }



    /**
    *   Function that checks whether the main sale is open
    *   @return Boolean that defines whether the main sale is open
    */
    function saleOpen() public view returns (bool) 
    {
        return saleStartTime != 0 && block.timestamp >= saleStartTime;
    }



    /**
    *   Function that returns the tokenURI of the Papers
    *   @param _tokenId Token ID of a paper 
    *   @return tokenURI String
    */
    function tokenURI(uint256 _tokenId) public view override returns (string memory) 
    {
        // if(tokenLevel[_tokenId] == 2){
        //     return string(abi.encodePacked(baseURI, "/diamond-paper", _tokenId.toString(), ".json"));
        // }
        // if(tokenLevel[_tokenId] == 1){
        //     return string(abi.encodePacked(baseURI, "/gold-paper", _tokenId.toString(), ".json"));
        // }
        // return string(abi.encodePacked(baseURI, "/paper", _tokenId.toString(), ".json"));
        return string(abi.encodePacked(baseURI, "/", _tokenId.toString(), ".json"));
    }



    /**
    *   Gets the upgrade costs for a certain Paper
    *   @param _tokenID The Paper ID
    *   @return The upgrade costs for given Paper ID
    */
    function getUpgradeCosts(uint256 _tokenID) public view returns (uint256) 
    {
        return upgradeCosts[_tokenID];
    }



    /**
    *   Get the current Combat Stats of a paper
    *   @param _tokenID Token ID of a Paper
    */
    function tokenStats(uint256 _tokenID) public view returns (Stats memory) 
    {
        return stats[_tokenID];
    }



    /**
    *   Upgrades the Combat Stats of a Paper
    *   @param _tokenID The tokenID of the Paper to be upgraded
    */    
    function upgradePaper(uint256 _tokenID) external 
    {
        require(pulp.balanceOf(msg.sender) >= upgradeCosts[_tokenID], "Not enough pulp to upgrade");
        require(this.ownerOf(_tokenID) == _msgSender(), "You don't own this Paper.");
        
        pulp.burn(msg.sender, upgradeCosts[_tokenID]);
        upgradeCosts[_tokenID] = upgradeCosts[_tokenID] * 110 / 100;
        // Upgrade Paper
        _upgradePaperStats(_tokenID, 0);
    }



    function getWhitelistAllocation(address _address) public view returns (uint16) 
    {
        return _whiteList[_address];
    }
    
//======================================================================================================================================================
//======================================================================================================================================================
//======================================================================================================================================================
//============ end public/external



    /**
    *   Mint a token if you are whitelisted and still have allocation available
    *   The max allocation for the whitelist is 10 Papers
    */
    function _mintTokens(uint16 _numTokens, address _for) internal 
    {        
        require(mintedTokens + _numTokens <= MAX_SUPPLY, "Insufficient Supply");
        require(_numTokens <= MAX_PER_MINT, "Only mint 10 Papers at once");

        uint256 totalPulpCost = 0;

        for(uint i = 0; i < _numTokens; i++){
            mintedTokens++;    
            uint256 tokenId = mintedTokens;
            _safeMint(_for, tokenId);
            Stats memory baseStats = Stats({ attack: 3, defense: 3, strength: 3, health: 100, regen: 1});
            stats[tokenId] = baseStats;
            upgradeCosts[tokenId] = 5000 ether;
            _upgradePaperStats(tokenId,0);
            // 10% CHANCE FOR EXTREME BOOST
            uint256 random = _rand(block.timestamp * totalSupply());
            if(random % 100 < 2){
                tokenLevel[tokenId] = 2;
                _upgradePaperStats(tokenId,1);
                _upgradePaperStats(tokenId,2);
                _upgradePaperStats(tokenId,3);
            } 
            else if (random % 100 < 10){
                tokenLevel[tokenId] = 1;
                _upgradePaperStats(tokenId,1);
            }
            totalPulpCost += mintCost(tokenId);
        }
        emit Minted(mintedTokens);
        if (totalPulpCost > 0) pulp.burn(_msgSender(), totalPulpCost);
    }



    /**
    *   Random function adapted from Sheep Game
    *   @return random uint256
    */
    function _rand(uint256 _seed) internal view returns (uint256) 
    {
        require(tx.origin == _msgSender(), "Only EOA");

        return uint256(
            keccak256(
                abi.encodePacked(
                    blockhash(block.number - 4),
                    tx.origin,
                    blockhash(block.number - 2),
                    blockhash(block.number - 3),
                    blockhash(block.number - 1),
                    _seed,
                    block.timestamp
                )
            )
        );
    }



    /**
    *   Internal function that updates two random Combat Stats
    *   @param _tokenID The tokenID of the Paper to be upgraded
    *   @param _offset used for multiple upgrades in the same transaction
    */ 
    function _upgradePaperStats(uint256 _tokenID, uint256 _offset) internal 
    {
        uint256 random = _rand(block.timestamp * upgradeCosts[_tokenID] + _tokenID + _offset);
        if (random % 100 < 20) {
            stats[_tokenID].attack += 2;
            stats[_tokenID].health += 30;            
        } 
        else if (random % 100 < 40) {
            stats[_tokenID].defense += 2;     
            stats[_tokenID].strength += 2;               
        } 
        else if (random % 100 < 60) {
            stats[_tokenID].regen += 1;
            stats[_tokenID].attack += 2;             
        } 
        else if (random % 100 < 80) {
            stats[_tokenID].health += 30;
            stats[_tokenID].strength += 2;
        } 
        else {
            stats[_tokenID].regen += 1;
            stats[_tokenID].defense += 2;
        }
    }



}
