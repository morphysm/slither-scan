pragma solidity ^0.8.4;






interface RockInterface{
    function ownerOf(uint256 tokenId) external view returns (address owner);
}


contract Arena is Ownable, PaperStats 
{

    using SafeMath for uint256;

    // reference to the Paper contract
    Paper public paper;
    // reference to the Pulp contract
    Pulp public pulp;
    // reference to the Rock contract
    address rockAddr = 0x8D01C8ee82E581E55C02117a676B5Bbd4734FABB;
    // Array of different enemies that can be attacked
    Enemy[] public enemies;


    // Structure for enemy Combat Stats + Reward
    struct Enemy {
        uint256 attack;
        uint256 defense;
        uint256 strength;
        uint256 health;
        uint256 regen;
        uint256 reward;
    }


    // Mapping from all Papers (defined by ID) to their respective current target
    mapping(uint256 => uint256) currentTarget;
    // Mapping from all Papers (defined by ID) to the timestamp when they started attacking an enemy
    mapping(uint256 => uint256) public startedAttacking;
    // Mapping from all Papers (defined by ID) to their respective Rock Buddies
    mapping(uint256 => uint256) currentRock;

    // Max amount of pulps ever in existance
    uint256 public MAXIMUM_PULP = 400000000000 ether;
    // Current pulps claimed
    uint256 public claimedPulp = 0;
    // boolean opening or closing the arena
    bool arenaClaimable = false;




    constructor(Paper _paper, Pulp _pulp) 
    {
        paper = _paper;
        pulp = _pulp;
    }



//======================================================================================================================================================



    /**
    *   Set the Pulp contract address
    *   @param _attack the attack value of the Enemy
    *   @param _defense the defense value of the Enemy
    *   @param _strength the strength value of the Enemy
    *   @param _health the health value of the Enemy
    *   @param _regen the regen value of the Enemy
    *   @param _reward the $PULP reward gained by killing this Enemy succesfully
    */
    function addEnemy(uint256 _attack, uint256 _defense, uint256 _strength, uint256 _health, uint256 _regen, uint256 _reward) external onlyOwner 
    {
        enemies.push(Enemy({ attack: _attack, defense: _defense, strength: _strength, health: _health, regen: _regen, reward: _reward}));
    }



    /**
    *   Remove an enemy from the enemy pool
    *   @param index the index in the array of the enemy that should be deleted
    */
    function removeEnemy(uint index) external onlyOwner 
    {
        if (index >= enemies.length) return;
        delete enemies[index];
    }



    function setArenaOpen(bool _opened) external onlyOwner 
    {
        arenaClaimable = _opened;
    }



    /**
    *   Set the Paper contract address
    *   @param _paper the address of the Paper contract
    */
    function setPaperAddress(Paper _paper) external onlyOwner 
    {
        paper = _paper;
    }



    /**
    *   Set the Pulp contract address
    *   @param _pulp the address of the Pulp contract
    */
    function setPulpAddress(Pulp _pulp) external onlyOwner 
    {
        pulp = _pulp;
    }



//======================================================================================================================================================
//======================================================================================================================================================
//======================================================================================================================================================
//============ end onlyOwner



    /**
    *   Changes the enemy target for a single Paper. 
    *   Changing target automatically claims all of your $PULP
    *   @param _tokenID The token ID of the Paper that you want to change the target of
    *   @param _target The index of the enemy you want to target
    */
    function changeTarget(uint256 _tokenID, uint256 _target) external 
    {
        require(paper.ownerOf(_tokenID) == _msgSender(), "You can only change the target of a Paper you own.");
        require(_target < enemies.length, "The enemy you are targetting doesn't exist");
        require(enemies[_target].health > 0, "The enemy you are targetting doesn't exist anymore");

        if(arenaClaimable) {
            _claimRewards(_msgSender(), currentRewards(_tokenID));
        }
        startedAttacking[_tokenID] = block.timestamp;
        currentTarget[_tokenID] = _target;
    }

    /**
    *   Changes the added rock for a single Paper. 
    *   Changing rock automatically claims all of your $PULP
    *   @param _tokenID The token ID of the Paper that you want to change the rock of
    *   @param _rockID The index of the Rock you want to add
    */
    function changeRock(uint256 _tokenID, uint256 _rockID) external 
    {
        require(paper.ownerOf(_tokenID) == _msgSender(), "You can only change the rock of a Paper you own.");
        require(RockInterface(rockAddr).ownerOf(_rockID) == _msgSender(), "You can only set a Rock you own.");

        if(arenaClaimable) {
            _claimRewards(_msgSender(), currentRewards(_tokenID));
        }

        // for (uint i=0; i < paper.MAX_SUPPLY(); i++) {
        //     if (currentRock[i] == _rockID) {
        //         delete currentRock[i];
        //     }
        // }

        currentRock[_tokenID] = _rockID;
    }
    



    /**
    *   Changes the enemy target for a single Paper. 
    *   @param _tokenID The token ID of the Paper 
    *   @return Current Enemy Stats
    */
    function getCurrentTargetStats(uint256 _tokenID) public view returns (Enemy memory) 
    {
        return enemies[currentTarget[_tokenID]];
    }



    /**
    *   Gets all current enemies to be attacked
    *   @return Currently available enemies
    */
    function getAllEnemies() public view returns (Enemy[] memory) 
    {
        return enemies;
    }



    /**
    *   Gets the number of enemies currently in the enemy pool
    *   @return number of enemies
    */
    function getEnemyLength() public view returns (uint256) 
    {
        return enemies.length;
    }



    /**
    *   Claims the rewards for the current Paper to the owner of the paper
    *   @param _tokenIDs Array of TokenIDs for which you want to claim rewards
    */
    function _claimMultipleRewards(uint256[] calldata _tokenIDs) public 
    {
        require(arenaClaimable, "Claiming is not available");

        uint256 totalRewards = 0;
        for (uint256 i = 0; i < _tokenIDs.length; i++) {
            require(paper.ownerOf(_tokenIDs[i]) == msg.sender, "You can only claim rewards for Papers you own.");

            if (startedAttacking[_tokenIDs[i]] != 0){
                totalRewards += currentRewards( _tokenIDs[i]);
                startedAttacking[_tokenIDs[i]] = block.timestamp;
            }
        }
        _claimRewards(msg.sender, totalRewards);
    }



    /**
    *   Claims the rewards for the current Paper to the owner of the paper
    *   @param _tokenID TokenID for which you want to claim rewards
    */
    function _userClaimReward(uint256 _tokenID) public 
    {
        require(arenaClaimable, "Claiming is not available");
        require(paper.ownerOf(_tokenID) == msg.sender, "You can only claim rewards for Papers you own.");

        if (startedAttacking[_tokenID] != 0){
            _claimRewards(msg.sender, currentRewards( _tokenID));
            startedAttacking[_tokenID] = block.timestamp;
        }
    }



    /**
    *   Calculates the time to kill the currently targetted enemy with your current Paper
    *   @param _tokenID The token ID of the Paper 
    */
    function calcKillTime(uint256 _tokenID, uint256 _enemyID) public view returns (uint256) 
    {
        Stats memory stats = paper.tokenStats(_tokenID);
        Enemy memory enemyStats = enemies[_enemyID];
        // Check Monster killTime
        uint256 emaxHit = ((enemyStats.strength + 11) * 64 + 320) * 1000 / 640;
        uint256 eattackRoll = (enemyStats.attack + 11) * 64;
        uint256 edefenseRoll = (stats.defense + 9) * 64;
        uint256 ehitChance = 0;
        if(eattackRoll > edefenseRoll) {
            ehitChance = 1000 - ((edefenseRoll + 2) * 1000) / (2 * (eattackRoll + 1));
        } 
        else {
            ehitChance = 1000 * eattackRoll / (2 * (edefenseRoll + 1));
        }
        uint256 eKdps = emaxHit * ehitChance / 2000; // dps (times 1000 for decimal)
        uint256 ekillTime = 0;
        if(eKdps <= (stats.regen * 100)){
            ekillTime = 999999999999999;
        } 
        else {
            ekillTime = (stats.health * 1000) / (eKdps - (stats.regen * 100)) + 1;
        }

        // Kill Time User vs. Monster
        uint256 maxHit = ((stats.strength + 11) * 64 + 320) * 1000 / 640; // maxHit (times 1000 for decimal)
        uint256 attackRoll = (stats.attack + 11) * 64; // attackRoll
        uint256 defenseRoll = (enemyStats.defense + 9) * 64; // defenseRoll 
        uint256 hitChance = 0;
        if(attackRoll > defenseRoll) {
            hitChance = 1000 - ((defenseRoll + 2) * 1000) / (2 * (attackRoll + 1));
        } 
        else {
            hitChance = 1000 * attackRoll / (2 * (defenseRoll + 1));
        }
        uint256 Kdps = maxHit * hitChance / 2000; // dps (times 1000 for decimal)
        uint256 killTime = 0;
        if(Kdps <= (enemyStats.regen * 100)){
            killTime = 999999999999999;
        } 
        else {
            killTime = (enemyStats.health * 1000) / (Kdps - (enemyStats.regen * 100)) + 1;
        }
         
        if(ekillTime < killTime){
            return 999999999999999;
        }
        return killTime;
    }



    /**
    *   Gets the current target index for a certain Paper
    *   @param _tokenID The token ID of the Paper 
    *   @return Current Enemy Stats
    */
    function getCurrentTarget(uint256 _tokenID) public view returns (uint) 
    {
        return currentTarget[_tokenID];
    }

    /**
    *   Gets the current RockID for a certain Paper
    *   @param _tokenID The token ID of the Paper 
    *   @return Current Rock ID
    */
    function getCurrentRock(uint256 _tokenID) public view returns (uint) 
    {
        return currentRock[_tokenID];
    }



    /**
    *   Calculates the currentRewards the Paper has gained by attacking the currently attacking enemy
    *   @param _tokenID The token ID of the Paper 
    */
    function currentRewards(uint256 _tokenID) public view returns (uint256) 
    {
        if(startedAttacking[_tokenID] == 0){
            return 0;
        }
        if(currentRock[_tokenID] > 0){
            return 2 * getCurrentTargetStats(_tokenID).reward * ((block.timestamp - startedAttacking[_tokenID])/calcKillTime(_tokenID, currentTarget[_tokenID])) * (10 ** 18);        
        }
        else {
            return getCurrentTargetStats(_tokenID).reward * ((block.timestamp - startedAttacking[_tokenID])/calcKillTime(_tokenID, currentTarget[_tokenID])) * (10 ** 18);        
        }
    }



//======================================================================================================================================================
//======================================================================================================================================================
//======================================================================================================================================================
//============ end public/external



    /**
    *   Claims the rewards for the current Paper to the owner of the paper
    *   @param _rewardAddress The address the $PULP rewards need to be sent to
    *   @param amount Reward amount of $PULP gained
    */
    function _claimRewards(address _rewardAddress, uint256 amount) internal 
    {
        require(claimedPulp + amount < MAXIMUM_PULP, "Maximum amount of PULP have been minted");
        require(arenaClaimable, "Claiming is not available");

        //Calculate Rewards
        claimedPulp += amount;
        pulp.mint(_rewardAddress, amount);
    }


}