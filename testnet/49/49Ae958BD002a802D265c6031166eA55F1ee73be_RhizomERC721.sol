pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol";
import "@openzeppelin/contracts/utils/Strings.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "./RandomNumber.sol";

contract RhizomERC721 is ERC721URIStorage, Ownable, Pausable {
    RandomNumber public randomNumber;

    string public BASE_URI;
    uint256 public QUANTITY_MINTED = 0;
    uint256 public MAX_SUPPLY = 10_000;
    uint256 public MINT_START_TIMESTAMP;
    uint256 public constant PRICE_PER_UNITY = 0.1 ether;

    uint256 public constant POWER_TYPE = 1;
    uint256 public constant PREMIUM_TYPE = 2;
    uint256 public constant PLATINUM_TYPE = 3;
    uint256 public constant GOLD_TYPE = 4;
    uint256 public constant SILVER_TYPE = 5;
    uint256 public constant BRONZE_TYPE = 6;

    uint256 public constant POWER_YIELD = 100;
    uint256 public constant PREMIUM_YIELD = 80;
    uint256 public constant PLATINUM_YIELD = 50;
    uint256 public constant GOLD_YIELD = 30;
    uint256 public constant SILVER_YIELD = 15;
    uint256 public constant BRONZE_YIELD = 5;

    mapping(uint256 => uint256) public tokenTypes; // maps tokenId to its type
    mapping(uint256 => uint256) public typeYields; // maps NFT type to yield

    event Minted(uint256 tokenId);
    event TypeSetted(uint256 tokenId, uint256 nftType);

    // constructor(string memory _baseUri, RandomNumber _randomNumber)
    constructor(string memory _baseUri) ERC721("RhizomERC721", "RHZERC721") {
        BASE_URI = _baseUri;

        // randomNumber = _randomNumber;
        // randomNumber.requestRandomWords();

        typeYields[POWER_TYPE] = POWER_YIELD;
        typeYields[PREMIUM_TYPE] = PREMIUM_YIELD;
        typeYields[PLATINUM_TYPE] = PLATINUM_YIELD;
        typeYields[GOLD_TYPE] = GOLD_YIELD;
        typeYields[SILVER_TYPE] = SILVER_YIELD;
        typeYields[BRONZE_TYPE] = BRONZE_YIELD;
    }

    // set the timestamp when the minting starts
    function setStartTime(uint256 _startTime) external onlyOwner {
        require(
            _startTime >= block.timestamp,
            "Start time cannot be in the past"
        );
        MINT_START_TIMESTAMP = _startTime;
    }

    function mintStarted() public view returns (bool) {
        return
            MINT_START_TIMESTAMP != 0 &&
            block.timestamp >= MINT_START_TIMESTAMP;
    }

    function _mintOne(address to) internal {
        require(QUANTITY_MINTED <= MAX_SUPPLY, "Cannot mint anymore.");
        uint256 tokenId = QUANTITY_MINTED + 1;

        if (QUANTITY_MINTED % 500 == 0) {
            randomNumber.requestRandomWords();
        }

        _safeMint(to, tokenId);
        QUANTITY_MINTED++;
        emit Minted(tokenId);
    }

    function _mintNFTs(uint256 quantity, address to) internal {
        for (uint256 iterator = 0; iterator < quantity; iterator++) {
            _mintOne(to);
        }
    }

    function mintMany(uint256 quantity) external payable whenNotPaused {
        // require(mintStarted(), "Mint not started yet.");
        require(
            quantity > 0 && quantity <= 10,
            "You can only mint 10 at a time."
        );
        require(
            (QUANTITY_MINTED + quantity) <= MAX_SUPPLY,
            "You can't mint that many right now."
        );

        // calculate the transaction cost
        uint256 transactionCost = PRICE_PER_UNITY * quantity;
        require(msg.value >= transactionCost, "Not enough ETH to mint.");

        // mint NFTs
        _mintNFTs(quantity, _msgSender());
    }

    function getYield(uint256 _tokenId) public view returns (uint256) {
        require(_exists(_tokenId), "Token does not exist");
        return typeYields[tokenTypes[_tokenId]];
    }

    function getType(uint256 _tokenId) public view returns (uint256) {
        require(_exists(_tokenId), "token does not exist");
        return tokenTypes[_tokenId];
    }

    // set the NFT type based in a random number called from RandomNumber w/ ChainLink
    function setNFTType(uint256 tokenId) external {
        require(
            tokenTypes[tokenId] == 0,
            "That token's type has already been set"
        );

        // randomNumber.requestRandomWords();
        uint256 points = random(101);

        if (points <= 60) {
            tokenTypes[tokenId] = BRONZE_TYPE;
            emit TypeSetted(tokenId, BRONZE_TYPE);
        }
        if (points > 60 && points <= 70) {
            tokenTypes[tokenId] = SILVER_TYPE;
            emit TypeSetted(tokenId, SILVER_TYPE);
        }
        if (points > 70 && points <= 80) {
            tokenTypes[tokenId] = GOLD_TYPE;
            emit TypeSetted(tokenId, GOLD_TYPE);
        }
        if (points > 80 && points <= 90) {
            tokenTypes[tokenId] = PLATINUM_TYPE;
            emit TypeSetted(tokenId, PLATINUM_TYPE);
        }
        if (points > 90 && points <= 99) {
            tokenTypes[tokenId] = PREMIUM_TYPE;
            emit TypeSetted(tokenId, PREMIUM_TYPE);
        }
        if (points == 100) {
            tokenTypes[tokenId] = POWER_TYPE;
            emit TypeSetted(tokenId, POWER_TYPE);
        }
    }

    // token metadata functions
    function maxSupply() public view returns (uint256) {
        return MAX_SUPPLY;
    }

    function totalSupply() public view returns (uint256) {
        return QUANTITY_MINTED;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return BASE_URI;
    }

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override
        returns (string memory)
    {
        require(
            _exists(tokenId),
            "ERC721Metadata: URI query for nonexistent token"
        );
        return
            string(
                abi.encodePacked(_baseURI(), "/", Strings.toString(tokenId))
            );
    }

    function random(uint256 number) public view returns (uint256) {
        return
            uint256(
                keccak256(
                    abi.encodePacked(
                        block.timestamp,
                        block.difficulty,
                        msg.sender
                    )
                )
            ) % number;
    }
}

