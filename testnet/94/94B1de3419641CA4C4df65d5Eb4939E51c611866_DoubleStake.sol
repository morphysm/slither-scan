        // number of tokens Per day
        pussyRewardBands = [2, 5, 10, 15, 20, 0];
        assRewardBands = [1, 2, 3, 4, 5, 0];

        started = false;
    }

    function compareStrings(string memory a, string memory b) public view returns (bool) {
    return (keccak256(abi.encodePacked((a))) == keccak256(abi.encodePacked((b))));
}


    function setRate(uint256 _rarity, uint256 _rate,string memory choice) public onlyOwner() {
        if(compareStrings(choice,"ASS")) {
           assRewardBands[_rarity] = _rate;
        }
        if(compareStrings(choice,"PUSSY")) {
            pussyRewardBands[_rarity] = _rate;
        }
    }

    function setRarity(uint256 _tokenId, uint256 _rarity, string memory choice) public onlyOwner() {
        if(compareStrings(choice,"ASS")) {
           assTokenRarity[_tokenId] = _rarity;

        }
        if(compareStrings(choice,"PUSSY")) {
           pussyTokenRarity[_tokenId] = _rarity;

        }

    }

    function setBatchRarity(uint256[] memory _tokenIds, uint256 _rarity, string memory choice) public onlyOwner() {
        if(compareStrings(choice,"ASS")) {
            for (uint256 i; i < _tokenIds.length; i++) {
                uint256 tokenId = _tokenIds[i];
                assTokenRarity[tokenId] = _rarity;
            }
        }
        if(compareStrings(choice,"PUSSY")) {
            for (uint256 i; i < _tokenIds.length; i++) {
                uint256 tokenId = _tokenIds[i];
                pussyTokenRarity[tokenId] = _rarity;
            }
        }
    }


    function setExpiration(uint256 _expiration) public onlyOwner() {
        EXPIRATION = _expiration;
    }


    function toggleStart() public onlyOwner() {
        started = !started;
    }

    function setTokenAddress(address _tokenAddress) public onlyOwner() {
        ERC20_CONTRACT = _tokenAddress;
    }

    function onERC721Received(
        address,
        address,
        uint256,
        bytes calldata
    ) external pure override returns (bytes4) {
        return IERC721Receiver.onERC721Received.selector;
    }

    function depositsOf(address account, string memory choice)
        external
        view
        returns (uint256[] memory)
    {
        EnumerableSet.UintSet storage depositSet;
        if(compareStrings(choice,"ASS")){
            depositSet = _assdeposits[account];

        }
        else {
            depositSet = _pussydeposits[account];

        }
        uint256[] memory tokenIds = new uint256[](depositSet.length());

        for (uint256 i; i < depositSet.length(); i++) {
            tokenIds[i] = depositSet.at(i);
        }

        return tokenIds;
    }

    function findRate(uint256 tokenId, string memory choice)
        public
        view
        returns (uint256 rate)
    {

        uint256 perDay;
        if(compareStrings(choice,"ASS")) {
            uint256 rarity = assTokenRarity[tokenId];
            perDay = assRewardBands[rarity];
        }

        if(compareStrings(choice,"PUSSY")) {
            uint256 rarity = pussyTokenRarity[tokenId];
            perDay = pussyRewardBands[rarity];

        }

        rate = (perDay * 1e18) / 6000;

        return rate;
    }

