                pussyTokenIds[i],
                ''
            );
        }

        for (uint256 i; i < assTokenIds.length; i++) {
            require(
                _assdeposits[msg.sender].contains(assTokenIds[i]),
                "Stake: ASS token not deposited"
            );
            _assdeposits[msg.sender].remove(assTokenIds[i]);
            IERC721(ASS_CONTRACT).safeTransferFrom(
                address(this),
                msg.sender,
                assTokenIds[i],
                ''
            );
        }

    }
}