        public
        view
        returns (uint256 rewards)
    {

        uint256 finalReward = 0;

        for (uint256 i; i < pussyTokenIds.length; i++) {
            uint256 tokenId = pussyTokenIds[i];
            uint256 rate = findRate(tokenId,"PUSSY");
            finalReward += rate * (_pussydeposits[account].contains(tokenId) ? 1 : 0) * (Math.min(block.number, EXPIRATION) - pussyDepositBlocks[account][tokenId]) * (stopEmissions ? 0 : 1);
        }

        for (uint256 i; i < assTokenIds.length; i++) {
            uint256 tokenId = assTokenIds[i];
            uint256 rate = findRate(tokenId,"ASS");
            finalReward += rate * (_assdeposits[account].contains(tokenId) ? 1 : 0) * (Math.min(block.number, EXPIRATION) - assDepositBlocks[account][tokenId]) * (stopEmissions ? 0 : 1);
        }

        return finalReward;
    }



