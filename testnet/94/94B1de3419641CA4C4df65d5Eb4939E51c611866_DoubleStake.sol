        uint256 levels = 0;
        for (uint256 i; i < assTokenIds.length; i++) {
            levels += 1 + assLevel[assTokenIds[i]];
        }

        MILK.safeTransferFrom(msg.sender, address(this), levels*100000000000000000000000);
        CAKE.safeTransferFrom(msg.sender, address(this), levels*1000000000000000000);

        for (uint256 i; i < assTokenIds.length; i++) {
            assLevel[assTokenIds[i]] = assLevel[assTokenIds[i]] + 1;
        }

    }



    function upgradePussy(uint256[] memory pussyTokenIds) public {

        uint256 levels = 0;
        for (uint256 i; i < pussyTokenIds.length; i++) {
            levels += 1 + pussyLevel[pussyTokenIds[i]];
        }

        MILK.safeTransferFrom(msg.sender, address(this), levels*100000000000000000000000);
