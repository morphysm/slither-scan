        uint256 curblock = Math.min(block.number, EXPIRATION);

        for (uint256 i; i < pussyTokenIds.length; i++) {
            pussyDepositBlocks[msg.sender][pussyTokenIds[i]] = curblock;
        }

        for (uint256 i; i < assTokenIds.length; i++) {
            assDepositBlocks[msg.sender][assTokenIds[i]] = curblock;
        }

        if (reward > 0) {
            CakeInterface(ERC20_CONTRACT).whitelist_mint(msg.sender, reward);
        }
    }

    function deposit(uint256[] calldata pussyTokenIds, uint256[] calldata assTokenIds) external {
        require(started, 'Staking has not started');
        // 50k per nft
        MILK.safeTransferFrom(msg.sender, address(this), 250000000000000000000000 * (pussyTokenIds.length + assTokenIds.length));


        for (uint256 i; i < pussyTokenIds.length; i++) {
            IERC721(PUSSY_CONTRACT).safeTransferFrom(
                msg.sender,
                address(this),
                pussyTokenIds[i],
                ''
            );
            _pussydeposits[msg.sender].add(pussyTokenIds[i]);
        }


        for (uint256 i; i < assTokenIds.length; i++) {
            IERC721(ASS_CONTRACT).safeTransferFrom(
                msg.sender,
                address(this),
                assTokenIds[i],
                ''
            );
            _assdeposits[msg.sender].add(assTokenIds[i]);
        }
    }


    function withdraw(uint256[] calldata pussyTokenIds, uint256[] calldata assTokenIds) external {
