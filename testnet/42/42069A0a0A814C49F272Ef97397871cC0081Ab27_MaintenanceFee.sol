pragma solidity 0.8.4;

import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC20/extensions/IERC20MetadataUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "./lib/Claimable.sol";
import "./lib/IValiFIERC20.sol";
import "./lib/Math.sol";
import "hardhat/console.sol";

interface I_ValiFiNFTv2 {
    function balanceOf(address owner) external view returns (uint256);

    function tokenHoldings(address _owner)
        external
        view
        returns (uint256[] memory);
}

interface IYieldBox is IERC721Upgradeable {
    /**
     * @dev Struct of the Yield Box
     */
    struct YieldBoxStruct {
        /** @dev Yield Box Status */
        bool status;
        /** @dev Yield Box Owner */
        address owner;
        /** @dev Yield Box Token ID */
        uint256 id;
        /** @dev Time of Creation */
        uint256 createdAt;
    }

    function getYieldBox(uint256 _tokenIDs)
        external
        returns (YieldBoxStruct memory);

    function tokenHoldings(address _owner)
        external
        view
        returns (uint256[] memory);

    function setRewardClaimed(uint256 tokenId) external;
}

interface IStakedYieldKey is I_ValiFiNFTv2 {
    /**
     * @dev Mapping of Stake Yield Keys
     */
    struct StakeYieldKey {
        bool isStaked;
        uint64 rewardClaimed;
        uint64[] startstake;
        uint64[] endstake;
    }

    function historyStakedYieldKey(address _wallet)
        external
        view
        returns (uint256[] memory);

    function getHistoricalStakedYieldKey(address wallet, uint256 tokenId)
        external
        view
        returns (
            uint64[] memory startstake,
            uint64[] memory endstake,
            uint64 rewarded
        );

    function tokenStaking(address _owner)
        external
        view
        returns (uint256[] memory stakeTokenIds);
}

interface IYieldKey is IStakedYieldKey {
    function amountKeys(uint256 tokenId) external view returns (uint256);
}

/**
 * @title Maintenance Fee Contract / ValiFI Protocols Rewards
 * @dev Implementation of the Maintenance Fee NFT ERC721.
 * @custom:a Alfredo Lopez / Marketingcycle / ValiFI
 */
contract MaintenanceFee is
    Math,
    Claimable,
    PausableUpgradeable,
    ReentrancyGuardUpgradeable
{
    using SafeMathUpgradeable for uint256;
    using AddressUpgradeable for address;
    using SafeERC20Upgradeable for IERC20Upgradeable;
    // Definition Reward per Day of YieldBox
    uint256 private REWARDS_PER_DAYS; // 3 VALIFI tokens per Day
    // percentage of the reward per day without the yield key in staking
    uint8 private REWARDS_PERCENTAGE;
    // Insance of the Smart Contract
    IYieldKey private YieldKey;
    IValiFIERC20 private ValiFIERC20;
    IYieldBox private YieldBox;
    IStakedYieldKey private StakedYieldKey;
    /** @dev Token Address ValiFI */
    address private tokenAddress;
    /** @dev USDT.e Token Compatible with Avalanche Bridge */
    address public constant USDT_ADDRESS =
        address(0xc7198437980c041c805A1EDcbA50c1Ce5db95118);
    /** @dev USDC.e Token Compatible with Avalanche Bridge */
    address public constant USDC_ADDRESS =
        address(0xA7D7079b0FEaD91F3e65f86E8915Cb59c1a4C664);
    /** @dev BUSD.e Token Compatible with Avalanche Bridge */
    address public constant BUSD_ADDRESS =
        address(0x19860CCB0A68fd4213aB9D8266F7bBf05A8dDe98);
    /** @dev DAI.e Token Compatible with Avalanche Bridge */
    address public constant DAI_ADDRESS =
        address(0xd586E7F844cEa2F87f50152665BCbc2C279D8d70);
    /** @dev USDC.e Fake Token Compatible with Avalanche Testnet */
    address public constant USDC_TESTNET_ADDRESS =
        address(0x6ED20721c5E5fB2a6769304F0d0711357b13e1d3);
    /**
     * @dev Struct of the Yield Box
     */
    struct MainFeeStruct {
        /** @dev Maintenance Fee Status */
        bool status;
        /** @dev Maintenance Fee Owner */
        address lastOwner;
        /** @dev Last Reward Paid Out */
        uint64 rewardClaimed;
        /** @dev Array Time Stamp of start Payment of Maint Fee */
        uint64[] startPayFee;
        /** @dev Array Time Stamp of end Payment of Maint Fee */
        uint64[] endPayFee;
    }
    /**@dev Mapping of Maintenance Fee Historically by Token Id*/
    mapping(uint256 => MainFeeStruct) public maintFee;
    /**@dev Mapping of ERC20 token for create Yield Box's */
    mapping(address => uint256) public amountCoin;

    /**
     * @dev Event of Maintenance Fee of Yield Box
     * @param owner of the Yield Box
     * @param id ID of the Yield Box
     * @param startPayFee of Creation the Yield Box
     * @param endPayFee of Creation the Yield Box
     */
    event MaintenanceFeePaid(
        address indexed owner,
        uint256 indexed id,
        uint64 startPayFee,
        uint64 endPayFee
    );
    /**
     * @dev Event of Reward
     * @param owner of the Yield Box
     * @param YieldBoxesIds ID of the Yield Box
     * @param rewardsClaimed TimeStamp of the Reward Claimed
     */
    event RewardsPaid(
        address indexed owner,
        uint256[] YieldBoxesIds,
        uint256 rewardsClaimed
    );
    /**
     * @dev Event when change the Cost per Token
     * @param tokenAddress address or ERC20 Token
     * @param costPerToken new value of the Cost per ERC20 Token
     */
    event SetCostPerToken(address tokenAddress, uint256 costPerToken);
    /**
     * @dev Event when change the Token Address of VALIFi ERC20
     * @param tokenAddress new value of Token Address of VALIFi ERC20
     */
    event SetTokenAddress(address tokenAddress);

    /**
     * @dev Inizialize the contract with the owner address.
     */
    function initialize(
        address _tokenAddress,
        address _yieldBox,
        address _yieldKey,
        address _stakedValiFiNFTv2
    ) public initializer {
        require(
            _tokenAddress != address(0),
            "Token Address of VALIFi ERC20 is not valid"
        );
        __Ownable_init();
        __Pausable_init();
        __ReentrancyGuard_init();
        tokenAddress = _tokenAddress;
        // amountCoin[USDT_ADDRESS] = MulDiv(1, USDT_ADDRESS, 1, 10); // cost per day of Maintenance Fee
        // amountCoin[USDC_ADDRESS] = MulDiv(1, USDC_ADDRESS, 1, 10); // cost per day of Maintenance Fee
        // amountCoin[BUSD_ADDRESS] = MulDiv(1, BUSD_ADDRESS, 1, 10); // cost per day of Maintenance Fee
        // amountCoin[DAI_ADDRESS] = MulDiv(1, DAI_ADDRESS, 1, 10); // cost per day of Maintenance Fee
        amountCoin[USDC_TESTNET_ADDRESS] = MulDiv(
            1,
            USDC_TESTNET_ADDRESS,
            1,
            10
        ); // cost per day of Maintenance Fee
        amountCoin[tokenAddress] = MulDiv(1, tokenAddress, 1, 10);
        YieldBox = IYieldBox(_yieldBox);
        YieldKey = IYieldKey(_yieldKey);
        ValiFIERC20 = IValiFIERC20(_tokenAddress);
        StakedYieldKey = IStakedYieldKey(_stakedValiFiNFTv2);
        REWARDS_PER_DAYS = 3 * 10**18;
        REWARDS_PERCENTAGE = 10;
    }

    /**
     * @dev Implementation / Instance of paused methods() in the ERC721.
     * @param status Setting the status boolean (True for paused, or False for unpaused)
     * See {ERC721Pausable}.
     */
    function pause(bool status) public onlyOwner {
        if (status) {
            _pause();
        } else {
            _unpause();
        }
    }

    /**
     * @dev This Method permit getting the Value in Stable Coin and Token ValiFI to create Yield Box.
     * @param _token Token Address of Stable Coin and Token ValiFI to create Yield Box
     * @return Value in Avax and Token ValiFI for create Yield Box
     */
    function getValue(address _token) public view returns (uint256) {
        return amountCoin[_token];
    }

    /**
     * @dev This Method for pay the Maintenance Fee of Yield Box.
     * @param _stable Token Address of Stable Coin and Token ValiFI to create Yield Box
     * @param _tokenIds Token Ids of Yield Box
     * @param startDaysPayed Start TimeStamp of payment
     * @param endDaysPayed End TimeStamp of payment
     */
    function payMaintenanceFee(
        address _stable,
        uint256[] calldata _tokenIds,
        uint64[] calldata startDaysPayed,
        uint64[] calldata endDaysPayed
    ) external nonReentrant validAddress(_stable) {
        require(
            !paused(),
            "ERC721Pausable: can't create any YieldBox while paused"
        );
        require(
            isValidToken(_stable),
            "ERC721 VALIFi: Token Stable Coin not permitted"
        );
        require(_tokenIds.length > 0, "Token Ids of Yield Box is not valid");
        // Calculate Days per TokenIds
        IERC20Upgradeable _stableToken = IERC20Upgradeable(_stable);
        require(
            _stableToken.balanceOf(_msgSender()) >=
                amountCoin[address(_stableToken)].mul(
                    getTotalDays(_tokenIds, startDaysPayed, endDaysPayed)
                ),
            "ERC721 VALIFi: Not enough Stable Coin"
        );
        // Transfer the Stable Coin to Create the Yield Box
        bool success_treasury = _stableToken.transferFrom(
            _msgSender(),
            ValiFIERC20.getValiFiWallet(2),
            amountCoin[address(_stableToken)].mul(
                getTotalDays(_tokenIds, startDaysPayed, endDaysPayed)
            )
        );
        require(
            success_treasury,
            "ERC721 VALIFi: Can't pay Maintenance Fee, you don't have enough stable coins"
        );
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            require(
                maintFee[_tokenIds[i]].startPayFee.length ==
                    maintFee[_tokenIds[i]].endPayFee.length,
                "ERC721 VALIFi: Inconsistence in the Maintenance Fee TimeStamp Payment"
            );
            maintFee[_tokenIds[i]].status = true;
            maintFee[_tokenIds[i]].lastOwner = _msgSender();
            maintFee[_tokenIds[i]].startPayFee[
                maintFee[_tokenIds[i]].startPayFee.length
            ] = startDaysPayed[i];
            maintFee[_tokenIds[i]].endPayFee[
                maintFee[_tokenIds[i]].endPayFee.length
            ] = endDaysPayed[i];
            emit MaintenanceFeePaid(
                _msgSender(),
                _tokenIds[i],
                startDaysPayed[i],
                endDaysPayed[i]
            );
        }
    }

    /**
     * @dev This Method verify if Maintenance Fee is Payed at the moment.
     * @param _tokenId Token Id of Yield Box
     * @return Status true if value is not doubt, or false if value is 0
     */
    function isAllFeePaidOut(uint256 _tokenId) public returns (bool) {
        require(
            !paused(),
            "ERC721Pausable: can't create any YieldBox while paused"
        );
        if (!maintFee[_tokenId].status) {
            return false;
        }
        if (maintFee[_tokenId].startPayFee.length > 0) {
            require(
                (maintFee[_tokenId].endPayFee.length ==
                    maintFee[_tokenId].startPayFee.length),
                "ERC721 VALIFi: Struct of Maintenance Fee is inconsistent"
            );
            require(
                (maintFee[_tokenId].endPayFee[
                    maintFee[_tokenId].endPayFee.length - 1
                ] >
                    maintFee[_tokenId].startPayFee[
                        maintFee[_tokenId].startPayFee.length - 1
                    ]),
                "ERC721 VALIFi: Maintenance Fee can't finalize before start"
            );
            uint256 amountFeeDaysPaid;
            for (
                uint256 i = 0;
                i < maintFee[_tokenId].startPayFee.length;
                i++
            ) {
                amountFeeDaysPaid += ((maintFee[_tokenId].endPayFee[i] -
                    maintFee[_tokenId].startPayFee[i]) / 1 days);
            }
            if (
                amountFeeDaysPaid >=
                ((block.timestamp - YieldBox.getYieldBox(_tokenId).createdAt) /
                    1 days)
            ) {
                return true;
            } else {
                return false;
            }
        } else {
            return false;
        }
    }

    /**
     * @dev This Method to calculate of Rewards per TokenIds of Yield Box.
     */
    function preReward() public nonReentrant returns (uint256 amount, uint256[] memory YieldBoxRewarded, uint256 time) {
        require(
            !paused(),
            "ERC721Pausable: can't pay any Reward per YieldBox while paused"
        );
        time = block.timestamp;
        uint256[] memory YieldBoxIds = YieldBox.tokenHoldings(_msgSender());
        YieldBoxRewarded = YieldBoxIds;
        require(YieldBoxIds.length > 0, "ERC721 VALIFi: No Yield Box");
        uint64[] memory startStaked;
        uint64[] memory endStaked;
        uint64 rewarded;
        uint256 indexYK;
        if (
            (YieldKey.tokenStaking(_msgSender()).length > 0) ||
            (StakedYieldKey.tokenStaking(_msgSender()).length > 0)
        ) {
            uint256 ykStaked = getYKAvailableStake() + getSKAvailableStake();
            uint256 ykAvailableClaim = ykStaked > YieldBoxIds.length
                ? YieldBoxIds.length
                : ykStaked;
            // case for YieldKey Legendary
            if (
                ((StakedYieldKey.tokenStaking(_msgSender()).length * 12) >=
                    ykAvailableClaim) &&
                (StakedYieldKey.tokenStaking(_msgSender()).length > 0)
            ) {
                (startStaked, endStaked, rewarded) = StakedYieldKey
                    .getHistoricalStakedYieldKey(
                        _msgSender(),
                        StakedYieldKey.tokenStaking(_msgSender())[0]
                    );
                amount =
                    getRewardDays(
                        startStaked,
                        endStaked,
                        rewarded,
                        YieldBoxIds,
                        ykAvailableClaim
                    ) *
                    REWARDS_PER_DAYS;
            } else if (
                ((StakedYieldKey.tokenStaking(_msgSender()).length * 12) <=
                    ykAvailableClaim) &&
                (StakedYieldKey.tokenStaking(_msgSender()).length > 0) &&
                (YieldKey.tokenStaking(_msgSender()).length == 0)
            ) {
                ykAvailableClaim = StakedYieldKey
                    .tokenStaking(_msgSender())
                    .length;
                (startStaked, endStaked, rewarded) = StakedYieldKey
                    .getHistoricalStakedYieldKey(
                        _msgSender(),
                        StakedYieldKey.tokenStaking(_msgSender())[0]
                    );
                amount =
                    getRewardDays(
                        startStaked,
                        endStaked,
                        rewarded,
                        YieldBoxIds,
                        ykAvailableClaim
                    ) *
                    REWARDS_PER_DAYS;
            } else if (
                ((StakedYieldKey.tokenStaking(_msgSender()).length == 0) &&
                    (getYKAvailableStake() >= ykAvailableClaim))
            ) {
                indexYK = getIndexYK(
                    YieldKey.tokenStaking(_msgSender()),
                    ykAvailableClaim
                );
                for (uint256 i = 0; i < indexYK; i++) {
                    (startStaked, endStaked, rewarded) = YieldKey
                        .getHistoricalStakedYieldKey(
                            _msgSender(),
                            YieldKey.tokenStaking(_msgSender())[i]
                        );
                    amount +=
                        getRewardDays(
                            startStaked,
                            endStaked,
                            rewarded,
                            YieldBoxIds,
                            ykAvailableClaim
                        ) *
                        REWARDS_PER_DAYS;
                }
            } else if (
                ((StakedYieldKey.tokenStaking(_msgSender()).length == 0) &&
                    (getYKAvailableStake() < ykAvailableClaim))
            ) {
                for (
                    uint256 i = 0;
                    i < YieldKey.tokenStaking(_msgSender()).length;
                    i++
                ) {
                    (startStaked, endStaked, rewarded) = YieldKey
                        .getHistoricalStakedYieldKey(
                            _msgSender(),
                            YieldKey.tokenStaking(_msgSender())[i]
                        );
                    amount +=
                        getRewardDays(
                            startStaked,
                            endStaked,
                            rewarded,
                            YieldBoxIds,
                            ykAvailableClaim
                        ) *
                        REWARDS_PER_DAYS;
                }
            }
        } else {
            for (uint256 i = 0; i < YieldBoxIds.length; i++) {
                if (
                    (time - YieldBox.getYieldBox(YieldBoxIds[i]).createdAt <
                        90 days) && (isAllFeePaidOut(YieldBoxIds[i]))
                ) {
                    amount += mulDiv(
                        ((time -
                            YieldBox.getYieldBox(YieldBoxIds[i]).createdAt) /
                            1 days),
                        REWARDS_PER_DAYS,
                        REWARDS_PERCENTAGE
                    );
                } else {
                    YieldBoxRewarded[i] = YieldBoxRewarded[
                        YieldBoxRewarded.length - 1
                    ];
                    delete YieldBoxRewarded[YieldBoxRewarded.length - 1];
                    amount += 0;
                }
            }
        }
    }

        /**
     * @dev This Method to calculate of Rewards per TokenIds of Yield Box.
     */
    function reward() external nonReentrant {
        (uint256 amount, uint256[] memory YieldBoxRewarded, uint256 time) = preReward();
        require(
            ValiFIERC20.balanceOf(ValiFIERC20.getValiFiWallet(2)) >= amount,
            "ERC721 VALIFi: Not enough VALIFi Coin in Treasury"
        );
        // Transfer the Stable Coin to Create the Yield Box
        bool success_treasury = ValiFIERC20.transferFrom(
            ValiFIERC20.getValiFiWallet(2),
            _msgSender(),
            amount
        );
        require(success_treasury, "ERC721 VALIFi: Can't pay Rewards");
        emit RewardsPaid(_msgSender(), YieldBoxRewarded, amount);
        for (uint256 i = 0; i < YieldBoxRewarded.length; i++) {
            maintFee[YieldBoxRewarded[i]].rewardClaimed = uint64(time);
            YieldBox.setRewardClaimed(YieldBoxRewarded[i]);
        }
    }

    /**
     * @dev This Method verify if Token Address is Valid to Create Yield Box.
     * @param _token Token Address of Stable Coin and Token ValiFI to create Yield Box
     * @return Status true if value is more then 0, or false if value is 0
     */
    function isValidToken(address _token) public view returns (bool) {
        return amountCoin[_token] > 0;
    }

    /**
     * @dev This Method permit set the Value in Stable Coin and Token ValiFI for create Yield Box.
     * @param _token Token Address of Stable Coin in Avalanche Network, permitted to create Yield Box
     * @param _value Value in Avax and Token ValiFI for create Yield Box
     */
    function setValue(address _token, uint256 _value) external onlyOwner {
        require(
            _value > 0,
            "ERC721 VALIFi: Can't set value, value must be greater than 0"
        );
        amountCoin[_token] = _value;
        emit SetCostPerToken(_token, _value);
    }

    /**
     * @dev This Method permit to set the ERC20 Token Address of VALIFi Project.
     * @param _tokenAddress Token Address
     */
    function setTokenAddress(address _tokenAddress) external onlyOwner {
        require(
            _tokenAddress != address(0),
            "Token Address of VALIFi ERC20 is not valid"
        );
        tokenAddress = _tokenAddress;
        emit SetTokenAddress(_tokenAddress);
    }

    /**
     * @dev This Method for Getting the Total Days to pay for all Yield Box of the Sender
     * @param _tokenIds Token Ids of Yield Box
     * @param startDaysPayed Start TimeStamp of payment
     * @param endDaysPayed End TimeStamp of payment
     */
    function getTotalDays(
        uint256[] calldata _tokenIds,
        uint64[] calldata startDaysPayed,
        uint64[] calldata endDaysPayed
    ) public view returns (uint256 totalDays) {
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            require(
                YieldBox.ownerOf(_tokenIds[i]) != _msgSender(),
                "At least one Token Ids of Yield Box is not valid or the Sender is not the owner"
            );
            require(
                endDaysPayed[i] < startDaysPayed[i],
                "Range of Days is not valid"
            );
            uint64 Days = endDaysPayed[i] - startDaysPayed[i];
            totalDays += (Days * uint64(1 days));
        }
    }

    function MulDiv(
        uint256 value,
        address token,
        uint256 mul,
        uint256 div
    ) public view returns (uint256) {
        return
            mulDiv(
                uint256(value).mul(
                    10**uint256(IERC20MetadataUpgradeable(token).decimals())
                ),
                mul,
                div
            );
    }

    /**
     * @dev This Method permit to get the Total YieldBox available to claim Rewards,
     * @dev based on the amount of YieldKeys (Ramdom or Legendary) available of the Sender)
     * @return result Total YieldBox available to claim Rewards
     */
    function getYKAvailableStake() public view returns (uint256 result) {
        uint256[] memory YKtokenIds = YieldKey.tokenStaking(_msgSender());
        for (uint256 i = 0; i < YKtokenIds.length; i++) {
            result += YieldKey.amountKeys(YKtokenIds[i]);
        }
    }

    /**
     * @dev This Method permit to get the Total YieldBox available to claim Rewards,
     * @dev based on the amount of YieldKeys (Ramdom or Legendary) available of the Sender)
     * @return result Total YieldBox available to claim Rewards
     */
    function getSKAvailableStake() public view returns (uint256 result) {
        uint256[] memory SKtokenIds = StakedYieldKey.tokenStaking(_msgSender());
        for (uint256 i = 0; i < SKtokenIds.length; i++) {
            result += 12;
        }
    }

    function getRewardDays(
        uint64[] memory startStaked,
        uint64[] memory endStaked,
        uint64 rewarded,
        uint256[] memory YieldBoxIds,
        uint256 ykAvailableClaim
    ) public view returns (uint256 amountDays) {
        for (
            uint256 i = 0;
            i <
            (
                ykAvailableClaim > YieldBoxIds.length
                    ? YieldBoxIds.length
                    : ykAvailableClaim
            );
            i++
        ) {
            amountDays += getIntersectionDays(
                startStaked,
                endStaked,
                rewarded,
                YieldBoxIds[i]
            );
        }
    }

    function getIntersectionDays(
        uint64[] memory startStaked,
        uint64[] memory endStaked,
        uint64 rewarded,
        uint256 YieldBoxId
    ) internal view returns (uint256 result) {
        uint64 rewardClaimed = maintFee[YieldBoxId].rewardClaimed;
        (uint64[] memory startPayFee, uint64[] memory endPayFee) = getRange(
            maintFee[YieldBoxId].startPayFee,
            maintFee[YieldBoxId].endPayFee,
            rewardClaimed
        );
        uint64 startIntersec = rewarded > rewardClaimed
            ? rewarded
            : rewardClaimed;
        uint64 endIntersec = endStaked[endStaked.length - 1] == uint64(0) ||
            endPayFee[endPayFee.length - 1] >= block.timestamp
            ? uint64(block.timestamp)
            : (
                endPayFee[endPayFee.length - 1] >
                    endStaked[endStaked.length - 1]
                    ? endPayFee[endPayFee.length - 1]
                    : endStaked[endStaked.length - 1]
            );
        endStaked[endStaked.length - 1] = uint64(block.timestamp);
        (startStaked, endStaked) = getRange(startStaked, endStaked, rewarded);
        uint256 numDays = (endIntersec - startIntersec) / 1 days; // Get the number of days to Verify if in range of Staked and Fee
        for (uint256 i = 0; i < numDays; i++) {
            uint64 dayToAnalyze = startIntersec + uint64(i * 1 days);
            if (
                getInRange(startStaked, endStaked, dayToAnalyze) &&
                (getInRange(startPayFee, endPayFee, dayToAnalyze))
            ) {
                result += 1;
            }
        }
    }

    function getInRange(
        uint64[] memory startRange,
        uint64[] memory endRange,
        uint64 value
    ) internal pure returns (bool) {
        return getIndex(startRange, endRange, value) > 0;
    }

    function getIndex(
        uint64[] memory startRange,
        uint64[] memory endRange,
        uint64 value
    ) internal pure returns (uint256) {
        for (uint256 i = 0; i < startRange.length; i++) {
            if (startRange[i] <= value && endRange[i] >= value) {
                return i;
            }
        }
        return 0;
    }

    function getRange(
        uint64[] memory startRange,
        uint64[] memory endRange,
        uint64 value
    ) internal pure returns (uint64[] memory start, uint64[] memory end) {
        uint256 index = getIndex(startRange, endRange, value);
        for (uint256 i = index; i < startRange.length; i++) {
            start[i - index] = startRange[i];
            end[i - index] = endRange[i];
        }
    }

    function getIndexYK(uint256[] memory YieldBoxIds, uint256 ykAvailableClaim)
        internal
        view
        returns (uint256)
    {
        uint256 result;
        for (uint256 i = 0; i < YieldBoxIds.length; i++) {
            result += YieldKey.amountKeys(YieldBoxIds[i]);
            if (result >= ykAvailableClaim) {
                return i;
            }
        }
        return 0;
    }
}

