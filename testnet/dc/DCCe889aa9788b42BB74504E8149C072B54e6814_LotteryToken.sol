pragma solidity 0.8.9;

import "@openzeppelin/contracts/access/Ownable.sol";
import "./ERC20.sol";
import "../interfaces/ILotteryToken.sol";
import "../interfaces/ILotteryGame.sol";
import "hardhat/console.sol";

/// @title LotteryToken
/// @dev Rebase token
contract LotteryToken is ILotteryToken, Ownable, ERC20 {
    uint256 public constant GAMES_PER_EPOCH = 30;
    uint256 public constant PRECISION = 1 ether;

    uint256 public constant TOTAL_ALLOCATED = 2_000_000_000;

    Epoch[] public epochs;
    Lottery[] public lotteries;

    bool public isTransferLocked;
    /// @notice address of rewardDistribution contract where all rewards are maintain
    /// @notice and user can claim it there
    address public rewardPool;
    address[] public participants;
    address[] private emptyAddr;
    uint256[] private emptyUInt;
    mapping(address => UserBalance) public balances;
    /// @notice A record of votes checkpoints for each account, by index
    mapping(address => mapping(uint256 => Snapshot)) public snapshots;
    /// @notice The number of checkpoints for each account
    mapping(address => uint256) public countSnapshots;
    /// @notice lottery game contract
    ILotteryGame public lotteryGame;
    /// @notice a record of accounts ids and points of games from what should be updated balances
    mapping(address => WhiteLlistedInfo) public whitelistedAccounts;

    modifier onlyLotteryGame() {
        require(
            msg.sender == address(lotteryGame),
            "Only allowed to call by Lottery game contract"
        );
        _;
    }

    /// @notice constructor
    /// @dev mint tokens
    /// @param _lotteryGame address of lottery game contract
    /// @param _tokenRecipient address where initial supply will be minted
    constructor(address _lotteryGame, address _tokenRecipient)
        ERC20("Lotto", "LOTTO")
    {
        lotteryGame = ILotteryGame(_lotteryGame);

        // 2,000,000,000 allocate for all
        _mint(_tokenRecipient, TOTAL_ALLOCATED * PRECISION);

        epochs.push(
            Epoch(
                0, // uint256 totalFees;
                0, // uint256 minParticipationFee;
                0, // uint256 firstGameId;
                0 // uint256 lastGameId;
            )
        );
        emptyAddr.push(address(0));
        emptyUInt.push(0);

        lotteries.push(
            Lottery(
                0, // uint256 id;
                0, // uint256 participationFee;
                0, // uint256 startedAt;
                0, // uint256 finishedAt;
                0, // uint256 participants;
                emptyAddr, // address[] winner;
                0, // uint256 epochId;
                emptyUInt, // uint256[] winningPrize;
                0, //uint256 reward;
                address(0), //address rewardPool;
                false // bool isActive;
            )
        );
    }

    /// @dev burn tokens by owner
    /// @param account address burt tokens for
    /// @param amount amount of tokens
    function burn(address account, uint256 amount) external onlyOwner {
        _burn(account, amount);
    }

    /// @dev disable transfers
    /// can be called by lottery game contract only
    function lockTransfer() public override onlyLotteryGame {
        isTransferLocked = true;
    }

    /// @dev enable transfers
    /// can be called by lottery game contract only
    function unlockTransfer() public override onlyLotteryGame {
        isTransferLocked = false;
    }

    /// @notice set lottery game address
    /// @param lotteryGame_ address of the main game
    function setLotteryGame(address lotteryGame_) external onlyOwner {
        lotteryGame = ILotteryGame(lotteryGame_);
    }

    /// @dev start new game
    /// @param _participationFee amount of tokens needed to participaint in the game
    function startLottery(uint256 _participationFee)
        public
        override
        onlyLotteryGame
        returns (Lottery memory _lotteryGame)
    {
        require(rewardPool != address(0), "ZERO_ADDRESS");
        Lottery storage lottery = lotteries[lotteries.length - 1];

        // in case if prev game was not finished, halt it
        if (lottery.finishedAt == 0) {
            lottery.finishedAt = block.timestamp;
            lottery.isActive = false;
        }

        Epoch storage currentEpoch = epochs[epochs.length - 1];

        uint256 newGameId = lotteries.length;

        if (newGameId - currentEpoch.firstLotteryId >= GAMES_PER_EPOCH) {
            currentEpoch.lastLotteryId = lottery.id;

            epochs.push(Epoch(0, _participationFee, newGameId, 0));

            currentEpoch = epochs[epochs.length - 1];
        } else {
            if (currentEpoch.minParticipationFee == 0) {
                currentEpoch.minParticipationFee = _participationFee;
            }
            if (currentEpoch.minParticipationFee > _participationFee) {
                currentEpoch.minParticipationFee = _participationFee;
            }
        }

        lotteries.push(
            Lottery(
                newGameId, // uint256 id;
                _participationFee, // uint256 participationFee;
                block.timestamp, // uint256 startedAt;
                0, // uint256 finishedAt;
                0, // uint256 participants;
                emptyAddr, // address[] winner;
                epochs.length - 1, // uint256 epochId;
                emptyUInt, // uint256[] winningPrize;
                0, //uint256 rewards;
                address(0), //address rewardPool;
                true // bool isActive;
            )
        );

        return lotteries[lotteries.length - 1];
    }

    /// @dev finish game
    /// @param _participants count of participants
    /// @param _winnerAddresses address of winner
    /// @param _marketingAddress marketing address
    /// @param _winningPrizeValues amount of winning prize in tokens
    /// @param _marketingFeeValue amount of marketing fee in tokens
    /// @param _rewards amount of community and governance rewards
    function finishLottery(
        uint256 _participants,
        address[] memory _winnerAddresses,
        address _marketingAddress,
        uint256[] memory _winningPrizeValues,
        uint256 _marketingFeeValue,
        uint256 _rewards
    )
        external
        override
        onlyLotteryGame
        returns (Lottery memory finishedLotteryGame)
    {
        Lottery storage lottery = lotteries[lotteries.length - 1];

        require(lottery.isActive == true, "Lottery game is not active");

        lottery.participants = _participants;
        lottery.winners = _winnerAddresses;
        lottery.winningPrize = _winningPrizeValues;
        lottery.rewards = _rewards;

        lottery.finishedAt = block.timestamp;
        lottery.isActive = false;

        Epoch storage epoch = epochs[lottery.epochId];
        epoch.lastLotteryId = lottery.id;
        epoch.totalFees = epoch.totalFees + lottery.participationFee;

        // change all winners balances
        uint256 winnersCount = _winnerAddresses.length;
        for (uint256 i = 0; i < winnersCount; i++) {
            uint256 winnerBalance = balanceOf(_winnerAddresses[i]) +
                _winningPrizeValues[i];

            balances[_winnerAddresses[i]] = UserBalance(
                lottery.id,
                winnerBalance,
                block.timestamp
            );

            emit Transfer(
                address(this),
                _winnerAddresses[i],
                _winningPrizeValues[i]
            );
        }

        //change game caller balance
        uint256 marketingBalance = balanceOf(_marketingAddress) +
            _marketingFeeValue;

        balances[_marketingAddress] = UserBalance(
            lottery.id,
            marketingBalance,
            block.timestamp
        );

        emit Transfer(address(this), _marketingAddress, _marketingFeeValue);

        //change reward pool balance
        uint256 rewardPoolBalance = balanceOf(rewardPool) + _rewards;

        balances[rewardPool] = UserBalance(
            lottery.id,
            rewardPoolBalance,
            block.timestamp
        );

        emit Transfer(address(this), rewardPool, _rewards);

        _writeSnapshotAfterGame();
        return lottery;
    }

    /// @notice Force finish of game with id <lotteryId>
    /// @param lotteryId id of lottery game to be needed shut down
    function forceFinish(uint256 lotteryId) external override onlyLotteryGame {
        require(lotteryId < lotteries.length, "INCORRECT_ID");
        Lottery storage lottery = lotteries[lotteryId];

        // in case if prev game was not finished, halt it
        if (lottery.finishedAt == 0) {
            lottery.finishedAt = block.timestamp;
            lottery.isActive = false;
        }
    }

    /// @notice Set address of reward pool to accumulate governance and community rewards at
    /// @dev Can be called only by lottery game contract
    /// @param _rewardPool address of reward distribution contract
    function setRewardPool(address _rewardPool)
        external
        override
        onlyLotteryGame
    {
        rewardPool = _rewardPool;
        emit RewardPoolChanged(_rewardPool);
    }

    /// @notice get totalSypply of tokens
    /// @dev used only in CustomLotteryGame
    function getTotalSupply() public view returns (uint256) {
        return _totalSupply;
    }

    /// @dev Returns the amount of tokens owned by `account`
    /// @param account address of user to take balance for
    /// @return return uint256 amount of user's balance
    function balanceOf(address account) public view override returns (uint256) {
        UserBalance storage userBalance = balances[account];

        if (lotteries.length == 0 || userBalance.balance == 0) {
            return userBalance.balance;
        }

        Lottery storage lottery = lotteries[userBalance.lastGameId];
        Epoch storage epoch = epochs[lottery.epochId];

        uint256 calculatedBalance = userBalance.balance;

        if (epoch.lastLotteryId > userBalance.lastGameId) {
            calculatedBalance = _calculateBalanceAfterGames(
                calculatedBalance,
                userBalance.lastGameId + 1,
                epoch.lastLotteryId,
                account
            );
        }

        if (lottery.epochId + 1 < epochs.length) {
            for (
                uint256 epochId = lottery.epochId + 1;
                epochId < epochs.length && calculatedBalance > 0;
                epochId++
            ) {
                epoch = epochs[epochId];

                if (calculatedBalance >= epoch.totalFees) {
                    calculatedBalance = calculatedBalance - epoch.totalFees;
                } else {
                    if (calculatedBalance > epoch.minParticipationFee) {
                        calculatedBalance = _calculateBalanceAfterGames(
                            calculatedBalance,
                            epoch.firstLotteryId,
                            epoch.lastLotteryId,
                            account
                        );
                    }
                }
            }
        }

        return calculatedBalance;
    }

    /// @dev Returns participants count
    function participantsCount() public view override returns (uint256) {
        return participants.length;
    }

    /// @dev Returns lotteries count
    function lotteriesCount() public view returns (uint256) {
        return lotteries.length;
    }

    /// @dev Returns epoch count
    function epochsCount() public view returns (uint256) {
        return epochs.length;
    }

    /// @notice Return if user is whitelisted or not
    /// @param user_ address to check info for
    /// @return return true or false to point if the address is whitelisted
    function isWhitelisted(address user_) external view override returns(bool){
        return whitelistedAccounts[user_].isWhitelisted;
    }
    
    /// @dev Returns last lottery
    function lastLottery()
        public
        view
        override
        returns (Lottery memory lottery)
    {
        if (lotteries.length == 0) {
            return
                Lottery(
                    0,
                    0,
                    0,
                    0,
                    0,
                    emptyAddr,
                    0,
                    emptyUInt,
                    0,
                    address(0),
                    false
                );
        }

        return lotteries[lotteries.length - 1];
    }

    /// @dev Returns last epoch
    function lastEpoch() external view override returns (Epoch memory epoch) {
        return epochs[epochs.length - 1];
    }

    /// @dev Return voting power of the 'account' at the specific period of time 'blockTimestamp'
    /// @param account address to check voting power for
    /// @param blockTimestamp timestamp in second to check voting power at
    function getVotingPowerAt(address account, uint256 blockTimestamp)
        external
        view
        returns (uint256)
    {
        require(blockTimestamp < block.timestamp, "Invalid block timestamp");

        uint256 numSnapshots = countSnapshots[account];
        if (numSnapshots == 0) {
            return 0;
        }

        // First check most recent balance
        if (
            snapshots[account][numSnapshots - 1].blockTimestamp <=
            blockTimestamp
        ) {
            return snapshots[account][numSnapshots - 1].votes;
        }

        // Next check implicit zero balance
        if (snapshots[account][0].blockTimestamp > blockTimestamp) {
            return 0;
        }

        uint256 lower = 0;
        uint256 upper = numSnapshots - 1;
        while (upper > lower) {
            uint256 center = upper - (upper - lower) / 2; // ceil, avoiding overflow
            Snapshot memory ss = snapshots[account][center];
            if (ss.blockTimestamp == blockTimestamp) {
                return ss.votes;
            } else if (ss.blockTimestamp < blockTimestamp) {
                lower = center;
            } else {
                upper = center - 1;
            }
        }
        return snapshots[account][lower].votes;
    }

    /// @dev Returns participants balances  in interval starting `from` to `count`
    function participantsBalances(uint256 from, uint256 count)
        public
        view
        override
        returns (
            address[] memory participantAddresses,
            uint256[] memory participantBalances
        )
    {
        uint256 finalCount = from + count <= participants.length
            ? count
            : participants.length - from;

        participantAddresses = new address[](finalCount);
        participantBalances = new uint256[](finalCount);

        for (uint256 i = from; i < from + finalCount; i++) {
            participantAddresses[i - from] = participants[i];
            participantBalances[i - from] = balanceOf(participants[i]);
        }
    }

    /// @notice added accounts to whitelist
    /// @dev owner should be a governance
    /// @param accounts addresses of accounts that will be added to wthitelist
    function addToWhitelist(address[] memory accounts)
        external
        override
        onlyOwner
    {
        require(accounts.length > 0, "EMPTY_LIST");
        uint256 lastExistedGameId = lotteries[lotteries.length - 1].id;
        for (uint256 i = 0; i < accounts.length; i++) {
            if (
                !whitelistedAccounts[accounts[i]].isWhitelisted &&
                accounts[i] != address(0)
            ) {
                whitelistedAccounts[accounts[i]]
                    .lastParticipatedGameId = lastExistedGameId;
                whitelistedAccounts[accounts[i]].isWhitelisted = true;
            }
        }

        emit AddedToWhitelist(accounts, lastExistedGameId);
    }

    /// @notice delete accounts from whitelist
    /// @dev owner should be a governance
    /// @dev updating accounts balances before deleteing
    /// @param accounts addresses of accounts that will be deleted from wthitelist
    function deleteFromWhitelist(address[] memory accounts)
        external
        override
        onlyOwner
    {
        require(accounts.length > 0, "EMPTY_LIST");

        for (uint256 i = 0; i < accounts.length; i++) {
            if (whitelistedAccounts[accounts[i]].isWhitelisted) {
                // change balances before deleting
                uint256 updatedBalance = balanceOf(accounts[i]);

                if (balances[accounts[i]].at == 0) {
                    participants.push(accounts[i]);
                }
                balances[accounts[i]] = UserBalance(
                    lotteries[lotteries.length - 1].id,
                    updatedBalance,
                    block.timestamp
                );

                delete whitelistedAccounts[accounts[i]];
            }
        }

        emit DeletedFromWhiteList(accounts);
    }

    function _calculateBalanceAfterGames(
        uint256 _calculatedBalance,
        uint256 _fromGameId,
        uint256 _toGameId,
        address _account
    ) internal view returns (uint256) {
        // is not whitelisted
        uint256 toGameId = !whitelistedAccounts[_account].isWhitelisted
            ? _toGameId
            : whitelistedAccounts[_account].lastParticipatedGameId;
        for (
            uint256 i = _fromGameId;
            i <= toGameId && _calculatedBalance > 0;
            i++
        ) {
            Lottery storage lottery = lotteries[i];
            if (_calculatedBalance >= lottery.participationFee) {
                _calculatedBalance =
                    _calculatedBalance -
                    lottery.participationFee;
            }
        }

        return _calculatedBalance;
    }

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal override(ERC20) {
        require(sender != address(0), "ERC20: transfer from the zero address");
        require(recipient != address(0), "ERC20: transfer to the zero address");
        require(
            !isTransferLocked,
            "The Game is running, all transfers are locked"
        );

        _beforeTokenTransfer(sender, recipient, amount);

        uint256 senderBalance = balanceOf(sender);
        require(
            senderBalance >= amount,
            "ERC20: transfer amount exceeds balance"
        );

        Lottery memory lottery = lastLottery();

        unchecked {
            balances[sender] = UserBalance(
                lottery.id,
                senderBalance - amount,
                block.timestamp
            );
        }

        if (balances[recipient].at == 0 && amount > 0) {
            participants.push(recipient);
        }

        uint256 recipientBalance = balanceOf(recipient) + amount;

        balances[recipient] = UserBalance(
            lottery.id,
            recipientBalance,
            block.timestamp
        );

        emit Transfer(sender, recipient, amount);
    }

    function _mint(address account, uint256 amount) internal override {
        require(account != address(0), "ERC20: mint to the zero address");

        _beforeTokenTransfer(address(0), account, amount);

        uint256 balance = balanceOf(account) + amount;

        if (balances[account].at == 0 && amount > 0) {
            participants.push(account);
        }

        Lottery memory lottery = lastLottery();

        balances[account].lastGameId = lottery.id;
        balances[account].balance = balance;
        balances[account].at = block.timestamp;

        _totalSupply = _totalSupply + amount;

        emit Transfer(address(0), account, amount);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        _moveVotingPower(from, to, amount);
    }

    function _burn(address account, uint256 amount) internal override {
        require(account != address(0), "ERC20: burn from the zero address");
        _beforeTokenTransfer(account, address(0), amount);
        uint256 balance = balanceOf(account);
        require(balance >= amount, "ERC20: burn amount exceeds balance");
        Lottery memory lottery = lastLottery();
        unchecked {
            balances[account].lastGameId = lottery.id;
            balances[account].balance = balance - amount;
            balances[account].at = block.timestamp;

            _totalSupply = _totalSupply - amount;
        }

        emit Transfer(account, address(0), amount);
    }

    function _moveVotingPower(
        address from,
        address to,
        uint256 amount
    ) internal {
        if (from != to && amount > 0) {
            if (from != address(0)) {
                uint256 fromSnapshotsCount = countSnapshots[from];
                uint256 fromOldAmount = fromSnapshotsCount > 0
                    ? snapshots[from][fromSnapshotsCount - 1].votes
                    : 0;
                uint256 fromNewAmount = fromOldAmount - amount;
                _writeShapshot(from, fromSnapshotsCount, fromNewAmount);
            }

            if (to != address(0)) {
                uint256 toSnapshotsCount = countSnapshots[to];
                uint256 toOldAmount = toSnapshotsCount > 0
                    ? snapshots[to][toSnapshotsCount - 1].votes
                    : 0;
                uint256 toNewAmount = toOldAmount + amount;
                _writeShapshot(to, toSnapshotsCount, toNewAmount);
            }
        }
    }

    function _writeSnapshotAfterGame() internal {
        for (uint256 i = 0; i < participants.length; i++) {
            address account = participants[i];
            uint256 accSnapshotsCount = countSnapshots[account];
            uint256 accNewAmount = balanceOf(account);
            _writeShapshot(account, accSnapshotsCount, accNewAmount);
        }
    }

    function _writeShapshot(
        address delegatee,
        uint256 numSnapshots,
        uint256 newVotes
    ) internal {
        uint256 blockTimestamp = block.timestamp;

        if (
            numSnapshots > 0 &&
            snapshots[delegatee][numSnapshots - 1].blockTimestamp ==
            blockTimestamp
        ) {
            snapshots[delegatee][numSnapshots - 1].votes = newVotes;
        } else {
            snapshots[delegatee][numSnapshots] = Snapshot(
                blockTimestamp,
                newVotes
            );
            countSnapshots[delegatee] = numSnapshots + 1;
        }
        emit VotingPowerChanged(delegatee, newVotes);
    }
}

