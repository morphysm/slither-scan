// File contracts/matic/SpaceMissions.sol


pragma solidity 0.8.9;


















// ? TO DELETE v

contract SpaceMissions is
    Initializable,
    IERC721ReceiverUpgradeable,
    ReentrancyGuardUpgradeable,
    AccessControlMixinUpgradeable,
    ERC1155Holder
{
    using SafeMathUpgradeable for uint256;
    using MathUpgradeable for uint256;
    using AddressUpgradeable for address;
    using CountersUpgradeable for CountersUpgradeable.Counter;
    using EnumerableSet for EnumerableSet.AddressSet;
    using EnumerableSet for EnumerableSet.UintSet;

    // STATE VARIABLES
    EnumerableSet.AddressSet private _whitelistRewardsSet;

    mapping(uint256 => SpaceMission) private _missions;
    CountersUpgradeable.Counter private _missionCounter; // number of types of missions

    mapping(uint256 => uint256) private _moonieCooldowns;

    mapping(uint256 => OngoingMission) private _ongoingMissions;
    CountersUpgradeable.Counter private _missionNonce; // number of every instance of mission
    EnumerableSet.UintSet ongoingMissionIds;

    IERC721Burnable private _nft;
    MoonieItem private _itemNft;
    IMoonieStats private _moonieStats;
    IRandomSource private _randomSource;
    IMoonchest private _moonchest;
    IPriceOracle private _priceOracle;

    IERC20 private _moonieToken;

    mapping(uint256 => uint256) private _nftMissionEndTime;

    address private _bank;

    bytes private _keyMNY;

    // global time unit to calculate reset Fees
    uint256 private _timeUnit;
    // global price for time unit
    uint256 private _timeUnitPrice;

    // STRUCTS

    struct OngoingMission {
        uint256 moonieTokenId;
        address owner;
        uint256 missionFinishTime;
        uint256 missionId;
    }

    struct SpaceMission {
        string missionName;
        uint256 missionLength;
        bool active; // blocks ability to start a new mission (missions already started can be finalized)
        uint256 successChance;
        uint256 deathChance;
        AttributeBonus[] attributeBonuses;
        TypeBonus[] typeBonuses;
        uint256 entryFee;
        uint256 cooldown; // e.g. 24 h = 86400
        uint256 nothingCooldown; // e.g. 72h = 259200
        MissionRequirements requirements;
        MissionRewards rewards;
    }

    struct NFTRequirement {
        uint256 nftType;
        uint256 nftNo;
    }

    struct MissionRequirements {
        uint256[] moonieTypesAllowedToEnter; // when empty, any type can enter
        NFTRequirement[] mooniesHeldRequired;
        NFTRequirement[] moonieItemsRequired;
        bool whitelist;
        EnumerableSet.AddressSet whitelistedWallets;
        uint256 mnyHeldRequired;
    }

    struct AttributeBonus {
        uint256 attributeIndex;
        uint256 attributeValue; // when 0, any value is accepted
        bool reduceSuccessChance;
        uint256 successChance;
        uint256 deathChance;
        bool removeCooldown;
    }

    struct TypeBonus {
        uint256 moonieType;
        bool reduceSuccessChance;
        uint256 successChance;
        uint256 deathChance;
    }

    struct RewardItem {
        uint256 moonieItemId;
        uint256 rewardChance;
    }

    struct MissionRewards {
        EnumerableSet.UintSet unlocksMissions;
        uint256 noCooldownChance;
        RewardItem[] rewardItems;
        uint256 mnyRewardChance;
        uint256 mnyRewardAmount;
    }

    struct UmbrellaValues {
        uint256 blockHeight;
        bytes32[] proofMNY;
        bytes valueMNY;
    }

    // EVENTS
    event MissionCreated(uint256 indexed missionId, address senderAddress);
    event MissionStarted(
        uint256 indexed missionId,
        uint256 indexed ongoingMissionId,
        uint256 indexed tokenId,
        address ownerAddress
    );
    event MissionFinished(
        uint256 indexed tokenId,
        uint256 indexed ongoingMissionId,
        address ownerAddress,
        uint256 indexed result
    );
    event CounterEvent(uint256 index);

    // STATE CONSTANTS
    bytes32 public constant OFFICER_ROLE = keccak256("OFFICER_ROLE");

    // MODIFIERS

    modifier onlyContract(address account) {
        require(
            account.isContract(),
            "MoonieGateway: The address must be a contract"
        );
        _;
    }

    // INITIALIZER

    function initialize(
        address nft_, // OK
        address itemNft_, // OK
        address moonieStats_, // OK
        address randomSource_, // OK
        address moonchest_, // OK
        address moonieToken_, // OK
        address bank_, // OK
        address priceOracle_ // OK
    )
        public
        initializer
        onlyContract(nft_)
        onlyContract(itemNft_)
        onlyContract(moonieStats_)
        onlyContract(randomSource_)
        onlyContract(priceOracle_)
    {
        _nft = IERC721Burnable(nft_);
        _itemNft = MoonieItem(itemNft_);
        _moonieStats = IMoonieStats(moonieStats_);
        _moonchest = IMoonchest(moonchest_);
        _randomSource = IRandomSource(randomSource_);
        _moonieToken = IERC20(moonieToken_);
        _bank = bank_;
        _priceOracle = IPriceOracle(priceOracle_);
        __ReentrancyGuard_init();
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        _missionNonce.increment();
        _missionCounter.increment();
        _keyMNY = "0xcoffe";
    }

    // VIEWS

    function nft() public view returns (IERC721) {
        return _nft;
    }

    function getMissionStats(uint256 missionId)
        external
        view
        returns (bytes memory)
    {
        SpaceMission storage mission = _missions[missionId];
        return
            abi.encode(
                mission.missionName,
                mission.missionLength,
                mission.active,
                mission.successChance,
                mission.deathChance,
                mission.entryFee,
                mission.cooldown,
                mission.nothingCooldown
            );
    }

    function getMissionName(uint256 ongoingMissionId)
        external
        view
        returns (bytes memory)
    {
        uint256 missionId = _ongoingMissions[ongoingMissionId].missionId;
        SpaceMission storage mission = _missions[missionId];
        return abi.encode(mission.missionName);
    }

    function getMissionAttributeBonusCount(uint256 missionId)
        external
        view
        returns (uint256)
    {
        SpaceMission storage mission = _missions[missionId];
        return mission.attributeBonuses.length;
    }

    function getMissionAttributeBonus(uint256 missionId, uint256 index)
        external
        view
        returns (
            uint256,
            uint256,
            bool,
            uint256,
            uint256
        )
    {
        SpaceMission storage mission = _missions[missionId];
        require(
            index < mission.attributeBonuses.length,
            "SpaceMission: Index out of band."
        );
        AttributeBonus storage attr = mission.attributeBonuses[index];
        return (
            attr.attributeIndex,
            attr.attributeValue,
            attr.reduceSuccessChance,
            attr.successChance,
            attr.deathChance
        );
    }

    function getMissionTypeBonusCount(uint256 missionId)
        external
        view
        returns (uint256)
    {
        SpaceMission storage mission = _missions[missionId];
        return mission.typeBonuses.length;
    }

    function getMissionTypeBonus(uint256 missionId, uint256 index)
        external
        view
        returns (
            uint256,
            bool,
            uint256,
            uint256
        )
    {
        SpaceMission storage mission = _missions[missionId];
        require(
            index < mission.typeBonuses.length,
            "SpaceMission: Index out of band."
        );
        TypeBonus storage typ = mission.typeBonuses[index];
        return (
            typ.moonieType,
            typ.reduceSuccessChance,
            typ.successChance,
            typ.deathChance
        );
    }

    function getMoonieTypesAllowedFromMissionRequirements(uint256 missionId)
        external
        view
        returns (bytes memory)
    {
        SpaceMission storage mission = _missions[missionId];
        return
            abi.encode(
                mission.requirements.moonieTypesAllowedToEnter,
                mission.requirements.whitelist /*, /*mission.requirements.whitelistedWallets*/
            );
    }

    function getMnyHeldFromMissionRequirements(uint256 missionId)
        external
        view
        returns (uint256)
    {
        SpaceMission storage mission = _missions[missionId];
        return mission.requirements.mnyHeldRequired;
    }

    function getMoonieTypesHeldFromMissionRequirements(uint256 missionId)
        external
        view
        returns (bytes memory)
    {
        SpaceMission storage mission = _missions[missionId];
        uint256 lenOfMoonieTypes = mission
            .requirements
            .mooniesHeldRequired
            .length;
        uint256[] memory moonieTypesToReturn = new uint256[](lenOfMoonieTypes);
        uint256[] memory moonieNoTypesToReturn = new uint256[](
            lenOfMoonieTypes
        );
        for (uint256 i = 0; i < lenOfMoonieTypes; i++) {
            moonieTypesToReturn[i] = mission
                .requirements
                .mooniesHeldRequired[i]
                .nftType;
            moonieNoTypesToReturn[i] = mission
                .requirements
                .mooniesHeldRequired[i]
                .nftNo;
        }
        return abi.encode(moonieTypesToReturn, moonieNoTypesToReturn);
    }

    function getMoonieItemsHeldFromMissionRequirements(uint256 missionId)
        external
        view
        returns (bytes memory)
    {
        SpaceMission storage mission = _missions[missionId];
        uint256 lenOfMoonieItemTypes = mission
            .requirements
            .moonieItemsRequired
            .length;
        uint256[] memory moonieItemTypesToReturn = new uint256[](
            lenOfMoonieItemTypes
        );
        uint256[] memory moonieNoItemTypesToReturn = new uint256[](
            lenOfMoonieItemTypes
        );
        for (uint256 i = 0; i < lenOfMoonieItemTypes; i++) {
            moonieItemTypesToReturn[i] = mission
                .requirements
                .moonieItemsRequired[i]
                .nftType;
            moonieNoItemTypesToReturn[i] = mission
                .requirements
                .moonieItemsRequired[i]
                .nftNo;
        }
        return abi.encode(moonieItemTypesToReturn, moonieNoItemTypesToReturn);
    }

    function getWhitelistRequirements(uint256 missionId)
        external
        view
        returns (bytes memory)
    {
        SpaceMission storage mission = _missions[missionId];
        uint256 lenOfWhitelist = mission
            .requirements
            .whitelistedWallets
            .length();
        address[] memory walletsWhitelisted = new address[](lenOfWhitelist);

        for (uint256 i = 0; i < lenOfWhitelist; i++) {
            walletsWhitelisted[i] = mission.requirements.whitelistedWallets.at(
                i
            );
        }
        return abi.encode(walletsWhitelisted);
    }

    function getMissionRewards(uint256 missionId)
        external
        view
        returns (bytes memory)
    {
        SpaceMission storage mission = _missions[missionId];
        uint256 lenOfUnlocksMissions = mission.rewards.unlocksMissions.length();
        uint256[] memory unlocksMissionsRewards = new uint256[](
            lenOfUnlocksMissions
        );

        for (uint256 i = 0; i < lenOfUnlocksMissions; i++) {
            unlocksMissionsRewards[i] = mission.rewards.unlocksMissions.at(i);
        }

        uint256 lenOfItemRewards = mission.rewards.rewardItems.length;
        uint256[] memory itemIdReward = new uint256[](lenOfItemRewards);
        uint256[] memory noItemIdReward = new uint256[](lenOfItemRewards);

        for (uint256 i = 0; i < lenOfItemRewards; i++) {
            itemIdReward[i] = mission.rewards.rewardItems[i].moonieItemId;
            noItemIdReward[i] = mission.rewards.rewardItems[i].rewardChance;
        }
        return
            abi.encode(
                unlocksMissionsRewards,
                mission.rewards.noCooldownChance,
                itemIdReward,
                noItemIdReward,
                mission.rewards.mnyRewardAmount,
                mission.rewards.mnyRewardChance
            );
    }

