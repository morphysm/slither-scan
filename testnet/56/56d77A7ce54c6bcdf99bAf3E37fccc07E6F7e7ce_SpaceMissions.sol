    function addAttributeBonuses(
        // "DONE" ?
        uint256 missionId,
        uint256[] memory rewardItemIndexes,
        uint256[] memory attributeValues,
        bool[] memory reduceSuccessChances,
        uint256[] memory successChances,
        uint256[] memory deathChances,
        bool[] memory removeCooldowns
    ) public only(OFFICER_ROLE) {
        require(
            rewardItemIndexes.length == attributeValues.length &&
                attributeValues.length == reduceSuccessChances.length &&
                reduceSuccessChances.length == successChances.length &&
                successChances.length == deathChances.length &&
                deathChances.length == removeCooldowns.length,
            "SpaceMissions: Wrong number of attribute bonuses parameters."
        );

        for (uint256 i = 0; i < rewardItemIndexes.length; i++) {
            _missions[missionId].attributeBonuses.push(
                AttributeBonus({
                    attributeIndex: rewardItemIndexes[i],
                    attributeValue: attributeValues[i], // when 0, any value is accepted
                    reduceSuccessChance: reduceSuccessChances[i],
                    successChance: successChances[i],
                    deathChance: deathChances[i],
                    removeCooldown: removeCooldowns[i]
                })
            );
        }
    }

    function addTypeBonuses(
        // "DONE" ?
