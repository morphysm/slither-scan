    // PUBLIC FUNCTIONS

    function finalizeMissions(uint256 ongoingMissionId) external {
        emit CounterEvent(0);
        require(
            _ongoingMissions[ongoingMissionId].missionFinishTime <=
                block.timestamp,
            "SpaceMissions: Moonie is still on a mission."
        );
        emit CounterEvent(1);

        uint256 outcome;
        uint256 extraSeed = 1;
        uint256 moonieId = _ongoingMissions[ongoingMissionId].moonieTokenId;
        uint256 moonieType = _moonieStats.getMoonieType(moonieId);
        bool wonResetCooldown = false;
        emit CounterEvent(2);

        SpaceMission storage mission = _missions[
            _ongoingMissions[ongoingMissionId].missionId
        ];
        emit CounterEvent(3);

        address owner = _ongoingMissions[ongoingMissionId].owner;
        emit CounterEvent(4);

        uint256 finalSuccessChance = mission.successChance;
        uint256 finalDeathChance = mission.deathChance;
        emit CounterEvent(5);

        for (uint256 i = 0; i < mission.typeBonuses.length; i++) {
            if (mission.typeBonuses[i].moonieType == moonieType) {
                if (mission.typeBonuses[i].reduceSuccessChance) {
                    finalSuccessChance -= mission.typeBonuses[i].successChance;
                    finalDeathChance += mission.typeBonuses[i].deathChance;
                } else {
                    finalSuccessChance += mission.typeBonuses[i].successChance;
                }
            }
        }
        emit CounterEvent(6);

        for (uint256 i = 0; i < mission.attributeBonuses.length; i++) {
            uint256 attrIndex = mission.attributeBonuses[i].attributeIndex;
            uint256 attrValue = mission.attributeBonuses[i].attributeValue;
            uint256 moonieAttrValue = _moonieStats.getMoonieStatByTokenId(
                moonieId,
                attrIndex
            );

            if (
                (attrValue == 0 && moonieAttrValue != 0) ||
                (attrValue == moonieAttrValue)
            ) {
                // any attr value or specific attr value
                if (mission.attributeBonuses[i].removeCooldown) {
                    wonResetCooldown = true;
                }

                if (mission.attributeBonuses[i].reduceSuccessChance) {
                    finalSuccessChance -= mission
                        .attributeBonuses[i]
                        .successChance;
                } else {
                    finalSuccessChance += mission
                        .attributeBonuses[i]
                        .successChance;
                }
            }
        }
        emit CounterEvent(7);

        uint256 randomSeed = _randomSource.getRandomness();
        uint256 result = uint256(
            keccak256(
                abi.encode(randomSeed, block.timestamp, moonieId, extraSeed)
            )
        ).mod(1000);
        extraSeed++;
        emit CounterEvent(8);

        // Moonie has successed - range: < 0 ; successChance )
        if (result < finalSuccessChance) {
            if (mission.rewards.unlocksMissions.length() > 0) {
                for (
                    uint256 i = 0;
                    i < mission.rewards.unlocksMissions.length();
                    i++
                ) {
                    _missions[mission.rewards.unlocksMissions.at(i)]
                        .requirements
                        .whitelistedWallets
                        .add(_msgSender());
                }
            }
            result = uint256(
                keccak256(
                    abi.encode(randomSeed, block.timestamp, moonieId, extraSeed)
                )
            ).mod(1000);
            extraSeed++;

            if (result < mission.rewards.noCooldownChance) {
                wonResetCooldown = true;
            }

            for (uint256 i = 0; i < mission.rewards.rewardItems.length; i++) {
                result = uint256(
                    keccak256(
                        abi.encode(
                            randomSeed,
                            block.timestamp,
                            moonieId,
                            extraSeed
                        )
                    )
                ).mod(1000);
                extraSeed++;

                if (result < mission.rewards.rewardItems[i].rewardChance) {
                    uint256 itemWon;

                    if (mission.rewards.rewardItems[i].moonieItemId > 5) {
                        itemWon = mission.rewards.rewardItems[i].moonieItemId;
                    } else {
                        itemWon = result.mod(5) + 1;
                    }
                    _itemNft.safeTransferFrom(
                        address(this),
                        owner,
                        itemWon,
                        1,
                        ""
                    );
                }
            }

            if (mission.rewards.mnyRewardAmount > 0) {
                result = uint256(
                    keccak256(
                        abi.encode(
                            randomSeed,
                            block.timestamp,
                            moonieId,
                            extraSeed
                        )
                    )
                ).mod(1000);
                extraSeed++;

                if (result < mission.rewards.mnyRewardChance) {
                    _moonieToken.transfer(
                        owner,
                        mission.rewards.mnyRewardAmount
                    );
                }
            }

            _nft.safeTransferFrom(address(this), owner, moonieId);

            if (wonResetCooldown) {
                _moonieCooldowns[moonieId] = 0;
            } else {
                _moonieCooldowns[moonieId] = block.timestamp.add(
                    mission.cooldown
                );
            }

            outcome = 0;
        }
        // Moonie has died - range: < susuccessChance ; deathChance + susuccessChance )
        else if (result < finalDeathChance + finalSuccessChance) {
            _nft.burn(moonieId);
            outcome = 1;
        }
        // Nothing happens - range: < deathChance + successChance ; 100 )
        else {
            _nft.safeTransferFrom(address(this), owner, moonieId);

            if (wonResetCooldown) {
                _moonieCooldowns[moonieId] = 0;
            } else {
                _moonieCooldowns[moonieId] = block.timestamp.add(
                    mission.nothingCooldown
                );
            }

            outcome = 2;
        }
        emit CounterEvent(9);

        emit MissionFinished(
            _ongoingMissions[ongoingMissionId].missionId,
            ongoingMissionId,
            owner,
            outcome
        );
    }

    function sendMoonieToMission(
        // "DONE" ?
        uint256 missionId,
        uint256 participatingMoonieTokenId,
        uint256[] memory heldMooniesTokenIds,
        uint256[] memory heldItemsTokenIds,
        bool skipCooldown,
        UmbrellaValues memory umbrellaValues
    ) external nonReentrant {
        SpaceMission storage mission = _missions[missionId];
        require(mission.active, "SpaceMission: Mission has to be activated.");

        if (mission.requirements.whitelist) {
            require(
                mission.requirements.whitelistedWallets.contains(_msgSender()),
                "SpaceMissions: Sender has to whitelisted to take part in this mission."
            );
        }
        require(
            _nft.ownerOf(participatingMoonieTokenId) == _msgSender(),
            "SpaceMissions: Sender has to be owner of Moonie."
        );
        if (mission.requirements.mnyHeldRequired > 0) {
            require(
                _moonieToken.balanceOf(_msgSender()) >=
                    mission.requirements.mnyHeldRequired,
                "SpaceMissions: Sender has insufficient funds in wallet."
            );
        }

        // cooldown and payment
        // deleted because stack to deep
        //bool moonieOnCooldown = _moonieCooldowns[participatingMoonieTokenId] >
        //    block.timestamp
        //    ? true
        //    : false;
        if (!skipCooldown) {
            require(
                //!moonieOnCooldown,
                _moonieCooldowns[participatingMoonieTokenId] < block.timestamp,
                "SpaceMissions: Moonie is still on cooldown"
            );
        }

        uint256 feesToPay = 0;
        if (mission.entryFee > 0) {
            feesToPay += mission.entryFee;
        }
        if (
            skipCooldown &&
            _moonieCooldowns[participatingMoonieTokenId] > block.timestamp
        ) {
            feesToPay +=
                ((_moonieCooldowns[participatingMoonieTokenId] -
                    uint256(block.timestamp)) /
                    _timeUnit +
                    1) *
                _timeUnitPrice;
        }

        if (feesToPay > 0) {
            require(
                _priceOracle.verifyProof(
                    umbrellaValues.blockHeight,
                    umbrellaValues.proofMNY,
                    _keyMNY,
                    umbrellaValues.valueMNY
                ),
                "Moonchest: Invalid proof for MNY"
            );

            _moonieToken.transferFrom(
                _msgSender(),
                _bank, /*?*/
                (feesToPay / abi.decode(umbrellaValues.valueMNY, (uint256))) *
                    1 ether
            );
        }

        // check moonie type
        uint256 moonieType;
        uint256[] memory moonieTypesAllowed = mission
            .requirements
            .moonieTypesAllowedToEnter;
        if (moonieTypesAllowed.length > 0) {
            bool moonieInRequirements = false;
            moonieType = _moonieStats.getMoonieType(participatingMoonieTokenId);

            for (uint256 i = 0; i < moonieTypesAllowed.length; i++) {
                if (moonieType == moonieTypesAllowed[i]) {
                    moonieInRequirements = true;
                }
            }
            require(moonieInRequirements, "SpaceMissinos: Wrong Moonie Type.");
        }

        // check moonie held type
        NFTRequirement[] memory moonieHeldTypesRequired = mission
            .requirements
            .mooniesHeldRequired;
        uint256[8] memory moonieTypesHeld;
        for (uint256 i = 0; i < heldMooniesTokenIds.length; i++) {
            require(
                _nft.ownerOf(heldMooniesTokenIds[i]) == _msgSender(),
                "SpaceMissions: Sender has to be owner of Moonies."
            );

            moonieType = _moonieStats.getMoonieType(heldMooniesTokenIds[i]);
            moonieTypesHeld[moonieType] += 1;
        }

        for (uint256 i = 0; i < moonieHeldTypesRequired.length; i++) {
            uint256 typeRequired = moonieHeldTypesRequired[i].nftType;
            uint256 noRequired = moonieHeldTypesRequired[i].nftNo;
            require(
                moonieTypesHeld[typeRequired] >= noRequired,
                "SpaceMissions: Sender doesn't have enough Moonies held."
            );
        }

        if (mission.requirements.moonieItemsRequired.length > 0) {
            NFTRequirement[] memory itemHeldTypesRequired = mission
                .requirements
                .moonieItemsRequired;

            // not the best idea to hardcode number of types?
            // but overall code now is much much cleaner and lighter
            // thanks to erc1155 batch functions
            // not there is only one balanceOfBatch and one transferBatch
            // is it a good idea to send transaction transferBatch with some numbers of elements equal 0?
            uint256[] memory noMoonieItemsToTransfer = new uint256[](11);
            uint256[] memory typesOfMoonieItemsToTransfer = new uint256[](11);
            address[] memory addressesOfUser = new address[](11);
            for (uint256 i = 1; i <= 11; i++) {
                typesOfMoonieItemsToTransfer[i - 1] = i;
                addressesOfUser[i - 1] = _msgSender();
            }
            uint256[] memory noMoonieItemsHeldByUser = new uint256[](11);
            noMoonieItemsHeldByUser = _itemNft.balanceOfBatch(
                addressesOfUser,
                typesOfMoonieItemsToTransfer
            );

            for (uint256 i = 0; i < itemHeldTypesRequired.length; i++) {
                // rocket parts
                if (itemHeldTypesRequired[i].nftType == 0) {
                    uint256 noRocketPartsHeld;
                    uint256 noSpecificRocketPartHeld;

                    uint256 noRequired = itemHeldTypesRequired[i].nftNo;

                    // check amount of rocket parts held by user
                    // add number of specific rocket parts held to noMoonieItemsToTransfer
                    // and if noRocketPartsHeld + noSpecificRocketPartHeld is greater than noRequired
                    // add noRequired - noRocketPartsHeld
                    for (uint256 j = 1; j <= 5; j++) {
                        noSpecificRocketPartHeld = noMoonieItemsHeldByUser[
                            j - 1
                        ];
                        if (
                            noSpecificRocketPartHeld + noRocketPartsHeld >
                            noRequired
                        ) {
                            noMoonieItemsToTransfer[j - 1] =
                                noRequired -
                                noRocketPartsHeld;
                            noRocketPartsHeld += noRequired - noRocketPartsHeld;
                        } else {
                            noMoonieItemsToTransfer[
                                j - 1
                            ] = noSpecificRocketPartHeld;
                            noRocketPartsHeld += noSpecificRocketPartHeld;
                        }
                    }
                    require(
                        noRocketPartsHeld >= noRequired,
                        "SpaceMission: Sender doesn't have enough Items held."
                    );

                    // other moonie items
                } else {
                    require(
                        noMoonieItemsHeldByUser[
                            itemHeldTypesRequired[i].nftType - 1
                        ] >= itemHeldTypesRequired[i].nftNo,
                        "SpaceMission: Sender doesn't have enough Items held."
                    );
                    noMoonieItemsToTransfer[
                        itemHeldTypesRequired[i].nftType - 1
                    ] = itemHeldTypesRequired[i].nftNo;
                }
            }
            _itemNft.safeBatchTransferFrom(
                _msgSender(),
                address(this),
                typesOfMoonieItemsToTransfer,
                noMoonieItemsToTransfer,
                ""
            );
        }

        // ?
        _nft.safeTransferFrom(
            _msgSender(),
            address(this),
            participatingMoonieTokenId
        );

        OngoingMission memory ongoingMission = OngoingMission({
            moonieTokenId: participatingMoonieTokenId,
            owner: _msgSender(),
            missionFinishTime: block.timestamp.add(mission.missionLength),
            missionId: missionId
        });

        _ongoingMissions[_missionNonce.current()] = ongoingMission;

        emit MissionStarted(
            missionId,
            _missionNonce.current(),
            participatingMoonieTokenId,
            _msgSender()
        );

        _missionNonce.increment();
    }

    function createNewMission(
        // "DONE"?
        string memory missionName,
        uint256 missionLength,
        uint256 deathChance,
        uint256 successChance,
        uint256 entryFee,
        //uint256 cooldownResetFeeForOneTimeUnit,
        //uint256 timeUnitOfCooldownInHours,
        uint256 cooldown,
        uint256 nothingCooldown
    ) public only(OFFICER_ROLE) {
        require(
            successChance + deathChance <= 1000,
            "SpaceMissions: Wrong mission result probabilities."
        );
        uint256 missionId = _missionCounter.current();
        SpaceMission storage newSpaceMission = _missions[missionId];

        newSpaceMission.missionName = missionName;
        newSpaceMission.missionLength = missionLength;
        newSpaceMission.active = false;
        newSpaceMission.successChance = successChance;
        newSpaceMission.deathChance = deathChance;

        newSpaceMission.entryFee = entryFee;
        //newSpaceMission.cooldownResetFeeForOneTimeUnit = cooldownResetFeeForOneTimeUnit;
        //newSpaceMission.timeUnitOfCooldownInHours = timeUnitOfCooldownInHours;
        newSpaceMission.cooldown = cooldown;
        newSpaceMission.nothingCooldown = nothingCooldown;

        MissionRequirements storage newMissionRequirements = newSpaceMission
            .requirements;

        newMissionRequirements.whitelist = false;

        EnumerableSet.AddressSet storage newAddressSet = newMissionRequirements
            .whitelistedWallets;

        MissionRewards storage newMissionRewards = newSpaceMission.rewards;

        newMissionRewards.noCooldownChance = 0;

        _missionCounter.increment();

        emit MissionCreated(missionId, _msgSender());
    }

    function setMissionRequirements(
        // "DONE"?
        uint256 missionId,
        uint256 mnyHeld,
        uint256[] memory moonieTypesAllowedToEnter,
        uint256[] memory moonieTypes,
        uint256[] memory moonieNo,
        uint256[] memory moonieItemTypes,
        uint256[] memory moonieItemNo,
        bool whitelist
    ) public only(OFFICER_ROLE) {
        require(
            moonieTypes.length == moonieNo.length,
            "SpaceMissions: Wrong number of required Moonie."
        );
        require(
            moonieItemTypes.length == moonieItemNo.length,
            "SpaceMissions: Wrong number of required Moonie Items."
        );

        delete _missions[missionId].requirements.moonieTypesAllowedToEnter;
        delete _missions[missionId].requirements.mooniesHeldRequired;
        delete _missions[missionId].requirements.moonieItemsRequired;

        MissionRequirements storage newRequirements = _missions[missionId]
            .requirements;

        for (uint256 i = 0; i < moonieTypesAllowedToEnter.length; i++) {
            _missions[missionId].requirements.moonieTypesAllowedToEnter.push(
                moonieTypesAllowedToEnter[i]
            );
        }

        for (uint256 i = 0; i < moonieTypes.length; i++) {
            _missions[missionId].requirements.mooniesHeldRequired.push(
                NFTRequirement({nftType: moonieTypes[i], nftNo: moonieNo[i]})
            );
        }

        for (uint256 i = 0; i < moonieItemTypes.length; i++) {
            _missions[missionId].requirements.moonieItemsRequired.push(
                NFTRequirement({
                    nftType: moonieItemTypes[i],
                    nftNo: moonieItemNo[i]
                })
            );
        }

        newRequirements.whitelist = whitelist;

        EnumerableSet.AddressSet storage newAddressSet = newRequirements
            .whitelistedWallets;

        newRequirements.mnyHeldRequired = mnyHeld;
    }

    function setMissionRewards(
        // "DONE"?
        uint256 missionId,
        uint256[] memory unlocksMissions,
        uint256 noCooldownChance,
        uint256 mnyAmount,
        uint256 mnyChance
    ) public only(OFFICER_ROLE) {
        for (uint256 i = 0; i < unlocksMissions.length; i++) {
            _missions[missionId].rewards.unlocksMissions.add(
                unlocksMissions[i]
            );
        }

        _missions[missionId].rewards.noCooldownChance = noCooldownChance;
        _missions[missionId].rewards.mnyRewardAmount = mnyAmount;
        _missions[missionId].rewards.mnyRewardChance = mnyChance;
    }

