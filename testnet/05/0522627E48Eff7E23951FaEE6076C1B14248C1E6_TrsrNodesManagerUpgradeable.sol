import "./TRSR.sol";
import "./PC8.sol";
import "./Pausable.sol";
import "./ReentrancyGuard.sol";

contract SwapperToken is Ownable, Pausable, ReentrancyGuard {

    TRSR trsr;
    PC8 pc8;
    uint public swapFee;
    uint maxSwapAuthorized;
    uint public totalSwap = 0;
    uint timeBeforeSwap = 1209600;
    mapping(address=>uint) public delayToSwap; // time before swap token

    mapping(address => bool) controllers;

    event tokenSwap(address indexed account, uint indexed amountSwap);

    constructor (address _trsr,address _pc8) {
        trsr = TRSR(_trsr);
        pc8 = PC8(_pc8);
        swapFee = 0.05 ether;
        maxSwapAuthorized = 100000000 ether;
    }

    function swapPC8ToTrsr(uint amount)
    external
    payable
    whenNotPaused
    nonReentrant
    {
        require(msg.value >= swapFee, "You don't have enough to pay the fees");
        uint balancePc8 = pc8.balanceOf(msg.sender);
        require(balancePc8 > 0, "Sorry your balance $pc8 is null");
        require(balancePc8 >= amount * 10**18, "Your balance $pc8 is to low");
        require(maxSwapAuthorized > totalSwap, "Sorry max tokens swapped");
        require((balancePc8 * 20) / 100 > amount * 10**18, "Only 20% your balance is swappable");
        uint dateBeforeSwap;

        require(delayToSwap[msg.sender] + timeBeforeSwap < block.timestamp, "Sorry the deadline has not passed");
        delayToSwap[msg.sender] = block.timestamp;

        pc8.burn(msg.sender,amount* 10**18);
        trsr.mint(msg.sender,amount* 10**18);
        totalSwap += amount;

        emit tokenSwap(msg.sender,amount);

    }

    function TimeBeforeNextSwap()
    external
    view
    returns(uint date)
    {
        return delayToSwap[msg.sender] + timeBeforeSwap;
    }

    function setMaxSwapAuthorized(uint _newMax)
    public
    onlyOwner
    {
        maxSwapAuthorized = _newMax;
    }

    function setTimeBeforeSwap(uint _newTime)
    public
    onlyOwner
    {
        timeBeforeSwap = _newTime;
    }

    function setSwapFee(uint _newFee)
    public
    onlyOwner
    {
        swapFee = _newFee;
    }

    function addTimeFirstSwap(address to, uint time)
    public
    {
        require(controllers[msg.sender], "Only controllers can call");
        // Fix first time swap pc8
        if (delayToSwap[to] == 0)
            delayToSwap[to] = block.timestamp;

    }

    function addController(address controller) external onlyOwner {
        controllers[controller] = true;
    }

    function removeController(address controller) external onlyOwner {
        controllers[controller] = false;
    }

    function pause() public onlyOwner {
        _pause();
    }

    function unpause() public onlyOwner {
        _unpause();
    }
}

