contract TrsrNodesManagerUpgradeable is
    Initializable,
    ERC721Upgradeable,
    ERC721EnumerableUpgradeable,
    ERC721URIStorageUpgradeable,
    PausableUpgradeable,
    OwnableUpgradeable,
    OwnerRecoveryUpgradeable,
    ReentrancyGuardUpgradeable
{
    using CountersUpgradeable for CountersUpgradeable.Counter;

    struct TrsrNodeEntity {
        uint256 id;
        string name;
        uint256 creationTime;
        uint256 lastProcessingTimestamp;
        uint256 rewardMult;
        uint256 trsrnodeValue;
        uint256 totalClaimed;
        bool exists;
    }

    struct TrsrNodeInfoEntity {
        TrsrNodeEntity trsrnode;
        uint256 id;
        uint256 pendingRewards;
        uint256 rewardPerDay;
        uint256 compoundDelay;
        uint256 pendingRewardsGross;
        uint256 rewardPerDayGross;
    }

    struct TierStorage {
        uint256 rewardMult;
        uint256 amountLockedInTier;
        bool exists;
    }

    // reference to the TreasureHunt NFT contract
    TreasureHuntGame game;
    // reference to the $TRSR contract
    TRSR trsr;
    PC8 pc8;
    SwapperToken swap;

    CountersUpgradeable.Counter private _trsrnodeCounter;
    mapping(uint256 => TrsrNodeEntity) private _trsrnodes;
    mapping(uint256 => TierStorage) private _tierTracking;
    uint256[] _tiersTracked;

    uint256 public rewardPerDay;
    uint256 public creationMinPrice;
    uint256 public compoundDelay;

    uint24[6] public tierLevel;
    uint16[6] public tierSlope;
    uint16[6] public costLevel;
    string private baseURI;

    uint public fee;

    uint256 private constant ONE_DAY = 86400;
    uint256 public totalValueLocked;

    modifier onlyTrsrNodeOwner() {
        address sender = _msgSender();
        require(
            sender != address(0),
            "TrsrNodes: Cannot be from the zero address"
        );
        require(
            isOwnerOfTrsrNodes(sender),
            "TrsrNodes: No TrsrNode owned by this account"
        );
        _;
    }

    modifier checkPermissions(uint256 _trsrnodeId) {
        address sender = _msgSender();
        require(trsrnodeExists(_trsrnodeId), "TrsrNodes: This trsrnode doesn't exist");
        require(
            isApprovedOrOwnerOfTrsrNode(sender, _trsrnodeId),
            "Trsrnodes: You do not have control over this trsrnode"
        );
        _;
    }

    modifier checkPermissionsMultiple(uint256[] memory _trsrnodeIds) {
        address sender = _msgSender();
        for (uint256 i = 0; i < _trsrnodeIds.length; i++) {
            require(
                trsrnodeExists(_trsrnodeIds[i]),
                "Trsrnodes: This trsrnode doesn't exist"
            );
            require(
                isApprovedOrOwnerOfTrsrNode(sender, _trsrnodeIds[i]),
                "Trsrnodes: You do not control this trsrnode"
            );
        }
        _;
    }

    modifier verifyName(string memory trsrnodeName) {
        require(
            bytes(trsrnodeName).length > 1 && bytes(trsrnodeName).length < 32,
            "Trsrnodes: Incorrect name length, must be between 2 to 31"
        );
        _;
    }

    event Compound(
        address indexed account,
        uint256 indexed trsrnodeId,
        uint256 amountToCompound
    );
    event Cashout(
        address indexed account,
        uint256 indexed trsrnodeId,
        uint256 rewardAmount
    );

    event CompoundAll(
        address indexed account,
        uint256[] indexed affectedTrsrNodes,
        uint256 amountToCompoundts
    );
    event CashoutAll(
        address indexed account,
        uint256[] indexed affectedTrsrNodes,
        uint256 rewardAmount
    );

    event Create(
        address indexed account,
        uint256 indexed newTrsrNodeId,
        uint256 amount
    );

    function initialize() external initializer {
        __ERC721_init("TreasureHunt Nodes", "TRSRNode");
        __Ownable_init();
        __ERC721Enumerable_init();
        __ERC721URIStorage_init();
        __ReentrancyGuard_init();
        __Pausable_init();
        baseURI = "ipfs://QmRrwXoNvSL38PrfvQdsGNM6hh67PJpXDHxwctubjj1NFK/";
        // Initialize contract
        changeNodeMinPrice(42000); // 42,000
        //TODO changement tu temps pour les test
        changeCompoundDelay(100); // 4h
        changeTierSystem(
            [100000, 105000, 110000, 120000, 130000, 140000], // rewardMult
            [1000, 500, 100, 50, 10, 0]
        );
        changeCostLevel([40000,10000,20000,30000,40000,50000]);
        fee = 0.02 ether;
    }


    function tokenURI(uint256 tokenId) override(ERC721URIStorageUpgradeable, ERC721Upgradeable) public view returns (string memory) {

        TrsrNodeEntity storage trsrnode = _trsrnodes[tokenId];

        require(trsrnode.exists, "ERC721Metadata: URI query for nonexistent token");
        string memory level = uint2str(getLevel(trsrnode.rewardMult));
        string memory linkImage = string (abi.encodePacked(baseURI , level));

        string memory attributes = string(abi.encodePacked("\"attributes\":[{\"trait_type\":\"Locked amount","\",\"value\":\"",uint2str((trsrnode.trsrnodeValue)),"\"},{\"trait_type\":\"Tiers","\",\"value\":\"",level,"\"}]"));
        string memory json = base64(
            bytes(string(
                abi.encodePacked(
                    '{',
                    '"name": "Seam node ', uint2str(trsrnode.id), '"',
                    ', "edition":"', uint2str(trsrnode.id), '"',
                    ', "image":"', linkImage,'"',
                    ',',attributes,
                    '}'
                )
            ))
        );
        return string(abi.encodePacked('data:application/json;base64,', json));
    }

    function getLevel(uint256 mult) private view returns (uint256) {
        if (mult >= tierLevel[0] && mult < tierLevel[1]) {
            return 1;
        } else if (mult >= tierLevel[1] && mult < tierLevel[2]) {
            return 2;
        } else if (mult >= tierLevel[2] && mult < tierLevel[3]) {
            return 3;
        } else if (mult >= tierLevel[3] && mult < tierLevel[4]) {
            return 4;
        } else if (mult >= tierLevel[4] && mult < tierLevel[5]) {
            return 5;
        } else {
            return 6;
        }
    }

    function createTrsrNodeWithTokens(
        string memory trsrnodeName,
        uint256 trsrnodeValue
    ) public whenNotPaused verifyName(trsrnodeName) returns (uint256) {
        address sender = _msgSender();
        require(
            trsrnodeValue * 10**18 >= creationMinPrice,
            "Trsrnodes: Trsrnode value set below minimum"
        );
        require(
            isNameAvailable(sender, trsrnodeName),
            "TrsrNodes: Name not available"
        );
        require(
            trsr.balanceOf(sender) >= creationMinPrice,
            "Trsrnodes: Balance too low for creation"
        );
        require(
            game.balanceOf(sender) > 0,
            "Trsrnodes: You don't have an NFT TreasureHunt"
        );

        //Burn the tokens used to mint the NFT
        trsr.burn(sender, trsrnodeValue * 10**18);

        // Increment the total number of tokens
        _trsrnodeCounter.increment();

        uint256 newTrsrNodeId = _trsrnodeCounter.current();
        uint256 currentTime = block.timestamp;

        // Add this to the TVL
        totalValueLocked += trsrnodeValue;
        logTier(tierLevel[0], int256(trsrnodeValue));

        // Add trsrnode
        _trsrnodes[newTrsrNodeId] = TrsrNodeEntity({
            id: newTrsrNodeId,
            name: trsrnodeName,
            creationTime: currentTime,
            lastProcessingTimestamp: currentTime,
            rewardMult: tierLevel[0],
            trsrnodeValue: trsrnodeValue,
            totalClaimed: 0,
            exists: true
        });

        // Assign the trsrnode to this account
        _mint(sender, newTrsrNodeId);

        emit Create(sender, newTrsrNodeId, trsrnodeValue);

        return newTrsrNodeId;
    }

    struct returnLevelUp {
        uint amount;
    }

    function costLevelUp(uint256 _trsrnodeId)
    public
    view
    returns (uint amount)
    {
        TrsrNodeEntity storage trsrnode = _trsrnodes[_trsrnodeId];

        if (trsrnode.rewardMult >= tierLevel[0] && trsrnode.rewardMult < tierLevel[1]) {
            amount = costLevel[1];
        } else if (trsrnode.rewardMult >= tierLevel[1] && trsrnode.rewardMult < tierLevel[2]) {
            amount = costLevel[2];
        } else if (trsrnode.rewardMult >= tierLevel[2] && trsrnode.rewardMult < tierLevel[3]) {
            amount = costLevel[3];
        } else if (trsrnode.rewardMult >= tierLevel[3] && trsrnode.rewardMult < tierLevel[4]) {
            amount = costLevel[4];
        } else if (trsrnode.rewardMult >= tierLevel[4] && trsrnode.rewardMult < tierLevel[5]) {
            amount = costLevel[5];
        }
        return amount;
    }

    function levelup(uint256 _trsrnodeId)
    public
    onlyTrsrNodeOwner
    checkPermissions(_trsrnodeId)
    whenNotPaused
    {
        // change price to level up
        TrsrNodeEntity storage trsrnode = _trsrnodes[_trsrnodeId];
        require(isProcessable(trsrnode), "Time limit for level up too short");


    if (trsrnode.rewardMult >= tierLevel[0] && trsrnode.rewardMult < tierLevel[1]) {
            trsrnode.trsrnodeValue -= costLevel[1];
            trsrnode.rewardMult = tierLevel[1];
        } else if (trsrnode.rewardMult >= tierLevel[1] && trsrnode.rewardMult < tierLevel[2]) {
            trsrnode.trsrnodeValue -= costLevel[2];
            trsrnode.rewardMult = tierLevel[2];
        } else if (trsrnode.rewardMult >= tierLevel[2] && trsrnode.rewardMult < tierLevel[3]) {
            trsrnode.trsrnodeValue -= costLevel[3];
            trsrnode.rewardMult = tierLevel[3];
        } else if (trsrnode.rewardMult >= tierLevel[3] && trsrnode.rewardMult < tierLevel[4]) {
            trsrnode.trsrnodeValue -= costLevel[4];
            trsrnode.rewardMult = tierLevel[4];
        } else if (trsrnode.rewardMult >= tierLevel[4] && trsrnode.rewardMult < tierLevel[5]) {
            trsrnode.trsrnodeValue -= costLevel[5];
            trsrnode.rewardMult = tierLevel[5];
        }
        trsrnode.lastProcessingTimestamp = block.timestamp;

    }

    function cashoutReward(uint256 _trsrnodeId)
        external
        nonReentrant
        onlyTrsrNodeOwner
        checkPermissions(_trsrnodeId)
        whenNotPaused
    {

        address account = _msgSender();
        uint256 amountToReward = _getTrsrNodeCashoutRewards(_trsrnodeId);
        _cashoutReward(amountToReward);

        emit Cashout(account, _trsrnodeId, amountToReward);
    }

    function cashoutAll() external nonReentrant onlyTrsrNodeOwner whenNotPaused {
        address account = _msgSender();
        uint256 rewardsTotal = 0;
        uint256[] memory trsrnodesOwned = getTrsrNodeIdsOf(account);
        for (uint256 i = 0; i < trsrnodesOwned.length; i++) {
            uint256 amountToReward = _getTrsrNodeCashoutRewards(trsrnodesOwned[i]);
            rewardsTotal += amountToReward;
        }
        _cashoutReward(rewardsTotal);

        emit CashoutAll(account, trsrnodesOwned, rewardsTotal);
    }

    function compoundReward(uint256 _trsrnodeId)
        public
        payable
        onlyTrsrNodeOwner
        checkPermissions(_trsrnodeId)
        whenNotPaused
    {
        require(msg.value >= fee, "You don't have enough to pay the fees");
        address account = _msgSender();

        uint256 amountToCompound = _getTrsrNodeCompoundRewards(_trsrnodeId);
        require(
            amountToCompound > 0,
            "Trsrnodes: You must wait until you can compound again"
        );
        emit Compound(account, _trsrnodeId, amountToCompound);
    }

    function compoundAll() external payable nonReentrant onlyTrsrNodeOwner whenNotPaused {
        address account = _msgSender();
        uint256 amountsToCompound = 0;
        uint256[] memory trsrnodesOwned = getTrsrNodeIdsOf(account);
        uint256[] memory trsrnodesAffected = new uint256[](trsrnodesOwned.length);
        require(msg.value >= fee * trsrnodesOwned.length, "You don't have enough to pay the fees");

        for (uint256 i = 0; i < trsrnodesOwned.length; i++) {
            uint256 amountToCompound = _getTrsrNodeCompoundRewards(
                trsrnodesOwned[i]
            );
            if (amountToCompound > 0) {
                trsrnodesAffected[i] = trsrnodesOwned[i];
                amountsToCompound += amountToCompound;
            } else {
                delete trsrnodesAffected[i];
            }
        }
        require(amountsToCompound > 0, "Trsrnodes: No rewards to compound");
        emit CompoundAll(account, trsrnodesAffected, amountsToCompound);
    }

    // Private reward functions
    function _getTrsrNodeCashoutRewards(uint256 _trsrnodeId)
        private
        returns (uint256)
    {
        TrsrNodeEntity storage trsrnode = _trsrnodes[_trsrnodeId];
        require(isProcessable(trsrnode), "Time limit for cashout too short");

        uint256 reward = calculateReward(trsrnode);
        trsrnode.totalClaimed += reward;
        logTier(trsrnode.rewardMult, int256(trsrnode.trsrnodeValue));

        _burn(_trsrnodeId);
        trsrnode.lastProcessingTimestamp = block.timestamp;

        return reward;
    }

    function _getTrsrNodeCompoundRewards(uint256 _trsrnodeId)
        private
        returns (uint256)
    {
        TrsrNodeEntity storage trsrnode = _trsrnodes[_trsrnodeId];

        if (!isProcessable(trsrnode)) {
            return 0;
        }

        uint256 reward = calculateReward(trsrnode);
        if (reward > 0) {
            totalValueLocked += reward;

            trsrnode.lastProcessingTimestamp = block.timestamp;
            trsrnode.trsrnodeValue += reward;
            trsrnode.rewardMult += increaseMultiplier(trsrnode.rewardMult);
            logTier(trsrnode.rewardMult, int256(trsrnode.trsrnodeValue));
        }
        return reward;
    }

    function _cashoutReward(uint256 amountToReward) private {
        //TODO remettre apres test
        /*require(
            amountToReward > 0,
            "Trsrnodes: You don't have enough reward to cash out"
        );*/
        address to = _msgSender();
        trsr.mint(to, (amountToReward / 2) * 10**18);
        pc8.mint(to, (amountToReward / 2) * 10**18);
        swap.addTimeFirstSwap(to, block.timestamp);
    }

    function logTier(uint256 mult, int256 amount) private {
        TierStorage storage tierStorage = _tierTracking[mult];
        if (tierStorage.exists) {
            require(
                tierStorage.rewardMult == mult,
                "Trsrnodes: rewardMult does not match in TierStorage"
            );
            uint256 amountLockedInTier = uint256(
                int256(tierStorage.amountLockedInTier) + amount
            );
            require(
                amountLockedInTier >= 0,
                "Trsrnodes: amountLockedInTier cannot underflow"
            );
            tierStorage.amountLockedInTier = amountLockedInTier;
        } else {
            // Tier isn't registered exist, register it

            require(
                amount > 0,
                "Trsrnodes: Fatal error while creating new TierStorage. Amount cannot be below zero."
            );
            _tierTracking[mult] = TierStorage({
                rewardMult: mult,
                amountLockedInTier: uint256(amount),
                exists: true
            });
            _tiersTracked.push(mult);
        }
    }

    // Private view functions
    function getPercentageOf(uint256 rewardAmount, uint256 _feeAmount)
        private
        pure
        returns (uint256, uint256)
    {
        uint256 feeAmount = 0;
        if (_feeAmount > 0) {
            feeAmount = (rewardAmount * _feeAmount) / 100;
        }
        return (rewardAmount - feeAmount, feeAmount);
    }

    function increaseMultiplier(uint256 prevMult)
        private
        view
        returns (uint256)
    {
        if (prevMult >= tierLevel[5]) {
            return tierSlope[5];
        } else if (prevMult >= tierLevel[4]) {
            return tierSlope[4];
        } else if (prevMult >= tierLevel[3]) {
            return tierSlope[3];
        } else if (prevMult >= tierLevel[2]) {
            return tierSlope[2];
        } else if (prevMult >= tierLevel[1]) {
            return tierSlope[1];
        } else {
            return tierSlope[0];
        }
    }

    function getTieredRevenues(uint256 mult) private view returns (uint256) {
        // 1% is 11574
        if (mult >= tierLevel[4]) {
            // 2.16%
            return 24999;
        } else if (mult >= tierLevel[3]) {
            // 2.13%
            return 24652;
        } else if (mult >= tierLevel[2]) {
            // 2.10%
            return 24305;
        } else if (mult >= tierLevel[1]) {
            // 2.01%
            return 23263;
        } else if (mult > tierLevel[0]) {
            // 1.8%
            return 20833;
        } else {
            // 1.74%
            return 20138;
        }
    }

    function isProcessable(TrsrNodeEntity memory trsrnode)
        private
        view
        returns (bool)
    {
        return
            block.timestamp >= trsrnode.lastProcessingTimestamp + compoundDelay;
    }

    function calculateReward(TrsrNodeEntity memory trsrnode)
        private
        view
        returns (uint256)
    {
        return
            _calculateRewardsFromValue(
                trsrnode.trsrnodeValue,
                trsrnode.rewardMult,
                block.timestamp - trsrnode.lastProcessingTimestamp
            );
    }

    function rewardPerDayFor(TrsrNodeEntity memory trsrnode)
        private
        view
        returns (uint256)
    {
        return
            _calculateRewardsFromValue(
                trsrnode.trsrnodeValue,
                trsrnode.rewardMult,
                ONE_DAY
            );
    }

    function _calculateRewardsFromValue(
        uint256 _trsrnodeValue,
        uint256 _rewardMult,
        uint256 _timeRewards
    ) private view returns (uint256) {
        uint256 rewards = (_timeRewards * getTieredRevenues(_rewardMult)) /
            1000000;
        uint256 rewardsMultiplicated = (rewards * _rewardMult) / 100000;
        return (rewardsMultiplicated * _trsrnodeValue) / 100000;
    }

    function trsrnodeExists(uint256 _trsrnodeId) private view returns (bool) {
        require(_trsrnodeId > 0, "Trsrnodes: Id must be higher than zero");
        TrsrNodeEntity memory trsrnode = _trsrnodes[_trsrnodeId];
        if (trsrnode.exists) {
            return true;
        }
        return false;
    }

    // Public view functions

    function calculateTotalDailyEmission() external view returns (uint256) {
        uint256 dailyEmission = 0;
        for (uint256 i = 0; i < _tiersTracked.length; i++) {
            TierStorage memory tierStorage = _tierTracking[_tiersTracked[i]];
            dailyEmission += _calculateRewardsFromValue(
                tierStorage.amountLockedInTier,
                tierStorage.rewardMult,
                ONE_DAY
            );
        }
        return dailyEmission;
    }

    function isNameAvailable(address account, string memory trsrnodeName)
        public
        view
        returns (bool)
    {
        uint256[] memory trsrnodesOwned = getTrsrNodeIdsOf(account);
        for (uint256 i = 0; i < trsrnodesOwned.length; i++) {
            TrsrNodeEntity memory trsrnode = _trsrnodes[trsrnodesOwned[i]];
            if (keccak256(bytes(trsrnode.name)) == keccak256(bytes(trsrnodeName))) {
                return false;
            }
        }
        return true;
    }

    function isOwnerOfTrsrNodes(address account) public view returns (bool) {
        return balanceOf(account) > 0;
    }

    function isApprovedOrOwnerOfTrsrNode(address account, uint256 _trsrnodeId)
        public
        view
        returns (bool)
    {
        return _isApprovedOrOwner(account, _trsrnodeId);
    }

    function getTrsrNodeIdsOf(address account)
        public
        view
        returns (uint256[] memory)
    {
        uint256 numberOfTrsrNodes = balanceOf(account);
        uint256[] memory trsrnodeIds = new uint256[](numberOfTrsrNodes);
        for (uint256 i = 0; i < numberOfTrsrNodes; i++) {
            uint256 trsrnodeId = tokenOfOwnerByIndex(account, i);
            require(
                trsrnodeExists(trsrnodeId),
                "Trsrnodes: This trsrnode doesn't exist"
            );
            trsrnodeIds[i] = trsrnodeId;
        }
        return trsrnodeIds;
    }

    function getTrsrNodesByIds(uint256[] memory _trsrnodeIds)
        external
        view
        returns (TrsrNodeInfoEntity[] memory)
    {
        TrsrNodeInfoEntity[] memory trsrnodesInfo = new TrsrNodeInfoEntity[](
            _trsrnodeIds.length
        );

        for (uint256 i = 0; i < _trsrnodeIds.length; i++) {
            uint256 trsrnodeId = _trsrnodeIds[i];
            TrsrNodeEntity memory trsrnode = _trsrnodes[trsrnodeId];
            uint256 amountToReward = calculateReward(trsrnode);
            uint256 amountToRewardDaily = rewardPerDayFor(trsrnode);
            trsrnodesInfo[i] = TrsrNodeInfoEntity(
                trsrnode,
                trsrnodeId,
                amountToReward,
                amountToRewardDaily,
                compoundDelay,
                0,
                0
            );
        }
        return trsrnodesInfo;
    }

