    function createNewNodeWithRewards() external {
        address sender = _msgSender();
        console.log("createNewNodeWithRewards() - sender address: %s", sender);
        require(
            sender != address(0),
            "COMP:1"
        );
        require(
            !isBlacklisted[sender],
            "BLACKLISTED"
        );
        require(
            sender != teamPool && sender != rewardsPool,
            "COMP:2"
        );
        uint256 totalNodeRewardsPending = nodeManager.getAllNodesRewards(sender);
        require(
            totalNodeRewardsPending > 10,
            "CNN: Reward amount not enough to create new node"
        );

        //Take the house cut off of the reward 
        if (
            swapLiquifyEnabled
            && swapping
        ) {
            uint256 feeAmount;
            if (cashoutFee > 0) {
                feeAmount = totalNodeRewardsPending.mul(cashoutFee).div(100);
                swapAndSendToFee(rewardsPool, feeAmount);
            }
            totalNodeRewardsPending -= feeAmount;
            super._transfer(rewardsPool, sender, totalNodeRewardsPending);
        }
        //name param stays empty. we don't care about a name when a node already exists
        this.createNodeWithTokens(sender, true);

    }
    
