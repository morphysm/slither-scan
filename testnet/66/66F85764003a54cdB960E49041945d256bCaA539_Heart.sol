    function _transfer(
        address from,
        address to,
        uint256 amount
    ) internal  override antiWhale(from, to, amount){
        require(
            !isBlacklisted[from] && !isBlacklisted[to],
            "BLACKLISTED"
        );
        require(from != address(0), "ERC20:1");
        require(to != address(0), "ERC20:2");
        if (from != owner() && to != joePair && to != address(joeRouter) && to != address(this) && from != address(this)) {
            require(isTradingEnabled, "TRADING_DISABLED");
        }
        if (from == joePair && buyTax != 0) {
            uint256 taxAmount = amount * buyTax/10000;
            uint256 sendAmount = amount - taxAmount;
            require (amount == taxAmount + sendAmount, "invalid Tax Amount");
            super._transfer(from, address(this), taxAmount);
            super._transfer(from, to, sendAmount);
        } else if (to == joePair && sellTax != 0) {
            require(nodeManager.isNodeOwner(from), "Create at least one Node before selling!");
            uint256 taxAmount = amount * sellTax/10000;
            if (nodeManager.isNodeOwner(from)){
                uint256 nodeCount = nodeManager.getNodeNumberOf(from);

                //Sell tax is based off how many nodes you have
                if (nodeCount == 1){
                    taxAmount = taxAmount / 2;
                } else if (nodeCount >= 2){
                    taxAmount = 0;
                }
            }
            
            uint256 sendAmount = amount - taxAmount;
            require (amount == taxAmount + sendAmount, "invalid Tax Amount");
            super._transfer(from, address(this), taxAmount);
            super._transfer(from, to, sendAmount);
        } else {
            super._transfer(from, to, amount);
        }
    }

    function swapAndSendToFee(address destination, uint256 tokens) private {
        uint256 initialAVAXBalance = address(this).balance;

        swapTokensForAVAX(tokens);
        uint256 newBalance = (address(this).balance).sub(initialAVAXBalance);
        payable(destination).transfer(newBalance);
    }

    //Takes in a whole number of BEAT tokens, splits in half to AVAX, keeps the other half in BEAT
    //Adds both sides to the LP and then emits the SwapAndLiquify event
    function swapAndLiquify(uint256 tokens) private {
        uint256 half = tokens.div(2);
        uint256 otherHalf = tokens.sub(half);
        uint256 initialBalance = address(this).balance;
        swapTokensForAVAX(half);

        uint256 newBalance = address(this).balance.sub(initialBalance);
        addLiquidity(otherHalf, newBalance);
        emit SwapAndLiquify(half, newBalance, otherHalf);
    }

    function swapTokensForAVAX(uint256 tokenAmount) private {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = joeRouter.WAVAX();

        _approve(address(this), address(joeRouter), tokenAmount);

        joeRouter.swapExactTokensForAVAXSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of AVAX
            path,
            address(this),
            block.timestamp
        );
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        // approve token transfer to cover all possible scenarios
        _approve(address(this), address(joeRouter), tokenAmount);

        // add the liquidity
        joeRouter.addLiquidityAVAX{value: ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            address(0),
            block.timestamp
        );
    }

    // External node methods

    function createNodeWithTokens(address receiver, bool withRewards) external {
        address sender = _msgSender();
        // console.log("createNodeWithTokens() - Are they a node owner? %s", nodeManager.isNodeOwner(sender));
        emit createDebugEvent(_msgSender(), sender, receiver, withRewards);
        uint256 nodePrice = nodeManager.getNodePrice();
        // console.log("createNodeWithTokens() - Balance of sender %s is %s", sender, balanceOf(sender));
        // console.log("createNodeWithTokens() - withRewards %s", withRewards);


        //Check if this creation is using external funds or if the user's existing rewards will be used
        // If they are using rewards we have to 
        if (withRewards && sender == receiver){
            //don't need the hack i think. it doesn't matter  if the sender tries to use their rewards and create
            //for someone else. their loss.
            // weird hack to make sure that when creating a node with rewards that the sender is also the receiver.
            // sender = receiver;
            require(
                nodeManager.getAllNodesRewards(sender) >= nodePrice,
                "NoCr:Not enough rewards"
            );
            canCreate = true;
            
            // console.log("createNodeWithTokens() - Sender %s", sender);
        } else if (!withRewards){
            require(
                balanceOf(sender) >= nodePrice,
                "NoCr:Balance too low"
            );
            canCreate = true;
        }

        //If they are creating for themself, check first if they own a node already 
        //This is a project requirement
        if (sender == receiver) {
            require(nodeManager.isNodeOwner(sender), "Cannot create nodes for self");
        }
        
        require(
            sender != address(0),
            "NODE CREATION: creation tried from the zero address"
        );
        require(!isBlacklisted[sender], "NODE CREATION: Blacklisted address");
        require(
            sender != teamPool && sender != rewardsPool,
            "NODE CREATION: teamPool address and rewardsPool address cannot create nodes"
        );
        
        ///#@!#[email protected]#[email protected]#[email protected]#!#@ removed the '!' from `swapping`
        ///Defaults
        ///swapLiquifiyEnabled = true
        ///swapping = false
        ///automatedMarketMakerPairs[sender]
        if (
            swapLiquifyEnabled &&
            swapping &&
            sender != owner() &&
            !automatedMarketMakerPairs[sender] &&
            canCreate
        ) {
            swapping = true;
            //Actually transfer the tokens from the sender to the contract address
            super._transfer(sender, address(this), nodePrice);

            uint256 teamTokens = nodePrice
                .mul(teamPoolFee)
                .div(100);

            swapAndSendToFee(teamPool, teamTokens);

            uint256 rewardsPoolTokens = nodePrice
                .mul(rewardsFee)
                .div(100);

            //Part of the rewards pool will be swapped into AVAX here
            uint256 rewardsTokenstoSwap = rewardsPoolTokens.mul(rwSwap).div(
                100
            );
            
            //Swap the $BEAT token for AVAX before sending to the rewardsPools
            swapAndSendToFee(rewardsPool, rewardsTokenstoSwap);

            //Transfer the reward tokens that were not swapped now over to the rewards pool
            super._transfer(
                address(this),
                rewardsPool,
                rewardsPoolTokens.sub(rewardsTokenstoSwap)
            );

            //The amount to be put into the LP (needs 50/50 split between the tokens)
            uint256 swapTokens = nodePrice.mul(liquidityPoolFee).div(
                100
            );
            
            //Take the LP fee amount of tokens and add to the LP 
            swapAndLiquify(swapTokens);

            //Take ALL tokens left inside the contract wallet and swap to AVAX
            //WHY?!?! Gas fees later?
            swapTokensForAVAX(balanceOf(address(this)));

            swapping = false;
            

        }
        //All swapping/fee taking is done. Now create node
        require(canCreate, "NCC: Not permitted to create nodes");
        nodeManager.createNode(sender, receiver, nodePrice, withRewards);
        canCreate = false;
    }

