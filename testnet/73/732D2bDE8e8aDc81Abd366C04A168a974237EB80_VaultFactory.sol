// VaultFactory.sol
// Manages vault pools. A vault is a yield pool that pays rewards in the deposited token.

/// @dev Safemath is redundant with Solidity 0.8+

pragma solidity ^0.8.6;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";

import "hardhat/console.sol";

// Vaults are similar to Farms, but don't require a reward token,
// instead relying on deposits to fund rewards. They're fun!

contract VaultFactory is Ownable, ReentrancyGuard {
	address public feeAddress; // fee wallet (for claims)
	address public treasuryAddress; // treasury (rewards) account
	uint256 public launchBlock; // the block rewards should start being generated
	uint256 lastRewardBlock; // Last block number of a reward calculation and distribution
	uint256 lockupTimeInSeconds = 3600;

	/// @notice Info of each user that stakes LP tokens.
	struct UserInfo {
		uint256 depositBalance; // balance the user has staked in the vault
		// uint256 lastPayoutBlock; // Last block number of a reward emission
		uint256 lastPayoutTime; // timestamp of last payout
	}
	/// @notice Map of each address that deposited tokens.
	mapping(uint256 => mapping(address => UserInfo)) public userInfo;

	/// @notice contains deposit and treasury amounts for each user
	struct VaultInfo {
		IERC20 depositedToken; // address of deposit token
		uint16 depositFeeBP; // deposit fee (goes to treasury) in basis points
		uint16 claimFeeBP; // claim fee (goes to devWallet) in basis points
		uint256 treasuryRewardBP; // reward percent accumulated after one period, divided amongst stakers (suggested 20%)
		uint256 treasuryBalance; // treasury for paying rewards out
		uint256 depositBalance; // total user deposits (using balanceOf would be a security issue)
		bool retired; // prevent new users depositing into this pool
	}
	VaultInfo[] public vaultInfo; // this is an array so we can iterate (without danger of a large iteration)

	constructor(
		address _feeAddress,
		address _treasuryAddress,
		uint256 _launchBlock
	) {
		feeAddress = _feeAddress;
		launchBlock = _launchBlock;
		treasuryAddress = _treasuryAddress;
	}

	/// create a new vault for a token
	function createVault(
		IERC20 _token,
		uint16 _depositFeeBP,
		uint16 _claimFeeBP,
		uint16 _treasuryRewardBP
	) public onlyOwner {
		require(
			_depositFeeBP < 10000 && _depositFeeBP > 0,
			"deposit fee must be between 0% and 100%"
		);

		require(
			_treasuryRewardBP <= 10000 && _treasuryRewardBP >= 0,
			"reward fee must be between 0% and 100%"
		);

		vaultInfo.push(
			VaultInfo({
				depositedToken: _token,
				depositFeeBP: _depositFeeBP,
				claimFeeBP: _claimFeeBP,
				treasuryRewardBP: _treasuryRewardBP,
				treasuryBalance: 0,
				depositBalance: 0,
				retired: false
			})
		);
	}

	function deposit(uint256 _vaultId, uint256 _amount) public nonReentrant {
		require(_amount > 0, "cannot deposit 0 funds");
		require(_vaultId < vaultInfo.length, "no such vault");

		VaultInfo storage vault = vaultInfo[_vaultId];
		UserInfo storage user = userInfo[_vaultId][msg.sender];

		// deposit token (minus fee) into vault
		vault.depositedToken.transferFrom(
			address(msg.sender),
			address(this),
			_amount
		);

		// calculate deposit fee (* 10000 to deal with basis points)
		uint256 depositFee = (_amount * vault.depositFeeBP) / 10000;

		user.lastPayoutTime = block.timestamp;

		// transfer fee to treasury
		vault.depositedToken.transfer(address(treasuryAddress), depositFee);
		vault.treasuryBalance += depositFee;

		// subtract fee from users deposit amount
		user.depositBalance += _amount - depositFee;
		vault.depositBalance += _amount - depositFee;
	}

	/// calculates the users share of a vault as a percentage BP
	function userVaultShareBP(uint256 _vaultId) public view returns (uint256) {
		require(_vaultId < vaultInfo.length, "no such vault");
		VaultInfo storage vault = vaultInfo[_vaultId];
		UserInfo storage user = userInfo[_vaultId][msg.sender];

		if (user.depositBalance == 0) {
			return 0;
		}

		return (user.depositBalance * 10000) / vault.depositBalance;
	}

	function pendingReward(uint256 _vaultId) public view returns (uint256) {
		require(_vaultId < vaultInfo.length, "no such vault");
		VaultInfo storage vault = vaultInfo[_vaultId];

		// if (user.lastPayoutTime >= block.timestamp) {
		// 	return 0;
		// }

		if (vault.treasuryBalance == 0) {
			return 0;
		}

		uint256 pendingTreasuryRewards = (vault.treasuryRewardBP *
			vault.treasuryBalance) / 10000;

		if (pendingTreasuryRewards == 0) {
			return 0;
		}

		uint256 pendingUserReward = (pendingTreasuryRewards *
			userVaultShareBP(_vaultId)) / 10000;

		return pendingUserReward;
	}

	function withdraw(uint256 _vaultId, uint256 _amount) public nonReentrant {
		require(_amount > 0, "cannot withdraw 0 funds");
		require(_vaultId < vaultInfo.length, "no such vault");

		VaultInfo storage vault = vaultInfo[_vaultId];
		UserInfo storage user = userInfo[_vaultId][msg.sender];

		require(_amount <= user.depositBalance, "attempt to overdraw funds");

		user.depositBalance -= _amount;
		vault.depositBalance -= _amount;
		vault.depositedToken.transfer(address(msg.sender), _amount);
	}

	function emergencyWithdraw(uint256 _vaultId) public nonReentrant {
		// TODO implement!
	}

	function claim(uint256 _vaultId) public nonReentrant returns (uint256) {
		require(_vaultId < vaultInfo.length, "no such vault");

		VaultInfo storage vault = vaultInfo[_vaultId];
		UserInfo storage user = userInfo[_vaultId][msg.sender];

		require(
			user.lastPayoutTime <= (block.timestamp - 1 days),
			"no rewards to claim yet"
		);

		// require(user.rewardDebt == 0, "no rewards to claim");

		// uint256 rewardDebt = user.rewardDebt;
		uint256 rewardDebt = pendingReward(_vaultId);

		// take the debt from the treasury
		vault.treasuryBalance -= rewardDebt;

		// collect a claim fee if there is one.
		if (vault.claimFeeBP > 0) {
			// calculate fee (* 10000 to deal with basis points)
			uint256 claimFee = (rewardDebt * vault.claimFeeBP) / 10000;

			// transfer the fee to the feeAddress
			vault.depositedToken.transferFrom(
				address(treasuryAddress),
				address(feeAddress),
				claimFee
			);

			// remove the fee from the amount owed to user
			rewardDebt -= claimFee;
		}

		user.lastPayoutTime = block.timestamp;
		vault.depositedToken.transferFrom(
			address(treasuryAddress),
			address(msg.sender),
			rewardDebt
		);

		return rewardDebt;
	}
}

