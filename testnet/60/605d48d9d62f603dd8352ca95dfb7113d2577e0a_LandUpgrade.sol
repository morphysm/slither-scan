  }

  function setToken(IERC20 value) public onlyOwner {
    token = value;
  }

  function setLand(ILand value) public onlyOwner {
    land = value;
  }

  function setSpell(ISpell value) public onlyOwner {
    spell = value;
  }

  function setRandomSource(IRandomSource value) external onlyOwner {
    randomSource = value;
  }

  function setBasePrice(uint256 value) external onlyOwner {
    basePrice = value;
  }

  function setExtraPricePerLevel(uint256 value) external onlyOwner {
    extraPricePerLevel = value;
  }

  function setBaseBurnFactor(uint256 value) public onlyOwner {
    baseBurnFactor = value;
  }

  function setBurnFactorPerLevel(uint256 value) public onlyOwner {
    burnFactorPerLevel = value;
  }

  function upgrade(
    uint256 token1,
    uint256 token2,
    bool useSpell,
    uint256 spellId
  ) public nonReentrant whenNotPaused {
    require(land.ownerOf(token1) == _msgSender(), "LandUpgrade: not owner of land 1");
    require(land.ownerOf(token2) == _msgSender(), "LandUpgrade: not owner of land 2");

    (uint256 type1, uint256 level1, ) = land.getTokenInfo(token1);
    (uint256 type2, uint256 level2, ) = land.getTokenInfo(token2);

    require(level1 == level2, "LandUpgrade: token levels must be equal");
    require(type2 >= type1, "LandUpgrade: types should be equal or token2 type must be higher");
    require(level1 < land.maxLevel() && level2 < land.maxLevel(), "LandUpgrade: level overflow");

    // Burn fee in system token
    token.transferFrom(_msgSender(), dev, getUpgradePrice(level2));

    // Burn a spell to level up the greater land
    if (useSpell) {
      require(spell.ownerOf(spellId) == _msgSender(), "LandUpgrade: not owner of the spell");
      spell.burn(spellId);
      land.burnAndMint(_msgSender(), [token1, token2], type2, level2 + 1);
      emit LandsUpgradedUp(_msgSender(), token1, token2);
      return;
    }

    uint256 rand = randomSource.getRandomMax(token1, PERC_MUL);

    // Sorry, so unlucky -_- Burn them all!
    if (rand <= getBurnFactor(level2)) {
      land.burnMultiple([token1, token2]);
      emit LandsBurned(_msgSender(), token1, token2);
      return;
    }

    uint256 type1percentage = (PERC_MUL * land.supplyByType(type1)) / land.totalSupply();
    uint256 type2percentage = (PERC_MUL * land.supplyByType(type2)) / land.totalSupply();

    // type1 probability = type1percentage * 100 / (type1percentage + type2percentage);

    if (rand > ((type1percentage * PERC_MUL) / (type1percentage + type2percentage))) {
      // token2 level up!
      //                                   ↓ random is somewhere here ↓
      // [--------------------------------|------------------------------] 100
      //         type1 probability             type2 probability
      // land.burnAndMint(_msgSender(), [token1, token2], type2, level2 + 1);
      emit LandsUpgradedUp(_msgSender(), token1, token2);
    } else {
      // token1 level up!
      //     ↓ random is somewhere here ↓
      // [--------------------------------|------------------------------] 100
      //         type1 probability             type2 probability
      // land.burnAndMint(_msgSender(), [token1, token2], type1, level1 + 1);
      emit LandsUpgraded(_msgSender(), token1, token2);
    }
  }

  function getBurnFactor(uint256 level) public view returns (uint256) {
    require(level >= land.minLevel() && level <= land.maxLevel(), "Invalid level");
    return baseBurnFactor.add(burnFactorPerLevel.mul(level));
  }

  function getUpgradePrice(uint256 level) public view returns (uint256) {
    require(level >= land.minLevel() && level <= land.maxLevel(), "Invalid level");
    return basePrice.add(extraPricePerLevel.mul(level));
  }

  event LandsBurned(address indexed account, uint256 token1, uint256 token2);
  event LandsUpgraded(address indexed account, uint256 token1, uint256 token2);
  event LandsUpgradedUp(address indexed account, uint256 token1, uint256 token2);
}