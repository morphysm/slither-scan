        returns (DataTypes.market_s memory);

    function repay(
        address _underlyingToken, 
        address _user, 
        uint256 _amount) external returns (uint256);

    function borrow(
        address _underlyingToken, 
        address _user, 
        uint256 _amount) external returns (uint256);
}

