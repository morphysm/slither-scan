pragma solidity ^0.8.0;

import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/interfaces/IERC721.sol";
import "@openzeppelin/contracts/interfaces/IERC20.sol";
import "@openzeppelin/contracts/interfaces/IERC2981.sol";
import "hardhat/console.sol";

contract MarketplaceAuction is Pausable {
    address private _owner;
    address private _wallet;

    AuctionAll[] public auctionsAll;

    mapping(address => mapping(uint => Auction)) public auctions;
    mapping(address => mapping(uint => AuctionPayableStatus)) public auctionsStatus;
    mapping(address => mapping(uint => Bid)) public bids;

    struct AuctionAll {
        address nftAddress;
        uint tokenID;
    }

    struct Bid {
        address from;
        uint256 amount;
        bool isActive;
    }

    struct Auction {
        address owner;
        address nftAddress;
        uint tokenID;
        uint startPrice;
        uint bidStep;
        uint blockDeadline;
        bool active;
        bool finalized;
        bool isToken;
        address tokenAddress;
    }

    struct AuctionPayableStatus {
        bool paid;
        bool paidCommission;
        bool transfer;
    }

    event BidSuccess(address from, address nftAddress, uint tokenID);
    event AuctionCreated(address owner, address nftAddress, uint tokenID);
    event AuctionCanceled(address owner, address to, address nftAddress, uint tokenID, uint lastBid);
    event AuctionFinalized(address owner, address to, address nftAddress, uint tokenID, uint lastBid);

    constructor(address wallet) {
        _wallet = wallet;
        _owner = msg.sender;
    }

    modifier isContractOwner() {
        require(_owner == msg.sender, "Marketplace Auction: not owner");
        _;
    }

    modifier isOwner(address nftAddress, uint tokenID) {
        require(auctions[nftAddress][tokenID].owner == msg.sender || _owner == msg.sender, "Marketplace Auction: not owner");
        _;
    }

    modifier contractIsTokenOwner(address nftAddress, uint tokenID) {
        address owner = IERC721(nftAddress).ownerOf(tokenID);
        require(owner == msg.sender && (IERC721(nftAddress).getApproved(tokenID) == address(this) || IERC721(nftAddress).isApprovedForAll(owner, address(this))), "Marketplace Auction: not approved");
        _;
    }

    function create(address nftAddress, uint tokenID, uint startPrice, uint bidStep, uint blockDeadline, bool isToken, address tokenAddress) public whenNotPaused() contractIsTokenOwner(nftAddress, tokenID) {
        require(block.timestamp < blockDeadline, "Marketplace createAuction: invalid blockDeadline");

        auctions[nftAddress][tokenID] = Auction(msg.sender, nftAddress, tokenID, startPrice, bidStep, blockDeadline, true, false, isToken, tokenAddress);
        auctionsStatus[nftAddress][tokenID] = AuctionPayableStatus(false, false, false);
        auctionsAll.push(AuctionAll(nftAddress, tokenID));

        emit AuctionCreated(msg.sender, nftAddress, tokenID);
    }

    function bid(address nftAddress, uint tokenID) public whenNotPaused() payable {
        Auction memory myAuction = auctions[nftAddress][tokenID];

        require(!myAuction.isToken, "Marketplace bid: invalid token");
        require(myAuction.owner != msg.sender, "Marketplace bid: invalid sender");
        require(block.timestamp < myAuction.blockDeadline, "Marketplace bid: block deadline");

        Bid memory lastBid = bids[nftAddress][tokenID];
        uint minBid = myAuction.startPrice;

        if (lastBid.isActive) minBid = lastBid.amount + myAuction.bidStep;
        require(minBid <= msg.value, "Marketplace bid: invalid bid amount");
        if (lastBid.isActive) require(payable(lastBid.from).send(lastBid.amount), "Marketplace bid: invalid transaction");

        bids[nftAddress][tokenID] = Bid(msg.sender, msg.value, true);

        emit BidSuccess(msg.sender, nftAddress, tokenID);
    }

    function bidToken(address nftAddress, uint tokenID, uint value) public whenNotPaused() {
        Auction memory myAuction = auctions[nftAddress][tokenID];

        require(myAuction.isToken, "Marketplace bidToken: invalid token");
        require(myAuction.owner != msg.sender, "Marketplace bidToken: invalid sender");
        require(block.timestamp < myAuction.blockDeadline, "Marketplace bidToken: block deadline");

        Bid memory lastBid = bids[nftAddress][tokenID];
        uint minBid = myAuction.startPrice;

        if (lastBid.isActive) minBid = lastBid.amount + myAuction.bidStep;
        require(minBid <= value, "Marketplace bidToken: invalid bid amount");
        require(IERC20(myAuction.tokenAddress).transferFrom(msg.sender, address(this), value), "Marketplace bidToken: invalid transaction");
        if (lastBid.isActive) require(IERC20(myAuction.tokenAddress).transfer(lastBid.from, lastBid.amount), "Marketplace bidToken: invalid transaction");

        bids[nftAddress][tokenID] = Bid(msg.sender, value, true);

        emit BidSuccess(msg.sender, nftAddress, tokenID);
    }

    function finalize(address nftAddress, uint tokenID) public whenNotPaused() {
        Auction memory myAuction = auctions[nftAddress][tokenID];
        AuctionPayableStatus memory myAuctionPayableStatus = auctionsStatus[nftAddress][tokenID];
        Bid memory lastBid = bids[nftAddress][tokenID];

        require(block.timestamp >= myAuction.blockDeadline, "Marketplace finalizeAuction: block deadline");

        if (lastBid.isActive) {
            uint commission = lastBid.amount * 25 / 1000;//todo var
            uint finaleAmount = lastBid.amount - commission;

            // TOKEN TRANSFER
            if (!myAuctionPayableStatus.transfer) {
                if (IERC721(myAuction.nftAddress).ownerOf(myAuction.tokenID) != myAuction.owner) {
                    cancel(nftAddress, tokenID);
                } else {
                    IERC721(myAuction.nftAddress).transferFrom(myAuction.owner, lastBid.from, myAuction.tokenID);
                    require(IERC721(myAuction.nftAddress).ownerOf(myAuction.tokenID) == lastBid.from, "Marketplace finalizeAuction: invalid transfer transaction");
                }
            }
            auctionsStatus[nftAddress][tokenID].transfer = true;

            // COMMISSION
            if (!myAuctionPayableStatus.paidCommission) {
                if (myAuction.isToken) require(IERC20(myAuction.tokenAddress).transfer(_wallet, commission), "Marketplace finalizeAuction: invalid commission transaction");
                else require(payable(_wallet).send(commission), "Marketplace finalizeAuction: invalid commission transaction");
            }
            auctionsStatus[nftAddress][tokenID].paidCommission = true;

            // TOKEN SALE
            if (!myAuctionPayableStatus.paid) {
                if (myAuction.isToken) require(IERC20(myAuction.tokenAddress).transfer(myAuction.owner, finaleAmount), "Marketplace finalizeAuction: invalid sale transaction");
                else require(payable(myAuction.owner).send(finaleAmount), "Marketplace finalizeAuction: invalid sale transaction");
            }
            auctionsStatus[nftAddress][tokenID].paid = true;

            auctions[nftAddress][tokenID].active = false;
            auctions[nftAddress][tokenID].finalized = true;
            emit AuctionFinalized(myAuction.owner, lastBid.from, myAuction.nftAddress, myAuction.tokenID, lastBid.amount);
        } else {
            auctions[nftAddress][tokenID].active = false;
            auctions[nftAddress][tokenID].finalized = true;

            emit AuctionCanceled(myAuction.owner, myAuction.owner, myAuction.nftAddress, myAuction.tokenID, 0);
        }
    }

    function cancel(address nftAddress, uint tokenID) public isOwner(nftAddress, tokenID) {
        Auction memory myAuction = auctions[nftAddress][tokenID];
        Bid memory lastBid = bids[nftAddress][tokenID];

        if (lastBid.isActive) {
            if (myAuction.isToken) require(IERC20(myAuction.tokenAddress).transfer(lastBid.from, lastBid.amount), "Marketplace cancelAuction: invalid transfer transaction");
            else require(payable(lastBid.from).send(lastBid.amount), "Marketplace cancelAuction: invalid send transaction");
        }

        auctions[nftAddress][tokenID].active = false;
        emit AuctionCanceled(msg.sender, msg.sender, myAuction.nftAddress, myAuction.tokenID, 0);
    }

    function changeWallet(address wallet_) public whenNotPaused() isContractOwner() {
        _wallet = wallet_;
    }

    function followBack() public whenPaused() isContractOwner() {
        for (uint i = 0; i < auctionsAll.length; i++) {
            if (auctions[auctionsAll[i].nftAddress][auctionsAll[i].tokenID].active) {
                cancel(auctionsAll[i].nftAddress, auctionsAll[i].tokenID);
            }
        }
    }

    function pause() public isContractOwner() whenNotPaused() {
        _pause();
    }

    function unpause() public isContractOwner() whenPaused() {
        _unpause();
    }

    function getWallet() public view returns (address) {
        return _wallet;
    }

    function getOwner() public view returns (address) {
        return _owner;
    }
}

