contract BingoToken is  Context, IERC20, Ownable{
    using SafeERC20 for IERC20;
    using Address for address;
    using SafeMath for uint;

    string private _name = 'bingo token';
    string private _symbol = 'BG';
    uint8 private _decimals = 18;
    uint private _totalSupply;
    uint public totalFee;

    mapping (address => uint) private _balances;
    mapping (address => mapping (address => uint)) private _allowances;
    mapping (address => uint) private _excludeFee;//白名单功能 0未在名单，1在名单
    mapping (address => uint) private _blackListMap;//黑名单功能 0正常，1拉黑
    mapping (address => uint) private _totalReceiveFeeMap;// 各个地址收的手续费map
    mapping (address => uint) private _buyFeeRateMap;// 买token时手续费百分比
    mapping (address => uint) private _saleFeeRateMap;// 卖token时手续费百分比

    address private _groupAddr = 0x88665eBf27793d29a9c691ec6E6Cf064C3F34e23;//团队归属地址
    address private _institutionalFinancingAddr = 0x88665eBf27793d29a9c691ec6E6Cf064C3F34e23;//机构融资
    address private _operateAddr = 0x88665eBf27793d29a9c691ec6E6Cf064C3F34e23;//早期运营
    address private _jackpotPoolAddr =  0x88665eBf27793d29a9c691ec6E6Cf064C3F34e23;//Jackpot池
    address private _IDOAddr = 0x88665eBf27793d29a9c691ec6E6Cf064C3F34e23;//IDO
    address private _clubRewardAddr = 0x88665eBf27793d29a9c691ec6E6Cf064C3F34e23;//CLUB奖励（战队排名奖励）
    address private _deadAddr = 0x000000000000000000000000000000000000dEaD;//销毁地址

    bool _pauseTransfer = false;// 是否暂停交易

    event PauseTransfer(bool isPause);

    //constructor
    constructor () {
        //发行量 10000000000
        _mint(_groupAddr, 600000000 ether);
        _mint(_institutionalFinancingAddr, 200000000 ether);
        _mint(_operateAddr, 200000000 ether);
        _mint(_jackpotPoolAddr, 5000000000 ether);
        _mint(_IDOAddr, 2000000000 ether);
        _mint(_clubRewardAddr, 2000000000 ether);

        //init buyFeeRateMap
        _buyFeeRateMap[_jackpotPoolAddr] = 3;
        _buyFeeRateMap[_clubRewardAddr] = 1;
        _buyFeeRateMap[_deadAddr] = 1;
        _buyFeeRateMap[_groupAddr] = 1;

        //init saleFeeRateMap
        _saleFeeRateMap[_jackpotPoolAddr] = 3;
        _saleFeeRateMap[_clubRewardAddr] = 1;
        _saleFeeRateMap[_deadAddr] = 7;
    }

    function _mint(address account, uint amount) internal {
        require(account != address(0), "ERC20: mint to the zero address");
        _totalSupply = _totalSupply.add(amount);
        _balances[account] = _balances[account].add(amount);
        emit Transfer(address(0), account, amount);
    }

    function name() public view virtual returns (string memory) {
        return _name;
    }
    function symbol() public view virtual returns (string memory) {
        return _symbol;
    }
    function decimals() public view virtual returns (uint8) {
        return _decimals;
    }
    function totalSupply() public view virtual override returns (uint256) {
        return _totalSupply;
    }
    function balanceOf(address account) public view virtual override returns (uint256) {
        return _balances[account];
    }
    function transfer(address recipient, uint amount) public virtual override returns (bool) {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }
    function allowance(address owner, address spender) public view virtual override returns (uint256) {
        return _allowances[owner][spender];
    }
    function approve(address spender, uint256 amount) public virtual override returns (bool) {
        _approve(_msgSender(), spender, amount);
        return true;
    }
    function transferFrom(address sender, address recipient, uint amount) public virtual override returns (bool) {
        _transfer(sender, recipient, amount);
        _approve(sender, _msgSender(), _allowances[sender][_msgSender()].sub(amount, "ERC20: transfer amount exceeds allowance"));
        return true;
    }
    function increaseAllowance(address spender, uint addedValue) public virtual returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender].add(addedValue));
        return true;
    }
    function decreaseAllowance(address spender, uint subtractedValue) public virtual returns (bool) {
        _approve(_msgSender(), spender, _allowances[_msgSender()][spender].sub(subtractedValue, "ERC20: decreased allowance below zero"));
        return true;
    }

    function _approve(address owner, address spender, uint amount) internal virtual {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function _transfer(address sender, address recipient, uint amount) internal{
        require(_pauseTransfer == false, "ERC20: transfer is paused");
        require(sender != address(0), "ERC20: transfer from the zero address");
        require(recipient != address(0), "ERC20: transfer to the zero address");
        require(_blackListMap[sender] == 0, "ERC20: sender be pulled black");
        require(_blackListMap[recipient] == 0, "ERC20: recipient be pulled black");

        //reduce
        _balances[sender] = _balances[sender].sub(amount, "ERC20: transfer amount exceeds balance");

        //add amount
        uint addValue = amount;
        //计算税费
        if (!(_excludeFee[sender] == 1 || _excludeFee[recipient] == 1)) {//发接方有一人白名单，则free
            uint onPercent = amount.div(100);//百分之一

            //计算税费
            uint buyJackpotFee = onPercent.mul(_buyFeeRateMap[_jackpotPoolAddr]);
            uint buyClubFee = onPercent.mul(_buyFeeRateMap[_clubRewardAddr]);
            uint buyDeadFee = onPercent.mul(_buyFeeRateMap[_deadAddr]);
            uint buyOperateFee = onPercent.mul(_buyFeeRateMap[_operateAddr]);
            uint saleJackpotFee = onPercent.mul(_saleFeeRateMap[_jackpotPoolAddr]);
            uint saleClubFee = onPercent.mul(_saleFeeRateMap[_clubRewardAddr]);
            uint saleDeadFee = onPercent.mul(_saleFeeRateMap[_deadAddr]);

            //记录税费
            _totalReceiveFeeMap[_jackpotPoolAddr] = _totalReceiveFeeMap[_jackpotPoolAddr].add(buyJackpotFee).add(saleJackpotFee);
            _totalReceiveFeeMap[_clubRewardAddr] = _totalReceiveFeeMap[_clubRewardAddr].add(buyClubFee).add(saleClubFee);
            _totalReceiveFeeMap[_deadAddr] = _totalReceiveFeeMap[_deadAddr].add(buyDeadFee).add(saleDeadFee);
            _totalReceiveFeeMap[_operateAddr] = _totalReceiveFeeMap[_operateAddr].add(buyOperateFee);
            uint currentFee = buyJackpotFee.add(buyClubFee).add(buyDeadFee).add(buyOperateFee);
            currentFee = currentFee.add(saleJackpotFee).add(saleClubFee).add(saleDeadFee);
            totalFee += currentFee;

            addValue = amount - currentFee;

            //添加税费到各个地址
            _balances[_jackpotPoolAddr] = _balances[_jackpotPoolAddr].add(buyJackpotFee).add(saleJackpotFee);
            _balances[_clubRewardAddr] = _balances[_clubRewardAddr].add(buyClubFee).add(saleClubFee);
            _balances[_deadAddr] = _balances[_deadAddr].add(buyDeadFee).add(saleDeadFee);
            _balances[_operateAddr] = _balances[_operateAddr].add(buyOperateFee);
        }

        _balances[recipient] = _balances[recipient].add(addValue);


        emit Transfer(sender, recipient, amount);
    }

    //设置免手续费地址（白名单）0不在白名单，1在白名单
    function setExcludeFee(address account, uint val) onlyOwner public returns (bool) {
        _excludeFee[account] = val;
        return true;
    }

    //设置是否暂停代币交易
    function pauseTransfer(bool isPause) onlyOwner external returns (bool){
        _pauseTransfer = isPause;
        emit PauseTransfer(isPause);
        return true;
    }

