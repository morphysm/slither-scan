pragma solidity ^0.8.0;

import "hardhat/console.sol";

import "@openzeppelin/contracts/token/ERC721/ERC721.sol";
import "@openzeppelin/contracts/finance/PaymentSplitter.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/Counters.sol";

contract Nodes is ERC721, PaymentSplitter, Ownable {
  using Counters for Counters.Counter;

  struct Node {
    uint128 creation;
    uint128 lastClaim;
  }

  event BlacklistUpdate(address indexed account, bool blacklisted);

  uint256 public startDate;

  Counters.Counter private _nodeCounter;
  mapping(uint256 => Node) private _nodes;
  mapping(address => bool) private _blacklist;

  constructor(
    address[] memory payees,
    uint256[] memory _shares,
    uint256 _startDate
  ) ERC721("Fruit Nodes", "Fruit") PaymentSplitter(payees, _shares) {
    startDate = _startDate;
  }

  function isBlacklisted(address account) external view returns (bool) {
    return _blacklist[account];
  }

  function today() public view returns (uint256) {
    return (block.timestamp - startDate) / (1 days);
  }

  function totalNodesCreated() external view returns (uint256) {
    return _nodeCounter.current();
  }

  function getNode(uint256 nodeId) external view returns (Node memory) {
    return _nodes[nodeId];
  }

  function createNode(address account, uint256 amount) external onlyOwner {
    require(
      startDate <= block.timestamp,
      "Nodes: Cannot create node before start"
    );
    for (uint256 i = 0; i < amount; i++) {
      _createNode(account, uint128(today()), uint128(today()));
    }
  }

  function updateClaim(uint256 nodeId) external onlyOwner returns (uint256) {
    require(_exists(nodeId), "Nodes: Node do not exists");
    require(!_blacklist[ownerOf(nodeId)], "Nodes: account is blacklisted");
    Node storage _node = _nodes[nodeId];
    uint128 claimDay = uint128(today());
    uint256 daysElapsed = claimDay - _node.lastClaim;
    _node.lastClaim = claimDay;
    return daysElapsed;
  }

  function setBlacklist(address account, bool blacklisted) external onlyOwner {
    if (_blacklist[account] != blacklisted) {
      _blacklist[account] = blacklisted;

      emit BlacklistUpdate(account, blacklisted);
    }
  }

  function addNode(
    address account,
    uint128 creationDay,
    uint128 claimDay
  ) external onlyOwner {
    _createNode(account, creationDay, claimDay);
  }

  function removeNode(uint256 nodeId) external onlyOwner {
    _burn(nodeId);
  }

  function _createNode(
    address account,
    uint128 creationDay,
    uint128 claimDay
  ) internal {
    require(balanceOf(account) <= 100, "Nodes: Account have already 100 Nodes");
    _nodeCounter.increment();
    _mint(account, _nodeCounter.current());
    _nodes[_nodeCounter.current()] = Node({
      creation: creationDay,
      lastClaim: claimDay
    });
  }

  function _beforeTokenTransfer(
    address from,
    address to,
    uint256
  ) internal view override {
    if (to != address(0)) {
      require(balanceOf(to) < 100, "Nodes: Account have already 100 Nodes");
    }
    require(!_blacklist[from], "Nodes: transfer from blacklist");
    require(!_blacklist[to], "Nodes: transfer to blacklist");
  }
}

