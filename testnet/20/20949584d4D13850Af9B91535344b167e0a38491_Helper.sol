import "hardhat/console.sol";

import "./Nodes.sol";
import "./Token.sol";
import "./Pool.sol";

import "./dependencies/IUniswapV2Router02.sol";
import "./dependencies/IUniswapV2Factory.sol";
import "./dependencies/IUniswapV2Pair.sol";

import "@openzeppelin/contracts/finance/PaymentSplitter.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";

contract Helper is PaymentSplitter, AccessControl {
  Nodes private nodes;
  FruitNodeToken private token;
  Pool private pool;
  IUniswapV2Router02 private router =
    IUniswapV2Router02(0x5db0735cf88F85E78ed742215090c465979B5006);
  // IUniswapV2Router02 private router = IUniswapV2Router02(0xa5E0829CaCEd8fFDD4De3c43696c57F7D7A678ff);

  bytes32 public constant MANAGER_ROLE =
    0xaf290d8680820aad922855f39b306097b20e28774d6c1ad35a20325630c3a02c;

  bytes32 public constant ENGINEER_ROLE =
    0x56a2da3687a5982774df44639b06a410da311ff14844c2f7ff0cab50d681571c;

  event SwapAndLiquify(
    uint256 amountToken,
    uint256 amountEth,
    uint256 amountLiquidity
  );

  uint256 public poolFee = 90;

  bool public isSwaping = true;

  uint256 public minToSwap = 1 * 10**18;
  uint256 public maxToSwap = 30 * 10**18;
  uint256 public price = 10 * 10**18;
  uint256 public rewardAmount = 5 * 10**17;
  uint256 public minPool = 10000 * 10**18;
  uint256 public poolAmount = 20000 * 10**18;

  bool public disableSell = false;

  constructor(
    address[] memory payees,
    uint256[] memory _shares,
    address payable _nodes,
    address payable _token
  ) PaymentSplitter(payees, _shares) {
    nodes = Nodes(_nodes);
    token = FruitNodeToken(_token);
    pool = new Pool(_token);
    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
  }

  function getPoolAddress() external view returns (address) {
    return address(pool);
  }

  function setMinToSwap(uint256 newValue) external onlyRole(ENGINEER_ROLE) {
    minToSwap = newValue;
  }

  function setMaxToSwap(uint256 newValue) external onlyRole(ENGINEER_ROLE) {
    maxToSwap = newValue;
  }

  function setPrice(uint256 newValue) external onlyRole(ENGINEER_ROLE) {
    price = newValue;
  }

  function setDisableSell(bool newValue) external onlyRole(ENGINEER_ROLE) {
    disableSell = newValue;
  }

  function setSwapping(bool newValue) external onlyRole(ENGINEER_ROLE) {
    isSwaping = newValue;
  }

  function setDefaultFees(uint256 feePool) external onlyRole(ENGINEER_ROLE) {
    poolFee = feePool;
  }

  function setRewardAmount(uint256 _amount) external onlyRole(ENGINEER_ROLE) {
    rewardAmount = _amount;
  }

  function setPoolUpdate(uint256 _minPool, uint256 _poolAmount)
    external
    onlyRole(ENGINEER_ROLE)
  {
    minPool = _minPool;
    poolAmount = _poolAmount;
  }

  function newPool() external onlyRole(DEFAULT_ADMIN_ROLE) {
    pool.pay(msg.sender, token.balanceOf(address(pool)));
    pool = new Pool(address(token));
  }

  function transferOwnership(address newAddress)
    external
    onlyRole(DEFAULT_ADMIN_ROLE)
  {
    nodes.transferOwnership(newAddress);
  }

  function addNode(
    address account,
    uint128 creationDay,
    uint128 claimDay
  ) external onlyRole(MANAGER_ROLE) {
    nodes.addNode(account, creationDay, claimDay);
  }

  function removeNode(uint256 nodeId) external onlyRole(MANAGER_ROLE) {
    nodes.removeNode(nodeId);
  }

  function setBlacklist(address account, bool value)
    external
    onlyRole(MANAGER_ROLE)
  {
    nodes.setBlacklist(account, value);
  }

  function createNodes(uint256 amount) external {
    require(!disableSell, "Helper: creation of node is disabled");
    require(msg.sender != address(0), "Helper: creation from the zero address");
    uint256 bill = price * amount;
    token.transferFrom(msg.sender, address(this), bill);
    nodes.createNode(msg.sender, amount);
    _split();
  }

  function claim(uint256[] calldata _nodes) public {
    uint256 reward = 0;
    for (uint256 i = 0; i < _nodes.length; i++) {
      require(
        nodes.ownerOf(_nodes[i]) == msg.sender,
        "Helper: You don't own all nodes"
      );
      uint256 daysElapsed = nodes.updateClaim(_nodes[i]);
      reward += _getReward(daysElapsed);
    }
    pool.pay(msg.sender, reward);
    if (token.balanceOf(address(pool)) < minPool) {
      token.addRewards(address(pool), poolAmount);
    }
  }

  function _split() internal {
    uint256 tokenBalance = token.balanceOf(address(this));

    uint256 poolTokens = (tokenBalance * poolFee) / 100;
    token.burn(poolTokens);

    _proccess();
  }

  function _proccess() internal {
    uint256 balance = token.balanceOf(address(this));
    if (isSwaping && balance >= minToSwap) {
      if (balance > maxToSwap) {
        balance = maxToSwap;
      }

      _swapAndLiquify(balance);
    }
  }

  function _swapAndLiquify(uint256 amount) internal {
    uint256 eth = amount / 2;
    uint256 tokens = amount - eth;

    uint256 ethBalance = address(this).balance;
    _swapForEth(eth);
    uint256 received = address(this).balance - ethBalance;

    _addLiquidity(tokens, received);
    emit SwapAndLiquify(eth, received, tokens);
  }

  function _swapForEth(uint256 amount) internal {
    address[] memory path = new address[](2);
    path[0] = address(token);
    path[1] = router.WAVAX();

    token.approve(address(router), amount);

    router.swapExactTokensForETH(
      amount,
      0,
      path,
      address(this),
      block.timestamp
    );
  }

  function _addLiquidity(uint256 tokenAmount, uint256 ethAmount) internal {
    token.approve(address(router), tokenAmount);

    router.addLiquidityETH{ value: ethAmount }(
      address(token),
      tokenAmount,
      0,
      0,
      address(this),
      block.timestamp
    );
  }

  function _getReward(uint256 daysElapsed) internal view returns (uint256) {
    return daysElapsed * rewardAmount;
  }
}

