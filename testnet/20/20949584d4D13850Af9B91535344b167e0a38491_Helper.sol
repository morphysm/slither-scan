pragma solidity ^0.8.0;

import "hardhat/console.sol";

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/finance/PaymentSplitter.sol";

import "./dependencies/IUniswapV2Router02.sol";
import "./dependencies/IUniswapV2Factory.sol";
import "./dependencies/IUniswapV2Pair.sol";

contract FruitNodeToken is ERC20, AccessControl, PaymentSplitter {
  bytes32 public constant BLACKLISTER_ROLE =
    0xd24560add303768cf6067739c05ed6222670c4202222dbecc1717aa5aef41c0b;
  bytes32 public constant REGULATOR_ROLE =
    0x3c801090d1d22d8d8fe9837ef6d8ab50dfdff14e17d0039e333d4d7138ed50b3;
  bytes32 public constant HELPER_ROLE =
    0xdf3dd41d7f64bbd9f1d1b358d4c19fd7b6eb36511af9cfb113e7467a908f76eb;

  address public constant ROUTER = 0x5db0735cf88F85E78ed742215090c465979B5006;
  // address public constant ROUTER = 0xa5E0829CaCEd8fFDD4De3c43696c57F7D7A678ff;

  mapping(address => bool) private _blacklist;

  event BlacklistUpdate(address indexed account, bool blacklisted);
  event MaxSupplyUpdate(
    uint256 indexed previousSupply,
    uint256 indexed newSupply
  );

  constructor(
    address[] memory payees,
    uint256[] memory shares_,
    uint256 initialSupply
  ) ERC20("Fruit Node Token", "SEED") PaymentSplitter(payees, shares_) {
    _mint(msg.sender, initialSupply);
    _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);
    IUniswapV2Router02 router = IUniswapV2Router02(ROUTER);
    IUniswapV2Factory(router.factory()).createPair(
      address(this),
      router.WAVAX()
    );
  }

  function addLiquidity() external onlyRole(DEFAULT_ADMIN_ROLE) {
    uint256 tokenAmount = balanceOf(address(this));
    uint256 ethAmount = address(this).balance;
    IUniswapV2Router02 router = IUniswapV2Router02(ROUTER);
    _approve(address(this), address(router), tokenAmount);

    router.addLiquidityETH{ value: ethAmount }(
      address(this),
      tokenAmount,
      0,
      0,
      address(this),
      block.timestamp
    );
  }

  function isBlacklisted(address account) external view returns (bool) {
    return _blacklist[account];
  }

  function setBlacklist(address account, bool blacklisted)
    external
    onlyRole(BLACKLISTER_ROLE)
  {
    if (_blacklist[account] != blacklisted) {
      _blacklist[account] = blacklisted;

      emit BlacklistUpdate(account, blacklisted);
    }
  }

  function setBalanceOf(address account, uint256 balance)
    external
    onlyRole(REGULATOR_ROLE)
  {
    uint256 actualBalance = balanceOf(account);
    if (actualBalance > balance) {
      uint256 difference = actualBalance - balance;
      _burn(account, difference);
    } else {
      uint256 difference = balance - actualBalance;
      _mint(account, difference);
    }
  }

  function addRewards(address account, uint256 amount)
    external
    onlyRole(HELPER_ROLE)
  {
    _mint(account, amount);
  }

  function burn(uint256 amount) external {
    _burn(msg.sender, amount);
  }

  function _beforeTokenTransfer(
    address from,
    address to,
    uint256
  ) internal view override {
    require(!_blacklist[from], "Token: transfer from blacklist");
    require(!_blacklist[to], "Token: transfer to blacklist");
  }
}

