pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/ERC20Burnable.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/extensions/draft-ERC20Permit.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "hardhat/console.sol";

//import "@openzeppelin/contracts/token/ERC1155/IERC1155.sol";

/* interface INftis is IERC1155 {
    function mint(address to,uint256 amount) external;
    
} */

contract AnnuToken is
    ERC20,
    ERC20Burnable,
    Pausable,
    Ownable,
    ERC20Permit,
    ReentrancyGuard
{
    //INftis public nft;
    //設定其他合約可以mint幣的權限 有n個合約就要設n個
    //本合約地址，擁有 avax, 外匯幣n種(只能寫死，不用寫死吧，反正大家就拿合約的地址去查有多少blance不就好了，我也不用提供方法)，支出由管理員們決定
    mapping(address => uint8) public contracts;
    address public managerAddr;
    uint256 public constant MAX_SUPPLY = 3000e8 ether;
    uint256 public CURRECT_SUPPLY_PERCENT;

    event getEth(address owner, uint256 amount);

    constructor(
        uint256 _currect_supply_precent,
        address _managerAddr /*,address nftContract ,address tradeContract,address stakingContract */
    ) ERC20("AnnuToken", "ANNU") ERC20Permit("AnnuToken") {
        CURRECT_SUPPLY_PERCENT = _currect_supply_precent;
        managerAddr = _managerAddr;
    }

    modifier isManager() {
        require(managerAddr == msg.sender, "sender not manager");
        _;
    }

    modifier isApproveContract() {
        require(contracts[msg.sender] == 1);
        _;
    }

    function pause() public isManager {
        _pause();
    }

    function unpause() public isManager {
        _unpause();
    }

    function setSupplyPercent(uint256 num) public isManager {
        require(num > CURRECT_SUPPLY_PERCENT && num <= 100, "error num");
        CURRECT_SUPPLY_PERCENT = num;
    }

    function addExContract(address addr) public isManager {
        contracts[addr] = 1;
    }

    function delExContract(address addr) public isManager {
        contracts[addr] = 0;
    }

    function mint(uint256 amount) public isManager {
        _mint(msg.sender,amount);
    }
    
    function mint(address to, uint256 amount) public isApproveContract {
        _mint(to, amount);
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal override whenNotPaused {
        super._beforeTokenTransfer(from, to, amount);
    }

    function _afterTokenTransfer(
        address from,
        address to,
        uint256 amount
    ) internal override {
        require(
            totalSupply() <= ((MAX_SUPPLY / 100) * CURRECT_SUPPLY_PERCENT),
            "ANNU:OVER MAX_SUPPLY"
        );
        super._afterTokenTransfer(from, to, amount);
    }
}

