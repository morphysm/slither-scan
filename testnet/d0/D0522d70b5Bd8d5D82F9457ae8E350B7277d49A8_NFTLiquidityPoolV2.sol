        _price = 0 ether; //  mint price
        _maxMintable = 1050; // max supply
        _maxMintPerTx = 10; // max mint per tx
        _nftRoyaltyRate = 5; // royalty nft every 5th mint
        _royaltyAmount = 300; // royalties 3%
    }

