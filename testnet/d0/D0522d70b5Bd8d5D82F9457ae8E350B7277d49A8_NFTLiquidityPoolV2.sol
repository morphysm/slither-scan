        emit TokenWithdrawal(owner(), amount, 0);
    }

    /// @inheritdoc LSSVMPair
    function rescueERC20(
        address receiver,
        ERC20 a,
        uint256 amount
    ) external override onlyOwner {
        a.safeTransfer(receiver, amount);
    }
