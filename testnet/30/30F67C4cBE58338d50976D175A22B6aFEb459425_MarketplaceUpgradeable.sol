// SPDX-License-Identifier: MIT
import "@openzeppelin/contracts-upgradeable/utils/ContextUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";

pragma solidity ^0.8.10;

contract CollectionUpgradeable is Initializable, ContextUpgradeable {
    error Collection__Listed();
    error Collection__NotExist();
    error Collection__InvalidType();
    error Collection__ExceedPercent();

    event CollectionsAdded(address indexed collection, uint256 adminFee, uint256 creatorFee, uint256 businessFee);
    event CollectionUpdated(address indexed collection, uint256 adminFee, uint256 creatorFee, uint256 businessFee);
    event CollectionsClosed(address indexed collection);

    struct Collection {
        uint256 adminFee;
        uint256 creatorFee;
        uint256 businessFee;
    }

    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.AddressSet;
    EnumerableSetUpgradeable.AddressSet internal _collectionAddressSet;

    uint256 public constant HUNDRED_PERCENT = 10000;
    mapping(address => Collection) private _collections;

    function __Collection_init() internal onlyInitializing {
        __Collection_init_unchained();
    }

    function __Collection_init_unchained() internal onlyInitializing {}

    function _acceptCollection(
        address collection_,
        uint256 adminFee_,
        uint256 creatorFee_,
        uint256 businessFee_
    ) internal {
        if (_collectionAddressSet.contains(collection_)) revert Collection__Listed();
        if (IERC721Upgradeable(collection_).supportsInterface(0x80ac58cd)) revert Collection__InvalidType(); // not support ERC721
        if (adminFee_ + creatorFee_ > HUNDRED_PERCENT) revert Collection__ExceedPercent();
        _collectionAddressSet.add(collection_);
        _collections[collection_] = Collection({ adminFee: adminFee_, creatorFee: creatorFee_, businessFee: businessFee_ });
        emit CollectionsAdded(collection_, adminFee_, creatorFee_, businessFee_);
    }

    function _modifyCollection(
        address collection_,
        uint256 adminFee_,
        uint256 creatorFee_,
        uint256 businessFee_
    ) internal {
        if (!_collectionAddressSet.contains(collection_)) revert Collection__NotExist();
        if (adminFee_ + creatorFee_ + businessFee_ > HUNDRED_PERCENT) revert Collection__ExceedPercent();
        _collections[collection_] = Collection({ adminFee: adminFee_, creatorFee: creatorFee_, businessFee: businessFee_ });
        emit CollectionUpdated(collection_, adminFee_, creatorFee_, businessFee_);
    }

    function _closeCollection(address collection_) internal {
        if (!_collectionAddressSet.contains(collection_)) revert Collection__NotExist();
        _collectionAddressSet.remove(collection_);
        emit CollectionsClosed(collection_);
    }

    function _canTokenBeListed(address collection_, uint256[] calldata tokenIds_) internal view returns (bool) {
        if (!_collectionAddressSet.contains(collection_)) return false;

        uint256 length = tokenIds_.length;
        address sender = _msgSender();
        IERC721Upgradeable collection = IERC721Upgradeable(collection_);

        for (uint256 i = 0; i < length; i++) {
            if (collection.ownerOf(tokenIds_[i]) != sender) {
                return false;
            }
        }
        return true;
    }

    function _calculatePriceAndFeesForCollection(address collection_, uint256 _price)
        internal
        view
        returns (
            uint256 netPrice,
            uint256 adminFee,
            uint256 creatorFee,
            uint256 businessFee
        )
    {
        adminFee = (_price * _collections[collection_].adminFee) / HUNDRED_PERCENT;
