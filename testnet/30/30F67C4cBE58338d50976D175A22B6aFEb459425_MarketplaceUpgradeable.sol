pragma solidity ^0.8.10;

import { Initializable } from "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import { UUPSUpgradeable } from "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import { IERC20Upgradeable } from "@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol";
import { IERC721Upgradeable } from "@openzeppelin/contracts-upgradeable/token/ERC721/IERC721Upgradeable.sol";
import { ERC20BurnableUpgradeable } from "@openzeppelin/contracts-upgradeable/token/ERC20/extensions/ERC20BurnableUpgradeable.sol";
import { EnumerableSetUpgradeable } from "@openzeppelin/contracts-upgradeable/utils/structs/EnumerableSetUpgradeable.sol";
import { SafeCastUpgradeable } from "@openzeppelin/contracts-upgradeable/utils/math/SafeCastUpgradeable.sol";
import { IMarketplace } from "./interfaces/IMarketplace.sol";
import { IERC721Metadata } from "./interfaces/IERC721Metadata.sol";
import { CollectionUpgradeable } from "./internal-upgradeable/CollectionUpgradeable.sol";
import { PaymentTokenUpgradeable } from "./internal-upgradeable/PaymentTokenUpgradeable.sol";
import { ReentrancyGuardUpgradeable } from "./internal-upgradeable/ReentrancyGuardUpgradeable.sol";
import { WithdrawTokenUpgradeable } from "./internal-upgradeable/WithdrawTokenUpgradeable.sol";

contract MarketplaceUpgradeable is IMarketplace, Initializable, UUPSUpgradeable, ReentrancyGuardUpgradeable, WithdrawTokenUpgradeable, CollectionUpgradeable, PaymentTokenUpgradeable {
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.UintSet;
    using SafeCastUpgradeable for uint256;

    struct Order {
        uint96 price;
        address seller;
        address paymentToken;
        address buyer;
        uint256[] tokenIds;
    }

    address public marketplaceAdmin;
    address public liquifier;
    // collection => orderId => orderDetail
    mapping(address => mapping(uint256 => Order)) private _orderDetails;

    function initialize() external initializer {
        address sender = _msgSender();
        __Ownable_init();
        __UUPSUpgradeable_init();
        __Collection_init();
        __PaymentToken_init();
        __ReentrancyGuard_init();
        _setLiquifier(0xA7B80BAa51C89A535Dc105ea2C908a2859d969eD);
        _setMarketplaceAdmin(sender);
    }

    function createOrder(
        address collection_,
        uint256 orderId_,
        uint256[] calldata tokenIds_,
        address paymentToken_,
        uint256 price_
    ) external nonReentrant {
        address sender = _msgSender();
        if (!_acceptedToken(paymentToken_)) revert Marketplace__TokenNotAccepted();
        if (!_canTokenBeListed(collection_, tokenIds_)) revert Marketplace__CanNotBeListed();
        if (_orderDetails[collection_][orderId_].price != 0) revert Marketplace__OrderAlreadyListed();
        _orderDetails[collection_][orderId_] = Order({ seller: sender, tokenIds: tokenIds_, paymentToken: paymentToken_, price: price_.toUint96(), buyer: address(0) });

        emit CreateOrder(collection_, orderId_, sender, tokenIds_, paymentToken_, price_);
    }

    function updateOrder(
        address collection_,
        uint256 orderId_,
        address newPaymentToken_,
        uint256 newPrice_
    ) external nonReentrant {
        address sender = _msgSender();
        Order memory order = _orderDetails[collection_][orderId_];
        if (!_acceptedToken(newPaymentToken_)) revert Marketplace__TokenNotAccepted();
        if (order.seller != sender) revert Marketplace__NotOwnerOder();
        if (order.buyer != address(0)) revert Marketplace__OrderSold();
        order.paymentToken = newPaymentToken_;
        order.price = newPrice_.toUint96();
        _orderDetails[collection_][orderId_] = order;
        emit UpdateOrder(collection_, orderId_, sender, newPaymentToken_, newPrice_);
    }

    function cancelOrder(address collection_, uint256 orderId_) external nonReentrant {
        address sender = _msgSender();
        Order memory order = _orderDetails[collection_][orderId_];
        if (order.seller != sender) revert Marketplace__NotOwnerOder();
        delete _orderDetails[collection_][orderId_];
        emit CancelOrder(collection_, orderId_, sender);
    }

    function buy(address collection_, uint256 orderId_) external payable nonReentrant {
        _buy(collection_, orderId_);
    }

    function acceptedCollection(
        address collection_,
        uint256 adminFee_,
        uint256 creatorFee_,
        uint256 businessFee_
    ) external onlyOwner {
        _acceptCollection(collection_, adminFee_, creatorFee_, businessFee_);
    }

    function modifyCollection(
        address collection_,
        uint256 adminFee_,
        uint256 creatorFee_,
        uint256 businessFee_
    ) external onlyOwner {
        _modifyCollection(collection_, adminFee_, creatorFee_, businessFee_);
    }

    function closeCollection(address collection_) external onlyOwner {
        _closeCollection(collection_);
    }

    function acceptPaymentTokens(address[] calldata addresses_) external onlyOwner {
        _acceptPaymentTokens(addresses_);
    }

    function cancelPaymentTokens(address[] calldata addresses_) external onlyOwner {
        _cancelPaymentTokens(addresses_);
    }

    function setMarketplaceAdmin(address admin_) external onlyOwner {
        _setMarketplaceAdmin(admin_);
    }

    function setLiquifier(address liquifier_) external onlyOwner {
        _setLiquifier(liquifier_);
    }

    function _setLiquifier(address liquifier_) internal {
        liquifier = liquifier_;
    }

    function _setMarketplaceAdmin(address admin_) internal {
        marketplaceAdmin = admin_;
    }

    function _buy(address collection_, uint256 orderId_) private {
        address sender = _msgSender();
        Order memory order = _orderDetails[collection_][orderId_];
        IERC721Upgradeable collection = IERC721Upgradeable(collection_);
        if (order.seller == sender) revert Marketplace__BuyOwn();
        if (order.price == 0) revert Marketplace__OrderNotListed();
        if (order.buyer != address(0)) revert Marketplace__OrderSold();
        if (!collection.isApprovedForAll(order.seller, address(this))) revert Marketplace__NotApprovedNFTs();

        (uint256 netPrice, uint256 adminFee, uint256 creatorFee, uint256 businessFee) = _calculatePriceAndFeesForCollection(collection_, uint256(order.price));

        (address creator, ) = IERC721Metadata(collection_).metadata(order.tokenIds[0]);

        if (order.paymentToken == address(0)) {
            if (msg.value < order.price) revert Marketplace__InsufficientBalance();
            __transfer(address(0), order.seller, netPrice);
            __transfer(address(0), marketplaceAdmin, adminFee);
            __transfer(address(0), creator, creatorFee);
            __transfer(address(0), liquifier, businessFee);
        } else {
            __transferFrom(order.paymentToken, sender, order.seller, netPrice);
            __transferFrom(order.paymentToken, sender, marketplaceAdmin, adminFee);
            __transferFrom(order.paymentToken, sender, creator, creatorFee);
            __transferFrom(order.paymentToken, sender, liquifier, businessFee);
        }

        uint256 length = order.tokenIds.length;
        for (uint256 i = 0; i < length; ) {
            collection.safeTransferFrom(order.seller, sender, order.tokenIds[i]);
            unchecked {
                ++i;
            }
        }
        order.buyer = sender;
        _orderDetails[collection_][orderId_] = order;

        emit Buy(collection_, orderId_, order.seller, sender, order.paymentToken, order.price, netPrice);
    }

    function viewOrderDetail(address collection_, uint256 orderId_) external view returns (Order memory) {
        return (_orderDetails[collection_][orderId_]);
    }

    function _authorizeUpgrade(address newImplementation) internal override onlyOwner {}
}

