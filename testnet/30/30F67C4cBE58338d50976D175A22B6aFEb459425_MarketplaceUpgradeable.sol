            IERC20Upgradeable(asset_).safeTransfer(to_, amount_);
        }
    }

    function __transferFrom(
        address asset_,
        address from_,
        address to_,
        uint256 amount_
    ) internal {
        if (amount_ == 0) return;
        IERC20Upgradeable(asset_).safeTransferFrom(from_, to_, amount_);
    }
}

