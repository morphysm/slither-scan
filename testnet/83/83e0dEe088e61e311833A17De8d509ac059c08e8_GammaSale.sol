pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "./Math.sol";
import "hardhat/console.sol";

struct PriceLog {
    uint32 timestamp;
    uint256 price;
}

contract GammaSale is Initializable {
    bool public paused;
    bool public buyEnabled;
    bool public sellEnabled;
    address public token;
    address public currency;
    uint32 public startTime;
    uint32 public endTime;
    uint32 public startWeight;
    uint32 public endWeight;
    uint32 public duration;
    uint32 public swapFee;
    uint32 public sellFee;
    uint32 public buyFee;
    uint256 public maxSwapAmount;
    uint256 public initialTokenAmount;
    uint256 public initialCurrencyAmount;
    mapping(address=>bool) blacklist;
    mapping(uint32 => uint32) private timeLogs;
    mapping(uint32 => uint256) private priceLogs;
    uint32 public logLength;
    address public owner;

    modifier onlyOwner() {
        require(msg.sender==owner, "Only owner can access.");
        _;
    }

    modifier checkSlippage(uint32 _slippage) {
        uint256 _priceBefore = 0;
        if(_slippage>0) _priceBefore = getTokenPrice();
        _;
        uint256 _priceAfter = getTokenPrice();
        if(_slippage>0) {
            uint256 _priceDelta = (_priceAfter > _priceBefore) ? (_priceAfter - _priceBefore) : (_priceBefore - _priceAfter);
            require(_priceDelta < _priceBefore * _slippage / 10000, "Overflow price slippage.");
        }
        timeLogs[logLength] = uint32(block.timestamp);
        priceLogs[logLength] = _priceAfter;
        logLength ++;
    }

    function initialize() public initializer {
        owner = msg.sender;
        duration = 7 days;
        startWeight = 9900;
        endWeight = 100;
        swapFee = 0;
        maxSwapAmount = 500 ether;
        paused = true;
    }

    receive() external payable {}

    function deny(address _account) public onlyOwner {
        blacklist[_account] = true;
    }

    function allow(address _account) public onlyOwner {
        if(blacklist[_account])
            blacklist[_account] = false;
    }

    function setPaused(bool _paused) public onlyOwner {
        require(block.timestamp>startTime, "Can pause after started.");
        paused = _paused;
    }

    function setSellEnabled(bool _sellEnabled) public onlyOwner {
        sellEnabled = _sellEnabled;
    }

    function bindToken(address _token) public onlyOwner {
        require(startTime==0 || block.timestamp<startTime, "Can set token before started.");
        token = _token;
    }

    function bindCurrency(address _token) public onlyOwner {
        require(startTime==0 || block.timestamp<startTime, "Can set currency before started.");
        currency = _token;
    }

    function setStartWeight(uint32 _weight) public onlyOwner {
        require(startTime==0 || block.timestamp<startTime, "Can set weight before started.");
        startWeight = _weight;
    }

    function setEndWeight(uint32 _weight) public onlyOwner {
        require(startTime==0 || block.timestamp<startTime, "Can set weight before started.");
        endWeight = _weight;
    }

    function setSwapFee(uint32 _fee) public onlyOwner {
        swapFee = _fee;
    }

    function setSwapFee(uint32 _sellFee, uint32 _buyFee) public onlyOwner {
        sellFee = _sellFee;
        buyFee = _buyFee;
    }

    function setMaxSwapAmount(uint256 _amount) public onlyOwner {
        maxSwapAmount = _amount;
    }

    function start(uint32 _startTime, uint32 _endTime) public onlyOwner {
        require(token!=address(0), "Have to bind Token first.");
        initialTokenAmount = IERC20(token).balanceOf(address(this));
        require(initialTokenAmount>0, "Insufficient Token balance.");
        
        if(currency==address(0)) {
            initialCurrencyAmount = address(this).balance;
        } else {
            initialCurrencyAmount = IERC20(currency).balanceOf(address(this));
        }
        require(initialCurrencyAmount>0, "Insufficient Currency balance.");
        
        startTime = _startTime==0?uint32(block.timestamp):_startTime;
        require(block.timestamp<=startTime, "Cannot start.");
        if(_endTime==0)
            endTime = startTime + duration;
        else {
            require(endTime>startTime, "End time must be later than start time.");
            endTime = _endTime;
            duration = endTime - startTime;
        }
        paused = false;
        sellEnabled = true;
        buyEnabled = true;
    }

    function getReserves() public view returns (uint256, uint256) {
        return (reserveToken(), reserveCurrency());
    }

    function reserveToken() public view returns (uint256) {
        return IERC20(token).balanceOf(address(this));
    }

    function reserveCurrency() public view returns (uint256) {
        if(currency==address(0))
            return address(this).balance;
        return IERC20(token).balanceOf(address(this));
    }

    /**********************************************************************************************
    // calcSpotPrice                                                                             //
    // sP = spotPrice                                                                            //
    // bI = tokenBalanceIn                ( bI / wI )         1                                  //
    // bO = tokenBalanceOut         sP =  -----------  *  ----------                             //
    // wI = tokenWeightIn                 ( bO / wO )     ( 1 - sF )                             //
    // wO = tokenWeightOut                                                                       //
    // sF = swapFee                                                                              //
    **********************************************************************************************/
    function calcSpotPrice(
        uint _tokenBalanceIn,
        uint _tokenWeightIn,
        uint _tokenBalanceOut,
        uint _tokenWeightOut,
        uint _swapFee
    ) public pure returns (uint) {
        uint numer = Math.bdiv(_tokenBalanceIn, _tokenWeightIn);
        uint denom = Math.bdiv(_tokenBalanceOut, _tokenWeightOut);
        uint ratio = Math.bdiv(numer, denom);
        uint scale = Math.bdiv(Math.BONE, Math.bsub(Math.BONE, _swapFee));
        return Math.bmul(ratio, scale);
    }

    /**********************************************************************************************
    // calcOutGivenIn                                                                            //
    // aO = tokenAmountOut                                                                       //
    // bO = tokenBalanceOut                                                                      //
    // bI = tokenBalanceIn              /      /            bI             \    (wI / wO) \      //
    // aI = tokenAmountIn    aO = bO * |  1 - | --------------------------  | ^            |     //
    // wI = tokenWeightIn               \      \ ( bI + ( aI * ( 1 - sF )) /              /      //
    // wO = tokenWeightOut                                                                       //
    // sF = swapFee                                                                              //
    **********************************************************************************************/
    function calcOutGivenIn(
        uint _tokenBalanceIn,
        uint _tokenWeightIn,
        uint _tokenBalanceOut,
        uint _tokenWeightOut,
        uint _tokenAmountIn,
        uint _swapFee
    ) public pure returns (uint) {
        uint weightRatio = Math.bdiv(_tokenWeightIn, _tokenWeightOut);
        uint adjustedIn = Math.bsub(Math.BONE, _swapFee);
        adjustedIn = Math.bmul(_tokenAmountIn, adjustedIn);
        uint y = Math.bdiv(_tokenBalanceIn, Math.badd(_tokenBalanceIn, adjustedIn));
        uint foo = Math.bpow(y, weightRatio);
        uint bar = Math.bsub(Math.BONE, foo);
        return Math.bmul(_tokenBalanceOut, bar);
    }

    /**********************************************************************************************
    // calcInGivenOut                                                                            //
    // aI = tokenAmountIn                                                                        //
    // bO = tokenBalanceOut               /  /     bO      \    (wO / wI)      \                 //
    // bI = tokenBalanceIn          bI * |  | ------------  | ^            - 1  |                //
    // aO = tokenAmountOut    aI =        \  \ ( bO - aO ) /                   /                 //
    // wI = tokenWeightIn           --------------------------------------------                 //
    // wO = tokenWeightOut                          ( 1 - sF )                                   //
    // sF = swapFee                                                                              //
    **********************************************************************************************/
    function calcInGivenOut(
        uint _tokenBalanceIn,
        uint _tokenWeightIn,
        uint _tokenBalanceOut,
        uint _tokenWeightOut,
        uint _tokenAmountOut,
        uint _swapFee
    ) public pure returns (uint) {
        uint weightRatio = Math.bdiv(_tokenWeightOut, _tokenWeightIn);
        uint diff = Math.bsub(_tokenBalanceOut, _tokenAmountOut);
        uint y = Math.bdiv(_tokenBalanceOut, diff);
        uint foo = Math.bpow(y, weightRatio);
        foo = Math.bsub(foo, Math.BONE);
        uint tokenAmountIn = Math.bsub(Math.BONE, _swapFee);
        tokenAmountIn = Math.bdiv(Math.bmul(_tokenBalanceIn, foo), tokenAmountIn);
        return tokenAmountIn;
    }

    function _getTokenWeight() private view returns (uint256) {
        if(startWeight > endWeight){
            if(block.timestamp >= endTime)
                return Math.BONE * endWeight / 10000;
            else 
                return Math.BONE * startWeight / 10000 - Math.BONE * (startWeight - endWeight) * (block.timestamp - startTime) / duration / 10000;
        }
        return Math.BONE * startWeight / 10000 + Math.BONE * (endWeight - startWeight) * (block.timestamp - startTime) / duration / 10000;
    }

    function _getCurrencyWeight() private view returns (uint256) {
        return Math.BONE - _getTokenWeight();   
    }
    
    function getCurrencyIn(uint256 _amount) public view returns (uint256) {
        (uint256 tokenBalanceOut, uint256 tokenBalanceIn) = getReserves();
        uint256 tokenWeightOut = _getTokenWeight();
        uint256 tokenWeightIn = _getCurrencyWeight();
        uint256 fee = Math.BONE * (buyFee==0?swapFee:buyFee) / 10000;
        return calcInGivenOut(tokenBalanceIn, tokenWeightIn, tokenBalanceOut, tokenWeightOut, _amount, fee);
    }

    function getCurrencyOut(uint256 _amount) public view returns (uint256) {
        (uint256 tokenBalanceIn, uint256 tokenBalanceOut) = getReserves();
        uint256 tokenWeightIn = _getTokenWeight();
        uint256 tokenWeightOut = _getCurrencyWeight();
        uint256 fee = Math.BONE * (sellFee==0?swapFee:sellFee) / 10000;
        return calcOutGivenIn(tokenBalanceIn, tokenWeightIn, tokenBalanceOut, tokenWeightOut, _amount, fee);
    }

    function getTokenIn(uint256 _amount) public view returns (uint256) {
        (uint256 tokenBalanceIn, uint256 tokenBalanceOut) = getReserves();
        uint256 tokenWeightIn = _getTokenWeight();
        uint256 tokenWeightOut = _getCurrencyWeight();
        uint256 fee = Math.BONE * (sellFee==0?swapFee:sellFee) / 10000;
        return calcInGivenOut(tokenBalanceIn, tokenWeightIn, tokenBalanceOut, tokenWeightOut, _amount, fee);
    }

    function getTokenOut(uint256 _amount) public view returns (uint256) {
        (uint256 tokenBalanceOut, uint256 tokenBalanceIn) = getReserves();
        uint256 tokenWeightOut = _getTokenWeight();
        uint256 tokenWeightIn = _getCurrencyWeight();
        uint256 fee = Math.BONE * (buyFee==0?swapFee:buyFee) / 10000;
        return calcOutGivenIn(tokenBalanceIn, tokenWeightIn, tokenBalanceOut, tokenWeightOut, _amount, fee);
    }

    function getTokenPrice() public view returns (uint256) {
        (uint256 tokenBalanceOut, uint256 tokenBalanceIn) = getReserves();
        uint256 tokenWeightOut = _getTokenWeight();
        uint256 tokenWeightIn = _getCurrencyWeight();
        uint256 fee = Math.BONE * (buyFee==0?swapFee:buyFee) / 10000;
        
        return calcSpotPrice(tokenBalanceIn, tokenWeightIn, tokenBalanceOut, tokenWeightOut, fee);
    }

    function getPriceLog() public view returns (PriceLog[] memory) {
        uint32 interval = 180;
        uint32 lastTime = 0;
        uint32 j = 0;
        for (uint32 i = 0; i < logLength; i++) {
            if(lastTime+interval>timeLogs[i]) continue;
            j++;
            lastTime = timeLogs[i];
        }
        PriceLog[] memory logs = new PriceLog[](j);
        j = 0;
        lastTime = 0;
        for (uint32 i = 0; i < logLength; i++) {
            if(lastTime+interval>timeLogs[i]) continue;
            logs[j].timestamp = timeLogs[i];
            logs[j].price = priceLogs[i];
            j++;
            lastTime = timeLogs[i];
        } 
        return logs;
    }

    function buyForExactToken(uint256 _amountToken, uint256 _maxCurrency, uint32 _slippage) public checkSlippage(_slippage) payable {
        require(blacklist[msg.sender] == false, "Not allowed!");
        require(!paused, "Public sale is paused.");
        require(buyEnabled, "Disabled to buy token.");
        require(startTime<=block.timestamp, "Public sale is not started.");
        require(endTime>=block.timestamp, "Public sale is finished.");
        require(_amountToken<=maxSwapAmount, "Cannot buy more than Max.");
        if(currency==address(0)) require(_maxCurrency==msg.value, "Invalid Currency value.");
        uint256 _amountCurrency = getCurrencyIn(_amountToken);
        require(_amountCurrency<=_maxCurrency, "Insufficient currency.");
        IERC20(token).transfer(msg.sender, _amountToken);
        if(currency!=address(0))
            IERC20(currency).transferFrom(msg.sender, address(this), _amountCurrency);
        else if(_maxCurrency > _amountCurrency) 
            payable(msg.sender).transfer(_maxCurrency - _amountCurrency);
    }

    function buyForExactCurrency(uint256 _amountCurrency, uint256 _minToken, uint32 _slippage) public checkSlippage(_slippage) payable {
        require(blacklist[msg.sender] == false, "Not allowed!");
        require(!paused, "Public sale is paused.");
        require(buyEnabled, "Disabled to buy token.");
        require(startTime<=block.timestamp, "Public sale is not started.");
        require(endTime>=block.timestamp, "Public sale is finished.");
        if(currency==address(0)) require(_amountCurrency==msg.value, "Invalid Currency value.");
        uint256 _amountToken = getTokenOut(_amountCurrency);
        require(_amountToken<=maxSwapAmount, "Cannot buy more than Max.");
        require(_amountToken>=_minToken, "Insufficient token out.");
        IERC20(token).transfer(msg.sender, _amountToken);
        if(currency!=address(0))
            IERC20(currency).transferFrom(msg.sender, address(this), _amountCurrency);
    }

    function sellForExactToken(uint256 _amountToken, uint256 _minCurrency, uint32 _slippage) public checkSlippage(_slippage) {
        require(blacklist[msg.sender] == false, "Not allowed!");
        require(!paused, "Public sale is paused.");
        require(sellEnabled, "Disabled to sell token.");
        require(startTime<=block.timestamp, "Public sale is not started.");
        require(endTime>=block.timestamp, "Public sale is finished.");
        require(_amountToken<=maxSwapAmount, "Cannot sell more than Max.");
        uint256 _amountCurrency = getCurrencyOut(_amountToken);
        require(_amountCurrency>=_minCurrency, "Insufficient currency out.");
        IERC20(token).transferFrom(msg.sender, address(this), _amountToken);
        if(currency==address(0))
            payable(msg.sender).transfer(_amountCurrency);
        else
            IERC20(currency).transfer(msg.sender, _amountCurrency);
    }

    function sellForExactCurrency(uint256 _amountCurrency, uint256 _maxToken, uint32 _slippage) public checkSlippage(_slippage) {
        require(blacklist[msg.sender] == false, "Not allowed!");
        require(!paused, "Public sale is paused.");
        require(sellEnabled, "Disabled to sell token.");
        require(startTime<=block.timestamp, "Public sale is not started.");
        require(endTime>=block.timestamp, "Public sale is finished.");
        uint256 _amountToken = getTokenIn(_amountCurrency);
        require(_amountToken<=maxSwapAmount, "Cannot sell more than Max.");
        require(_amountToken<=_maxToken, "Invalid token out.");
        IERC20(token).transferFrom(msg.sender, address(this), _amountToken);
        if(currency==address(0))
            payable(msg.sender).transfer(_amountCurrency);
        else
            IERC20(currency).transfer(msg.sender, _amountCurrency);
    }
}

