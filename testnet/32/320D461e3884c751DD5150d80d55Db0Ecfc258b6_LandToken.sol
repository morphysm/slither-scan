pragma solidity 0.7.5;

contract VaultOwned is Ownable {
    
    EnumerableSet.AddressSet internal _callers;

    function addCaller(address _newCaller) public onlyManager() returns (bool) {
        require(_newCaller != address(0), "VaultOwned: address is zero");
        return EnumerableSet.add(_callers, _newCaller);
    }

    function delCaller(address _delCaller) public onlyManager() returns (bool) {
        require(_delCaller != address(0), "VaultOwned: address is zero");
        return EnumerableSet.remove(_callers, _delCaller);
    }

    function isCaller(address _caller) public view returns (bool) {
        return EnumerableSet.contains(_callers, _caller);
    }

    function getCaller(uint256 _index) public view returns (address) {
        require(_index <= getCallerLength() - 1, "VaultOwned: index out of bounds");
        return EnumerableSet.at(_callers, _index);
    }

    function getCallerLength() public view returns (uint256) {
        return EnumerableSet.length(_callers);
    }

    /**
    * @dev Throws if called by any account other than the vault.
    */
    modifier onlyVault() {
        require(isCaller(msg.sender), "CowStaking: not the caller");
        _;
    }

}

