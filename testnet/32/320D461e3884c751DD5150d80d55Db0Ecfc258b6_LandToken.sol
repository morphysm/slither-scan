contract LandToken is ERC20Permit, VaultOwned {

    using SafeMath for uint256;

    uint256 public constant maxSupply = 3e26;

    constructor() ERC20("AVAX Land", "LAND", 18) {
    }

    function mint(address account_, uint256 amount_) external onlyVault() {
        require(_totalSupply.add(amount_) <= maxSupply,"LnadToken: exceeds the maxSupply");
        _mint(account_, amount_);
    }

    /**
     * @dev Destroys `amount` tokens from the caller.
     *
     * See {ERC20-_burn}.
