	/// -----------------------------------------------------------------------
	/// Delegator Storage
	/// -----------------------------------------------------------------------

	uint256 constant TOKEN = 1;
	uint256 constant DELEGATE_TOKEN = 2;

	bool public paused;

	bytes32 public constant DELEGATION_TYPEHASH =
		keccak256('Delegation(address delegatee,uint256 nonce,uint256 deadline)');

	mapping(address => mapping(address => address)) internal _delegates;

	mapping(address => mapping(address => mapping(uint256 => Checkpoint))) public checkpoints;

	mapping(address => mapping(address => uint256)) public numCheckpoints;

	struct Checkpoint {
		uint32 fromTimestamp;
		uint96 votes;
	}

	/// -----------------------------------------------------------------------
	/// Delegation Settings
	/// -----------------------------------------------------------------------

	function setExtension(bytes calldata extensionData) external {
		console.logString('in EXT - set');

		(IRoundtableDAO dao, address[] memory members) = abi.decode(
			extensionData,
			(IRoundtableDAO, address[])
		);

		if (msg.sender != address(dao)) revert Forbidden();

		uint256 len = members.length;

		for (uint256 i; i < len; ) {
			// If member does not hold MEMBERSHIP token
			if (dao.balanceOf(members[i], 0) == 0) revert Forbidden();

			// If not all members are included in the setup (reverts in cases where a new member is added after delegation is proposed)
			if (dao.memberCount() != len) revert MembersMissing();

			// If the extension is already set, balances should be updated in the callExtension function
			if (
				checkpoints[address(dao)][members[i]][numCheckpoints[address(dao)][members[i]]].votes != 0
			) revert AlreadySet();

			_moveDelegates(address(dao), address(0), members[i], dao.balanceOf(members[i], TOKEN));
			unchecked {
				++i;
			}
		}

		emit ExtensionSet(address(dao), members);
	}

	/// -----------------------------------------------------------------------
	/// Delegator Functions
	/// -----------------------------------------------------------------------

	function callExtension(
		address sender,
		uint256 votesIn,
		bytes calldata extensionData
	) external nonReentrant returns (bool mint, uint256 amount) {
		(
			IRoundtableDAO dao,
			address from,
			address to,
			uint256 votes,
			uint256 typeOfCall,
			uint32 timestamp
		) = abi.decode(extensionData, (IRoundtableDAO, address, address, uint256, uint256, uint32));

		// 0) Get votes
		if (typeOfCall == 0) {
			mint = false;
			amount = getPriorVotes(address(dao), from, timestamp);
			// amount = getCurrentVotes(address(dao), from);
		}

		// 1) update votes
		if (typeOfCall == 1) {
			if (msg.sender != address(dao) && msg.sender != from) revert Forbidden();

			if (dao.balanceOf(to, 0) == 0) revert Forbidden();

			_moveDelegates(address(dao), from, to, votes);
		}

		// 2) get reclaim votes
		// TODO consider need for this if transfer is used. In this cases typeOfCall = 1 is enough to transfer back shares

		emit ExtensionCalled(address(dao), from, to, amount, typeOfCall);
	}

	function delegates(address dao, address delegator) public view virtual returns (address) {
		address current = _delegates[dao][delegator];

		return current == address(0) ? delegator : current;
	}

	function getCurrentVotes(address dao, address account) public view virtual returns (uint256) {
		// this is safe from underflow because decrement only occurs if `nCheckpoints` is positive
		unchecked {
			uint256 nCheckpoints = numCheckpoints[dao][account];

			return nCheckpoints != 0 ? checkpoints[dao][account][nCheckpoints - 1].votes : 0;
		}
	}

	function delegate(address dao, address delegatee) public payable virtual {
		_delegate(dao, msg.sender, delegatee);
	}

