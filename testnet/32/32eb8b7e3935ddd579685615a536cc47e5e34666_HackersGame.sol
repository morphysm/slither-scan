    // Hacker - 1, Cube - 2
    function payFee(uint _cubeOrHacker, uint _Id) external payable { 
        require(_cubeOrHacker <= 2 && _cubeOrHacker > 0,"invalid NFT type");
        require(msg.value >= fee,"not enough AVAX");
        address payable _to = payable(0x5B38Da6a701c568545dCfcB03FcB875f56beddC4);
        _to.transfer(msg.value);
        if (_cubeOrHacker == 1){
            
            if (hackerFeeTimePaid[_Id] > block.timestamp - 30 days){
                hackerFeeTimePaid[_Id] += 30 days;
            }
            hackerFeeTimePaid[_Id] = block.timestamp;
        }
        if (_cubeOrHacker == 2){
            
            if (cubeFeeTimePaid[_Id] > block.timestamp - 30 days){
                cubeFeeTimePaid[_Id] += 30 days;
            }
            cubeFeeTimePaid[_Id] = block.timestamp;
        }

        
    }

    function lendCube(uint _CubeNFT, uint _price) external{
        uint _Id = stakedCubeId[_CubeNFT];
        require(cubeFeeTimePaid[_CubeNFT] > block.timestamp - 30 days,"unpaid fee");
        require(cube[_Id].transferable == true, "not transferable");
        require(cube[_Id].owner == msg.sender,"not owner");
        cube[_Id].transferable = false;
        cubeRentAble[_CubeNFT] = true;
        cubeRentPrice[_CubeNFT] = _price * 10**18;
    }

    function rentCube(uint _cubeNFTId, uint _hackerNFTId,uint _cubeTimeInDays) external payable{
        uint _hackerId = stakedHackerId[_hackerNFTId];
        uint _cubeId = stakedCubeId[_cubeNFTId];
        require(hacker[_hackerId].cube == 0);
        require(cubeRentAble[_cubeNFTId] == true);
        require(hacker[_hackerId].owner == msg.sender);
        HACKaddress.transferFrom(msg.sender,cube[_cubeId].owner, cubeRentPrice[_cubeNFTId] * _cubeTimeInDays);
        hacker[_hackerId].cube = _cubeNFTId;
        cubeRented[_cubeNFTId] = true;
        cubeRentAble[_cubeNFTId] = false;
        cubeRentedWith[_cubeNFTId] = _hackerNFTId;
        cubeRentedUntil[_cubeNFTId] = block.timestamp + _cubeTimeInDays * 1 days;
        cubeRenter[_cubeNFTId] = msg.sender;

    }

    function cancelCubeLending(uint _cubeId) public {
        require(cubeRentedUntil[_cubeId] < block.timestamp,"Still rented");
        cubeRentAble[_cubeId] = false;
        cubeRented[_cubeId] = false;
        cubeRentPrice[_cubeId] = 0;
        cubeRentedUntil[_cubeId] = 0;
        hacker[cubeRentedWith[_cubeId]].cube = 0;

    }
}