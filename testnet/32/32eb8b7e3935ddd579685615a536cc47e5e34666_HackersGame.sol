            _cube_levels[cube[_cubeId].ID][cubePart]++;
            DynamicCUBE(address(cubeNFT0)).levelUpCube(_cubeNFTId,_cube_levels[_cubeNFTId][1],_cube_levels[_cubeNFTId][2],_cube_levels[_cubeNFTId][3],_cube_levels[_cubeNFTId][4],_cube_levels[_cubeNFTId][5]);
            return 2;
        } return 3;
