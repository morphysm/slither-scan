pragma solidity ^0.8.7;

import "@uniswap/v2-core/contracts/interfaces/IUniswapV2Factory.sol";
import "@uniswap/v2-periphery/contracts/interfaces/IUniswapV2Router02.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts-upgradeable/proxy/utils/UUPSUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/token/ERC721/utils/ERC721HolderUpgradeable.sol";
import "./interfaces/INode.sol";
import "./interfaces/IERC20.sol";

contract StakeNode is Initializable, OwnableUpgradeable, ERC721HolderUpgradeable ,UUPSUpgradeable{

    INode private _node;
    IERC20 private _tokenCep;
    IERC20 private _tokenBusd;
    IUniswapV2Router02 private _pangolineV2Router;

    address private _admin;
    address private _rewardWallet;

    uint256 private _rigelSold;
    uint256 private _siriusSold;
    uint256 private _vegaSold;

    uint256 private _rigelLockPeriod;
    uint256 private _siriusLockPeriod; 
    uint256 private _vegaLockPeriod;

    uint256 private _tax;

     // Starting index of the tier of token;
    uint256[] private _tierStartIndex;

    uint256[] private _totalNFT; 

    struct RewardFee{
        uint256 rigel;
        uint256 sirius;
        uint256 vega;
    }

    RewardFee private _rewardFee;

    struct BuyFee{
        uint256 rigel;
        uint256 sirius;
        uint256 vega;
    }

    BuyFee private _buyFee;

    struct RewardClaimed{
        uint256 rigel;
        uint256 sirius;
        uint256 vega;
    }

    mapping(address => RewardClaimed) private _rewardClaimed;

    mapping(uint256 => address) private _nftToOwner;

    mapping(address => uint256[]) private _allRigelNodeOfOwner;

    mapping(address => uint256[]) private _allSiriusNodeOfOwner;

    mapping(address => uint256[]) private _allVegaNodeOfOwner;

    mapping(uint256 => uint256) private _NFTLockPeriod;

    mapping(uint256 => uint256) private _stakeTime;

    mapping(uint256 => uint256) private _lastClaimedAt;

    //  address _NFTContract,
    //     address _cepToken,
    //     address _busdToken,
    //     address _pangolinRouter,
    //     address _rewardAddr,
   
    function initialize(
        address[5] memory _addr,
        uint256 tax_,
        uint256 _rigelReward,
        uint256 _siriusReward,
        uint256 _vegaReward,
        uint256 _rigelPrice,
        uint256 _siriusPrice,
        uint256 _vegaPrice
    ) initializer public {

        __Ownable_init();
        __UUPSUpgradeable_init();

        _node = INode(_addr[0]);
        _tokenCep = IERC20(_addr[1]);
        _tokenBusd = IERC20(_addr[2]);
        _pangolineV2Router = IUniswapV2Router02(_addr[3]);

        _rewardWallet = _addr[4]; 
        
        _rewardFee.rigel = _rigelReward;
        _rewardFee.sirius = _siriusReward;
        _rewardFee.vega = _vegaReward;

        _tax = tax_;

        _setIndex();        

        _buyFee.rigel = _rigelPrice;
        _buyFee.sirius = _siriusPrice;
        _buyFee.vega = _vegaPrice;

       

        _admin = _msgSender();

    }   

    function _setIndex() internal {
        _tierStartIndex.push(0);
        _tierStartIndex.push(50);
        _tierStartIndex.push(100);

        _totalNFT.push(50);
        _totalNFT.push(50);
        _totalNFT.push(50);

         _rigelLockPeriod = 90 days;
         _siriusLockPeriod = 60 days; 
        _vegaLockPeriod = 30 days;
    }

    function getRigelNodes(address _address) external view returns(uint256[] memory) {
        return _allRigelNodeOfOwner[_address];
    }

    function getSiriusNodes(address _address) external view returns(uint256[] memory) {
        return _allSiriusNodeOfOwner[_address];
    }

    function getVegaNodes(address _address) external view returns(uint256[] memory) {
        return _allVegaNodeOfOwner[_address];
    }

    function getTotalRigelNode() external view returns(uint256){
        return _totalNFT[0];
    }

    function getTotalSiriusNode() external view returns(uint256){
        return _totalNFT[1];
    }

    function getTotalVegalNode() external view returns(uint256){
        return _totalNFT[2];
    }

    function getNode() external view returns(address){
        return address(_node);
    }

    function getCepheusToken() external view returns(address){
        return address(_tokenCep);
    }

    function getBusdToken() external view returns(address){
        return address(_tokenBusd);
    }
    
    function getPangolineRouter() external view returns(address){
        return address(_pangolineV2Router);
    }
    
    function getAdmin() external view returns(address){
        return address(_admin);
    }

    function getRigelSold() external view returns(uint256){
        return _rigelSold;
    }

    function getSiriusSold() external view returns(uint256){
        return _siriusSold;
    }

    function getVegaSold() external view returns(uint256){
        return _vegaSold;
    }

    function getRigelLockPeriod() external view returns(uint256){
        return _rigelLockPeriod;
    }

    function getSiriusLockPeriod() external view returns(uint256){
        return _siriusLockPeriod;
    }

    function getVegaLockPeriod() external view returns(uint256){
        return _vegaLockPeriod;
    }

    function getTax() external view returns(uint256){
        return _tax;
    }

    function getRigelRewardFee() external view returns(uint256){
        return _rewardFee.rigel;
    }
    
    function getSiriusRewardFee() external view returns(uint256){
        return _rewardFee.sirius;
    }

    function getVegaRewardFee() external view returns(uint256){
        return _rewardFee.vega;
    }

    function getRigelPrice() external view returns(uint256){
        return _buyFee.rigel;
    }

    function getSiriusPrice() external view returns(uint256){
        return _buyFee.sirius;
    }

    function getVegaPrice() external view returns(uint256){
        return _buyFee.vega;
    }

    function getRigelRewardClaimed(address _address) external view returns(uint256){
        return _rewardClaimed[_address].rigel;
    }

    function getSiriusRewardClaimed(address _address) external view returns(uint256){
        return _rewardClaimed[_address].sirius;
    }

    function getRigelVegaClaimed(address _address) external view returns(uint256){
        return _rewardClaimed[_address].vega;
    }

    function getAllRigelNode(address _address) external view returns(uint256[] memory){
        return _allRigelNodeOfOwner[_address];
    }

    function getAllSiriusNode(address _address) external view returns(uint256[] memory){
        return _allSiriusNodeOfOwner[_address];
    }

    function getAllVegaNode(address _address) external view returns(uint256[] memory){
        return _allVegaNodeOfOwner[_address];
    }

    function getNFTLockPeriod(uint256 _tokenID) external view returns(uint256){
        return _NFTLockPeriod[_tokenID];
    }

    function getStakeTime(uint256 _tokenID) external view returns(uint256){
        return _stakeTime[_tokenID];
    }

    function getLastClaimedAt(uint256 _tokenID) external view returns(uint256){
        return _lastClaimedAt[_tokenID];
    }

   
    function createNode(uint256 _amount, string memory _type) external {
        uint256 _tokenID;
        if(
            keccak256(abi.encodePacked(_type)) == 
            keccak256(abi.encodePacked('Rigel'))
            ) {
            require(_amount == _buyFee.rigel,
            "Value invalid");

            _tierStartIndex[0]++;

            require(_rigelSold <= _totalNFT[0], "This type is sold out");

            _tokenID = _tierStartIndex[0];
            
        }

        if(
            keccak256(abi.encodePacked(_type)) == 
            keccak256(abi.encodePacked('Sirius'))
            ) {
            require(_amount == _buyFee.sirius,
            "Value invalid");

             _tierStartIndex[1]++;

            require(_siriusSold <= _totalNFT[1], "This type is sold out");

            _tokenID = _tierStartIndex[1];
        }

        if(
            keccak256(abi.encodePacked(_type)) == 
            keccak256(abi.encodePacked('Vega'))
            ) {
            require(_amount == _buyFee.vega,
            "Value invalid");

             _tierStartIndex[2]++;

            require(_vegaSold <= _totalNFT[2], "This type is sold out");

            _tokenID = _tierStartIndex[2];
        }

        uint256 _taxAmount = (_amount * _tax)/100;
        uint256 _remainAmount = _amount - _taxAmount;

        _tokenBusd.transferFrom(_msgSender(), _admin, _remainAmount);
        // _buyBack(_taxAmount);
        _node.safeTransferFrom(_admin, _msgSender(), _tokenID);
        
    }
   
    function claimReward(uint256 _tokenID) public {
        uint256 _reward = getReward(_tokenID);
        string memory _nftType = _node.getNodeType(_tokenID);
        _tokenCep.transferFrom(_admin,_msgSender(), _reward);
        _lastClaimedAt[_tokenID] = block.timestamp;

        if(
            keccak256(abi.encodePacked(_nftType)) == 
            keccak256(abi.encodePacked('Rigel'))
            ) {

                _rewardClaimed[_msgSender()].rigel += _reward;
        }

        if(
            keccak256(abi.encodePacked(_nftType)) == 
            keccak256(abi.encodePacked('Sirius'))
            ) {

               _rewardClaimed[_msgSender()].sirius += _reward;
        }

        if(
            keccak256(abi.encodePacked(_nftType)) == 
            keccak256(abi.encodePacked('Vega'))
            ) {

                _rewardClaimed[_msgSender()].vega += _reward;
        }
    }

    function getReward(uint256 _tokenID) public view returns(uint256){

        string memory _nftType = _node.getNodeType(_tokenID);
        uint256 _rewardPerSecond;

        if(
            keccak256(abi.encodePacked(_nftType)) == 
            keccak256(abi.encodePacked('Rigel'))
            ) {

                _rewardPerSecond = _rewardFee.rigel / 1 days;
        }

        if(
            keccak256(abi.encodePacked(_nftType)) == 
            keccak256(abi.encodePacked('Sirius'))
            ) {

                _rewardPerSecond = _rewardFee.sirius / 1 days;
        }

        if(
            keccak256(abi.encodePacked(_nftType)) == 
            keccak256(abi.encodePacked('Vega'))
            ) {

                _rewardPerSecond = _rewardFee.vega / 1 days;
        }
               
        if(_lastClaimedAt[_tokenID] > 0){
            return (block.timestamp - _lastClaimedAt[_tokenID]) * _rewardPerSecond;
        
        } else {

            if(_stakeTime[_tokenID] > 0){
                
                return (block.timestamp - _stakeTime[_tokenID]) * _rewardPerSecond;

            }        
        }

        return 0;
        
    }
    
    function stake(uint256 _tokenID) public {
       
        string memory _nftType = _node.getNodeType(_tokenID);

        _node.safeTransferFrom(_msgSender(), address(this), _tokenID, "");
        _nftToOwner[_tokenID] = _msgSender();
        _stakeTime[_tokenID] = block.timestamp;

        if(
            keccak256(abi.encodePacked(_nftType)) == 
            keccak256(abi.encodePacked('Rigel'))
            ) {

                _NFTLockPeriod[_tokenID] = block.timestamp + _rigelLockPeriod;
                _allRigelNodeOfOwner[_msgSender()].push(_tokenID);
        }

        if(
            keccak256(abi.encodePacked(_nftType)) == 
            keccak256(abi.encodePacked('Sirius'))
            ) {

                _NFTLockPeriod[_tokenID] = block.timestamp + _siriusLockPeriod;
                _allSiriusNodeOfOwner[_msgSender()].push(_tokenID);
        }

        if(
            keccak256(abi.encodePacked(_nftType)) == 
            keccak256(abi.encodePacked('Vega'))
            ) {

                _NFTLockPeriod[_tokenID] = block.timestamp + _vegaLockPeriod;
                _allVegaNodeOfOwner[_msgSender()].push(_tokenID);
        }
    }

    function unStake(uint256 _tokenID) external{

        require(block.timestamp > _NFTLockPeriod[_tokenID], "Your NFT is locked");
        require(_msgSender() == _nftToOwner[_tokenID], "You are not the owner of the NFT");

        _node.safeTransferFrom(address(this), _msgSender(), _tokenID, "");

        string memory _nftType = _node.getNodeType(_tokenID);

        claimReward(_tokenID);
        delete _lastClaimedAt[_tokenID];
        delete _stakeTime[_tokenID];
        delete _NFTLockPeriod[_tokenID];
        delete _nftToOwner[_tokenID];

        uint256 _rigelLength = _allRigelNodeOfOwner[_msgSender()].length;
        uint256 _siriusLength = _allSiriusNodeOfOwner[_msgSender()].length;
        uint256 _vegaLength = _allVegaNodeOfOwner[_msgSender()].length;


        if(
            keccak256(abi.encodePacked(_nftType)) == 
            keccak256(abi.encodePacked('Rigel'))
            ) 
            {
                for(uint i; i <  _rigelLength; i++){
                    if(_allRigelNodeOfOwner[_msgSender()][i] == _tokenID ){
                        _allRigelNodeOfOwner[_msgSender()][i] = _rigelLength;
                        _allRigelNodeOfOwner[_msgSender()].pop();
                    }
                }               
            }

        if(
            keccak256(abi.encodePacked(_nftType)) == 
            keccak256(abi.encodePacked('Sirius'))
            ) 
            {
                for(uint i; i <  _siriusLength; i++){
                    if(_allSiriusNodeOfOwner[_msgSender()][i] == _tokenID ){
                        _allSiriusNodeOfOwner[_msgSender()][i] = _siriusLength;
                        _allSiriusNodeOfOwner[_msgSender()].pop();
                    }
                }               
            }

        if(
            keccak256(abi.encodePacked(_nftType)) == 
            keccak256(abi.encodePacked('Vega'))
            ) 
            {
                for(uint i; i <  _vegaLength; i++){
                    if(_allVegaNodeOfOwner[_msgSender()][i] == _tokenID ){
                        _allVegaNodeOfOwner[_msgSender()][i] = _vegaLength;
                        _allVegaNodeOfOwner[_msgSender()].pop();
                    }
                }               
            }


        
    }
   
    function _buyBack(uint256 _amount) internal {

        address[] memory path = new address[](2);
        path[0] = address(_tokenCep); 
        path[1] = address(_tokenBusd); 

        _pangolineV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens
       (
            _amount,
            0,
            path,
            address(this),
            block.timestamp + 10
        );

        _tokenCep.burn(_node.balanceOf(address(this)));
    }

    function _authorizeUpgrade(address newImplementation)
        internal
        onlyOwner
        override
    {}

    function setNode(address _address) external onlyOwner(){
        _node = INode(_address);
    }

    function setTokenCep(address _address) external onlyOwner() {
        _tokenCep = IERC20(_address);
    }

    function setTokenBUSD(address _address) external onlyOwner() {
        _tokenBusd = IERC20(_address);
    }

    function setPangoline(address _address) external onlyOwner() {
        _pangolineV2Router = IUniswapV2Router02(_address);
    }

    function setAdmin(address _address) external onlyOwner(){
        _admin = _address;
    }

    function setRewardWallet(address _address) external onlyOwner() {
        _rewardWallet = _address;
    }

    function setRigelLockPeriod(uint256 _value) external onlyOwner() {
        _rigelLockPeriod = _value;
    }

    function setSiriusLockPeriod(uint256 _value) external onlyOwner() {
        _siriusLockPeriod = _value;
    }

    function setVegaLockPeriod(uint256 _value) external onlyOwner() {
        _vegaLockPeriod = _value;
    }

    function setTax(uint256 _value) external onlyOwner(){
        _tax = _value;
    }

     
}

