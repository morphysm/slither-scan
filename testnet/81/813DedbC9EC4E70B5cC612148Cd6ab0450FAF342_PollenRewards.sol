pragma solidity ^0.8.4;

import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/utils/math/SafeMath.sol";
import "@openzeppelin/contracts/security/Pausable.sol";

import "./RhizomERC721.sol";
import "./PollenRewardToken.sol";

contract PollenRewards is ReentrancyGuard, Ownable, Pausable {
    using SafeMath for uint256;

    // PPS = Pollen per second
    // PPM = Pollen per minute

    // Constants
    uint256 public constant YIELD_PPS = 16666666666666667; // 1 Pollen per minute
    uint256 public constant REWARD_FACTOR = 1;

    uint256 public startTime;

    // Staking

    struct StakedNFT {
        address owner;
        uint256 tokenId;
        uint256 startTimestamp;
        bool staked;
    }

    struct StakedNFTInfo {
        uint256 tokenId;
        uint256 nftPPM;
        uint256 pollenRewardTokens;
    }

    mapping(uint256 => StakedNFT) public stakedNFTs; // tokenId => StakedNFT
    mapping(address => mapping(uint256 => uint256)) private ownedNFTStakes; // (address, index) => tokenid
    mapping(uint256 => uint256) private ownedNFTStakesIndex; // tokenId => index in its owner's stake list
    mapping(address => uint256) public ownedNFTStakesBalance; // address => stake count

    mapping(uint256 => uint256) private nftPollenRewardToken; // tokenId => PollenRewardToken amount

    mapping(address => uint256[6]) private numberOfNFTs; // address => [number of power, premium, platinum, gold, silver, bronze]
    mapping(address => uint256) private totalPPM; // address => total PPM

    RhizomERC721 public rhizomERC721;
    PollenRewardToken public pollenRewardToken;

    constructor(
        RhizomERC721 _rhizomERC721,
        PollenRewardToken _pollenRewardToken
    ) {
        rhizomERC721 = _rhizomERC721;
        pollenRewardToken = _pollenRewardToken;
    }

    function _getPowerNFTNumber(address _owner)
        internal
        view
        returns (uint256)
    {
        return numberOfNFTs[_owner][0];
    }

    function _getPremiumNFTNumber(address _owner)
        internal
        view
        returns (uint256)
    {
        return numberOfNFTs[_owner][1];
    }

    function _getPlatinumNFTNumber(address _owner)
        internal
        view
        returns (uint256)
    {
        return numberOfNFTs[_owner][2];
    }

    function _getGoldNFTNumber(address _owner) internal view returns (uint256) {
        return numberOfNFTs[_owner][3];
    }

    function _getSilverNFTNumber(address _owner)
        internal
        view
        returns (uint256)
    {
        return numberOfNFTs[_owner][4];
    }

    function _getBronzeNFTNumber(address _owner)
        internal
        view
        returns (uint256)
    {
        return numberOfNFTs[_owner][5];
    }

    function getPollenTokenRewardsAccruedForManyNFTs(
        uint256[] calldata _tokenIds
    ) public view returns (uint256[] memory) {
        uint256[] memory output = new uint256[](_tokenIds.length);
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            output[i] = _getPollenRewardTokenForNFT(_tokenIds[i], false);
        }
        return output;
    }

    function pollenTokenRewardAccruedCalculation(uint256 _powerStake)
        public
        pure
        returns (uint256)
    {
        return (YIELD_PPS * (_powerStake + REWARD_FACTOR)) / 100;
    }

    function _getPollenRewardTokenForNFT(uint256 _tokenId, bool checkOwnership)
        internal
        view
        returns (uint256)
    {
        StakedNFT memory stakedNFT = stakedNFTs[_tokenId];
        address owner = stakedNFT.owner;

        require(stakedNFT.staked, "This token isn't staked");

        if (checkOwnership) {
            require(owner == _msgSender(), "You don't own this token");
        }

        uint256 ppm = rhizomERC721.getYield(_tokenId);

        return pollenTokenRewardAccruedCalculation(ppm);
    }

    function getTotalPPM(address _owner) public view returns (uint256) {
        return totalPPM[_owner];
    }

    function stakeStarted() public view returns (bool) {
        return startTime != 0 && block.timestamp >= startTime;
    }

    function setStartTime(uint256 _startTime) external onlyOwner {
        require(
            _startTime >= block.timestamp,
            "Start time cannot be in the past"
        );
        require(!stakeStarted(), "Stake already started");
        startTime = _startTime;
    }

    function _claimPollenRewardToken(address _owner) internal {
        uint256 totalClaimed = 0;

        uint256 nftBalance = ownedNFTStakesBalance[_owner];

        for (uint256 i = 0; i < nftBalance; i++) {
            uint256 tokenId = ownedNFTStakes[_owner][i];

            totalClaimed += _getPollenRewardTokenForNFT(tokenId, true);

            delete nftPollenRewardToken[tokenId];

            stakedNFTs[tokenId].startTimestamp = block.timestamp;
        }

        pollenRewardToken.mint(_msgSender(), totalClaimed);
    }

    function claimPollenRewardToken() public nonReentrant whenNotPaused {
        address owner = _msgSender();
        _claimPollenRewardToken(owner);
    }

    function unstakeNFTs(uint256[] calldata _tokenIds)
        public
        nonReentrant
        whenNotPaused
    {
        address owner = _msgSender();

        _claimPollenRewardToken(owner);

        for (uint256 i = 0; i < _tokenIds.length; i++) {
            //unstake Food Trucks
            uint256 tokenId = _tokenIds[i];

            require(
                stakedNFTs[tokenId].owner == owner,
                "You don't own this token"
            );
            require(stakedNFTs[tokenId].staked, "NFT needs to be staked");
            uint256 nftType = rhizomERC721.getType(tokenId);

            numberOfNFTs[owner][nftType - 1]--;

            totalPPM[owner] -= rhizomERC721.getYield(tokenId);
        }

        withdrawNFTs(_tokenIds);
    }

    function stakeMany(uint256[] calldata _tokenIds)
        public
        nonReentrant
        whenNotPaused
    {
        require(stakeStarted(), "Stake has not started");

        address owner = _msgSender();

        require(_tokenIds.length <= 10, "You can't stake that many NFTs");

        for (uint256 i = 0; i < _tokenIds.length; i++) {
            uint256 tokenId = _tokenIds[i];

            require(
                rhizomERC721.ownerOf(tokenId) == owner,
                "You don't own this token."
            );
            require(rhizomERC721.getType(tokenId) > 0, "Type not setted yet.");
            require(!stakedNFTs[tokenId].staked, "NFT is already staked");

            _addNFTToPollenRewards(tokenId, owner);

            uint256 nftType = rhizomERC721.getType(tokenId);

            numberOfNFTs[owner][nftType - 1]++;

            totalPPM[owner] += rhizomERC721.getYield(tokenId);

            rhizomERC721.transferFrom(owner, address(this), tokenId);
        }
    }

    function _addNFTToPollenRewards(uint256 _tokenId, address _owner) internal {
        stakedNFTs[_tokenId] = StakedNFT({
            owner: _owner,
            tokenId: _tokenId,
            startTimestamp: block.timestamp,
            staked: true
        });
        _addStakeToOwnerEnumeration(_owner, _tokenId);
    }

    function _addStakeToOwnerEnumeration(address _owner, uint256 _tokenId)
        internal
    {
        uint256 length = ownedNFTStakesBalance[_owner];
        ownedNFTStakes[_owner][length] = _tokenId;
        ownedNFTStakesIndex[_tokenId] = length;
        ownedNFTStakesBalance[_owner]++;
    }

    function withdrawNFTs(uint256[] calldata _tokenIds)
        internal
        nonReentrant
        whenNotPaused
    {
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            uint256 tokenId = _tokenIds[i];
            rhizomERC721.transferFrom(address(this), _msgSender(), tokenId);
        }
    }

    function stakeOfOwnerByIndex(address _owner, uint256 _index)
        public
        view
        returns (uint256)
    {
        require(
            _index < ownedNFTStakesBalance[_owner],
            "owner index out of bounds"
        );
        return ownedNFTStakes[_owner][_index];
    }

    function batchedStakesOfOwner(
        address _owner,
        uint256 _offset,
        uint256 _maxSize
    ) public view returns (StakedNFTInfo[] memory) {
        if (_offset >= ownedNFTStakesBalance[_owner]) {
            return new StakedNFTInfo[](0);
        }

        uint256 outputSize = _maxSize;
        if (_offset + _maxSize >= ownedNFTStakesBalance[_owner]) {
            outputSize = ownedNFTStakesBalance[_owner] - _offset;
        }
        StakedNFTInfo[] memory outputs = new StakedNFTInfo[](outputSize);

        for (uint256 i = 0; i < outputSize; i++) {
            uint256 tokenId = stakeOfOwnerByIndex(_owner, _offset + i);

            uint256 nftPPM = rhizomERC721.getYield(tokenId);

            outputs[i] = StakedNFTInfo({
                tokenId: tokenId,
                nftPPM: nftPPM,
                pollenRewardTokens: _getPollenRewardTokenForNFT(tokenId, false)
            });
        }

        return outputs;
    }

    function setPaused(bool _paused) external onlyOwner {
        if (_paused) _pause();
        else _unpause();
    }
}

