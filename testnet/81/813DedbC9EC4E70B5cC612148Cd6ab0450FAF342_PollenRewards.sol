pragma solidity ^0.8.4;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/access/Ownable.sol";

contract PollenRewardToken is ERC20("Pollen Reward Token", "POLLEN"), Ownable {
    uint256 public constant ONE_POLLEN = 1e18;
    uint256 public NUM_LP_POLLEN = 30_000_000;

    address public pollenRewardsAddress;

    bool public pollenLPMinted = false;

    function mintLPPollen() external onlyOwner {
        require(!pollenLPMinted, "Pollen LP has already been minted.");
        pollenLPMinted = true;
        _mint(owner(), NUM_LP_POLLEN * ONE_POLLEN);
    }

    function setNumLPPollen(uint256 _numLPPollen) external onlyOwner {
        NUM_LP_POLLEN = _numLPPollen;
    }

    function setPollenRewardsAddress(address _pollenRewardsAddress)
        external
        onlyOwner
    {
        pollenRewardsAddress = _pollenRewardsAddress;
    }

    // external
    function mint(address _to, uint256 _amount) external {
        require(
            _msgSender() == pollenRewardsAddress,
            "Sender does not have permission"
        );
        _mint(_to, _amount);
    }
}

