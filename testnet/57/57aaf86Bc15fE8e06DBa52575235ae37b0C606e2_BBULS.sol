pragma solidity 0.8.1;

contract BBULS is ERC20("BBULS", "BBULS"), Ownable, ReentrancyGuard, PullPayment {
    
    struct WithdrawEvent {
        bool nonceSeen;
        bool isCanceled;
    }
    
    //--------------------------------------------------------------------------------------------------------------
    //MARK: Token Parameters
    //--------------------------------------------------------------------------------------------------------------
    uint256 numberOfTokens = 100_000_000;
    address validatorSigner = 0x807E8Af46673Cd86815F0162fFBeD3e032E25d5a;

    //--------------------------------------------------------------------------------------------------------------
    //MARK: Pausable Properties/Events
    //--------------------------------------------------------------------------------------------------------------
    event Pause();
    event Unpause();
    bool public paused = false;

    //--------------------------------------------------------------------------------------------------------------
    //MARK: Deposit-Withdraw Properties/Events
    //--------------------------------------------------------------------------------------------------------------
    using ECDSA for bytes32;
    
    mapping(address => mapping(uint => WithdrawEvent)) public seenNonces;
    mapping(address => uint256[]) private depositNonce;

    event withdrawSuccessfull(address toAddress, uint amount);
    event withdrawCancelSuccessfull(address toAddress);
    event depositSuccessfull(address toAddress, uint amount);

    //--------------------------------------------------------------------------------------------------------------
    //MARK: Vesting Schedule Limits
    //--------------------------------------------------------------------------------------------------------------
    uint256 gameRewardsTotalSupply = 85_000_000*10**18; // in vesting schedule (game + airdrop supply 5M)
    uint256 advisoryTotalSupply = 10_000_000*10**18;
    uint256 listingTotalSupply = 5_000_000*10**18;

    string AdvisoryKey      = "Advisory";
    string GameRewardsKey   = "Game Rewards";
    string ListingKey       = "Listing";
    string DepositWalletKey = "Deposit";
    mapping(string => uint256) private bbulsCache;
    

    //--------------------------------------------------------------------------------------------------------------
    //MARK: Constructor
    //--------------------------------------------------------------------------------------------------------------
    constructor() {
        // number of tokens minted = n * 10^18
        _mint(address(this), numberOfTokens * 10**18);
    }
    
    function setSigner(address _signerAddress) public onlyOwner {
        validatorSigner = _signerAddress;
    }

    // Burns the callers tokens
    function burnOwnTokens(uint256 _amount) external {
        _burn(msg.sender, _amount);
    }
    
    function getChainId() internal view returns (uint) {
        uint256 chainId;
        assembly { 
            chainId := chainid()
        }
        return chainId;
    }
    
    //--------------------------------------------------------------------------------------------------------------
    //MARK: Deposit
    //--------------------------------------------------------------------------------------------------------------

    function deposit(uint256 _amount) public payable whenNotPaused {
        depositNonce[msg.sender].push(_amount);
        bbulsCache[DepositWalletKey] += _amount;
        _transfer(msg.sender, address(this), _amount);
        emit depositSuccessfull(address(msg.sender), _amount);
    }
    
    function getDepositPrice(address senderAddress, uint nonce, bytes memory signature) public view returns (uint256 amount) {
        bytes32 message = keccak256(abi.encodePacked(senderAddress, nonce));
        require(checkSignature(message, signature), "Invalid Signature");
        return depositNonce[senderAddress][nonce];
    }

    function getDepositCurrentNonce(address senderAddress, bytes memory signature) public view returns (uint256 amount) {
        bytes32 message = keccak256(abi.encodePacked(senderAddress));
        require(checkSignature(message, signature), "Invalid Signature");
        return depositNonce[senderAddress].length - 1;
    }
    
    //--------------------------------------------------------------------------------------------------------------
    //MARK: Withdraw
    //--------------------------------------------------------------------------------------------------------------
    
    
    function withdraw(address payable toAddress, uint256 amountWei, uint nonce, bytes memory signature, bool cancelWithdraw) external whenNotPaused nonReentrant() {
        bytes32 message = keccak256(abi.encodePacked(toAddress, amountWei, nonce));
        require(checkSignature(message, signature), "Invalid Signature");
        require(!(seenNonces[toAddress][nonce].nonceSeen), "Already use!");
        if (cancelWithdraw) {
            seenNonces[toAddress][nonce] = WithdrawEvent(true, true);
            emit withdrawCancelSuccessfull(address(toAddress));
        } else {
            require(bbulsCache[GameRewardsKey] < gameRewardsTotalSupply, "GameRewards limit exceeded!");
            require(bbulsCache[GameRewardsKey] + amountWei <= gameRewardsTotalSupply, "GameRewards limit exceeded!");
            _transfer(address(this), address(toAddress), amountWei);
            seenNonces[toAddress][nonce] = WithdrawEvent(true, false);
            bbulsCache[GameRewardsKey] += amountWei;
            emit withdrawSuccessfull(address(toAddress), amountWei);
        }
    }
    
    //--------------------------------------------------------------------------------------------------------------
    //MARK: Listing-Advisory Withdraws
    //--------------------------------------------------------------------------------------------------------------
    function listingTransfer(address toAddress, uint256 amountWei) public onlyOwner {
        require(bbulsCache[ListingKey] <= listingTotalSupply, "Listing limit exceeded!");
        require(bbulsCache[ListingKey] + amountWei <= listingTotalSupply, "Listing limit exceeded!");
        _transfer(address(this), address(toAddress), amountWei);
        bbulsCache[ListingKey] += amountWei;
    }

    function advisoryWithdraw(address toAddress, uint256 amountWei) public onlyOwner {
        require(bbulsCache[AdvisoryKey] <= advisoryTotalSupply, "Advisory limit exceeded!");
        require(bbulsCache[AdvisoryKey] + amountWei <= advisoryTotalSupply, "Advisory limit exceeded!");
        _transfer(address(this), address(toAddress), amountWei);
        bbulsCache[AdvisoryKey] += amountWei;
    }

    //--------------------------------------------------------------------------------------------------------------
    //MARK: Pausable Modifiers
    //--------------------------------------------------------------------------------------------------------------
    modifier whenNotPaused() {
        require(!paused);
        _;
    }

    modifier whenPaused() {
        require(paused);
        _;
    }

    function pause() onlyOwner whenNotPaused public {
        paused = true;
        emit Pause();
    }

    function unpause() onlyOwner whenPaused public {
        paused = false;
        emit Unpause();
    }
    
    
    
    //--------------------------------------------------------------------------------------------------------------
    //MARK: Signature Verify Methods
    //--------------------------------------------------------------------------------------------------------------
    function safe32(uint n, string memory errorMessage) internal pure returns (uint32) {
        require(n < 2**32, errorMessage);
        return uint32(n);
    }

    function recoverSigner(bytes32 message, bytes memory sig) public pure returns (address) {
        uint8 v;
        bytes32 r;
        bytes32 s;

        (v, r, s) = splitSignature(sig);
        return ecrecover(message, v, r, s);
    }

    function splitSignature(bytes memory sig) public pure returns (uint8, bytes32, bytes32) {
        require(sig.length == 65);
        bytes32 r;
        bytes32 s;
        uint8 v;

        assembly {
        // first 32 bytes, after the length prefix
            r := mload(add(sig, 32))
        // second 32 bytes
            s := mload(add(sig, 64))
        // final byte (first byte of the next 32 bytes)
            v := byte(0, mload(add(sig, 96)))
        }

        return (v, r, s);
    }

    //--------------------------------------------------------------------------------------------------------------
    //MARK: Signature Helper
    //--------------------------------------------------------------------------------------------------------------
    function checkSignature(bytes32 message, bytes memory signature) private view returns (bool) {
        bytes32 messageHash = message.toEthSignedMessageHash();
        address signer = recoverSigner(messageHash, signature);
        return signer == validatorSigner;
    }

    //--------------------------------------------------------------------------------------------------------------
    //MARK: BBULS Cache Getter
    //--------------------------------------------------------------------------------------------------------------
    function getBBULSCacheParams(string memory key, bytes memory signature) public view returns (uint256) {
        bytes32 message = keccak256(abi.encodePacked(key, signature));
        require(checkSignature(message, signature), "Invalid Signature");
        return bbulsCache[key];
    }
}