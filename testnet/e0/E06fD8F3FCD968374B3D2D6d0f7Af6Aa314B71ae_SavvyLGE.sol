pragma solidity 0.8.11;






// @title Savvy LGE
// @author Savvy Finance
// @dev a way to make fair launch more fun and flexible

contract SavvyLGE is Ownable, Initializable {
    using Math for uint256;
    using SafeERC20 for IERC20;

    uint256 public constant BASIS_POINTS = 10_000;

    // VestMode:: various VestMode options
    // term:: vesting terms for each VestMode
    // price:: discounted price for this VestMode
    struct VestMode {
        uint256 term;
        uint256 price;
    }
    uint256 public numVestMode;
    mapping(uint256 => VestMode) public vestModes;

    // saleAsset:: token for sale (SVY)
    // counterAsset:: token accepted for sale (USDC)
    // pricePerShare:: base price for 1 unit ($counterAsset) per share
    // multisig:: address where funds raised are sent
    IERC20 public saleAsset;
    IERC20 public counterAsset;
    uint256 public pricePerShare;
    address public multisig;

    // raised:: amount of counterAsset raised
    // shareSupply:: total amount of shares in existence
    // totalSaleAsset:: initial saleAsset token balance in LGE
    uint256 public raised;
    uint256 public shareSupply;
    uint256 public totalSaleAsset;

    // beginning:: start time of event
    // end:: end time of event - when vesting begins
    // paused:: boolean for if LGE contract is paused
    uint256 public beginning;
    uint256 public end;
    bool public paused = false;

    // price:: 9000 = 90.00% of full value
    // limit:: the maximum amount that can be purchased from the NFT
    // term:: the vesting term for the particular NFT
    struct License {
        uint256 price;
        uint256 limit;
    }

    // shares:: the amount of shares purchased by the user
    // term:: the user's weighted average term
    struct Terms {
        uint256 shares;
        uint256 term;
    }

    // remaining:: amount of shares remaining for this NFT
    // activated:: true if the NFT has been used at all
    struct Allocation {
        uint256 remaining;
        bool activated;
    }

    // licenses:: mapped to ERC721 address, shows base allocation info
    // allocations:: mapped to NFT IDs, nested within the NFT address, shows current allocation info
    // terms:: mapped to user address, stores user's terms
    // claimed:: mapped to user address, stores the amount of sale asset claimed so far
    mapping(address => License) public licenses;
    mapping(address => mapping(uint256 => Allocation)) public allocations;
    mapping(address => Terms) public terms;
    mapping(address => uint256) public claimed;

    struct BatchPricingData {
        uint256 nftPerShare;
        uint256 nftTotalCost;
        uint256 nftTotalShares;
        uint256 perShare;
        uint256 totalAvailable;
        uint256 totalCost;
    }

    event UpdatedSaleAsset(address indexed _saleAsset, uint256 _totalSaleAsset);
    event WithdrewSaleAsset(address indexed _saleAsset, uint256 _totalSaleAsset);
    event UpdatedTimestamps(uint256 _beginning, uint256 _end);
    event UpdatedVestModes(uint256 _numVestMode);

    modifier notExpired {
        require(block.timestamp <= end, "LGE has ended.");
        _;
    }
    
    modifier unPaused {
        require(!paused, "LGE is paused.");
        _;
    }

    // @dev initialize LGE contract
    function initialize (
        address _saleAsset,
        address _counterAsset,
        address _multisig,
        uint256 _beginning,
        uint256 _end,
        VestMode[] memory _vestModes
    ) external initializer {
        _updateSaleAsset(_saleAsset);
        
        require(_counterAsset != address(0), "invalid counter asset");
        counterAsset = IERC20(_counterAsset);
        pricePerShare = 10 ** ERC20(_counterAsset).decimals();

        require(_multisig != address(0), "invalid multisig address");
        multisig = _multisig;

        _updateTimestamps(_beginning, _end);
        _updateVestModes(_vestModes);
    }

    // @dev admin function to add a license to a given NFT project. Could use some guardrails.
    function addLicense(
        address addr,
        uint256 price,
        uint256 limit
    ) public onlyOwner notExpired returns (bool) {
        paused = true;
        licenses[addr] = License(price, limit);
        paused = false;
        return true;
    }

    // @dev routing function to protect internals and simplify front end integration
    // amount:: the amount of shares the user would like to buy
    // NFT:: the address of the NFT terms a user would like to use - 0 to use default terms
    // nftTokenIndex:: ID paired with the NFT contract
    // mode:: vest mode
    function buy(
        uint256 amount,
        address NFT,
        uint256 nftTokenIndex,
        uint256 mode
    ) public notExpired unPaused returns (bool) {
        uint256 currentTime = block.timestamp;
        console.log("current", currentTime);
        console.log("beginning", beginning);
        require(currentTime >= beginning, "lge has not begun!");
        require(amount != 0, "buy: please input amount");
        if (NFT != address(0)) {
            require(
                licenses[NFT].limit != 0,
                "buy: this NFT is not eligible for whitelist"
            );
            require(
                IERC721(NFT).ownerOf(nftTokenIndex) == msg.sender,
                "buyer is not the owner"
            );
        }
        _buy(amount, NFT, nftTokenIndex, mode);
        return true;
    }

    // @dev front end helper function - settles large amount of purchases with multiple...
    // ...NFTs and sinks remaining desired into a default purchase
    // totalAmount:: the total amount of shares user wants to purchase
    // NFTs:: array of NFTs user would like to cash out
    // nftTokenIndexes:: paired with the NFT addresses
    // mode:: vest mode
    function batchPurchase(
        uint256 totalAmount,
        address[] calldata NFTs,
        uint256[] calldata nftTokenIndexes,
        uint256 mode
    ) external notExpired unPaused returns (bool) {
        require(
            NFTs.length == nftTokenIndexes.length,
            "array lengths do not match"
        );
        uint256 remaining = totalAmount;
        for (uint256 i = 0; i < NFTs.length; i++) {
            (uint256 available, , ) = getPricingData(
                NFTs[i],
                nftTokenIndexes[i],
                mode
            );
            uint256 amount = Math.min(available, remaining);
            if (amount > 0) {
                buy(amount, NFTs[i], nftTokenIndexes[i], mode);
                remaining -= amount;
            }
            if (remaining == 0) {
                return true;
            }
        }
        if (remaining > 0) {
            buy(remaining, address(0), 0, mode);
            return true;
        }
        return true;
    }

    // @dev used to issue sale asset to users after the LGE, claims all unclaimed sale asset from last claim to current
    function claim() external returns (bool) {
        require(block.timestamp > end, "lge has not ended");

        uint256 totalOwed = (totalSaleAsset * terms[msg.sender].shares) / shareSupply;
        uint256 perSecond = totalOwed / terms[msg.sender].term;
        uint256 secondsClaimed = claimed[msg.sender] / perSecond;
        uint256 lastClaim = end + secondsClaimed;
        uint256 owed = (block.timestamp - lastClaim) * perSecond;
        if (claimed[msg.sender] + owed > totalOwed) {
            owed = totalOwed - claimed[msg.sender];
        }
        claimed[msg.sender] += owed;
        saleAsset.safeTransfer(msg.sender, owed);
        return true;
    }

    // @dev withdraws saleAsset token balance to multisig
    function withdrawSaleAsset() public onlyOwner returns (bool) {
        paused = true;
        _withdrawSaleAsset();
        paused = false;
        return true;
    }

    // @dev update saleAsset token contract address
    function updateSaleAsset(address _saleAsset) public onlyOwner notExpired returns (bool) {
        paused = true;
        _withdrawSaleAsset();
        _updateSaleAsset(_saleAsset);
        paused = false;
        return true;
    }

    // @dev admin function to update existing vest mode
    function updateVestMode(uint256 mode, VestMode memory vestMode) public onlyOwner notExpired returns (bool) {
        require(mode < numVestMode, "invalid vest mode");
        require(vestMode.price < BASIS_POINTS, "invalid discount");
        paused = true;
        vestModes[mode] = vestMode;
        paused = false;
        return true;
    }

    // @dev admin function to update entire vesting modes
    function updateVestModes(VestMode[] memory _vestModes) public onlyOwner notExpired returns (bool) {
        paused = true;
        _updateVestModes(_vestModes);
        paused = false;
        return true;
    }

    // @dev admin function to update LGE beginning & end timestamps
    function updateTimestamps(uint256 _beginning, uint256 _end) public onlyOwner returns (bool) {
        paused = true;
        _updateTimestamps(_beginning, _end);
        paused = false;
        return true;
    }

    // @dev helper function for the front end
    // @returns weighted average price per share, total cost, and total shares available for all NFTs passed in
    function getBatchPricing(
        uint256 totalAmount,
        address[] calldata NFTs,
        uint256[] calldata nftTokenIndexes,
        uint256 mode
    ) public view returns (BatchPricingData memory data) {
        require(mode < numVestMode, "invalid vest mode");

        uint256 remaining = totalAmount;
        uint256 totalShares;
        for (uint256 i = 0; i < NFTs.length; i++) {
            (uint256 available, uint256 _perShare, ) = getPricingData(
                NFTs[i],
                nftTokenIndexes[i],
                mode
            );
            uint256 amount = Math.min(available, remaining);
            data.perShare = findWeightedAverage(
                amount,
                totalShares,
                _perShare,
                data.perShare
            );
            totalShares += amount;
            remaining -= amount;
            data.totalAvailable += available;
        }
        data.nftPerShare = data.perShare;
        data.nftTotalShares = totalShares;
        data.nftTotalCost = data.perShare * totalShares;
        uint256 vestModePrice = _getVestCostPerShare(vestModes[mode].price);
        if (remaining > 0) {
            data.perShare = findWeightedAverage(
                remaining,
                totalShares,
                vestModePrice,
                data.perShare
            );
        }
        data.totalCost = data.perShare * totalAmount;
    }

    // @dev helper function for the front end
    // @returns the weighted average terms for all NFTs passed in
    function getBatchTerms(
        uint256 totalAmount,
        address[] calldata NFTs,
        uint256[] calldata nftTokenIndexes,
        uint256 mode
    ) public view returns (uint256 nftTerm, uint256 term) {
        require(mode < numVestMode, "invalid vest mode");

        uint256 remaining = totalAmount;
        uint256 totalShares;
        uint256 _term = vestModes[mode].term;
        for (uint256 i = 0; i < NFTs.length; i++) {
            (uint256 available, , ) = getPricingData(
                NFTs[i],
                nftTokenIndexes[i],
                mode
            );
            uint256 amount = Math.min(available, remaining);
            term = findWeightedAverage(amount, totalShares, _term, term);
            totalShares += amount;
            remaining -= amount;
        }
        nftTerm = term;
        if (remaining > 0) {
            term = findWeightedAverage(remaining, totalShares, _term, term);
        }
    }

    // @dev useful function to return commonly used pricing data
    // @returns available shares in the NFT, the price per share, and the total cost
    // ensures uninstantiated allocations are accounted for properly
    function getPricingData(
        address NFT,
        uint256 nftTokenIndex,
        uint256 mode
    ) public view returns (
        uint256 available,
        uint256 perShare,
        uint256 totalCost
    ) {
        require(mode < numVestMode, "invalid vest mode");

        Allocation memory alloc = allocations[NFT][nftTokenIndex];
        if (alloc.activated) {
            available = allocations[NFT][nftTokenIndex].remaining;
        } else {
            available = licenses[NFT].limit;
        }
        perShare = _getCostPerShare(NFT, vestModes[mode].price);
        totalCost = available * perShare;
    }

    // @dev utility function to find weighted averages without any underflows or zero division problems.
    // use x to determine weights, with y being the values you're weighting
    // addedValue:: new amount of x being added
    // oldValue:: current amount of x
    // weightedNew:: new amount of y being added to weighted average
    // weightedOld:: current weighted average of y
    function findWeightedAverage(
        uint256 valueNew,
        uint256 valueOld,
        uint256 weightNew,
        uint256 weightOld
    ) public pure returns (uint256) {
        uint256 totalNew = (valueNew * weightNew) + (valueOld * weightOld);
        uint256 valueTotal = valueNew + valueOld;
        if (valueTotal == 0) {
            return 0;
        }
        return totalNew / valueTotal;
    }

    // @dev function that applies NFT term & vestMode term to the user's purchase
    // @note transferring user funds directly to multisig
    // amount:: amount of shares user wants to buy
    // NFT:: address of the NFT terms user is using
    // nftTokenIndex:: ID paired with the NFT contract
    // mode:: vest mode
    function _buy(
        uint256 amount,
        address NFT,
        uint256 nftTokenIndex,
        uint256 mode
    ) internal {
        require(mode < numVestMode, "invalid vest mode");
        VestMode memory vestMode = vestModes[mode];
        uint256 price = vestMode.price;
        uint256 term = vestMode.term;

        if (NFT != address(0)) {
            Allocation storage alloc = allocations[NFT][nftTokenIndex];
            // managing per-NFT state
            if (!alloc.activated) {
                _activate(NFT, nftTokenIndex, amount);
            } else {
                require(
                    alloc.remaining >= amount,
                    "_buy: insufficient remaining"
                );
                alloc.remaining -= amount;
            }
        }
        uint256 costPerShare = _getCostPerShare(NFT, price);
        uint256 cost = amount * costPerShare;
        counterAsset.safeTransferFrom(msg.sender, multisig, cost);

        _updateTerms(amount, term);
        shareSupply += amount;
        raised += cost;
    }

    // @dev calculates cost per share based on vest mode
    // vestModePrice:: discounted price of vest mode
    function _getVestCostPerShare(
        uint256 vestModePrice
    ) internal view returns (uint256 costPerShare) {
        costPerShare = _getCostPerShare(address(0), vestModePrice);
    }

    // @dev calculates cost per share based on NFT and vest mode
    // NFT:: address of the NFT terms user is using
    // vestModePrice:: discounted price of vest mode
    function _getCostPerShare(
        address NFT,
        uint256 vestModePrice
    ) internal view returns (uint256 costPerShare) {
        costPerShare = pricePerShare * vestModePrice / BASIS_POINTS;
        if (NFT != address(0)) {
            uint256 nftPrice = licenses[NFT].price;
            if (nftPrice != 0) {
                costPerShare = costPerShare * nftPrice / BASIS_POINTS;
            }
        }
    }

    // @dev pulls license state into allocation state and updates the amount to
    // ensure double spends aren't possible
    // addr:: address of the NFT whose license you're using
    // index:: id of the NFT you're activating
    // amount:: amount being spent out of that NFT
    function _activate(
        address addr,
        uint256 index,
        uint256 amount
    ) internal {
        require(amount <= licenses[addr].limit, "activate: amount too high");
        Allocation storage alloc = allocations[addr][index];
        alloc.remaining = licenses[addr].limit - amount;
        alloc.activated = true;
    }

    // @dev updates the user's terms
    // @_shares:: amount of shares being added to total - used to determine weight
    // @_term:: new term for shares being added - weighted against existing shares/term
    function _updateTerms(uint256 _shares, uint256 _term) internal {
        Terms storage userTerms = terms[msg.sender];

        userTerms.term = findWeightedAverage(
            _shares,
            userTerms.shares,
            _term,
            userTerms.term
        );

        userTerms.shares += _shares;
    }

    // @dev withdraw all saleAsset from this contract to multisig
    function _withdrawSaleAsset() internal {
        uint256 currentSaleAsset = saleAsset.balanceOf(address(this));
        saleAsset.safeTransferFrom(address(this), multisig, currentSaleAsset);
        emit WithdrewSaleAsset(address(saleAsset), currentSaleAsset);
    }

    // @dev update saleAsset token contract address
    function _updateSaleAsset(address _saleAsset) internal {
        require(_saleAsset != address(0), "invalid saleAsset token");
        IERC20 newSaleAsset = IERC20(_saleAsset);
        uint256 currentSaleAsset = newSaleAsset.balanceOf(address(this));
        require(currentSaleAsset > 0, "need to transfer saleAsset tokens to LGE");
        saleAsset = newSaleAsset;
        totalSaleAsset = currentSaleAsset;
        emit UpdatedSaleAsset(_saleAsset, currentSaleAsset);
    }

    // @dev admin function to update entire vesting modes
    function _updateVestModes(VestMode[] memory _vestModes) internal {
        numVestMode = _vestModes.length;
        for (uint256 i; i < numVestMode; i++) {
            require(_vestModes[i].price < BASIS_POINTS, "invalid discount");
            vestModes[i] = _vestModes[i];
        }
        emit UpdatedVestModes(numVestMode);
    }

    // @dev admin function to update LGE beginning & end timestamps
    function _updateTimestamps(uint256 _beginning, uint256 _end) internal {
        require(_end > _beginning, "LGE ends before beginning");
        require((_end - _beginning) <= 5 days, "LGE too long");
        beginning = _beginning;
        end = _end;
        emit UpdatedTimestamps(_beginning, _end);
    }
}