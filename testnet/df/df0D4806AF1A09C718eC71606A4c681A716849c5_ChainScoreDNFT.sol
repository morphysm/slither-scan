import "@chainscore/client/contracts/ChainScoreClient.sol";

/// @custom:security-contact [emailÂ protected]
contract ChainScoreDNFT is
    ERC721,
    ERC721Enumerable,
    Pausable,
    AccessControl,
    ChainScoreClient
{
    using Counters for Counters.Counter;
    bytes32 public constant PAUSER_ROLE = keccak256("PAUSER_ROLE");
    bytes32 public constant MINTER_ROLE = keccak256("MINTER_ROLE");
    
    Counters.Counter private _tokenIdCounter;

    mapping(address => uint) public scores;
    address public CSClient;
    uint public fee;

    constructor(address score, address oracle) ERC721("ChainScore dNFT", "dNFT") ChainScoreClient(score, oracle) {
        _grantRole(DEFAULT_ADMIN_ROLE, msg.sender);
        _grantRole(PAUSER_ROLE, msg.sender);
        // oracle can mint
        _grantRole(MINTER_ROLE, oracle);
    }

    function pause() public onlyRole(PAUSER_ROLE) {
        _pause();
    }

    function unpause() public onlyRole(PAUSER_ROLE) {
        _unpause();
    }

    function mint() public {
        sendChainscoreRequest(msg.sender, this.safeMint.selector, keccak256("CREDIT_SCORE"), 1*(10**18));
    }

    function safeMint(bytes32 requestId, address to, uint score) public onlyRole(MINTER_ROLE) {
        validateChainscoreCallback(requestId);
        if(balanceOf(msg.sender) == 0){
            uint256 tokenId = _tokenIdCounter.current();
            _tokenIdCounter.increment();
            _safeMint(to, tokenId);
        }
        scores[to] = score;
    }

    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal override(ERC721, ERC721Enumerable) whenNotPaused {
        require(from == address(0) || to == address(0), "Transfer not permitted");
        super._beforeTokenTransfer(from, to, tokenId);
    }

    // The following functions are overrides required by Solidity.

    function _burn(uint256 tokenId)
        internal
        override(ERC721)
    {
        super._burn(tokenId);
    }

