        string memory svgData = getSvg(tokenId);

        string memory json = string(bytes(
                string(
                    abi.encodePacked(
                        '{"name": "ChainScore dNFT", "description": "On-chain credit scoring protocol for DeFi", "image_data": "', bytes(svgData),'"}'
                    )
                )
            )
        );
        return string(abi.encodePacked("data:application/json;base64,", json));
    }

