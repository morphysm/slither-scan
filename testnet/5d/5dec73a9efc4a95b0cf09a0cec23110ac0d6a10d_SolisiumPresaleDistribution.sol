contract SolisiumPresaleDistribution is Ownable {
    using SafeMath for uint256;
    IERC20 public SolisiumToken;

    mapping(address => uint256) public privatePresaleAmounts;
    address[] public privatePresalers;

    mapping(address => uint256) public publicPresaleAmounts;
    address[] public publicPresalers;

    mapping(address => uint256) public teamLeadersAmounts;
    address[] public teamLeaders;

    uint256 SumTokensTeamLeader;
    uint256 SumTokensPrivate;
    uint256 SumTokensPublic;

    uint256 PrivateSaleTokenRate = 3;
    uint256 PublicSaleTokenRate  = 2;
    //Listing : 127.713921 per BNB
    //Pvt sale: 159.642401 per BNB
    //Pub sale: 141.904356  per BNB

    constructor(address _solisiumTokenAddress) public {
        SolisiumToken = IERC20(_solisiumTokenAddress);
        addTeamLeader();
        addPrivatePresale();
        addPublicPresale();
    }

    function getTotalDistributed() public view returns (uint256){
        return (SumTokensTeamLeader + SumTokensPrivate + SumTokensPublic);
    }

    function addTeamLeader() internal {
        teamLeadersAmounts[0x843f2C19bc6df9E32B482E2F9ad6C078001088b1] = 50000000000000000000;

        teamLeaders.push(0x843f2C19bc6df9E32B482E2F9ad6C078001088b1);

    }

    function addPrivatePresale() internal{

        privatePresaleAmounts[0xb453935F18709f860b9c3242405d8dE7E8803b5C] = 44000000000000000000;
        privatePresalers.push(0xb453935F18709f860b9c3242405d8dE7E8803b5C);

    }

    function addPublicPresale() internal {
        publicPresaleAmounts[0x4165fba7090C71D1be4B0f67d2e6Ff4eA9457aDE] = 15000000000000000000;
        publicPresalers.push(0x4165fba7090C71D1be4B0f67d2e6Ff4eA9457aDE);

    }

    function withdrawRemaining() public onlyOwner{
        SolisiumToken.transfer(address(owner()), SolisiumToken.balanceOf(address(this)));
    }

    function distributeTeamLeaders() public onlyOwner {
        for (uint256 i = 0; i < teamLeaders.length; i++) {
            uint256 amountToSend = teamLeadersAmounts[teamLeaders[i]];
            uint256 TokenAmount = amountToSend.mul(PrivateSaleTokenRate).div(1e6);
            SumTokensTeamLeader += TokenAmount;
            SolisiumToken.transfer(teamLeaders[i], TokenAmount);
        }
    }

    function distributePrivatePresale() public onlyOwner {
        for (uint256 i = 0; i < privatePresalers.length; i++) {
            uint256 amountToSend = privatePresaleAmounts[privatePresalers[i]];
            uint256 TokenAmount = amountToSend.mul(PrivateSaleTokenRate).div(1e6);
            SumTokensPrivate += TokenAmount;
            SolisiumToken.transfer(privatePresalers[i], TokenAmount);
        }
    }

    function distributePublicPresale() public onlyOwner {
        for (uint256 i = 0; i < publicPresalers.length; i++) {
            uint256 amountToSend = publicPresaleAmounts[publicPresalers[i]];
            uint256 TokenAmount = amountToSend.mul(PublicSaleTokenRate).div(1e6);
            SumTokensPublic += TokenAmount;
            SolisiumToken.transfer(publicPresalers[i], TokenAmount);
        }
    }

}