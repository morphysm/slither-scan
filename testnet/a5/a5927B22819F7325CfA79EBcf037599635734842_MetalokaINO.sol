//SPDX-License-Identifier: Unlicense
pragma solidity 0.8.15;

import "@openzeppelin/contracts/access/AccessControl.sol";
import "@openzeppelin/contracts/security/Pausable.sol";
import "@openzeppelin/contracts/security/ReentrancyGuard.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

import {Range, ReturnSetting} from "./lib/Structs.sol";
import {SaleType, Rare} from "./lib/Enums.sol";
import {OPERATOR_ROLE} from "./lib/Constants.sol";

abstract contract BaseINO is AccessControl, Pausable, ReentrancyGuard {
    // The admin which sign data
    address internal adminSigner;

    /** Wallet address which:
     * - Receive NFT which user when return
     * - Receive ERC20 when user buy
     */
    address internal fundReceiver;

    /** Wallet address which:
     * - Hold NFT and transfer to user when user buy
     * - Hold ERC20 and transfer to user when return
     */
    address internal vaultWallet;

    // Use to invalid order signature
    uint256 public orderNonce;

    // Use to invalid return signature
    uint256 public returnNonce;

    // Use to invalid ref signature
    uint256 public refNonce;

    // Use to invalid ref signature
    uint256 public whitelistNonce;

    address public saleToken;
    address public nftAddress;

    // Reward rate for ref's owner when buy with ref code
    uint256 public referRewardRate;

    // SaleType => Time Range
    mapping(SaleType => Range) public saleTypeTimes;

    // Rare => Discount amount when refer friends
    mapping(Rare => uint256) public referDiscounts;

    ReturnSetting[] public returnSettings;

    constructor(address vaultWallet_, address fundReceiver_) {
        _grantRole(DEFAULT_ADMIN_ROLE, _msgSender());

        vaultWallet = vaultWallet_;
        fundReceiver = fundReceiver_;
    }

    modifier onlyAdmin() {
        _checkRole(DEFAULT_ADMIN_ROLE);
        _;
    }

    function pause() public onlyAdmin {
        _pause();
    }

    function unpause() public onlyAdmin {
        _unpause();
    }

    function setAdminSigner(address signer) public onlyAdmin {
        adminSigner = signer;
    }

    function setNFTAddress(address newAddress) public onlyAdmin {
        nftAddress = newAddress;
    }

    function setSaleToken(address newAddress) public onlyAdmin {
        saleToken = newAddress;
    }

    function setVaultAddress(address wallet) public onlyAdmin {
        vaultWallet = wallet;
    }

    function configSaleTypeTime(
        SaleType saleType,
        uint256 start,
        uint256 end
    ) external onlyRole(OPERATOR_ROLE) {
        saleTypeTimes[saleType].start = start;
        saleTypeTimes[saleType].end = end;
    }

    function setOrderNonce(uint256 nonce_) external onlyRole(OPERATOR_ROLE) {
        orderNonce = nonce_;
    }

    function setReturnNonce(uint256 newNonce) external onlyRole(OPERATOR_ROLE) {
        returnNonce = newNonce;
    }

    function setRefNonce(uint256 newNonce) external onlyRole(OPERATOR_ROLE) {
        refNonce = newNonce;
    }

    function setWhitelistNonce(uint256 newNonce)
        external
        onlyRole(OPERATOR_ROLE)
    {
        whitelistNonce = newNonce;
    }

    function configSale(address _nftAddress, address _saleToken)
        external
        onlyRole(OPERATOR_ROLE)
    {
        nftAddress = _nftAddress;
        saleToken = _saleToken;
    }

    function setReferRewardRate(uint256 newValue)
        external
        onlyRole(OPERATOR_ROLE)
    {
        referRewardRate = newValue;
    }

    function setFundReceiver(address receiver) external onlyAdmin {
        fundReceiver = receiver;
    }

    function withdrawFund(
        address token,
        uint256 amount,
        address to
    ) external onlyAdmin {
        IERC20(token).transferFrom(address(this), to, amount);
    }

    function isSaleOpen(SaleType saleType) internal view returns (bool) {
        return
            saleTypeTimes[saleType].start <= block.timestamp &&
            block.timestamp <= saleTypeTimes[saleType].end;
    }

    function configReturn(
        uint256[] calldata returnRates,
        uint256[] calldata durations,
        uint256[] calldata feeRates
    ) external onlyRole(OPERATOR_ROLE) {
        require(
            returnRates.length == durations.length,
            "Config: Should same length"
        );
        require(
            returnRates.length == feeRates.length,
            "Config: Should same length"
        );

        // using push and pop is the only way to change the
        // length of an array
        if (durations.length < returnSettings.length) {
            while (returnSettings.length > durations.length)
                returnSettings.pop();
        } else if (durations.length > returnSettings.length) {
            while (returnSettings.length < durations.length)
                returnSettings.push();
        }

        uint256 lastDuration = 0;
        for (uint256 i = 0; i < durations.length; i++) {
            require(
                durations[i] > lastDuration,
                "Config: Duration should be in asc order"
            );
            lastDuration = durations[i];

            returnSettings[i] = ReturnSetting(
                returnRates[i],
                durations[i],
                feeRates[i]
            );
        }

        for (uint256 i = durations.length; i < returnSettings.length; i++) {
            returnSettings.pop();
        }
    }

    function _matchReturnSetting(uint256 soldAt)
        internal
        view
        returns (ReturnSetting memory)
    {
        uint256 duration = block.timestamp - soldAt;

        for (uint256 i = 0; i < returnSettings.length; i++) {
            if (duration < returnSettings[i].duration) {
                return returnSettings[i];
            }
        }

        revert("Not match any return settings");
    }

    function configReferDiscountForRare(uint256[] calldata amounts)
        external
        onlyRole(OPERATOR_ROLE)
    {
        require(amounts.length == 5, "Should provide 5 values");

        referDiscounts[Rare.Myth] = amounts[0];
        referDiscounts[Rare.Legend] = amounts[1];
        referDiscounts[Rare.Epic] = amounts[2];
        referDiscounts[Rare.Uncommon] = amounts[3];
        referDiscounts[Rare.Common] = amounts[4];
    }

    function getReferDiscounts() external view returns (uint256[5] memory) {
        return [
            referDiscounts[Rare.Myth],
            referDiscounts[Rare.Legend],
            referDiscounts[Rare.Epic],
            referDiscounts[Rare.Uncommon],
            referDiscounts[Rare.Common]
        ];
    }
}

