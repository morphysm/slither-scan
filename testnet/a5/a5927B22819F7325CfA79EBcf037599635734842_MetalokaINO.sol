pragma solidity 0.8.15;

import "@openzeppelin/contracts/proxy/utils/Initializable.sol";
import "@openzeppelin/contracts/utils/cryptography/MerkleProof.sol";
import "@openzeppelin/contracts/token/ERC721/IERC721.sol";
import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "@openzeppelin/contracts/utils/cryptography/ECDSA.sol";
import "@openzeppelin/contracts/utils/cryptography/draft-EIP712.sol";
import "@openzeppelin/contracts/utils/cryptography/SignatureChecker.sol";
import "hardhat/console.sol";

import "./BaseINO.sol";
import "./ITransferCounter.sol";
import {SaleInfo, ReturnInfo, Order, Ref} from "./lib/Structs.sol";
import {MULTIPLIER, ORDER_TYPE_HASH, WHITELIST_TYPE_HASH, REF_TYPE_HASH, RETURN_TYPE_HASH} from "./lib/Constants.sol";

contract MetalokaINO is BaseINO, EIP712 {
    mapping(address => bool) public whitelistClaimed;
    mapping(bytes32 => SaleInfo) public saleInfo;
    mapping(bytes32 => ReturnInfo) public returnInfo;

    event INOSale(
        bytes32 hash,
        SaleType saleType,
        address buyer,
        uint256 basePrice,
        uint256 paidAmount,
        uint256 discountAmount,
        address token,
        uint256 tokenId
    );

    event SaleReturn(
        bytes32 hash,
        address buyer,
        address token,
        uint256 tokenId,
        uint256 returnAmount
    );

    event ReturnRequest(
        bytes32 hash,
        address token,
        uint256 tokenId,
        address user,
        uint256 returnAmount
    );
    event PayRefRewards(
        bytes32 hash,
        SaleType saleType,
        address token,
        uint256 tokenId,
        address recipient,
        uint256 amount
    );

    constructor(address vaultWallet_, address fundAddress_)
        BaseINO(vaultWallet_, fundAddress_)
        EIP712("MetaLoka INO", "1")
    {
        _pause();
    }

    function buyWithReturnable(
        Order calldata order,
        Ref calldata ref,
        uint256 paidAmount
    ) external nonReentrant whenNotPaused {
        require(
            isSaleOpen(SaleType.WithReturnable),
            "Buy With Returnable: Not Open"
        );

        bytes32 orderHash = hashKey(nftAddress, order.tokenId, order.salt);
        require(saleInfo[orderHash].buyer == address(0), "Buy: Already sold");
        _requireValidOrder(order, SaleType.WithReturnable);

        uint256 discountAmount = 0;
        uint256 refRewards;
        if (ref.owner != address(0)) {
            _requireValidRef(ref);
            discountAmount += referDiscounts[order.rare];
            refRewards = (order.basePrice * referRewardRate) / MULTIPLIER;
        }

        require(
            order.basePrice - discountAmount == paidAmount,
            "Buy: Invalid paidAmount"
        );

        _saveSaleInfo(
            orderHash,
            order,
            paidAmount,
            discountAmount,
            SaleType.WithReturnable
        );

        _doTransfer(order.tokenId, paidAmount - refRewards);

        if (refRewards != 0) {
            _paidRefReward(
                orderHash,
                SaleType.WithReturnable,
                order.tokenId,
                refRewards,
                ref.owner
            );
        }

        emit INOSale(
            orderHash,
            SaleType.WithReturnable,
            _msgSender(),
            order.basePrice,
            paidAmount,
            discountAmount,
            nftAddress,
            order.tokenId
        );
    }

    function buyWithWhitelist(
        Order calldata order,
        uint256 paidAmount,
        bytes memory whitelistSig
    ) external nonReentrant whenNotPaused {
        bytes32 orderHash = hashKey(nftAddress, order.tokenId, order.salt);

        require(saleInfo[orderHash].buyer == address(0), "Buy: Already sold");
        require(!whitelistClaimed[_msgSender()], "Whitelist: Claimed");

        _requireValidOrder(order, SaleType.Whitelist);
        uint256 discountAmount = 0;
        require(
            order.basePrice - discountAmount == paidAmount,
            "Buy: Invalid paidAmount"
        );

        _requireValidWhitelist(_msgSender(), whitelistSig);

        // Save sale info
        _saveSaleInfo(
            orderHash,
            order,
            paidAmount,
            discountAmount,
            SaleType.Whitelist
        );

        // Mark claimed
        whitelistClaimed[_msgSender()] = true;

        _doTransfer(order.tokenId, paidAmount);

        emit INOSale(
            orderHash,
            SaleType.Whitelist,
            _msgSender(),
            order.basePrice,
            paidAmount,
            discountAmount,
            nftAddress,
            order.tokenId
        );
    }

    function _saveSaleInfo(
        bytes32 hash,
        Order calldata order,
        uint256 paidAmount,
        uint256 discountAmount,
        SaleType saleType
    ) internal {
        saleInfo[hash].soldAt = block.timestamp;
        saleInfo[hash].basePrice = order.basePrice;
        saleInfo[hash].paidAmount = paidAmount;
        saleInfo[hash].discountAmount = discountAmount;
        saleInfo[hash].buyer = _msgSender();
        saleInfo[hash].saleType = saleType;
    }

    function _doTransfer(uint256 tokenId, uint256 payAmount) internal {
        // User pay funds
        IERC20(saleToken).transferFrom(_msgSender(), fundReceiver, payAmount);

        // Perform transfer NFT to user
        IERC721(nftAddress).safeTransferFrom(
            vaultWallet,
            _msgSender(),
            tokenId
        );
    }

    function _paidRefReward(
        bytes32 hash,
        SaleType saleType,
        uint256 tokenId,
        uint256 payAmount,
        address recipient
    ) internal {
        IERC20(saleToken).transferFrom(_msgSender(), recipient, payAmount);

        emit PayRefRewards(
            hash,
            saleType,
            nftAddress,
            tokenId,
            recipient,
            payAmount
        );
    }

    function _requireValidOrder(Order memory order, SaleType saleType)
        internal
        view
    {
        bytes32 digest = _hashTypedDataV4(
            keccak256(
                abi.encode(
                    ORDER_TYPE_HASH,
                    saleType,
                    orderNonce,
                    order.salt,
                    nftAddress,
                    order.tokenId,
                    order.rare,
                    order.basePrice
                )
            )
        );

        require(
            SignatureChecker.isValidSignatureNow(
                adminSigner,
                digest,
                order.sig
            ),
            "Order: Signature is invalid"
        );
    }

    function _requireValidRef(Ref memory ref) internal view {
        require(ref.owner != _msgSender(), "You can not refer yourself");

        bytes32 digest = _hashTypedDataV4(
            keccak256(abi.encode(REF_TYPE_HASH, ref.owner, refNonce))
        );

        require(
            SignatureChecker.isValidSignatureNow(adminSigner, digest, ref.sig),
            "Ref: Signature is invalid"
        );
    }

    function _requireValidWhitelist(address user, bytes memory sig)
        internal
        view
    {
        bytes32 digest = _hashTypedDataV4(
            keccak256(abi.encode(WHITELIST_TYPE_HASH, user, whitelistNonce))
        );

        require(
            SignatureChecker.isValidSignatureNow(adminSigner, digest, sig),
            "Whitelist: Signature is invalid"
        );
    }

    function _requireValidReturn(
        address user,
        address token,
        uint256 tokenId,
        uint256 salt,
        bytes memory sig
    ) internal view {
        bytes32 digest = _hashTypedDataV4(
            keccak256(
                abi.encode(
                    RETURN_TYPE_HASH,
                    user,
                    returnNonce,
                    token,
                    tokenId,
                    salt
                )
            )
        );

        require(
            SignatureChecker.isValidSignatureNow(adminSigner, digest, sig),
            "Return: Signature is invalid"
        );
    }

    function validateReturnable(bytes32 hash, uint256 tokenId) internal view {
        require(saleInfo[hash].buyer != address(0), "Return: Not sold");
        require(!returnInfo[hash].returned, "Return: Already returned");
        require(
            _msgSender() == saleInfo[hash].buyer,
            "Return: Only buyer can return"
        );
        require(
            ITransferCounter(nftAddress).tranfersCount(tokenId) <= 2,
            "Return: Break the transfer rules"
        );
        require(
            IERC721(nftAddress).ownerOf(tokenId) == _msgSender(),
            "Return: Not owned NFT"
        );
    }

    function createReturnRequest(
        address token,
        uint256 tokenId,
        uint256 salt,
        uint256 expectedReturnAmount
    ) external nonReentrant whenNotPaused {
        bytes32 hash = hashKey(token, tokenId, salt);
        validateReturnable(hash, tokenId);
        require(
            returnInfo[hash].requestAt == 0,
            "Return: Already created return request"
        );

        returnInfo[hash].requestAt = block.timestamp;

        ReturnSetting memory setting = _matchReturnSetting(
            saleInfo[hash].soldAt
        );

        uint256 feeRate = setting.feeRate;

        if (saleInfo[hash].saleType == SaleType.Whitelist) {
            feeRate = 0;
        }

        uint256 base = saleInfo[hash].basePrice - saleInfo[hash].discountAmount;
        uint256 returnAmount = (base * setting.returnRate) /
            MULTIPLIER -
            (base * feeRate) /
            MULTIPLIER;

        require(
            returnAmount == expectedReturnAmount,
            "Return: Expected return value not match"
        );
        returnInfo[hash].returnAmount = returnAmount;

        emit ReturnRequest(
            hash,
            nftAddress,
            tokenId,
            _msgSender(),
            returnAmount
        );
    }

    function returnSale(
        address token,
        uint256 tokenId,
        uint256 salt,
        bytes memory sig
    ) external nonReentrant whenNotPaused {
        bytes32 hash = hashKey(token, tokenId, salt);
        validateReturnable(hash, tokenId);

        require(returnInfo[hash].requestAt > 0, "Return: Not requested");
        require(!returnInfo[hash].returned, "Return: Already returned");
        _requireValidReturn(_msgSender(), token, tokenId, salt, sig);

        // Save state
        returnInfo[hash].returned = true;

        // Send NFT to operator
        IERC721(nftAddress).safeTransferFrom(
            _msgSender(),
            fundReceiver,
            tokenId
        );

        // Return sale to buyer
        IERC20(saleToken).transferFrom(
            vaultWallet,
            _msgSender(),
            returnInfo[hash].returnAmount
        );

        emit SaleReturn(
            hash,
            _msgSender(),
            saleToken,
            tokenId,
            returnInfo[hash].returnAmount
        );
    }

    function hashKey(
        address token,
        uint256 tokenId,
        uint256 salt
    ) internal pure returns (bytes32) {
        return keccak256(abi.encodePacked(token, tokenId, salt));
    }
}

