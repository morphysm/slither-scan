pragma solidity 0.8.10;

import "@openzeppelin/contracts-upgradeable/token/ERC721/extensions/ERC721EnumerableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/access/AccessControlUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/PausableUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/security/ReentrancyGuardUpgradeable.sol";
import "@openzeppelin/contracts-upgradeable/interfaces/IERC20MetadataUpgradeable.sol";
import "./lib/Claimable.sol";
import "./lib/Math.sol";

interface I_ValiFiNFTv2 is IERC721Upgradeable {
    function tokenHoldings(address _owner)
        external
        view
        returns (uint256[] memory);
}

interface IYieldKey is I_ValiFiNFTv2 {
    function capacityAmount(uint256 tokenId) external view returns (uint256);
}

/**
 * @title Yield Box Contract / NFT Smart Contract ERC721
 * @dev Implementation of the Yield Box NFT ERC721.
 * @custom:a Alfredo Lopez / Marketingcycle / ValiFI
 */
contract YieldBox is
    Claimable,
    Math,
    ERC721EnumerableUpgradeable,
    PausableUpgradeable,
    AccessControlUpgradeable,
    ReentrancyGuardUpgradeable
{
    using SafeMathUpgradeable for uint256;
    using AddressUpgradeable for address;
    using SafeERC20Upgradeable for IERC20Upgradeable;
    // Interface of the Yield Key contract
    I_ValiFiNFTv2 private ValiFiNFTv2;
    IYieldKey private YieldKey;
    // Role Reward claimer
    bytes32 public constant ROLE_REWARD_CLAIMER =
        keccak256("ROLE_REWARD_CLAIMER");
    /** @dev Max Total Supply 2 million */
    uint256 private constant TOTAL_SUPPLY = ~(uint256(0)) >> 1;
    /** @dev Token Address ValiFI */
    address private tokenAddress;
    /** @dev Base Token URI */
    string private BASE_URI;
    /** @dev Liquidity Address VaLiFi */
    address private liquidity;
    /** @dev rewards Address VaLiFi */
    address private rewards;
    /** @dev treasuryAddress Address VaLiFi */
    address private treasuryAddress;
    /** @dev payrollFund Address VaLiFi */
    address private payrollFund;
    /** @dev Amount of Yield Box are minted in the Each Phase*/
    uint256 private _batchSize;
    /**
     * @dev Struct of the Yield Box
     */
    struct YieldBoxStruct {
        /** @dev Yield Box Status */
        bool status;
        /** @dev Time of Last Claim */
        uint64 rewardClaimed;
        /** @dev Yield Box Owner */
        address owner;
        /** @dev Yield Box Token ID */
        uint256 id;
        /** @dev Time of Creation */
        uint256 createdAt;
    }
    /**@dev Mapping of Yield Box by Token Id*/
    mapping(uint256 => YieldBoxStruct) public yieldBox;
    /**@dev Mapping of ERC20 token for create Yield Box's */
    mapping(address => uint256[5]) public amountCoin;
    /**
     * @dev Event of Yield Box Created
     * @param owner of the Yield Box
     * @param id ID of the Yield Box
     * @param createdAt of Creation the Yield Box
     */
    event YieldBoxCreated(
        address indexed owner,
        uint256 indexed id,
        uint256 createdAt
    );
    /**
     * @dev Event when change the BASE URL
     * @param baseURI new value of the Base URI
     */
    event SetBaseURI(string baseURI);
    /**
     * @dev Event when change the Cost per Token
     * @param tokenAddress address or ERC20 Token
     * @param costPerToken new value of the Cost per ERC20 Token
     */
    event SetCostPerToken(address tokenAddress, uint256[5] costPerToken);
    /**
     * @dev Event when change the Batch Size
     * @param batchSize new value of the Batch Size
     */
    event SetBatchSize(uint256 batchSize);
    /**
     * @dev Event when change the Token Address of VALIFi ERC20
     * @param tokenAddress new value of Token Address of VALIFi ERC20
     */
    event SetTokenAddress(address tokenAddress);

    /**
     * @dev Event when change the Project Address of Yield Box Smart Contract
     * @param projectAddress new value of Token Address of Yield Box Smart Contract
     */
    event SetProjectAddress(address[4] projectAddress);
    /**
     * @dev Event when change the Status of the Yield Box (Active/Inactive)
     * @param tokenId ID of the Yield Box
     * @param status new value of the Status of the Yield Box
     */
    event SetStatus(uint256 tokenId, bool status);
    /**
     * @dev Event when change the Status the Group of Yield Box (Active/Inactive)
     * @param tokenId ID of the Yield Box
     * @param status new value of the Status the Group of Yield Box
     */
    event SetGroupStatus(uint256[] tokenId, bool[] status);

    /**
     * @dev Inizialize the contract with the owner address.
     */
    function initialize(
        address _tokenAddress,
        string memory _baseTokenURI,
        address _ValiFiNFTv2,
        address _YieldKey,
        address[] memory _projectsAddress // [0] = Liquidity Address, [1] = Rewards Address, [2] = Treasury Address, [3] = Payroll Fund Address
    ) public initializer {
        require(
            _tokenAddress != address(0),
            "Token Address of VALIFi ERC20 is not valid"
        );
        __ERC721_init("Yield Box ValiFi", "YBV");
        __Ownable_init();
        __Pausable_init();
        __ReentrancyGuard_init();
        _setupRole(DEFAULT_ADMIN_ROLE, _msgSender());
        tokenAddress = _tokenAddress;
        BASE_URI = _baseTokenURI;
        // Token YieldKey
        ValiFiNFTv2 = I_ValiFiNFTv2(_ValiFiNFTv2);
        YieldKey = IYieldKey(_YieldKey);
        // VALIFI Wallets Address
        liquidity = _projectsAddress[0];
        rewards = _projectsAddress[1];
        treasuryAddress = _projectsAddress[2];
        payrollFund = _projectsAddress[3];
        _batchSize = uint256(1000);
    }

    /**
     * @dev This Method permit getting Maximun total Supply .
     * See {ERC721-_burn}.
     */
    function getMaxTotalSupply() public pure returns (uint256) {
        return TOTAL_SUPPLY;
    }

    /**
     * @dev Base URI for computing {tokenURI}. If set, the resulting URI for each
     * token will be the concatenation of the `baseURI` and the `tokenId`. Empty
     * by default, can be overriden in child contracts.
     */
    function _baseURI() internal view virtual override returns (string memory) {
        return BASE_URI;
    }

    /**
     * @dev Implementation / Instance of paused methods() in the ERC721.
     * @param status Setting the status boolean (True for paused, or False for unpaused)
     * See {ERC721Pausable}.
     */
    function pause(bool status) public onlyOwner {
        if (status) {
            _pause();
        } else {
            _unpause();
        }
    }

    /**
     * @dev Creates new tokens for `to`.
     * @param tokenId Token Id to mint
     *
     * See {ERC721-_mint}.
     *
     * Requirements:
     *
     * - the caller must have the `OWNER`.
     * - After upgrade the SmartContract and Eliminate this method
     */
    function safeMint(uint256 tokenId) public onlyOwner {
        require(
            getMaxTotalSupply() >= totalSupply().add(tokenId),
            "ERC721 VALIFi: Can't Mint, it exceeds the maximum supply "
        );
        super._safeMint(owner(), tokenId);
    }

    /**
     * @dev Create a new token for `to` and set the tokenId.
     * @param _stable The address of stableCoin.
     */
    function create(address _stable) external nonReentrant whenNotPaused {
        require(
            getMaxTotalSupply() >= totalSupply().add(uint256(1)),
            "ERC721 VALIFi: Can't Mint, it exceeds the maximum supply "
        );
        require(
            isValidToken(_stable),
            "ERC721 VALIFi: Token Stable Coin not permitted"
        );
        require(
            balanceOf(_msgSender()) <=
                getYieldCapacityAmountAllocated(_msgSender()),
            "ERC721 VALIFi: Don't have enough Slot to Create YielqBox"
        );
        require(
            _batchSize > uint256(0),
            "ERC721 VALIFi: can't create any YieldBox pls Try Later or Wait a new Batches"
        );
        /**
         * @dev Validate is the Creator send the correct amount of tokens and Avax.
         * @dev Config Wallet of VALIFi Foundation
         * @dev Liquidity Pool Wallet of VALIFi Foundation index 0
         * @dev Rewards Wallet of VALIFi Foundation index 1
         * @dev Treasury Wallet of VALIFi Foundation index 2
         * @dev Payroll Fund Wallet of VALIFi Foundation index 3
         * TODO: update the logic based on: https://valifi-app.gitbook.io/valifi-whitepaper/fundamentals/the-valifi-engine-back-end
         */
        uint256 _decimalStable = IERC20MetadataUpgradeable(_stable).decimals();
        uint256 _decimalToken = IERC20MetadataUpgradeable(tokenAddress)
            .decimals();
        IERC20Upgradeable _stableToken = IERC20Upgradeable(_stable);
        require(
            _stableToken.balanceOf(_msgSender()) >=
                (amountCoin[_stable][0] + amountCoin[_stable][1]) *
                    10**_decimalStable,
            "ERC721 VALIFi: Not enough Stable Coin"
        );
        // Transfer the Stable Coin to Create the Yield Box
        bool success = _stableToken.transferFrom(
            _msgSender(),
            treasuryAddress,
            amountCoin[_stable][0] * 10**_decimalStable // 4.5 VALI in Stable Coin
        );
        require(
            success,
            "ERC721 VALIFi: Can't create Yield Box, you don't have enough stable coins"
        );
        success = _stableToken.transferFrom(
            _msgSender(),
            liquidity,
            amountCoin[_stable][1] * 10**_decimalStable // 0.5 VALI in Stable Coin
        );
        require(
            success,
            "ERC721 VALIFi: Can't create Yield Box, you don't have enough stable coins"
        );

        IERC20Upgradeable _token = IERC20Upgradeable(tokenAddress);
        // Transfer the ValiFi Token to Create the Yield Box
        require(
            _token.balanceOf(_msgSender()) >=
                (amountCoin[_stable][2] +
                    amountCoin[_stable][3] +
                    amountCoin[_stable][4]) *
                    10**_decimalToken,
            "ERC721 VALIFi: Can't create Yield Box, you don't have enough ValiFI tokens"
        );
        success = _token.transferFrom(
            _msgSender(),
            rewards,
            amountCoin[_stable][2] * 10**_decimalToken // 4 VALI
        );
        require(
            success,
            "ERC721 VALIFi: Can't create Yield Box, you don't have enough ValiFI tokens"
        );
        success = _token.transferFrom(
            _msgSender(),
            liquidity,
            amountCoin[_stable][3] * 10**_decimalToken // 0.5 VALI
        );
        require(
            success,
            "ERC721 VALIFi: Can't create Yield Box, you don't have enough ValiFI tokens"
        );
        success = _token.transferFrom(
            _msgSender(),
            payrollFund,
            amountCoin[_stable][4] * 10**_decimalToken // 0.5 VALI
        );
        require(
            success,
            "ERC721 VALIFi: Can't create Yield Box, you don't have enough ValiFI tokens"
        );
        uint256 _tokenId = totalSupply();
        /** @dev Mint of BFT Yield Box*/

        super._safeMint(_msgSender(), _tokenId);
        _batchSize = _batchSize.sub(uint256(1));
        /**
         * @dev After Created, Setup the Yield Box Details
         */
        YieldBoxStruct memory _yieldBox = YieldBoxStruct(
            true,
            uint64(block.timestamp),
            _msgSender(),
            _tokenId,
            block.timestamp
        );
        yieldBox[_tokenId] = _yieldBox;
        emit YieldBoxCreated(_msgSender(), _tokenId, _yieldBox.createdAt);
    }

    /**
     * @dev See {ERC721-_beforeTokenTransfer}.
     *
     * Requirements:
     *
     * - the contract must not be paused.
     */
    function _beforeTokenTransfer(
        address from,
        address to,
        uint256 tokenId
    ) internal virtual override notBlacklisted(from) {
        require(
            !paused(),
            "ERC721Pausable: can't create any YieldBox while paused"
        );
        require(
            !isBlacklisted(to),
            "VE: can't transfer to blacklisted address"
        );
        if (from != owner()) {
            require(
                from == address(0),
                "ERC721 VALIFi: Can't transfer from a non-owner address"
            );
        }
        // In case the Owner Transfer an Yield Box
        yieldBox[tokenId].owner = to;

        super._beforeTokenTransfer(from, to, tokenId);
    }

    /**
     * @dev Helpers Funtions.
     */

    /**
     * @dev This Method permit getting the Token Address.
     * @return Token Address
     */
    function getTokenAddress() public view returns (address) {
        return tokenAddress;
    }

    /**
     * @dev This Method permit getting the Value in Stable Coin and Token ValiFI to create Yield Box.
     * @param _token Token Address of Stable Coin and Token ValiFI to create Yield Box
     * @return Value in Avax and Token ValiFI for create Yield Box
     */
    function getValue(address _token) public view returns (uint256[5] memory) {
        return amountCoin[_token];
    }

    /**
     * @dev This Method verify if Token Address is Valid to Create Yield Box.
     * @param _token Token Address of Stable Coin and Token ValiFI to create Yield Box
     * @return Status true if value is more then 0, or false if value is 0
     */
    function isValidToken(address _token) public view returns (bool) {
        return amountCoin[_token][0] > 0;
    }

    /**
     * @dev This Method permit set Base Token URI.
     */
    function setBaseURI(string memory baseURI_) public onlyOwner {
        BASE_URI = baseURI_;
        emit SetBaseURI(baseURI_);
    }

    /**
     * @dev This Method permit set the Value in Stable Coin and Token ValiFI for create Yield Box.
     * @param _token Token Address of Stable Coin in Avalanche Network, permitted to create Yield Box
     * @param _value Value in Avax and Token ValiFI for create Yield Box
     */
    function setValue(address _token, uint256[5] memory _value)
        external
        onlyOwner
    {
        require(
            _value[0] > 0 &&
                _value[1] > 0 &&
                _value[2] > 0 &&
                _value[3] > 0 &&
                _value[4] > 0,
            "ERC721 VALIFi: Can't set value, value must be greater than 0"
        );
        amountCoin[_token] = _value;
        emit SetCostPerToken(_token, _value);
    }

    /**
     * @dev This Method permit set the Project Address to Deposit the Yield Box Payments.
     * @param _projectAddress Value in Avax and Token ValiFI for create Yield Box
     */
    function setProjectAddress(address[4] memory _projectAddress)
        external
        onlyOwner
    {
        // [0] = Liquidity Address, [1] = Rewards Address, [2] = Treasury Address, [3] = Payroll Fund Address
        require(
            _projectAddress[0] != address(0) &&
                _projectAddress[1] != address(0) &&
                _projectAddress[2] != address(0) &&
                _projectAddress[3] != address(0),
            "ERC721 VALIFi: Can't set value, value must be greater than 0"
        );
        liquidity = _projectAddress[0];
        rewards = _projectAddress[1];
        treasuryAddress = _projectAddress[2];
        payrollFund = _projectAddress[3];
        emit SetProjectAddress(_projectAddress);
    }

    /**
     * @dev This Method permit to set the ERC20 Token Address of VALIFi Project.
     * @param _tokenAddress Token Address
     */
    function setTokenAddress(address _tokenAddress)
        external
        validAddress(_tokenAddress)
        onlyOwner
    {
        tokenAddress = _tokenAddress;
        emit SetTokenAddress(_tokenAddress);
    }

    /**
     * @dev This Method permit to get the Batch Size for Mint Yield Box.
     * @return _amount Amount of Yield Box to Mint per Batches
     */
    function getBatchSize() public view returns (uint256 _amount) {
        return _batchSize;
    }

    /**
     * @dev This Method permit to set the Batch Size for Mint Yield Box.
     * @param _amount Amount of Yield Box to Mint per Batches
     */
    function setBatchSize(uint256 _amount) external onlyOwner {
        _batchSize = _amount;
        emit SetBatchSize(_amount);
    }

    /**
     * @dev This Method permit to set rewards Claimed.
     * @param tokenId Token Id
     */
    function setRewardClaimed(bool status, uint256 tokenId)
        external
        onlyRole(ROLE_REWARD_CLAIMER)
    {
        yieldBox[tokenId].status = status;
        yieldBox[tokenId].rewardClaimed = uint64(block.timestamp);
    }

    function getYieldCapacityAmountAllocated(address _userAddress)
        public
        view
        returns (uint256 result)
    {
        uint256[] memory tokenIdsYK = YieldKey.tokenHoldings(_userAddress);
        uint256 tokenIdsLK = ValiFiNFTv2.balanceOf(_userAddress);
        result = tokenIdsLK * 12;
        // Validate YieldKey Random in Staking
        for (uint256 i = 0; i < tokenIdsYK.length; i++) {
            result += YieldKey.capacityAmount(tokenIdsYK[i]);
        }
    }

    function MulDiv(
        uint256 value,
        address token,
        uint256 mul,
        uint256 div
    ) public view returns (uint256) {
        return
            mulDiv(
                uint256(value).mul(
                    10**uint256(IERC20MetadataUpgradeable(token).decimals())
                ),
                mul,
                div
            );
    }

    /**
     * @dev Overloading previous method to Verify if the address is owner of one or several Token of NFT smart Contract, passed in the args
     * @param _owner address to owner of the tokens
     * @return tokenIds Array of token Ids of Yield Keys who the _owner address is the owner
     */
    function tokenHoldings(address _owner)
        public
        view
        returns (uint256[] memory)
    {
        uint256 ownerTokenCount = balanceOf(_owner);
        uint256[] memory tokenIds = new uint256[](ownerTokenCount);
        for (uint256 i; i < ownerTokenCount; i++) {
            tokenIds[i] = tokenOfOwnerByIndex(_owner, i);
        }
        return tokenIds;
    }

    /**
     * @dev Get Yield-Keys staked by an address
     * @param _wallet Wallet address of the owner
     * @return rewardedTokenIds Array of token Ids of Yield-Keys owned by the wallet
     */
    function yieldBoxRewarded(address _wallet)
        public
        view
        returns (uint256[] memory rewardedTokenIds)
    {
        uint256 index;
        uint256[] memory _tokenIds = tokenHoldings(_wallet);
        if (_tokenIds.length == 0) {
            return rewardedTokenIds;
        }
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            if (yieldBox[_tokenIds[i]].status) {
                index++;
            }
        }
        if (index == 0) {
            return rewardedTokenIds;
        }
        rewardedTokenIds = new uint256[](index);
        index = 0;
        for (uint256 i = 0; i < _tokenIds.length; i++) {
            if (yieldBox[_tokenIds[i]].status) {
                rewardedTokenIds[index] = _tokenIds[i];
                index++;
            }
        }
    }

    function supportsInterface(bytes4 interfaceId)
        public
        view
        override(AccessControlUpgradeable, ERC721EnumerableUpgradeable)
        returns (bool)
    {
        return
            interfaceId == type(IAccessControlUpgradeable).interfaceId ||
            interfaceId == type(IERC721Upgradeable).interfaceId ||
            interfaceId == type(IERC721EnumerableUpgradeable).interfaceId ||
            interfaceId == type(IERC721MetadataUpgradeable).interfaceId ||
            super.supportsInterface(interfaceId);
    }
}

