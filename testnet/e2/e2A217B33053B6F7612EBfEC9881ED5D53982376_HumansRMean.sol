                    notRandom(
                        uint256(
                            keccak256(
                                abi.encodePacked(
                                    (_nrandomSeed + 101) * i,
                                    (_tokenId + 202) * i,
                                    "lead"
                                )
                            )
                        )
                    ) % 4,
                    0
                );
            }
            if (i == 1) {
                _tokenStats[_tokenId].gather._value = Math.max(
                    notRandom(
                        uint256(
                            keccak256(
                                abi.encodePacked(
                                    (_nrandomSeed + 101) * i,
                                    (_tokenId + 202) * i,
                                    "gather"
                                )
                            )
                        )
                    ) % 4,
