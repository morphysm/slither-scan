pragma solidity ^0.8.2;















contract HumansRMean is Controlled, ReentrancyGuard {
    using SafeMath for uint256;

    // Properties
    NiceZombie public zombie;
    ZombieTool public tool;
    Parties public parties;
    Pricing public pricing;
    Bank public bank;

    // Constants Here
    uint256 private constant MINT_WITH_AVAX = 0;
    uint256 private constant MINT_WITH_NUG = 1;
    uint256 public constant TOOL_LIMIT = 70000;
    uint256 public constant BNUG = 0;
    uint256 public constant BCHEM = 1;

    // Events
    event MintZombie(address indexed sender, uint256 amount);
    event ZombieRerolled(uint256 _tokenId);

    function setup(
        NiceZombie _z,
        ZombieTool _t,
        Parties _parties,
        Pricing _pricing,
        Bank _bank
    ) public onlyOwner {
        zombie = _z;
        tool = _t;
        parties = _parties;
        pricing = _pricing;
        bank = _bank;
    }

    function balance() external view onlyController returns (uint256) {
        return address(this).balance;
    }

    function withdraw() external onlyController {
        payable(owner()).transfer(address(this).balance);
    }

    function mintZombies(uint256 amount, uint256 _paymentType)
        external
        payable
        whenNotPaused
        nonReentrant
    {
        require(amount > 0, "at least 1 zombie");
        require(
            _paymentType == MINT_WITH_AVAX || _paymentType == MINT_WITH_NUG,
            "Invalid Payment Type"
        );

        if (_paymentType == MINT_WITH_AVAX) {
            require(
                amount * pricing.zombiePriceAvax() == msg.value,
                "Invalid payment"
            );

            for (uint256 i = 0; i < amount; i++) {
                zombie.safeMint(_msgSender());
                bank.claim(_msgSender(), BCHEM, pricing.chemRewardPerMint());
            }

            emit MintZombie(_msgSender(), amount);
        }

        if (_paymentType == MINT_WITH_NUG) {
            require(pricing.acceptingNug(), "Nug purchases unavailable");
            require(
                bank.balanceOf(_msgSender(), BNUG) >=
                    amount * pricing.zombiePriceNug(),
                "Insufficient Nug"
            );
            bank.adminTransfer(
                _msgSender(),
                address(bank),
                BNUG,
                amount * pricing.zombiePriceNug()
            );
            for (uint256 i = 0; i < amount; i++) {
                zombie.safeMint(_msgSender());
                bank.claim(_msgSender(), BCHEM, pricing.chemRewardPerMint());
            }
            emit MintZombie(_msgSender(), amount);
        }
    }

    function rerollZombie(uint256 _tokenId)
        external
        payable
        whenNotPaused
        nonReentrant
    {
        require(zombie.ownerOf(_tokenId) == _msgSender(), "Not yours");
        uint256 price = pricing.rerollPrice();
        require(price == msg.value, "Invalid payment");
        zombie.rerollStats(_tokenId);
        emit ZombieRerolled(_tokenId);
    }

    function levelZombie(
        uint256 _tokenId,
        uint256 _l,
        uint256 _g,
        uint256 _s
    ) external payable whenNotPaused nonReentrant {
        require(pricing.acceptingChem(), "Chem purchases unavailable");
        require(zombie.ownerOf(_tokenId) == _msgSender(), "Not yours");

        uint256 price = pricing.levelZombiePrice() * (_l + _g + _s);
        (uint256 _cl, uint256 _cg, uint256 _cs) = zombie.getStats(_tokenId);
        require(
            _cl + _l <= 10 && _cg + _g <= 10 && _cs + _s <= 10,
            "Skill overflow"
        );
        require(
            bank.balanceOf(_msgSender(), BCHEM) >= price,
            "Insufficient Chem"
        );

        bank.adminTransfer(_msgSender(), address(bank), BCHEM, price);
        for (uint256 i = 0; i < 3; i++) {
            uint256 amount;
            if (i == 0) amount = _l;
            else if (i == 1) amount = _g;
            else if (i == 2) amount = _s;
            for (uint256 j = 0; j < amount; j++) {
                if (i == 0) zombie.addSkill(_tokenId, "lead");
                else if (i == 1) zombie.addSkill(_tokenId, "gather");
                else if (i == 2) zombie.addSkill(_tokenId, "support");
            }
        }
    }

    function mintTool(
        uint256 _l,
        uint256 _g,
        uint256 _s,
        string memory _label
    ) external payable whenNotPaused nonReentrant {
        require(tool.totalSupply() <= TOOL_LIMIT, "Sold out.");
        uint256 skillz = _l + _g + _s;
        uint256 price = pricing.toolPrice(skillz);

        // low level tools take avax
        if (skillz < 3) {
            require(price == msg.value, "Invalid payment");
            tool.safeMint(_msgSender(), _l, _g, _s, _label);
            // high level tools require nug
        } else {
            require(pricing.acceptingNug(), "Nug purchases unavailable");
            require(
                bank.balanceOf(_msgSender(), BNUG) >= price,
                "Insufficient Nug"
            );
            bank.adminTransfer(_msgSender(), address(bank), BNUG, price);
            tool.safeMint(_msgSender(), _l, _g, _s, _label);
        }
    }
}