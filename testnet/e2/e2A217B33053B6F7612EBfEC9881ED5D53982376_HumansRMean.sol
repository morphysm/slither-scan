        uint112 nug;
        uint112 wavax;
    }

    // Peer Contracts
    NiceZombie public zombie;
    address public nugPool;
    address public chemPool;
    TargetValue public tval;

    /**
     * Setup Methods
     */
    function setup(
        NiceZombie _zombie,
        uint256 _g0,
        uint256 _g3,
        uint256 _g0Price,
        uint256 _endprice
    ) public onlyController {
        zombie = _zombie;
        GEN0_TOTAL = _g0;
        GEN3_TOTAL = _g3;
        GEN0_PRICE = _g0Price;
        END_PRICE = _endprice;
    }

    function gens(
        uint256 _g0,
        uint256 _g3,
        uint256 _g0Price,
        uint256 _endprice
    ) public onlyController {
        GEN0_TOTAL = _g0;
        GEN3_TOTAL = _g3;
        GEN0_PRICE = _g0Price;
        END_PRICE = _endprice;
    }

    function _setTargetValue(uint112 nug, uint112 wavax) internal {
        require(wavax > 0 && nug > 0, "Invalid target values");
        tval = TargetValue(nug, wavax);
    }

    function setNugPool(address _nugPool) public onlyOwner {
        nugPool = _nugPool;
        (uint112 nug, uint112 wavax, ) = IPangolinPair(nugPool).getReserves();
        _setTargetValue(nug, wavax);
    }

    function setChemPool(address _chemPool) public onlyOwner {
        chemPool = _chemPool;
    }

    // Modifiers / Helpers
    function acceptingNug() public view returns (bool) {
        return nugPool != address(0);
    }

    modifier nugReady() {
        require(acceptingNug(), "Nug Pool Needed");
        _;
    }

    function acceptingChem() public view returns (bool) {
        return chemPool != address(0);
    }

    modifier chemReady() {
        require(acceptingChem(), "Chem Pool Needed");
        _;
    }

    function taxRate(
        uint112 inug,
        uint112 iwavax,
        uint112 cnug,
        uint112 cwavax
    ) public pure returns (uint256) {
        uint256 initial = inug / iwavax;
        uint256 current = cnug / cwavax;

        if (initial >= current) return 0;
        return (1000 * (current - initial)) / current;
    }

    function currentNugTax() public view nugReady returns (uint256) {
        (uint112 nug, uint112 wavax, ) = IPangolinPair(nugPool).getReserves();
        return taxRate(tval.nug, tval.wavax, nug, wavax);
    }

    function assessNugTax(uint256 amount) public view returns (uint256) {
        return amount - (amount * currentNugTax()) / 1000;
    }

    function nugDepositReward(uint256 loyalty, uint256 amount)
        public
        view
        returns (uint256)
    {
        return (loyalty * currentNugTax() * amount) / 200000;
    }

    function avaxToNug(uint256 avaxCost)
        public
        view
        nugReady
        returns (uint256)
    {
        (uint112 nugs, uint112 wavax, ) = IPangolinPair(nugPool).getReserves();

        require(nugs > 0 && wavax > 0, "Invalid Pool State");
        return quote(avaxCost, wavax, nugs);
    }

    function avaxToChem(uint256 avaxCost)
        public
        view
        chemReady
        returns (uint256)
    {
        (uint112 chems, uint112 wavax, ) = IPangolinPair(chemPool)
            .getReserves();

        require(chems > 0 && wavax > 0, "Invalid Pool State");
        return quote(avaxCost, wavax, chems);
    }

    function zombiePriceNug() public view returns (uint256) {
        return avaxToNug(zombiePriceAvax());
    }

    function zombiePriceChem() public view returns (uint256) {
        return avaxToChem(zombiePriceAvax());
    }

    function zombiePriceAvax() public view returns (uint256) {
        if (zombie.totalSupply() > GEN0_TOTAL) {
            return
                Math.max(
                    GEN0_PRICE -
                        (zombie.totalSupply() - GEN0_TOTAL) *
                        ((GEN0_PRICE - END_PRICE) / (GEN3_TOTAL - GEN0_TOTAL)),
                    END_PRICE
                );
        }

        return GEN0_PRICE;
    }

    function toolPrice(uint256 totalSkill) public view returns (uint256) {
        require(totalSkill <= 9, "Invalid tool");
        if (totalSkill >= 3) {
            return
                avaxToNug(
                    Math.min((zombiePriceAvax() * totalSkill) / 6, 3 ether)
                );
        }

        return (zombiePriceAvax() * totalSkill) / 9;
    }

    function chemRewardPerMint() public view returns (uint256) {
        if (zombie.totalSupply() <= GEN0_TOTAL) {
            return 1 ether;
        } else if (zombie.totalSupply() > GEN0_TOTAL) {
            uint256 chemReduction = ((zombie.totalSupply() - GEN0_TOTAL) *
                (1 ether / (GEN3_TOTAL - GEN0_TOTAL)));

            return 1 ether - Math.min(1 ether, Math.max(0, chemReduction));
        }

        return 0;
    }

    function rerollPrice() public view returns (uint256) {
        return zombiePriceAvax() / 6;
    }

    function levelZombiePrice() public view returns (uint256) {
        return avaxToChem(zombiePriceAvax() / 3);
    }
}


