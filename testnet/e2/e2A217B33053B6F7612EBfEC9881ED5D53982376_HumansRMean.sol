contract Parties is Controlled, ReentrancyGuard {
    using SafeMath for uint256;

    // Structures
    struct Party {
        uint256 leader;
        uint256[] members;
        uint256[] tools;
        uint256 stakeTime;
    }

    uint256 public gatherFactor = 1;

    // Properties
    NiceZombie private zombie;
    ZombieTool private tool;
    Bank private bank;

    mapping(address => Party[]) private parties;

    // Constants Here
    uint256 public constant BNUG = 0;
    uint256 public constant BCHEM = 1;

    // Events
    event PartyStaked(address indexed sender, Party party);
    event PartyUnstaked(address indexed sender, uint256 partyIndex);
    event NugsClaimed(
        address indexed sender,
        uint256 partyIndex,
        uint256 rewards,
        bool unstake
    );

    function setup(
        NiceZombie _z,
        ZombieTool _t,
        Bank _bank
    ) public onlyOwner {
        zombie = _z;
        tool = _t;
        bank = _bank;
    }

    function setGatherFactor(uint256 _g) public onlyController {
        require(_g >= 1, "Gather factor must be at least 1");
        gatherFactor = _g;
    }

    modifier partyExists(uint256 partyIndex) {
        require(
            partyIndex >= 0 && parties[_msgSender()].length > partyIndex,
            "No such party"
        );
        _;
    }

    function getParties() external view returns (Party[] memory) {
        return parties[_msgSender()];
    }

    function adminPartyTime(
        address partyOwner,
        uint256 partyIndex,
        uint256 adjustmentSeconds
    ) external onlyOwner {
        require(
            partyIndex >= 0 && parties[partyOwner].length > partyIndex,
            "No such party"
        );
        require(adjustmentSeconds <= 3600, "Too big of an adjustment");

        parties[partyOwner][partyIndex].stakeTime -= adjustmentSeconds;
    }

    function totalStats(uint256 partyIndex)
        public
        view
        partyExists(partyIndex)
        returns (
            uint256 totalGather,
            uint256 totalSupport,
            uint256 timestamp
        )
    {
        Party memory p = parties[_msgSender()][partyIndex];
        timestamp = p.stakeTime;
        (, totalGather, totalSupport) = zombie.getStats(p.leader);
        for (uint256 i = 0; i < p.members.length; i++) {
            (, uint256 _g, uint256 _s) = zombie.getStats(p.members[i]);
            totalGather += _g;
            totalSupport += _s;
        }
        for (uint256 i = 0; i < p.tools.length; i++) {
            (, uint256 _g, uint256 _s) = zombie.getStats(p.tools[i]);
            totalGather += _g;
            totalSupport += _s;
        }
    }

    function calcClaim(
        uint256 g,
        uint256 s,
        uint256 sec
    ) public pure returns (uint256) {
        uint256 t = (10**5 * sec) / 3600;

        // remove thousand factor at some point
        uint256 limit = (g * sqrt((10**10 * 4) / (1 + s)));

        if (t <= limit) {
            return ((t * t * (1 + s)) / g / 10**5) * (1 ether / 10**5);
        }

        return (5 * t + 4 * 10**5 * g - limit * 5) * (1 ether / 10**5);
    }

    function partyClaimable(uint256 partyIndex)
        public
        view
        partyExists(partyIndex)
        returns (uint256)
    {
        (uint256 g, uint256 s, uint256 ts) = totalStats(partyIndex);

        // solhint-disable-next-line not-rely-on-time
        return calcClaim(g, s, (block.timestamp - ts) / 1 seconds);
    }

    function stake(
        uint256 leader,
        uint256[] memory members,
        uint256[] memory tools
    ) external whenNotPaused nonReentrant {
        require(parties[_msgSender()].length < 6, "Limit 5 parties.");
        require(zombie.ownerOf(leader) == _msgSender(), "Not yours.");

        (uint256 _lead, , ) = zombie.getStats(leader);

        require(tools.length <= members.length, "One tool per zombie.");
        bool hasLeader = false;
        for (uint256 t = 0; t < tools.length; t++) {
            require(tool.ownerOf(tools[t]) == _msgSender(), "Not yours.");
            (uint256 _toolLead, , ) = tool.getStats(tools[t]);
            if (_toolLead > 0) {
                require(!hasLeader, "leadership already boosted");
                hasLeader = true;
                _lead += _toolLead;
            }

            tool.transferFromTo(_msgSender(), address(this), tools[t]);
        }

        require(_lead >= members.length, "Too many party members.");

        zombie.transferFromTo(_msgSender(), address(this), leader);
        for (uint256 z = 0; z < members.length; z++) {
            require(zombie.ownerOf(members[z]) == _msgSender(), "Not yours.");
            zombie.transferFromTo(_msgSender(), address(this), members[z]);
        }

        parties[_msgSender()].push(
            // solhint-disable-next-line not-rely-on-time
            Party(leader, members, tools, block.timestamp)
        );

        emit PartyStaked(
            _msgSender(),
            parties[_msgSender()][parties[_msgSender()].length - 1]
        );
    }

    function claim(uint256 partyIndex, bool unstake)
        external
        whenNotPaused
        nonReentrant
        partyExists(partyIndex)
    {
        // Get copy of the party claim
        uint256 getEmNugs = partyClaimable(partyIndex);
        require(getEmNugs > 0, "Nothing to claim yet");

        Party storage party = parties[_msgSender()][partyIndex];

        if (unstake) {
            zombie.transferFromTo(address(this), _msgSender(), party.leader);
            for (uint256 z = 0; z < party.members.length; z++) {
                zombie.transferFromTo(
                    address(this),
                    _msgSender(),
                    party.members[z]
                );
            }

            for (uint256 t = 0; t < party.tools.length; t++) {
                tool.transferFromTo(address(this), _msgSender(), party.tools[t]);
            }

            // Remove the party
            for (
                uint256 i = partyIndex;
                i < parties[_msgSender()].length - 1;
                i++
            ) {
                parties[_msgSender()][i] = parties[_msgSender()][i + 1];
            }
            parties[_msgSender()].pop();
            emit PartyUnstaked(_msgSender(), partyIndex);
        } else {
            // solhint-disable-next-line not-rely-on-time
            party.stakeTime = block.timestamp;
        }

        bank.claim(_msgSender(), BNUG, getEmNugs);
        emit NugsClaimed(_msgSender(), partyIndex, getEmNugs, unstake);
    }
}


