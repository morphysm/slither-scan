        return
            bytes(baseURI).length > 0
                ? string(
                    abi.encodePacked(
                        baseURI,
                        tokenId.toString(),
                        "/",
                        _tokenStats[tokenId].lead._value.toString(),
                        "/",
                        _tokenStats[tokenId].gather._value.toString(),
                        "/",
                        _tokenStats[tokenId].support._value.toString()
