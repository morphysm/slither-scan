contract Bank is ERC1155, Controlled, ReentrancyGuard {
    mapping(address => uint256) public loyaltyTimes;
    uint256 public penalty = 14 days;

    // Peer Contracts
    Pricing public pricing;
    Nug public nug;
    Chem public chem;

    // Constants Here
    uint256 public constant BNUG = 0;
    uint256 public constant BCHEM = 1;

    constructor() ERC1155("") {}

    /**
     * Setup Methods
     */
    function setup(
        Nug _nug,
        Chem _chem,
        Pricing _pricing,
        uint256 _p
    ) public onlyController {
        nug = _nug;
        chem = _chem;
        pricing = _pricing;
        penalty = _p * 1 days;
    }

    // Treasury
    function allBalance()
        external
        view
        onlyController
        returns (uint256, uint256)
    {
        return (
            balanceOf(address(this), BNUG),
            balanceOf(address(this), BCHEM)
        );
    }

    function adminWithdrawal() external onlyController {
        uint256 n = balanceOf(address(this), BNUG);
        uint256 c = balanceOf(address(this), BCHEM);

        if (n > 1 ether) {
            _burn(address(this), BNUG, n);
            nug.mint(_msgSender(), n);
        }

        if (c > 1 ether) {
            _burn(address(this), BCHEM, c);
            chem.mint(_msgSender(), c);
        }
    }

    // Modifiers
    function calcLoyalty(
        uint256 start,
        uint256 end,
        uint256 duration
    ) public pure returns (uint256) {
        return Math.max(Math.min((((end - start) * 100) / duration), 100), 0);
    }

    function loyaltyClaim(
        uint256 amount,
        uint256 id,
        uint256 loyalty
    ) public pure returns (uint256) {
        if (id == BNUG) {
            return ((100 + loyalty) * amount) / 200;
        } else if (id == BCHEM) {
            return (loyalty * amount) / 100;
        }

        return 0;
    }

    function _resetLoyalty(address _for) internal {
        // solhint-disable-next-line not-rely-on-time
        loyaltyTimes[_for] = block.timestamp - penalty;
    }

    function resetLoyalty(address _for) public onlyController {
        _resetLoyalty(_for);
    }

    function _loyaltyEvent(address _for) internal {
        // solhint-disable-next-line not-rely-on-time
        loyaltyTimes[_for] = block.timestamp;
    }

    function loyaltyEvent(address _for) public onlyController {
        _loyaltyEvent(_for);
    }

    function getLoyalty(address _of) public view returns (uint256) {
        if (loyaltyTimes[_of] > 0) {
            // solhint-disable-next-line not-rely-on-time
            return calcLoyalty(loyaltyTimes[_of], block.timestamp, penalty);
        }

        return 0;
    }

    function conditionalLoyaltyReset(address account) internal {
        if (loyaltyTimes[account] == 0) _resetLoyalty(account);
    }

    function claim(
        address account,
        uint256 id,
        uint256 amount
    ) public onlyController nonReentrant {
        conditionalLoyaltyReset(account);
        _mint(account, id, loyaltyClaim(amount, id, getLoyalty(account)), "");
    }

    function depositNug(uint256 amount) public nonReentrant {
        require(pricing.acceptingNug(), "Deposit not permitted yet.");
        require(nug.balanceOf(_msgSender()) >= amount, "Invalid Amount");

        // Deposity NUG and mint appropriate BNUG - Assess Deposit Reward
        nug.burnFrom(_msgSender(), amount);
        conditionalLoyaltyReset(_msgSender());
        _mint(
            _msgSender(),
            BNUG,
            amount + pricing.nugDepositReward(getLoyalty(_msgSender()), amount),
            ""
        );
    }

    function withdrawNug(uint256 amount) public nonReentrant {
        require(pricing.acceptingNug(), "Withdraw not permitted yet.");
        require(amount <= balanceOf(_msgSender(), BNUG), "Invalid Amount");

        // Ding to loyalty
        _loyaltyEvent(_msgSender());

        // Burn BNUG and mint appropriate NUG - Assess Tax
        _burn(_msgSender(), BNUG, amount);
        nug.mint(_msgSender(), pricing.assessNugTax(amount));
    }

    function depositChem(uint256 amount) public nonReentrant {
        require(pricing.acceptingChem(), "Deposit not permitted yet.");
        require(chem.balanceOf(_msgSender()) >= amount, "Invalid Amount");

        // Deposity NUG and mint appropriate BCHEM
        chem.burnFrom(_msgSender(), amount);
        _mint(_msgSender(), BCHEM, amount, "");
    }

    function withdrawChem(uint256 amount) public nonReentrant {
        require(pricing.acceptingChem(), "Withdraw not permitted yet.");
        require(amount <= balanceOf(_msgSender(), BCHEM), "Invalid Amount");

        // Ding to loyalty
        _loyaltyEvent(_msgSender());

        // Burn BCHEM and mint appropriate NUG
        _burn(_msgSender(), BCHEM, amount);
        chem.mint(_msgSender(), amount);
    }

    function adminMint(
        address account,
        uint256 id,
        uint256 amount
    ) public onlyController {
        _mint(account, id, amount, "");
    }

    function adminBurn(
        address account,
        uint256 id,
        uint256 value
    ) public onlyController {
        _burn(account, id, value);
    }

    function adminTransfer(
        address from,
        address to,
        uint256 id,
        uint256 amount
    ) public onlyController {
        _safeTransferFrom(from, to, id, amount, "");
    }

    /* solhint-disabled */
    function onERC1155Received(
        address operator,
        address from,
        uint256 id,
        uint256 value,
        bytes calldata data
    ) external pure returns (bytes4) {
        return
            bytes4(
                keccak256(
                    "onERC1155Received(address,address,uint256,uint256,bytes)"
                )
            );
    }

    function safeTransferFrom(
        address from,
        address to,
        uint256 id,
        uint256 amount,
        bytes memory data
    ) public override onlyController {
       return super.safeTransferFrom(from, to, id, amount, data);
    }

    function safeBatchTransferFrom(
        address from,
        address to,
        uint256[] memory ids,
        uint256[] memory amounts,
        bytes memory data
    ) public override onlyController {
       return super.safeBatchTransferFrom(from, to, ids, amounts, data);
    }
    /* solhint-enable */
}


